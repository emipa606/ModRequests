<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>ETE_AssembleFood</defName>
		<label>assemble a bland meal</label>
		<description>Use organic matter to somehow make a tasteless yet edible meal.</description>
		<jobString>Assembling food.</jobString>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>2500</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ETE_OrganicMatter</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
		<products>
			<ETE_MealBland>1</ETE_MealBland>
		</products>
	</RecipeDef>
	
