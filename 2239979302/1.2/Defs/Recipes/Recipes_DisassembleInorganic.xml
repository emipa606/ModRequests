<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>ETE_DisassembleInorganic</defName>
		<label>disassemble inorganics</label>
    <description>Break down inorganic items into inorganic matter.</description>
    <jobString>Disassembling inorganics.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>1500</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<ingredients>
      <li>
        <filter>
          <categories>
            <li>ResourcesRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
		<fixedIngredientFilter>
      <categories>
        <li>ResourcesRaw</li>
      </categories>
			<disallowedCategories>
				<li>Manufactured</li>
				<li>PlantMatter</li>
				<li>Textiles</li>
				<li>Drugs</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>Chemfuel</li>
				<li>Wort</li>
				<li>WoodLog</li>
				<li>MedicineHerbal</li>
				<li>ETE_InorganicMatter</li>
				<li>ETE_OrganicMatter</li>
			</disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <ETE_InorganicMatter>1</ETE_InorganicMatter>
    </products>
	</RecipeDef>
</Defs>