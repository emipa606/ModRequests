<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
    	<success>Always</success>
		<mods>
			<li>Orassans</li>
			<li>Orassans Lite</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<success>Always</success>
			<mods>
				<li>Royalty</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="Underlay_CarpetFineBase">
    						<texturePath>Terrain/CatLavenderCarpet</texturePath>
    						<costList>
    							<Cloth>0</Cloth>
      							<CatLavenderCloth>35</CatLavenderCloth>
    						</costList>
    						<designatorDropdown>Floor_Carpet_Fine</designatorDropdown>
							<statBases>
								<Beauty>5</Beauty>
								<Flammability>0.32</Flammability>
								<WorkToBuild>5000</WorkToBuild>
    						</statBases>
    						<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    						<resourcesFractionWhenDeconstructed>0.15</resourcesFractionWhenDeconstructed>
        					<defName>CarpetCatLilyArmored</defName>
    						<label>armored fine cat lily carpet</label>
    						<renderPrecedence>199</renderPrecedence>
							<description>Soft and silky cat lavender carpet, for that extra sense of pomp with armored underlay.</description>
						</TerrainDef>
					</value>
				</li>
				</operations>
			</match>
		</match>
    </Operation>
</Patch>