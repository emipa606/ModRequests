<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
    	<success>Always</success>
		<mods>
        	<li>Biomes! Islands</li>
        </mods>
        <match Class="PatchOperationSequence">
  		<operations>
			<li Class="PatchOperationAdd">
             	<xpath>/Defs</xpath>
				<value>
					<TerrainDef ParentName="Underlay_TileStoneBase">
						<defName>BiomesIslands_TileCoralArmored</defName>
						<label>armored coral tile</label>
						<color>(250,180,147)</color>
						<costList>
							<BiomesIslands_BlocksCoral>4</BiomesIslands_BlocksCoral>
						</costList>
					</TerrainDef>
  			 	</value>
			</li>
			<li Class="PatchOperationAdd">
             	<xpath>/Defs</xpath>
				<value>
					<TerrainDef ParentName="Underlay_FlagstoneBase">
						<defName>BiomesIslands_FlagstoneCoral</defName>
						<label>armored coral flagstone</label>
						<color>(250,180,147)</color>
						<costList>
							<BiomesIslands_BlocksCoral>4</BiomesIslands_BlocksCoral>
						</costList>
					</TerrainDef>
  			 	</value>
			</li>
			</operations>
		</match>
    </Operation>
</Patch>