<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>XFMParmor_ModuleSupply</defName>
    <label>field supply ration</label>
    <description>Prefabricated, packaged food and hormone that fulfills pilot's sleeping and eating needs. Though this is not a complete substitute of actual bed rest.</description>
    <graphicData>
      <texPath>XFMParmor/Items/ModuleSupply</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>700</MaxHitPoints>
      <MarketValue>20</MarketValue>
      <Mass>0.1</Mass>
      <Flammability>0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
</Defs>