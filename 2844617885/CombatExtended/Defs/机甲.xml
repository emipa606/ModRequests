<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <MParmorLibrary.MechanicalArmorDef>
    <defName>MechanicalArmor_Black</defName>
	  <label>crow</label>
	  <description>placeholder.</description>
	  <machine>4800</machine>
    <shield>1200</shield>
    <shieldBrokenCD>480</shieldBrokenCD>
    <shieldRecoveryCD>250</shieldRecoveryCD>
    <shieldRecovery>3</shieldRecovery>
    <moveSpeed>4.5</moveSpeed>
    <core>XFMParmor_MechanicalArmorCore_Black</core>
    <weapon><li>XFMParmor_Weapon_Black</li></weapon>
    <wreckage>XFMParmor_Wreckage_Black</wreckage>
    <frontGraphic>
      <texPath>XFMParmor/MParmor/Black/front</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>Cutout</shaderType>
      <drawSize>(3,3)</drawSize>
    </frontGraphic>
    <wreckageGraphic>
      <texPath>XFMParmor/MParmor/Black/wreckage</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>Cutout</shaderType>
      <drawSize>(3,3)</drawSize>
    </wreckageGraphic>
    <costList>
      <XFMParmor_PartEnergy>1</XFMParmor_PartEnergy>
      <XFMParmor_PartControl>1</XFMParmor_PartControl>
      <XFMParmor_PartWeapon>1</XFMParmor_PartWeapon>
      <XFMParmor_PartArmor>2</XFMParmor_PartArmor>
      <XFMParmor_PartShield>2</XFMParmor_PartShield>
    </costList>
    <researchPrerequisites>
      <li>XFMParmor_Prototype</li>
    </researchPrerequisites>
  </MParmorLibrary.MechanicalArmorDef>

