  <!--=============================== Eggs ================================-->
  <!--=====================================================================-->


  <!-- Raptor -->

  <ThingDef ParentName="EggFertBase">
    <defName>EggWoWRaptorFertilized</defName>
    <label>raptor egg (fert.)</label>
    <description>A fertilized chicken egg. If all goes well, it should hatch into a raptor juvenile. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(245,245,245)</color>
    </graphicData>
    <statBases>
      <MarketValue>250.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>30</hatcherDaystoHatch>
        <hatcherPawn>WoW_Animal_Raptor</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

