      <combatPower>250</combatPower>
      <wildGroupSize>
         <min>1</min>
         <max>3</max>
      </wildGroupSize>
      <alternateGraphicChance>0.9</alternateGraphicChance>
      <alternateGraphics>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Black</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/BlackSpotted</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/BlackStriped</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/BlackStripedLight</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Brown</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Dark</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Dawn</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Gold</texPath>
