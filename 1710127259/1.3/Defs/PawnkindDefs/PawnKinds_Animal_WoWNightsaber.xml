            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Moon</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Red</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Storm</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Yellow</texPath>
         </li>
      </alternateGraphics>
      <lifeStages>
         <li>
            <label>nightsaber kitten</label>
            <labelPlural>nightsaber kittens</labelPlural>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Mist</texPath>
               <drawSize>1.5</drawSize>
               <shadowData>
                  <volume>(0.2, 0.1, 0.2)</volume>
                  <offset>(0,0,-0.2)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cougar/Dessicated_Cougar</texPath>
               <drawSize>1.5</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Mist</texPath>
               <drawSize>2</drawSize>
               <shadowData>
                  <volume>(0.3, 0.3, 0.3)</volume>
                  <offset>(0,0,-0.3)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cougar/Dessicated_Cougar</texPath>
               <drawSize>2.25</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Nightsaber/Nightsaber/Mist</texPath>
               <drawSize>3</drawSize>
               <shadowData>
                  <volume>(0.4, 0.4, 0.4)</volume>
                  <offset>(0,0,-0.2)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cougar/Dessicated_Cougar</texPath>
               <drawSize>3</drawSize>
            </dessicatedBodyGraphicData>
         </li>
      </lifeStages>
   </PawnKindDef>
	
