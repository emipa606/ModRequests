   <PawnKindDef ParentName="AnimalKindBase">
      <defName>WoW_Animal_Hawkstrider</defName>
      <label>hawkstrider</label>
      <race>WoW_Animal_Hawkstrider</race>
      <combatPower>100</combatPower>
      <wildGroupSize>
         <min>1</min>
         <max>3</max>
      </wildGroupSize>
      <alternateGraphicChance>0.86</alternateGraphicChance>
      <alternateGraphics>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Black</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Blue</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Crimson</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Dark</texPath>
