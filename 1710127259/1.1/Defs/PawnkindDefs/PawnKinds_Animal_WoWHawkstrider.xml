            <texPath>Things/WoWAnimals/Hawkstrider/Green</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Purple</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/Smoky</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/Hawkstrider/White</texPath>
         </li>
      </alternateGraphics>
      <lifeStages>
         <li>
            <label>hawkstrider chick</label>
            <labelPlural>hawkstrider chicks</labelPlural>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Hawkstrider/Red</texPath>
               <drawSize>1</drawSize>
               <shadowData>
                  <volume>(0.2, 0.1, 0.2)</volume>
                  <offset>(0,0,-0.2)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Ostrich/Dessicated_Ostrich</texPath>
               <drawSize>1.5</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Hawkstrider/Red</texPath>
               <drawSize>1.5</drawSize>
               <shadowData>
                  <volume>(0.3, 0.3, 0.3)</volume>
                  <offset>(0,0,-0.3)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Ostrich/Dessicated_Ostrich</texPath>
               <drawSize>2.25</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/Hawkstrider/Red</texPath>
               <drawSize>2.5</drawSize>
               <shadowData>
                  <volume>(0.4, 0.4, 0.4)</volume>
