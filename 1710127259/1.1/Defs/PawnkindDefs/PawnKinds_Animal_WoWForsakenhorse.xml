<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   
   <PawnKindDef ParentName="AnimalKindBase">
      <defName>WoW_Animal_Forsakenhorse</defName>
      <label>skeletal horse</label>
      <race>WoW_Animal_Forsakenhorse</race>
      <combatPower>75</combatPower>
      <labelFemale>skeletal horse mare</labelFemale>
      <ecoSystemWeight>0.35</ecoSystemWeight>
      <wildGroupSize>
         <min>2</min>
         <max>6</max>
      </wildGroupSize>
      <alternateGraphicChance>0.8</alternateGraphicChance>
      <alternateGraphics>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Blue</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Green</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Grey</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Purple</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Red</texPath>
         </li>
         <li>
            <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Yellow</texPath>
         </li>
      </alternateGraphics>
      <lifeStages>
         <li>
            <label>skeletal horse foal</label>
            <labelPlural>skeletal horse foals</labelPlural>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Black</texPath>
               <drawSize>1.5</drawSize>
               <shadowData>
                  <volume>(0.2, 0.1, 0.2)</volume>
                  <offset>(0,0,-0.2)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cow/Dessicated_Cow</texPath>
               <drawSize>1.5</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Black</texPath>
               <drawSize>2</drawSize>
               <shadowData>
                  <volume>(0.3, 0.3, 0.3)</volume>
                  <offset>(0,0,-0.3)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cow/Dessicated_Cow</texPath>
               <drawSize>2.25</drawSize>
            </dessicatedBodyGraphicData>
         </li>
         <li>
            <bodyGraphicData>
               <texPath>Things/WoWAnimals/ForsakenHorse/Normal/Black</texPath>
               <drawSize>3</drawSize>
               <shadowData>
                  <volume>(0.4, 0.4, 0.4)</volume>
                  <offset>(0,0,-0.2)</offset>
               </shadowData>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
               <texPath>Things/Pawn/Animal/Cow/Dessicated_Cow</texPath>
               <drawSize>3</drawSize>
            </dessicatedBodyGraphicData>
         </li>
      </lifeStages>
   </PawnKindDef>
	
