<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<ThingDef Name="SEX_PortalBase" Abstract="True">
		<altitudeLayer>Building</altitudeLayer>
		<drawGUIOverlay>true</drawGUIOverlay>
		<useHitPoints>false</useHitPoints>
		<tickerType>Normal</tickerType>
		<passability>PassThroughOnly</passability>
		<selectable>true</selectable>
		<category>Building</category>
		<receivesSignals>true</receivesSignals>
		<rotatable>false</rotatable>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
			<deconstructible>false</deconstructible>
			<claimable>false</claimable>
			<expandHomeArea>false</expandHomeArea>
			<alwaysDeconstructible>false</alwaysDeconstructible>
		</building>
		<pathCost>50</pathCost>
	</ThingDef>
	<ThingDef ParentName="SEX_PortalBase" Name="SEX_PortalBase_SalvagePoint" Abstract="True">
		<category>Building</category>
		<thingClass>SexWithHighmateHarem.Lootbox</thingClass>
		<useHitPoints>true</useHitPoints>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
		</statBases>
		<pathCost>40</pathCost>
		<fillPercent>0.40</fillPercent>
	</ThingDef>
	<ThingDef ParentName="SEX_PortalBase_SalvagePoint" Name="SEX_PortalBase_SalvagePoint_Unloadable" Abstract="True">
		<minifiedDef IsNull="True"/>
		<building>
			<deconstructible>true</deconstructible>
			<claimable>false</claimable>
		</building>
		<pathCost>20</pathCost>
		<fillPercent>0.20</fillPercent>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<Mass>35</Mass>
			<MarketValue>10</MarketValue>
		</statBases>
		<tradeTags>
			<li>SEX_SalvagePoint</li>
		</tradeTags>
	</ThingDef>
	<ThingDef ParentName="SEX_PortalBase_SalvagePoint_Unloadable" Name="SEX_Case" Abstract="True">
		<tradeTags>
			<li>SEX_SalvageCase</li>
		</tradeTags>
	</ThingDef>
	<ThingDef ParentName="SEX_PortalBase_SalvagePoint_Unloadable">
		<defName>SEX_JunkyardResourceCrateVertical</defName>
		<label>warehouse crate</label>
		<description>A sturdy warehouse crate. Can be cracked open to reveal the loot inside.</description>
		<graphicData>
			<graphicClass>Graphic_Random</graphicClass>
			<texPath>Things/Buildings/Lootable/SEX_ResourceCrateVertical</texPath>
			<drawSize>(1,2)</drawSize>
		</graphicData>
		<size>(1,2)</size>
		<modExtensions>
			<li Class="SexWithHighmateHarem.SEX_LootableBuildingsJunkyard">
				<sound>AncientCommsConsoleActivated</sound>
				<openedGraphicdata>
					<graphicClass>Graphic_Random</graphicClass>
					<texPath>Things/Buildings/Lootable/SEX_ResourceCrateVerticalOpen</texPath>
					<drawSize>(1,2)</drawSize>
				</openedGraphicdata>
				<loots>
					<li>SEX_JunkyardResources</li>
				</loots>
			</li>
		</modExtensions>
	</ThingDef>
