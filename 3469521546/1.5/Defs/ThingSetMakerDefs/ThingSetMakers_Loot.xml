<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingSetMakerDef>
		<defName>SEX_JunkyardResources</defName>
		<root Class="ThingSetMaker_RandomOption">
			<options>
				<li>
					<weight>1</weight>
					<thingSetMaker Class="ThingSetMaker_StackCount">
						<fixedParams>
							<filter>
								<thingDefs>
									<li>Uranium</li>
									<li>Plasteel</li>
									<li>Steel</li>
									<li>WoodLog</li>
									<li>Chemfuel</li>
								</thingDefs>
							</filter>
							<countRange>12~55</countRange>
						</fixedParams>
					</thingSetMaker>
				</li>
			</options>
		</root>
	</ThingSetMakerDef>
	<ThingSetMakerDef>
		<defName>SEX_JunkyardComponents</defName>
		<root Class="ThingSetMaker_RandomOption">
			<options>
				<li>
					<weight>0.5</weight>
					<thingSetMaker Class="ThingSetMaker_StackCount">
						<fixedParams>
							<filter>
								<thingDefs>
									<li>ComponentIndustrial</li>
									<li>VVE_CarExhaust</li>
									<li>VVE_CarSuspension</li>
									<li>VVE_CarBattery</li>
									<li>VVE_CarWiring</li>
									<li>VVE_CarRadiator</li>
								</thingDefs>
							</filter>
							<countRange>1~3</countRange>
						</fixedParams>
					</thingSetMaker>
				</li>
				<li>
					<weight>0.5</weight>
					<thingSetMaker Class="ThingSetMaker_StackCount">
						<fixedParams>
							<filter>
								<thingDefs>
									<li>VVE_EngineBlock</li>
									<li>VVE_CarAlternator</li>
									<li>VVE_CarWheel</li>
									<li>VVE_CarFuelTank</li>
									<li>VVE_CarTransmission</li>
								</thingDefs>
							</filter>
							<countRange>1~1</countRange>
						</fixedParams>
					</thingSetMaker>
				</li>
			</options>
		</root>
	</ThingSetMakerDef>
</Defs>