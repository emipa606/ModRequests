<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[SYR] Trait Value</li> <!-- Checks for this mod, if not active there is no reason to add values -->
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>*/TraitDef[defName="NanoImmune"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>200</li>
              </traitValues>
            </li>
          </value>
        </li>

        <li Class="PatchOperationAddModExtension">
          <xpath>*/TraitDef[defName="Unbreakable"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
