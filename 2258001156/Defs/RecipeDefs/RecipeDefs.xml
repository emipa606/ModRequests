<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<RecipeDef>
		<defName>MakeNBlock</defName>
		<label>process nutrient solution into a block</label>
		<description>Process nutrient solution(20) into nutrient solution blocks(20).</description>
		<jobString>Processing nutrient solution into blocks.</jobString>
		<workAmount>4800</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>NSolution</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<products>
			<NBlock>10</NBlock>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>NSolution</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
		<researchPrerequisites>
			<li>TofuProduction</li>
			<li>AdvRoboticAnimals</li>
		</researchPrerequisites>
	</RecipeDef>
	<RecipeDef>
		<defName>Make_ChemfuelFromNBlock</defName>
		<label>make chemfuel from nutrient blocks</label>
		<description>Make a batch of chemfuel by subjecting nutrient blocks to intense heat.</description>
		<jobString>Refining chemfuel from nutrient blocks.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>5000</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<skillRequirements>
			<Crafting>9</Crafting>
			<Intellectual>10</Intellectual>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>NBlock</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>NBlock</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Chemfuel>15</Chemfuel>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
		<researchPrerequisite>
			<li>AdvEnhancement</li>
		</researchPrerequisite>
	</RecipeDef>
	<!--FractalPill-->
	<RecipeDef>
		<defName>Make_FractalFromNBlock</defName>
		<label>make a fractal pill from compressed nutrient blocks</label>
		<description>Using once forgotten techniques you combine an exorbitant amount of nutrient blocks and chemical eggs and refine them down to make a single fractal pill by refining nutrient blocks.</description>
		<jobString>Refining Fractal Pill.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>150000</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<skillRequirements>
			<Crafting>18</Crafting>
			<Intellectual>18</Intellectual>
			<Medicine>18</Medicine>
			<Artistic>18</Artistic>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>NBlock</li>
					</thingDefs>
				</filter>
				<count>300</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ChemicalEgg</li>
					</thingDefs>
				</filter>
				<count>150</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ChemicalEgg</li>
				<li>NBlock</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<FractalPill>1</FractalPill>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>T5Research</li>
		</researchPrerequisites>
	</RecipeDef>
	<!--Steel Weave-->
	<RecipeDef>
		<defName>ProcessSteelWool</defName>
		<label>weave steel wool</label>
		<description>Weave raw steel wool into steel weave using a chemical base derived from Nutrient Block and Chemical Eggs.</description>
		<jobString>Refining steel weave.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workSkill>Crafting</workSkill>
		<workAmount>1800</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>NBlock</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ChemicalEgg</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SteelWool</li>
					</thingDefs>
				</filter>
				<count>75</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>NBlock</li>
				<li>ChemicalEgg</li>
				<li>SteelWool</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<SteelWeave>50</SteelWeave>
		</products>
		<skillRequirements>
			<Crafting>15</Crafting>
			<Intellectual>15</Intellectual>
		</skillRequirements>
		<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>T5Research</li>
		</researchPrerequisites>
	</RecipeDef>
	<!--Mapo mapo-->
	<RecipeDef ParentName="CookMealBase">
		<defName>MakeMapoNBlock</defName>
		<label>cook mapo nutrient block (lavish)</label>
		<description>Cook mapo nutrient block, an incredibly spicy lavish meal. Requires meat, raw or canned nutrient block, and spiced nutrient block.</description>
		<jobString>Cooking mapo nutrient block.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedCategories>
				<li>PlantFoodRaw</li>
				<li>VCE_Condiments</li>
				<li>AnimalProductRaw</li>
				<li>VCEF_RawFishCategory</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				<li>NBlock</li>
				<li>CannedNBlock</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>SpicedNBlock</li>
				<li>NBlock</li>
				<li>CannedNBlock</li>
			</thingDefs>
		</defaultIngredientFilter>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
					<disallowedThingDefs>
						<li>NBlock</li>
						<li>CannedNBlock</li>
					</disallowedThingDefs>
				</filter>
				<count>.5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>NBlock</li>
						<li>CannedNBlock</li>
					</thingDefs>
				</filter>
				<count>.5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SpicedNBlock</li>
					</thingDefs>
				</filter>
				<count>1.0</count>
			</li>
		</ingredients>
		<products>
			<MapoNBlock>1</MapoNBlock>
		</products>
		<skillRequirements>
			<Cooking>8</Cooking>
		</skillRequirements>
						<researchPrerequisites>
							<li>TofuProduction</li>
							<li>AdvRoboticAnimals</li>
						</researchPrerequisites>
	</RecipeDef>
	<RecipeDef ParentName="CookMealBase">
		<defName>MakeMapoNBlockBulk</defName>
		<label>cook mapo nutrient block (lavish) x4</label>
		<description>Cook mapo nutrient block, an incredibly spicy lavish meal. Requires meat, raw or canned nutrient block, and spiced nutrient block.</description>
		<jobString>Cooking mapo nutrient block.</jobString>
		<workAmount>3200</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedCategories>
				<li>PlantFoodRaw</li>
				<li>VCE_Condiments</li>
				<li>AnimalProductRaw</li>
				<li>VCEF_RawFishCategory</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				<li>NBlock</li>
				<li>CannedNBlock</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>SpicedNBlock</li>
				<li>NBlock</li>
				<li>CannedNBlock</li>
			</thingDefs>
		</defaultIngredientFilter>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
					<disallowedThingDefs>
						<li>NBlock</li>
						<li>CannedNBlock</li>
					</disallowedThingDefs>
				</filter>
				<count>2.0</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>NBlock</li>
						<li>CannedNBlock</li>
					</thingDefs>
				</filter>
				<count>2.0</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SpicedNBlock</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<products>
			<MapoNBlock>4</MapoNBlock>
		</products>
		<skillRequirements>
			<Cooking>8</Cooking>
		</skillRequirements>
		<researchPrerequisites>
			<li>TofuProduction</li>
			<li>AdvEnhancement</li>
		</researchPrerequisites>
	</RecipeDef>
</Defs>