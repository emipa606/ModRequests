<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Clone Force 99 | Crosshair -->
  <ThingDef ParentName="ArmorMachineableBase">
		<defName>CF99_Crosshair</defName>
		<label>Clone Force 99 Crosshair Armor</label>
		<description>Armour from Clone Force 99 Crosshair.</description>
		<thingClass>D9ASB.RangedShieldBelt</thingClass>
		<tickerType>Normal</tickerType>
		<techLevel>Spacer</techLevel>
		<tradeability>Sellable</tradeability>
		<generateCommonality>0</generateCommonality>
		<generateAllowChance>0</generateAllowChance>
	<graphicData>
      <texPath>Things/Armor/Crosshair/CF99_Crosshair</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>	
    <statBases>
		<WorkToMake>36000</WorkToMake>
		<MaxHitPoints>300</MaxHitPoints>
		<Mass>12.5</Mass>
		<Flammability>0.1</Flammability>
		<ArmorRating_Sharp>0.99</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.30</ArmorRating_Blunt>
		<ArmorRating_Heat>0.90</ArmorRating_Heat>
		<Insulation_Cold>30</Insulation_Cold>
		<Insulation_Heat>20</Insulation_Heat>
		<EquipDelay>2.5</EquipDelay>
		<EnergyShieldRechargeRate>0.07</EnergyShieldRechargeRate>
		<EnergyShieldEnergyMax>1.10</EnergyShieldEnergyMax>
    </statBases>
    <equippedStatOffsets>
		<MoveSpeed>-0.15</MoveSpeed>
	</equippedStatOffsets>
    <costList>
		<Steel>120</Steel>
		<Plasteel>50</Plasteel>
		<ComponentIndustrial>10</ComponentIndustrial>
		<ComponentSpacer>2</ComponentSpacer>
		<Uranium>3</Uranium>
    </costList>
    <recipeMaker>
		<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
		<researchPrerequisite>Fabrication</researchPrerequisite>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
		<recipeUsers Inherit="False">
			<li>FabricationBench</li>
        </recipeUsers>
    </recipeMaker>
    <apparel>
		<bodyPartGroups>
			<li>Neck</li>
			<li>Torso</li>
			<li>Shoulders</li>
			<li>Waist</li>
			<li>Arms</li>
			<li>Legs</li>
		</bodyPartGroups>
		<wornGraphicPath>Things/Armor/Crosshair/CF99_Crosshair</wornGraphicPath>
		<layers>
			<li>Shell</li>
		</layers>
		<tags>
			<li>SpacerMilitary</li>
		</tags>
		<defaultOutfitTags>
			<li>Soldier</li>
		</defaultOutfitTags>
		<useDeflectMetalEffect>true</useDeflectMetalEffect>
	</apparel>
	<thingCategories Inherit="False">
		<li>Apparel</li>
	</thingCategories>
  </ThingDef>

