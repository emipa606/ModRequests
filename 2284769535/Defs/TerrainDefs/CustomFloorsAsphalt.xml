<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <DesignatorDropdownGroupDef>
    <defName>Dropdown_LTO2</defName>
  </DesignatorDropdownGroupDef>

  <TerrainDef Name="CustomFloorBase2" ParentName="FloorBase" Abstract="True">
    <texturePath>Terrain/Surfaces/AsphaltA</texturePath>
    <designationCategory>Floors</designationCategory>
    <designatorDropdown>Dropdown_LTO2</designatorDropdown>
    <edgeType>FadeRough</edgeType>
    <costList></costList>
    <statBases>
      <Cleanliness>0</Cleanliness>
      <Beauty>0</Beauty>
      <Flammability>0</Flammability>
      <WorkToBuild>350</WorkToBuild>
    </statBases>
  </TerrainDef>
  
  
  <!-- ================= Concrete and Asphalt ================= -->
  
  
    <TerrainDef ParentName="CustomFloorBase2">
    <defName>AsphaltA</defName>
    <label>Asphalt</label>
    <description>Rough asphalt. It is cheap and quick to build, but is also quite ugly.</description>
    <texturePath>Terrain/Surfaces/AsphaltA</texturePath>
    <edgeType>Hard</edgeType>
    <renderPrecedence>70</renderPrecedence>
    <statBases>
      <Beauty>-1</Beauty>
      <WorkToBuild>150</WorkToBuild>
    </statBases>
    <costList>
      <Steel>1</Steel>
    </costList>
    <designationHotKey>Misc6</designationHotKey>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </TerrainDef>

  <TerrainDef ParentName="CustomFloorBase2">
    <defName>ConcreteA</defName>
    <label>concrete</label>
    <description>Rough concrete. It is cheap and quick to build, but is also quite ugly.</description>
    <texturePath>Terrain/Surfaces/ConcreteA</texturePath>
    <edgeType>Hard</edgeType>
    <renderPrecedence>70</renderPrecedence>
    <statBases>
      <Beauty>-1</Beauty>
      <WorkToBuild>100</WorkToBuild>
    </statBases>
    <costList>
      <Steel>1</Steel>
    </costList>
    <designationHotKey>Misc6</designationHotKey>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </TerrainDef>
 

