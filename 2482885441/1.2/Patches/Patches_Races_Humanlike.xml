<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Adds this mod's recipes to humans -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Human"]/recipes</xpath>
		<value>
			<li>AdministerMechSerumGrower</li>
			<li>AdministerMechSerumRevitalizer</li>
			<li>AdministerMechSerumAmnesiac</li>
			<li>InstallMindSuppressor</li>
		</value>
	</Operation>

</Patch>