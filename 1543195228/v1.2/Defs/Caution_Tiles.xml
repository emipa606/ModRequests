  <TerrainDef ParentName="FloorBase">
    <defName>YellowStripedTile</defName>
    <label>yellow caution</label>
	<designatorDropdown>CautionStripes</designatorDropdown>
    <renderPrecedence>240</renderPrecedence>
    <description>A metal tile, painted with yellow stripes to indicate critical areas.</description>
    <texturePath>StripeBase</texturePath>
    <color>(175, 146, 70)</color>
    <statBases>
      <Beauty>1</Beauty>
      <Cleanliness>0.2</Cleanliness>
      <WorkToBuild>800</WorkToBuild>
    </statBases>
    <costList>
      <Steel>7</Steel>
    </costList>
    <constructEffect>ConstructMetal</constructEffect>
  </TerrainDef>

