<Patch>
 
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- BabyFood -->

				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="BabyFood"]/graphicData/texPath</xpath>
					<value>
						<texPath>RusticMeal/BabyFood</texPath>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="BabyFood"]/graphicData</xpath>
					<value>
						<drawRotated>false</drawRotated>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
 
</Patch>