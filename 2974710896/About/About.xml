<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ModMetaData>
  <name>BlueprintOverMinifiable</name>
  <author>Maarx</author>
  <url>https://github.com/maarxx/BlueprintOverMinifiable/</url>
  <description>The mod allows you to place blueprints to construct/install over top of uninstallable things. Pawns will then automatically uninstall that thing to make room for the blueprint.</description>
  <packageId>maarx.blueprintoverminifiable</packageId>
  <supportedVersions>
    <li>1.4</li>
  </supportedVersions>
  <modDependencies>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
  </modDependencies>
  <loadAfter>
    <li>brrainz.harmony</li>
  </loadAfter>
</ModMetaData>
