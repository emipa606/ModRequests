<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<!-- ============================ Forsaken Base ========================== -->
	<PawnKindDef Name="WoW_Forsaken_PawnkindBase" Abstract="True">
		<defaultFactionType>Forsaken</defaultFactionType>
		<race>WoW_Forsaken</race>
		<!--<baseRecruitDifficulty>0.90</baseRecruitDifficulty>-->
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>WoW_Forsaken_Backstory</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
		<apparelTags>
			<li>WoW_Medieval_Apparel</li>
		</apparelTags>
		<apparelColor>(74,128,126)</apparelColor>
		<initialResistanceRange>10~20</initialResistanceRange>
		<initialWillRange>0~5</initialWillRange>
		<xenotypeSet Inherit="False">
			<xenotypeChances>
				<Baseliner MayRequire="Ludeon.RimWorld.Biotech">1.00</Baseliner>
				<Dirtmole MayRequire="Ludeon.RimWorld.Biotech">0.00</Dirtmole>
				<Genie MayRequire="Ludeon.RimWorld.Biotech">0.00</Genie>
				<Highmate MayRequire="Ludeon.RimWorld.Biotech">0.00</Highmate>
				<Hussar MayRequire="Ludeon.RimWorld.Biotech">0.00</Hussar>
				<Impid MayRequire="Ludeon.RimWorld.Biotech">0.00</Impid>
				<Neanderthal MayRequire="Ludeon.RimWorld.Biotech">0.00</Neanderthal>
				<Pigskin MayRequire="Ludeon.RimWorld.Biotech">0.00</Pigskin>
				<Sanguophage MayRequire="Ludeon.RimWorld.Biotech">0.00</Sanguophage>
				<Waster MayRequire="Ludeon.RimWorld.Biotech">0.00</Waster>
				<Yttakin MayRequire="Ludeon.RimWorld.Biotech">0.00</Yttakin>
			</xenotypeChances>
		</xenotypeSet>
	</PawnKindDef>
	<!-- ============================ Forsaken Base ========================== -->
	
	<!-- ============================ Forsaken Player Pawn ========================== -->
	<PawnKindDef ParentName="WoW_Forsaken_PawnkindBase">
		<defName>WoWForsaken</defName>
		<label>forsaken colonist</label>
		<combatPower>50</combatPower>
		<isFighter>false</isFighter>
		<defaultFactionType>PlayerColony</defaultFactionType>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
		<apparelMoney>
			<min>350</min>
			<max>500</max>
		</apparelMoney>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<initialResistanceRange>10~20</initialResistanceRange>
		<initialWillRange>0~5</initialWillRange>
	</PawnKindDef>
   <!-- ============================ Forsaken Player Pawn ========================== -->
	
