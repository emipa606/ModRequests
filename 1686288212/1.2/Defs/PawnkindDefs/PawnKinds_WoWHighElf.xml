<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<!-- ============================ High Elf Base ========================== -->
	<PawnKindDef Name="WoW_HighElf_PawnkindBase" Abstract="True">
		<defaultFactionType>HighElf</defaultFactionType>
		<race>WoW_HighElf</race>
		<chemicalAddictionChance>0.035</chemicalAddictionChance>
		<baseRecruitDifficulty>0.90</baseRecruitDifficulty>
