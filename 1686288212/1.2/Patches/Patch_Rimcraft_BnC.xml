<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Baby and Children</li>
			<li>BnC Alien Support</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RimWorldChildren.SupportAlienDef[defName="makeAlienChild"]/supportAlienRaces</xpath>
					<value>
						<li>WoW_BloodElf</li>
						<li>WoW_NightElf</li>
						<li>WoW_HighElf</li>
						<li>WoW_Human</li>
						<li>WoW_Orc</li>
						<li>WoW_Troll</li>
						<li>WoW_Forsaken</li>
						<li>WoW_Draenei</li>
						<li>WoW_Worgen</li>
						<li>WoW_Tauren</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>