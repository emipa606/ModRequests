	<!-- Forsaken Leather -->
	<ThingDef ParentName="LeatherBase">
		<defName>Leather_WoWForsaken</defName>
		<label>forsaken leather</label>
		<description>Rotting, dried, scraped, Forsaken skin. Most people find this material utterly disgusting and horrifying. This leather being rotted, neither protects nor insulates particularly well. Smells pretty bad too.</description>
		<graphicData>
			<color>(146,150,133)</color>
		</graphicData>
		<statBases>
				<MarketValue>1.6</MarketValue>
				<StuffPower_Armor_Sharp>0.10</StuffPower_Armor_Sharp>
				<StuffPower_Insulation_Cold>2</StuffPower_Insulation_Cold>
				<StuffPower_Insulation_Heat>2</StuffPower_Insulation_Heat>
			</statBases>
		<stuffProps>
			<color>(146,150,133)</color>
			<commonality>0.0001</commonality>
			<statFactors>
					<Beauty>-3</Beauty>
				</statFactors>
		</stuffProps>
	</ThingDef>
	
