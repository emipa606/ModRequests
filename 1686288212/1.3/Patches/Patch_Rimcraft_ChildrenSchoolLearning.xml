<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Children, school and learning</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ============================ Rimcraft Child Backstories ========================== -->
				<!-- BloodElf Child Backstory -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<AlienRace.BackstoryDef>
							<defName>WoW_BloodElf_Child_Colony_NewbornCSL</defName>
							<title>Newborn Bloodelf</title>
							<baseDescription>[PAWN_nameDef] was born!</baseDescription>
							<bodyTypeMale>Male</bodyTypeMale>
							<bodyTypeFemale>Female</bodyTypeFemale>
							<slot>Childhood</slot>
							<skillGains>
							</skillGains>
							<forcedTraits>
							</forcedTraits>
							<spawnCategories>
								<li>DontSpawnThisEver</li>
							</spawnCategories>
						</AlienRace.BackstoryDef>
					</value>
				</li>		
			</operations>
		</match>
	</Operation>
</Patch>