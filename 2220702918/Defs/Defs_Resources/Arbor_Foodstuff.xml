<?xml version="1.0" encoding="utf-8" ?>

<Defs>

<!-- =============================== Oculisk Eggs =========================== -->

	<ThingDef ParentName="EggFertBase">
		<defName>Arbor_OculiskEggFertilized</defName>
		<label>oculisk egg (fert.)</label>
		<description>An egg so big it has to be held with two hands. It feels like jelly, and is transparent other than the baby tentacled tarantula inside. If all goes well, it should hatch into an oculisk.</description>
		<tradeability>None</tradeability>
		<statBases>
			<MarketValue>9.0</MarketValue>
		</statBases>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
			<color>(151,48,48)</color>
		</graphicData>
		<thingSetMakerTags>
			<li>NotForGuests</li>
		</thingSetMakerTags>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>18</hatcherDaystoHatch>
				<hatcherPawn>ocularlisk</hatcherPawn>
			</li>
		</comps>
	</ThingDef>
	
  <ThingDef ParentName="EggUnfertBase">
    <defName>Arbor_OculiskEggUnfertilized</defName>
    <label>oculisk egg (unfert.)</label>
    <description>An egg so big it has to be held with two hands. It feels like jelly, and is transparent other than the half-formed tarantula-squid inside. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <color>(151,48,48)</color>
    </graphicData>
    <statBases>
      <MarketValue>7.0</MarketValue>
    </statBases>
  </ThingDef>


 
