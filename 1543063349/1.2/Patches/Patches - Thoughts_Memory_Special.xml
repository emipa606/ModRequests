<Patch>

	<!-- ArtifactMoodBoost -->
	
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThoughtDef[defName = "ArtifactMoodBoost"]/stackLimit</xpath>
		<value>
			<stackLimit>4</stackLimit>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>*/ThoughtDef[defName = "ArtifactMoodBoost"]</xpath>
		<value>
			<stackedEffectMultiplier>0.4</stackedEffectMultiplier>
		</value>
	</Operation>
	
</Patch>
