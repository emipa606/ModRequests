	<AlienRace.BackstoryDef>
		<defName>MantisChildBS05</defName>
		<title>Scab teaser</title>
		<titleShort>Scab</titleShort>
		<baseDescription>[PAWN_nameDef] loves to scratch his own wounds. He feels he makes his flesh cleaner by almost scaryfying himself. It's not really [PAWN_nameDef]'s fault when [PAWN_pronoun] gets wounded.</baseDescription>
		<slot>Childhood</slot>
		
		<spawnCategories>
		  <li>MantisBackstory</li>
		</spawnCategories>
		
		<skillGains>
			<li><defName>Melee</defName><amount>4</amount></li>
			<li><defName>Medicine</defName><amount>2</amount></li>
			<li><defName>Shooting</defName><amount>-2</amount></li>
			<li><defName>Social</defName><amount>-4</amount></li>			
		</skillGains>
		
		<forcedTraits>
			<li><defName>Masochist</defName></li>
			<li><defName>Immunity</defName><degree>1</degree></li>
			<li><defName>Bloodlust</defName></li>
		</forcedTraits>
		
	</AlienRace.BackstoryDef>		
