<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonMantis</defName>
    <rulePack>
		<rulesStrings>
        <li>name->[MantisLocations] '[MantisNames]' [MantisGangsta]</li>
		<li>name->[MantisLocations] '[MantisAdj] [MantisRaces]' [MantisGangsta]</li>
		<li>name->[MantisAdj] '[MantisLocations] [Weapons]' [MantisNames]</li>
		
		<li>name->[tribalword] '[MantisAdj] [MantisLocations]' [MantisNames]</li>
		<li>name->[tribalword] '[MantisRaces] [MantisGangsta]' [MantisLocations]</li>
		
        <li>name->[MantisNames] '[MantisRaces] [Weapons]' [MantisLocations]</li>		
		<li>name->[MantisNames] '[MantisRaces] [MantisLocations]' [MantisGangsta]</li>
		
		<li>name->[MantisAdj] '[MantisLocations] [MantisGangsta]' [MantisRaces]</li>
        <li>name->[negative] '[MantisGangsta] [Weapons]' [MantisLocations]</li>

		<li>name->[MantisRaces] '[Bodyparts] [MantisLocations]' [MantisGangsta]</li>
		<li>name->[MantisGangsta] '[Bodyparts] [Weapons]' [MantisGangsta]</li>
		</rulesStrings>
	  
	<rulesRaw>
		<li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
	
        <li Class="Rule_File">
          <keyword>badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisAdj</keyword>
          <path>MantisNames/MantisAdj</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisLocations</keyword>
          <path>MantisNames/MantisLocations</path>
        </li>
		<!-- noun -->
		<li Class="Rule_File">
          <keyword>MantisNames</keyword>
          <path>MantisNames/MantisNames</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisGangsta</keyword>
          <path>MantisNames/MantisGangsta</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisRaces</keyword>
          <path>MantisNames/MantisRaces</path>
        </li>
		<li Class="Rule_File">
          <keyword>Weapons</keyword>
			<path>Words/Nouns/Weapons</path>
		</li>
		<li Class="Rule_File">
			<keyword>Bodyparts</keyword>
			<path>Words/Nouns/Bodyparts</path>
		</li>
		
	</rulesRaw>
	  
    </rulePack>
  </RulePackDef>
  
</Defs>