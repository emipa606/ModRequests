<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Architect</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="CobblestoneBase">
							<!--VFEArch_CobblestoneFloorBrick-->
							<defName>VFEArch_BrickFloorSandstone</defName>
							<label>brick cobblestone floor</label>
							<color>(177,110,96)</color>
							<costList>
								<N7_FiredBrick>4</N7_FiredBrick>
							</costList>
							<researchPrerequisites>
								<li>BasicCeramics</li>
							</researchPrerequisites>
						</TerrainDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="PolishedStoneFloorBase">
							<defName>VFEArch_PolishedStoneFloorPorcelain</defName>
							<label>porcelain polished stone floor</label>
							<color>(246,246,245)</color>
							<costList>
								<N7_Porcelain>12</N7_Porcelain>
							</costList>
							<researchPrerequisites>
								<li>ImprovedCeramics</li>
							</researchPrerequisites>
						</TerrainDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="StoneBridgeBase">
							<defName>VFEArch_BridgeBrick</defName>
							<label>brick bridge</label>
							<color>(177,110,96)</color>
							<costList>
								<N7_FiredBrick>12</N7_FiredBrick>
							</costList>
							<uiIconPath>StoneBridge/StoneBridgeBrick_MenuIcon</uiIconPath>
							<researchPrerequisites>
								<li>BasicCeramics</li>
							</researchPrerequisites>
						</TerrainDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="CobblestoneBase">
							<defName>VFEArch_MudFloorCobblestone</defName>
							<label>mudbrick cobblestone floor</label>
							<color>(162,152,121)</color>
							<costList>
								<N7_DriedMudbrick>4</N7_DriedMudbrick>
							</costList>
						</TerrainDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef ParentName="TileStoneBase">
							<defName>N7_TileMud</defName>
							<label>brick tile</label>
							<color>(162,152,121)</color>
							<costList>
								<N7_DriedMudbrick>4</N7_DriedMudbrick>
							</costList>
						</TerrainDef>
					</value>
				</li>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Royalty</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationAdd">
								<xpath>/Defs</xpath>
								<value>
									<TerrainDef ParentName="FineMetalTileBase">
										<defName>VFEArch_FineHardenedCeramicTile</defName>
										<label>fine hardened ceramic tile</label>
										<renderPrecedence>241</renderPrecedence>
										<color>(69,67,67)</color>
										<statBases>
											<Beauty>3</Beauty>
											<WorkToBuild>800</WorkToBuild>
											<Cleanliness>0.2</Cleanliness>
										</statBases>
										<costList>
											<N7_HardenedCeramic>20</N7_HardenedCeramic>
										</costList>
										<researchPrerequisites>
											<li>AdvancedCeramics</li>
										</researchPrerequisites>
									</TerrainDef>
								</value>
							</li>
						</operations>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>