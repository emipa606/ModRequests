<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>CaravanChair_TWB</defName>
    <label>Caravan Chair</label>
    <description>A lightweight, cushioned dining chair suitable for camping while on caravans.</description>

    <graphicData>
      <texPath>Things/Building/CaravanChair_TWB</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
      <shadowData>
        <volume>(0.35, 0, 0.35)</volume>
      </shadowData>
    </graphicData>

    <rotatable>false</rotatable>

    <altitudeLayer>Building</altitudeLayer>
    <pathCost>30</pathCost>
    <socialPropernessMatters>true</socialPropernessMatters>

    <statBases>
      <MaxHitPoints>170</MaxHitPoints>
      <WorkToBuild>8000</WorkToBuild>
      <Mass>1</Mass>
      <Flammability>1.0</Flammability>
      <Comfort>0.8</Comfort>
    </statBases>

    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>

    <costStuffCount>30</costStuffCount>

    <building>
      <isSittable>true</isSittable>
    </building>

    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>

    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>

    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>

    <minifiedDef>MinifiedThing</minifiedDef>

  </ThingDef>

</Defs>