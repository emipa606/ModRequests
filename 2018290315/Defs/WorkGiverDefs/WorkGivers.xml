    <workType>Crafting</workType>
    <priorityInType>97</priorityInType>
    <fixedBillGiverDefs>
      <li>CaravanBiofuelRefinery_TWB</li>
    </fixedBillGiverDefs>
    <verb>refine chemicals</verb>
    <gerund>refining chemicals at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>



</Defs>