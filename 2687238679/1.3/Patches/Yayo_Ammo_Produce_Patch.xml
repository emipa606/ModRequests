<Patch>

    <!--  Primitive  -->
    <!--Makes primitive ammo use only wood. This simplifies cost calculation. Same as other types.-->
    <Operation Class="XmlExtensions.PatchOperationSafeReplace">
        <xpath>Defs/RecipeDef[defName = "Make_yy_ammo_primitive"]</xpath>
        <safetyDepth>0</safetyDepth>
        <value>
            <ingredients>
                <li>
                    <filter>
                        <thingDefs>
                            <li>WoodLog</li>
                        </thingDefs>
                    </filter>
                    <count>13</count>
                </li>
            </ingredients>
            <fixedIngredientFilter>
                <thingDefs>
                    <li>WoodLog</li>
                </thingDefs>
            </fixedIngredientFilter>
            <defaultIngredientFilter>
                <thingDefs>
                    <li>WoodLog</li>
                </thingDefs>
            </defaultIngredientFilter>
        </value>
    </Operation>

