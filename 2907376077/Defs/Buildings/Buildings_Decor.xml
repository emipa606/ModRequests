    <defName>AtwoodAfricanVioletA</defName>
    <label>African violet 1x1</label>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
		</researchPrerequisites>
    <description>A popular decorative houseplant originating from Old Earth and maintained through the millennia. Does not require watering or replanting.

Comes in four different colours, which can be selected by rotating the plant.</description>
    <graphicData>
      <texPath>Things/Decor/AfricanViolet</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
       <shaderType>CutoutComplex</shaderType>
	 <drawSize>(1,1)</drawSize>
	<shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
      <damageData>
        <rect>(0.09375,0.296875,0.796875,0.703125)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
	<designationCategory>Decor</designationCategory>
    <rotatable>true</rotatable>
    	<defaultPlacingRot>South</defaultPlacingRot>
		<uiIconPath>Things/Decor/AfricanViolet_south</uiIconPath>
		<uiIconScale>0.85</uiIconScale>
<socialPropernessMatters>true</socialPropernessMatters>
    <fillPercent>0.40</fillPercent>
	<thingCategories>
		<li>BuildingsFurniture</li>
	</thingCategories>
    <statBases>
      <MaxHitPoints>20</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Mass>1</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>6</Beauty>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
  	<building>
	<isEdifice>false</isEdifice>
	</building>
         <size>(1,1)</size>
 <pathCost>10</pathCost>
    	<clearBuildingArea>false</clearBuildingArea>
	<passability>PassThroughOnly</passability>
 		<constructionSkillPrerequisite>2</constructionSkillPrerequisite>
		<tradeability>None</tradeability>
</ThingDef>

