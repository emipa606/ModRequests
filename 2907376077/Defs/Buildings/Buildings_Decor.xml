	<!-- ======================================== Candles ============================================== -->

	<ThingDef ParentName="AtwoodFurnitureBase">
		<defName>AtwoodCandles</defName>
		<label>candles</label>
		<minifiedDef>MinifiedThing</minifiedDef>
		<description>Candles to light the dark places. They can be placed almost anywhere and provide light and a tiny amount of heat. The candles stay lit for several days, after which they need refueling with wood fibre. 

Rotate to switch candle types.</description>
		<category>Building</category>
		<graphicData>
			<texPath>Things/Decor/AtwoodCandles</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		      <drawSize>(1.2,1.2)</drawSize>
</graphicData>
			<thingCategories>
		<li>BuildingsFurniture</li>
	</thingCategories>
<rotatable>true</rotatable>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<clearBuildingArea>false</clearBuildingArea>
<passability>PassThroughOnly</passability>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
			<isEdifice>false</isEdifice>
      <paintable>true</paintable>
		</building>
          <size>(1,1)</size>
<pathCost>5</pathCost>
		<tickerType>Normal</tickerType>
		<drawerType>RealtimeOnly</drawerType>
		<fillPercent>0.15</fillPercent>
		<statBases>
			<MaxHitPoints>30</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Mass>0.5</Mass>
			<Flammability>0</Flammability>
			<Beauty>2</Beauty>
		</statBases>
		<selectable>true</selectable>
		<stuffCategories>
			<li>Stony</li>
			<li>Metallic</li>
		      <li>Woody</li>
</stuffCategories>
		<costStuffCount>15</costStuffCount>
		<costList>
			<Cloth>5</Cloth>
		</costList>
		<soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
				<resourcesFractionWhenDeconstructed>0.5</resourcesFractionWhenDeconstructed>
<placeWorkers>
			<li>PlaceWorker_Heater</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.2</fuelConsumptionRate>
				<fuelCapacity>5.0</fuelCapacity>
				<fuelConsumptionPerTickInRain>0.001</fuelConsumptionPerTickInRain>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</fuelFilter>
		<initialFuelPercent>1</initialFuelPercent>
<destroyOnNoFuel>false</destroyOnNoFuel>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>4</glowRadius>
				<glowColor>(252,187,113,0)</glowColor>
			</li>
			<li Class="CompProperties_HeatPusher">
				<heatPerSecond>2</heatPerSecond>
				<heatPushMaxTemperature>25</heatPushMaxTemperature>
			</li>
		</comps>
<constructionSkillPrerequisite>2</constructionSkillPrerequisite>
<designationCategory>Decor</designationCategory>
	</ThingDef>


