<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>GeneRemover</defName>
		<label>gene remover</label>
		<description>A cutting edge genetic manipulator. Specialized AI enables isolation and termination of unwanted genes. After a successfull procedure, the patient must go through recovery period as their genes restructure themselves. The recovery period duration depends heavily on the metabolic efficiency of the targeted gene.</description>
		<thingClass>GeneRemover.Building_GeneRemover</thingClass>
		<containedPawnsSelectable>true</containedPawnsSelectable>
		<passability>Impassable</passability>
		<fillPercent>0.4</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>GeneRemover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,2)</drawSize>
			<shadowData>
				<volume>(3, 2, 1.9)</volume>
			</shadowData>
		</graphicData>
		<size>(3, 2)</size>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>Biotech</designationCategory>
		<uiOrder>2101</uiOrder>
		<researchPrerequisites>
			<li>GeneRemover</li>
		</researchPrerequisites>
		<building>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
		</building>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>27000</WorkToBuild>
			<Mass>85</Mass>
			<Flammability>0.5</Flammability>
		</statBases>
		<costList>
			<Steel>150</Steel>
			<Plasteel>150</Plasteel>
			<Gold>50</Gold>
			<ComponentSpacer>6</ComponentSpacer>
		</costList>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>800</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
	</ThingDef>
</Defs>