<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<HediffDef>
		<defName>Cube_WellFed</defName>
		<label>well fed (from foodcube)</label>
		<makesSickThought>false</makesSickThought>
		<description>I have eaten a food cube.</description>
		<tendable>false</tendable>
		<isBad>false</isBad>
		<defaultLabelColor>(92,62,198)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<comps>
			<li Class="VanillaCookingExpanded.HediffCompProperties_WhileHavingThoughts">
				<thoughtDefs>
					<li>Super_AteCube</li>						
				</thoughtDefs>
			</li>
		</comps>
		<stages>
			<li>   
				<statOffsets>
					<HungerRateMultiplier>-0.35</HungerRateMultiplier>					
				</statOffsets>
			</li>
		</stages>
	</HediffDef>
	
