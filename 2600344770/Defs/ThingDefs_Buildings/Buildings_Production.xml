  <ThingDef ParentName="BenchBase">
    <defName>HandFlourMill</defName>
    <label>hand grain mill</label>
    <description>A hand powered mill for turning wheatstraw into strawflour. More efficient than using a butcher spot or table.</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/FlourMill</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4,4)</drawSize>
      <shaderType>CutoutComplex</shaderType>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <costStuffCount>100</costStuffCount>
    <costList>
      <Steel>50</Steel>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2000</WorkToBuild>
      <MaxHitPoints>300</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <WorkTableWorkSpeedFactor>1</WorkTableWorkSpeedFactor>
    </statBases>
    <size>(2,2)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(1,0,2)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <comps>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <researchPrerequisites>
    </researchPrerequisites>
  </ThingDef>
  
	<ThingDef ParentName="BuildingBase">
		<defName>Super_GrogBarrel</defName>
		<label>grog barrel</label>
		<description>A barrel for fermenting raw wheatstraw into grog.</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/GrogBarrel/GrogBarrel</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.45</fillPercent>
		<pathCost>60</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<costList>
			<Steel>10</Steel>
			<WoodLog>30</WoodLog>
		</costList>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Building/Production/GrogBarrel/GrogBarrel</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<qualitySelector>false</qualitySelector>
				<acceptsNoneAsInput>false</acceptsNoneAsInput>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<resetSemiautomaticIcon>UI/Super_Grogcancel</resetSemiautomaticIcon>
				<chooseIngredientsIcon>UI/Super_Grogstart</chooseIngredientsIcon>
				<showProgressBar>true</showProgressBar>
				<bringIngredientsIcon>UI/Super_Grogstart</bringIngredientsIcon>
				<cancelIngredientsIcon>UI/Super_Grogreset</cancelIngredientsIcon>
				<noPowerDestroysProgress>false</noPowerDestroysProgress>
				<removeProductIcon>UI/Super_Grogcancel</removeProductIcon>
				<isTemperatureDependingMachine>true</isTemperatureDependingMachine>
				<minTemp>-1</minTemp>
				<maxTemp>32</maxTemp>
				<destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>true</rotatable>
		<designationCategory>Production</designationCategory>
		<constructEffect>ConstructWood</constructEffect>
		<researchPrerequisites>
			<li>Brewing</li>
		</researchPrerequisites>
	</ThingDef>
	
