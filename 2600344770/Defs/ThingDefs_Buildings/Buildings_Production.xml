<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>Super_Breadoven</defName>
		<label>fueled bread oven</label>
		<description>A large metal oven that needs to be refueled with wood periodically. Strawflour is inserted to be baked into bread</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/Fueled_BreadOven</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(3.5,3.5)</drawSize>
      <shadowData>
        <volume>(1.95,0.5,1.3)</volume>
      </shadowData>
      <damageData>
        <rect>(0.3,0.1,2.4,1.8)</rect>
      </damageData>
    </graphicData>
		<altitudeLayer>Building</altitudeLayer>
    <statBases>
      <WorkToBuild>4500</WorkToBuild>
      <MaxHitPoints>300</MaxHitPoints>
      <Cleanliness>0</Cleanliness>
      <Beauty>0</Beauty>
    </statBases>
		<tickerType>Normal</tickerType>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<useHitPoints>True</useHitPoints>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
    <costList>
      <Steel>80</Steel>
    </costList>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<sowTag>SupportPlantsOnly</sowTag>
			<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
			<ai_chillDestination>false</ai_chillDestination>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Building/Production/OnBreadOven</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<qualitySelector>false</qualitySelector>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<resetSemiautomaticIcon>UI/Super_Breadcancel</resetSemiautomaticIcon>
				<chooseIngredientsIcon>UI/Super_Breadstart</chooseIngredientsIcon>
				<showProgressBar>true</showProgressBar>
				<bringIngredientsIcon>UI/Super_Breadstart</bringIngredientsIcon>
				<cancelIngredientsIcon>UI/Super_Breadreset</cancelIngredientsIcon>
				<noPowerDestroysProgress>true</noPowerDestroysProgress>
				<destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>10</fuelConsumptionRate>
				<fuelCapacity>20.0</fuelCapacity>
				<fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>0</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<consumeFuelOnlyWhenUsed>false</consumeFuelOnlyWhenUsed>
			</li>
      <li Class="CompProperties_HeatPusher">
        <compClass>CompHeatPusherPowered</compClass>
        <heatPerSecond>3</heatPerSecond>
      </li>
		</comps>
    <researchPrerequisites>
    </researchPrerequisites>
	</ThingDef>
	
