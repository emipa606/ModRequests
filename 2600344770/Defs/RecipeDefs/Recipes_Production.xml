<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Super_Mill_Wheatstraw</defName>
    <label>make strawflour</label>
    <description>Use wheatstraw to create strawflour</description>
    <jobString>Milling wheatstraw</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workAmount>1600</workAmount>
    <recipeUsers>
      <li>CraftingSpot</li>
	  <li>TableButcher</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Super_Wheatstraw</li>
          </thingDefs>
        </filter>
        <count>75</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
          <thingDefs>
            <li>Super_Wheatstraw</li>
          </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Super_Strawflour>75</Super_Strawflour>
    </products>
    <workSkill>Cooking</workSkill>
  </RecipeDef>
  
