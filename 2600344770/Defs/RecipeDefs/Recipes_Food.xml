<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- Cube -->
  <RecipeDef>
    <defName>Make_Super_Cube</defName>
    <label>make foodcube</label>
    <description>Make a foodcube, a preserved combination of greenfruit and bread. It doesn't taste amazing, but lasts a long time without refrigeration.</description>
    <jobString>Making foodcube.</jobString>
    <workAmount>450</workAmount>
    <recipeUsers>
      <li>ElectricStove</li>
	  <li>FueledStove</li>
    </recipeUsers>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>false</allowMixingIngredients>
      <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Super_Bread</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>RawGreenfruit</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
    </ingredients>
    <products>
      <Super_Cube>1</Super_Cube>
    </products>
    <fixedIngredientFilter>
          <thingDefs>
            <li>RawGreenfruit</li>
			            <li>Super_Bread</li>
          </thingDefs>
    </fixedIngredientFilter>
    <workSkill>Cooking</workSkill>
    <researchPrerequisite>Pemmican</researchPrerequisite>
  </RecipeDef>
