<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Bread -->
	<ThingDef ParentName="MealBase">
		<defName>Super_Bread</defName>
		<label>bread</label>
		<description>A simple type baked bread. Keeps for some time.</description>
		<stackLimit>12</stackLimit>
		<graphicData>
			<texPath>Things/Item/Food/super_bread</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>16</MarketValue>
      <Mass>0.2</Mass>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
	<tasteThought>Super_AteBread</tasteThought>
      <preferability>MealAwful</preferability>
      <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <allowedArchonexusCount>40</allowedArchonexusCount>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>12</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>
<!-- Food Cube -->
  <ThingDef ParentName="MealBase">
    <defName>Super_Cube</defName>
    <label>foodcube</label>
    <description>A preserved mashed mixture of bread and greenfruit. Moderately tasty. It lasts a very long time without refrigeration. Great for traveling.</description>
    <graphicData>
      <texPath>Things/Item/Food/super_cube</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>22</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
	<tasteThought>Super_AteCube</tasteThought>
      <preferability>MealSimple</preferability>
      <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
