<?xml version="1.0" encoding="utf-8" ?>

<Defs>
    
<!-- ========== Octopus ========== -->
	
	<BodyDef>
		<defName>Octopus</defName>
		<label>octopus</label>
		<corePart>
			<def>MM_OctopusMantle</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<groups>
				<li>Torso</li>
			</groups>
			<parts>
				<li>
					<def>MM_Gill</def>
					<customLabel>left gill</customLabel>
					<coverage>0.01</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>MM_Gill</def>
					<customLabel>right gill</customLabel>
					<coverage>0.01</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>Heart</def>
					<coverage>0.02</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>Kidney</def>
					<coverage>0.03</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>Liver</def>
					<coverage>0.01</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>Stomach</def>
					<coverage>0.08</coverage>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				
				<li>
					<def>Head</def>
					<coverage>0.75</coverage>
					<height>Middle</height>
					<groups>
						<li>UpperHead</li>
						<li>FullHead</li>
					</groups>
					<parts>
						<li>
							<def>Beak</def>
							<coverage>0.04</coverage>
							<height>Middle</height>
							<groups>
								<li>FullHead</li>
								<li>Beak</li>
							</groups>
						</li>
						<li>
							<def>Eye</def>
							<customLabel>left eye</customLabel>
							<coverage>0.02</coverage>
							<height>Top</height>
							<groups>
								<li>Eyes</li>
								<li>FullHead</li>
								<li>UpperHead</li>
							</groups>
						</li>
						<li>
							<def>Eye</def>
							<customLabel>right eye</customLabel>
							<coverage>0.02</coverage>
							<height>Top</height>
							<groups>
								<li>Eyes</li>
								<li>FullHead</li>
								<li>UpperHead</li>
							</groups>
						</li>
						<li>
							<def>Skull</def>
							<coverage>0.04</coverage>
							<depth>Inside</depth>
							<height>Top</height>
							<groups>
								<li>UpperHead</li>
								<li>FullHead</li>
							</groups>
							<parts>
								<li>
								<def>Brain</def>
								<coverage>0.7</coverage>
								<depth>Inside</depth>
								<groups>
									<li>UpperHead</li>
									<li>FullHead</li>
								</groups>
								</li>
							</parts>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>top left tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Top</height>
							<groups>
								<li>Tentacles</li>
								<li>TopLeftTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>top right tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Top</height>
							<groups>
								<li>Tentacles</li>
								<li>TopRightTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>upper left tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Middle</height>
							<groups>
								<li>Tentacles</li>
								<li>UpperLeftTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>upper right tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Middle</height>
							<groups>
								<li>Tentacles</li>
								<li>UpperRightTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>lower left tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Middle</height>
							<groups>
								<li>Tentacles</li>
								<li>LowerLeftTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>lower right tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Middle</height>
							<groups>
								<li>Tentacles</li>
								<li>LowerRightTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>bottom left tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Bottom</height>
							<groups>
								<li>Tentacles</li>
								<li>BottomLeftTentacle</li>
							</groups>
						</li>
						<li>
							<def>MM_Tentacle</def>
							<customLabel>bottom right tentacle</customLabel>
							<coverage>0.07</coverage>
							<height>Bottom</height>
							<groups>
								<li>Tentacles</li>
								<li>BottomRightTentacle</li>
							</groups>
						</li>
					</parts>
				</li>
				<li>
					<def>MM_Siphon</def>
					<coverage>0.02</coverage>
					<height>Bottom</height>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
			</parts>
		</corePart>
	</BodyDef>
	
</Defs>