<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<BodyDef>
		<defName>Snail</defName>
		<label>snail</label>
		<corePart>
			<def>SnakeBody</def>
			<height>Middle</height>
			<groups>
				<li>Torso</li>
				<li>Feet</li>
			</groups>
			<parts>
				<li>
					<def>TurtleShell</def>
					<coverage>0.7</coverage>
					<height>Top</height>
					<groups>
						<li>Torso</li>
					</groups>
					<parts>
						<li>
							<def>Heart</def>
							<coverage>0.02</coverage>
							<depth>Inside</depth>
							<groups>
								<li>Torso</li>
							</groups>
						</li>
						<li>
							<def>Kidney</def>
							<coverage>0.05</coverage>
							<depth>Inside</depth>
							<groups>
								<li>Torso</li>
							</groups>
						</li>
						<li>
							<def>Liver</def>
							<coverage>0.20</coverage>
							<depth>Inside</depth>
							<groups>
								<li>Torso</li>
							</groups>
						</li>
						<li>
							<def>Lung</def>
							<coverage>0.15</coverage>
							<depth>Inside</depth>
							<groups>
								<li>Torso</li>
							</groups>
						</li>
						<li>
							<def>Stomach</def>
							<coverage>0.08</coverage>
							<depth>Inside</depth>
							<groups>
								<li>Torso</li>
							</groups>
						</li>
					</parts>
				</li>
				<li>
					<def>MM_RespiratoryPore</def>
					<coverage>0.005</coverage>
					<height>Top</height>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>InsectHead</def>
					<coverage>0.2</coverage>
					<height>Middle</height>
					<groups>
						<li>FullHead</li>
						<li>HeadAttackTool</li>
					</groups>
					<parts>
						<li>
							<def>Brain</def>
							<coverage>0.2</coverage>
							<depth>Inside</depth>
							<groups>
								<li>FullHead</li>
								<li>UpperHead</li>
							</groups>
						</li>
						<li>
							<def>MM_Ommatophore</def>
							<customLabel>left eye stalk</customLabel>
							<coverage>0.2</coverage>
							<height>Top</height>
							<groups>
								<li>FullHead</li>
								<li>UpperHead</li>
							</groups>
							<parts>
								<li>
									<def>Eye</def>
									<customLabel>left eye</customLabel>
									<coverage>0.3</coverage>
									<height>Top</height>
									<groups>
										<li>FullHead</li>
										<li>UpperHead</li>
										<li>Eyes</li>
									</groups>
								</li>
							</parts>
						</li>
						<li>
							<def>MM_Ommatophore</def>
							<customLabel>right eye stalk</customLabel>
							<coverage>0.2</coverage>
							<height>Top</height>
							<groups>
								<li>FullHead</li>
								<li>UpperHead</li>
							</groups>
							<parts>
								<li>
									<def>Eye</def>
									<customLabel>right eye</customLabel>
									<coverage>0.3</coverage>
									<height>Top</height>
									<groups>
										<li>FullHead</li>
										<li>UpperHead</li>
										<li>Eyes</li>
									</groups>
								</li>
							</parts>
						</li>
						<li>
							<def>SnakeMouth</def>
							<coverage>0.1</coverage>
							<height>Bottom</height>
							<groups>
								<li>FullHead</li>
								<li>Mouth</li>
								<li>Teeth</li>
							</groups>
						</li>
					</parts>
				</li>
			</parts>
		</corePart>
	</BodyDef>

</Defs>