	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Alpha Biomes</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="CarrionCrawler"]/race/wildBiomes</xpath>
							<value>
								<AB_GallatrossGraveyard>0.1</AB_GallatrossGraveyard>
								<AB_GelatinousSuperorganism>0.075</AB_GelatinousSuperorganism>
								<AB_OcularForest>0.1</AB_OcularForest>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="InfernoBeetle"]/race/wildBiomes</xpath>
							<value>
								<AB_FeraliskInfestedJungle>0.05</AB_FeraliskInfestedJungle>
								<AB_MycoticJungle>0.025</AB_MycoticJungle>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="LandKraken"]/race/wildBiomes</xpath>
							<value>
								<AB_MycoticJungle>0.05</AB_MycoticJungle>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="SanguineDrake"]/race/wildBiomes</xpath>
							<value>
								<AB_GallatrossGraveyard>0.1</AB_GallatrossGraveyard>
								<AB_OcularForest>0.1</AB_OcularForest>
							</value>
						</li>
					</operations>
				</match>
			</li>

			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="CarrionCrawler"]/race/wildBiomes</xpath>
							<value>
								<Savanna>0.075</Savanna>
								<Wasteland>0.01</Wasteland>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="InfernoBeetle"]/race/wildBiomes</xpath>
							<value>
								<Savanna>0.01</Savanna>
								<Volcano>0.2</Volcano>
								<Wetland>0.075</Wetland>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="LandKraken"]/race/wildBiomes</xpath>
							<value>
								<Wetland>0.1</Wetland>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="SanguineDrake"]/race/wildBiomes</xpath>
							<value>
								<Savanna>0.075</Savanna>
								<Volcano>0.05</Volcano>
								<Wasteland>0.01</Wasteland>
							</value>
						</li>
					</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Nature's Pretty Sweet</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="CarrionCrawler"]/race/wildBiomes</xpath>
							<value>
								<TKKN_Desert>0.01</TKKN_Desert>
								<TKKN_Grasslands>0.05</TKKN_Grasslands>
								<TKKN_Savanna>0.075</TKKN_Savanna>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="InfernoBeetle"]/race/wildBiomes</xpath>
							<value>
								<TKKN_RedwoodForest>0.1</TKKN_RedwoodForest>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="SanguineDrake"]/race/wildBiomes</xpath>
							<value>
								<TKKN_Desert>0.01</TKKN_Desert>
								<TKKN_Grasslands>0.05</TKKN_Grasslands>
								<TKKN_Oasis>0.01</TKKN_Oasis>
								<TKKN_Savanna>0.075</TKKN_Savanna>
								<TKKN_VolcanicFlow>0.01</TKKN_VolcanicFlow>
							</value>
						</li>
					</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>[RF] Realistic Planets [1.0]</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="CarrionCrawler"]/race/wildBiomes</xpath>
							<value>
								<RRP_Grassland>0.025</RRP_Grassland>
								<RRP_Savanna>0.03</RRP_Savanna>
								<RRP_Steppes>0.025</RRP_Steppes>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="InfernoBeetle"]/race/wildBiomes</xpath>
							<value>
								<RRP_Savanna>0.05</RRP_Savanna>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="MM_PolarColossus"]/race/wildBiomes</xpath>
							<value>
								<RRP_Permafrost>0.05</RRP_Permafrost>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="SanguineDrake"]/race/wildBiomes</xpath>
							<value>
								<RRP_Grassland>0.025</RRP_Grassland>
								<RRP_Savanna>0.03</RRP_Savanna>
								<RRP_TemperateDesert>0.025</RRP_TemperateDesert>
								<RRP_Oasis>0.025</RRP_Oasis>
							</value>
						</li>
					</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Terra Project (Core)</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="CarrionCrawler"]/race/wildBiomes</xpath>
							<value>
								<DesertHighPlains>0.01</DesertHighPlains>
								<Savanna>0.05</Savanna>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="InfernoBeetle"]/race/wildBiomes</xpath>
							<value>
								<Atoll>0.1</Atoll>
								<InfestedMountains>0.05</InfestedMountains>
								<SpikeMountains>0.1</SpikeMountains>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="LandKraken"]/race/wildBiomes</xpath>
							<value>
								<Archipelago>0.01</Archipelago>
								<Atoll>0.025</Atoll>
								<CaveOasis>0.01</CaveOasis>
								<TundraSkerries>0.01</TundraSkerries>
								<VolcanicIsland>0.02</VolcanicIsland>
							</value>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="SanguineDrake"]/race/wildBiomes</xpath>
							<value>
								<Archipelago>0.01</Archipelago>
								<DeepRavine>0.05</DeepRavine>
								<Oasis>0.09</Oasis>
								<Savanna>0.05</Savanna>
							</value>
						</li>
					</operations>
				</match>
			</li>
		</operations>
	</Operation>
	
