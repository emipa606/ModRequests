<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- FOOD -->
	<ThingDef ParentName="MealLavishBase">
		<defName>GARF_Lasagna</defName>
		<label>Lasagna</label>
		<description>The lasagna is a type of pasta, long preceding the colonization of space. Layers of meat, tomato cause, and sometimes cheese are placed between thin layers of pasta and baked to perfection.</description>
		<graphicData>
			<texPath>Things/Lasagna</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>40</MarketValue>
			<WorkToMake>800</WorkToMake>
			<Nutrition>1</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealLavish</preferability>
			<tasteThought>GARF_AteLasagna</tasteThought>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<stackLimit>10</stackLimit>
		<uiIconForStackCount>10</uiIconForStackCount>
	</ThingDef>

</Defs>
