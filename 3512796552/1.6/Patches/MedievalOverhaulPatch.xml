<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="DankPyon_DryingRack_Small"]/comps/li[@Class="ProcessorFramework.CompProperties_Processor"]/capacity</xpath>
                    <value>
                        <capacity>99</capacity>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="DankPyon_DryingRack_Big"]/comps/li[@Class="ProcessorFramework.CompProperties_Processor"]/capacity</xpath>
                    <value>
                        <capacity>198</capacity>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>
