<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- SGC: Patches a few Vanilla Expanded items into a special reward pool -->

<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Vanilla Factions Expanded - Vikings</li>
	</mods>	
<match Class="PatchOperationInsert">
	<success>Always</success>
	<xpath>Defs/ThingDef[defName="CryptoWeaponAdvanced" or thingCategories="WeaponsRangedCrypto" or weaponTags="CryptoHeavyMelee" or weaponTags="CryptoLightMelee"]/tradeability</xpath>
	<value>
	
	<thingSetMakerTags><li>RewardStandardTribalSpecial</li><li>RewardStandardArtifact</li></thingSetMakerTags>
	
	</value>
</match>

</Operation>
</Patch>



