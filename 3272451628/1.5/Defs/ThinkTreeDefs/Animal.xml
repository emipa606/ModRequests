                  <mtbHours>1.5</mtbHours>
                  <subNodes>
                    <li Class="ThinkNode_ConditionalRequireCapacities">
                      <requiredCapacities>
                        <li>Manipulation</li>
                      </requiredCapacities>
                      <subNodes>
                        <li Class="SentientAnimals.JobGiver_Clean" />
                        <li Class="SentientAnimals.JobGiver_Nurse" />
                      </subNodes>
                    </li>
                  </subNodes>
                </li>
              </subNodes>
            </li>
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
</Defs>