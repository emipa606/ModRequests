<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- chip I -->
    <RecipeDef ParentName="DSU_BaseRecipe_Electronics">
        <defName>Make_DSU_ElectronicChip_I</defName>
        <label>make basic AI chip</label>
        <jobString>Making basic AI chip</jobString>
        <workAmount>2000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentIndustrial</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentIndustrial</li>
                <li>Steel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <DSU_ElectronicChip_I>1</DSU_ElectronicChip_I>
        </products>
        <skillRequirements>
            <Crafting>8</Crafting>
        </skillRequirements>
    </RecipeDef>

    <!-- chip II -->
    <RecipeDef ParentName="DSU_BaseRecipe_Electronics">
        <defName>Make_DSU_ElectronicChip_II</defName>
        <label>make advanced AI core</label>
        <jobString>Making Adv. AI core</jobString>
        <workAmount>4000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>DSU_ElectronicChip_I</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentSpacer</li>
                <li>Steel</li>
                <li>DSU_ElectronicChip_I</li>
                <li>Plasteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <DSU_ElectronicChip_II>1</DSU_ElectronicChip_II>
        </products>
        <skillRequirements>
            <Crafting>15</Crafting>
        </skillRequirements>
    </RecipeDef>

</Defs>
