<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <RecipeDef>
    <defName>SqueezeOutSulfer</defName>
    <label>Squeeze out sulfer</label>
    <description>Squeeze out sulfer from bombrat or bomalope leathers.</description>
    <jobString>Squeezing.</jobString>
    <workAmount>800</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
      <workSpeedStat>DrugProductionSpeed</workSpeedStat>
      <workSkill>Medicine</workSkill>
    <ingredients>
	<li>
		<filter>
	          <thingDefs>
	            <li>Boomrat_Leather</li>
	            <li>Boomalope_Leather</li>
	          </thingDefs>
		</filter>
		<count>5</count>
	</li>
    </ingredients>
    <fixedIngredientFilter>
	          <thingDefs>
	            <li>Boomrat_Leather</li>
	            <li>Boomalope_Leather</li>
	          </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
	          <thingDefs>
	            <li>Boomrat_Leather</li>
	            <li>Boomalope_Leather</li>
	          </thingDefs>
    </defaultIngredientFilter>
    <products>
      <Sulfur>25</Sulfur>
    </products>
    <recipeUsers>
       <li>DrugLab</li>
    </recipeUsers>
  </RecipeDef>
  
</RecipeDefs>