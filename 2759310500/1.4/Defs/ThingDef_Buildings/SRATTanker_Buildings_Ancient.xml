  <!--Ancient Wall Fridge-->
  <ThingDef ParentName="BuildingBase">
    <defName>AncientWallFridge</defName>
    <label>Ancient Wall Fridge</label>
    <description>A device that once held refrigerated or frozen goods long ago. All useful components have long since been taken.</description>
    <graphicData>
      <texPath>Things/Building/Ruins/AncientWallFridge</texPath>
      <graphicClass>Graphic_Random</graphicClass>
	  <drawSize>(1.4,1)</drawSize>
    </graphicData>
	<size>(1,1)</size>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <coversFloor>true</coversFloor>
    <placingDraggableDimensions>1</placingDraggableDimensions>
    <tickerType>Never</tickerType>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <holdsRoof>true</holdsRoof>
    <staticSunShadowHeight>1.0</staticSunShadowHeight>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <fertility>0</fertility>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <Beauty>-10</Beauty>
      <Flammability>0</Flammability>
      <WorkToBuild>600</WorkToBuild>
    </statBases>
    <building>
      <isInert>true</isInert>
      <ai_chillDestination>false</ai_chillDestination>
    </building>
    <costList>
      <Steel>15</Steel>
    </costList>
	<designationCategory MayRequire="vanillaexpanded.vfepropsanddecor">PropsandDecor</designationCategory>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <repairEffect>ConstructDirt</repairEffect>
  </ThingDef>

