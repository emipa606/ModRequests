<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Ancient Gas Pump-->
  <ThingDef ParentName="BuildingBase">
    <defName>AncientGasPump</defName>
    <label>ancient fuel pump</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>A device that pumped fuel into vehicles a long ago. The hose has deteriorated, and all useful components have long since been taken.</description>
    <graphicData>
      <texPath>Things/Building/Ruins/AncientGasPump</texPath>
      <graphicClass>Graphic_Random</graphicClass>
	  <drawSize>(1.5,1)</drawSize>
	  <drawRotated>false</drawRotated>
    </graphicData>
	<size>(1,1)</size>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <startingHpRange>0.5~1.3</startingHpRange>
    <altitudeLayer>Building</altitudeLayer>
    <pathCost>42</pathCost>
    <pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <Beauty>-10</Beauty>
      <Flammability>0</Flammability>
      <WorkToBuild>600</WorkToBuild>
    </statBases>
    <building>
      <isInert>true</isInert>
      <ai_chillDestination>false</ai_chillDestination>
    </building>
    <costList>
      <Steel>15</Steel>
    </costList>
	<designationCategory MayRequire="vanillaexpanded.vfepropsanddecor">PropsandDecor</designationCategory>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <repairEffect>ConstructDirt</repairEffect>
  </ThingDef>
  
