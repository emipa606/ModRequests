    </comps>
    <researchPrerequisites>
      <li>Research_SRTFLogisticPodSystem</li>
    </researchPrerequisites>
    <uiIconScale>0.50</uiIconScale>
    <killedLeavings>
      <ChunkSlagSteel>12</ChunkSlagSteel>
    </killedLeavings>
  </ThingDef>
