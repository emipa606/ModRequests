<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseMotor">
    <defName>motor_yy_combatCar</defName> <!-- motor_ + building's defname -->
    <label>Combat Car</label>
    <description>Combat Car</description>

    <statBases>
      <Mass>300</Mass>
      <MoveSpeed>18.0</MoveSpeed>
      <Flammability>0.1</Flammability>
      <MarketValue>5000</MarketValue>
      <ArmorRating_Blunt>0.40</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.70</ArmorRating_Sharp>
      <ArmorRating_Heat>1.00</ArmorRating_Heat>
    </statBases>
    
    
    <race>
        <baseBodySize>2.0</baseBodySize>
        <baseHealthScale>5</baseHealthScale>
    </race>

    <butcherProducts>
      <Steel>100</Steel>
      <Plasteel>0</Plasteel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </butcherProducts>
        
    <tools>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>


  </ThingDef>
  
  
  
  
  <PawnKindDef ParentName="MotorKindBase">
    <defName>motor_yy_combatCar</defName> <!-- motor_ + building's defname -->
    <label>Combat Car</label>
    <labelPlural>Combat Car</labelPlural>
    <race>motor_yy_combatCar</race> <!-- motor_ + building's defname -->
    <combatPower>270</combatPower>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>yy/combatCar</texPath>
          <drawSize>(4,4)</drawSize>
          <shadowData>
            <volume>(0.95, 0.4, 0.6)</volume>
            <offset>(0,0,-0.3)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>yy/combatCar</texPath>
          <drawSize>(4,4)</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>

    

  </PawnKindDef>


  

</Defs>