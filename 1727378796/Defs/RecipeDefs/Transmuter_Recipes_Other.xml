<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>MakeComponent_TM</defName>
    <label>transmute a resource into components</label>
    <description>Transmute a resource into components.</description>
    <jobString>Transmuting resources into components.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <recipeUsers>
	<li>Transmuter</li>
    </recipeUsers>
    <workAmount>500</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>ResourcesRaw</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>ResourcesRaw</li>
			</categories>
		</fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>ResourcesRaw</li>
      </categories>
      <disallowedThingDefs>
        <li>Jade</li>
        <li>Silver</li>
        <li>Gold</li>
        <li>Uranium</li>
        <li>Plasteel</li>
	      <li>Steel</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <products>
      <ComponentIndustrial>10</ComponentIndustrial>
    </products>
    <researchPrerequisite>TM_Component</researchPrerequisite>
    <skillRequirements>
      <Crafting>5</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

