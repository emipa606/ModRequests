<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>MakeTMPowerCell</defName>
    <label>make a power cell</label>
    <description>Make a power cell.</description>
    <jobString>Making power cell.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <recipeUsers>
	<li>FabricationBench</li>
    </recipeUsers>
    <workAmount>500</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Uranium</li>
        <li>ComponentIndustrial</li>
	<li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <TM_PowerCell>1</TM_PowerCell>
    </products>
    <researchPrerequisite>TM_Cells</researchPrerequisite>
    <skillRequirements>
      <Crafting>5</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

  <RecipeDef>
    <defName>MakeSteel</defName>
    <label>transmute a resource into steel</label>
    <description>Transmute a resource into steel.</description>
    <jobString>Transmuting resources into steel.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <recipeUsers>
	<li>Transmuter</li>
    </recipeUsers>
    <workAmount>500</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>ResourcesRaw</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>ResourcesRaw</li>
			</categories>
		</fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>ResourcesRaw</li>
      </categories>
      <disallowedThingDefs>
        <li>Jade</li>
        <li>Silver</li>
        <li>Gold</li>
        <li>Uranium</li>
        <li>Plasteel</li>
	      <li>Steel</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <products>
      <Steel>10</Steel>
    </products>
    <researchPrerequisite>TM_Metals</researchPrerequisite>
    <skillRequirements>
      <Crafting>5</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

