<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="REDSpirit.REDS_Patch_Mod_Operation">

		<match Class="PatchOperationAdd">
			<xpath>/Defs/XenotypeDef[defName="REDS_Spirit_Elysian"]/genes</xpath>
			<value>			
				<li>REDS_EctoplasmBlood</li>
				<li>REDS_Body_Ectoplasm</li>			
				<li>Sterile</li>
				<li>Beauty_Beautiful</li>
				<li>Hair_Grayless</li>
				<li>MeleeDamage_Weak</li>
			</value>
			
			
		</match>
		
		<nomatch Class="PatchOperationReplace">
					<xpath>/Defs/XenotypeDef[defName="REDS_Spirit_Elysian"]/inheritable</xpath>
					<value>
						<inheritable>false</inheritable>
					</value>
		</nomatch>

	</Operation>

</Patch>

