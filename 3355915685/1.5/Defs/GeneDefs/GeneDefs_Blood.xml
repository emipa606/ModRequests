<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>REDS_EctoplasmBlood</defName>
		<label>ectoplasm</label>
		<displayCategory>Healing</displayCategory>	
		<description>Carriers of this gene have psychic properties in their blood that increase the danger of bleeding out and negatively impact the mind-muscle connection.</description>
		<iconPath>UI/Icons/Genes/Gene_Ectoplasm</iconPath>
		<prerequisite>REDS_Ectoplasm_Gene</prerequisite>	
		<displayOrderInCategory>0</displayOrderInCategory>
		<exclusionTags>
			<li>AG_BloodType</li>
			<li>BloodType</li>
			<li>Blood</li>
			<li>AG_Bleeding</li>		
			<li>Bleeding</li>
		</exclusionTags>
		
		<biostatCpx>1</biostatCpx>
		<biostatMet>1</biostatMet>
		
		<symbolPack>
			<wholeNameSymbols>
				<li>
					<symbol>ectoplasm</symbol>
				</li>

			</wholeNameSymbols>
			<prefixSymbols>
				<li>
					<symbol>ghost</symbol>
				</li>
				<li>
					<symbol>ecto</symbol>
				</li>

			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>plasm</symbol>
				</li>

			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">

				<customBloodThingDef>REDS_EctoplasmBlood</customBloodThingDef>
				<customBloodIcon>UI/Icons/Medical/REDS_EctoplasmIcon</customBloodIcon>
				<customBloodEffect>REDS_Damage_HitEctoplasm</customBloodEffect>
				<customWoundsFromFleshtype>REDS_WoundsEctoplasm</customWoundsFromFleshtype>
				<customBloodSmearThingDef>REDS_Filth_Ectoplasm_BloodSmear</customBloodSmearThingDef>
				
			</li>
		</modExtensions>
		
	</GeneDef>


</Defs>