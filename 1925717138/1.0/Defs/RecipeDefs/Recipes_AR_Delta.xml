<Defs>

  <!-- ======= M4A1 SD Delta ======= -->
	<RecipeDef>
    <defName>CPCraft_M4A1Delta_Weapon_Vanilla</defName>
    <label>make M4A1 SD Delta</label>
    <description>make an M4A1 SD Delta.</description>
    <jobString>making an M4A1 SD Delta.</jobString>
    <workSkill>Crafting</workSkill>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
	<workAmount>40000</workAmount>
	<unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
	<recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <ingredients>
	  		<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>60</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>7</count>
			</li>
    </ingredients>
		<products>
			<RNGun_M4A1Delta>1</RNGun_M4A1Delta> <!-- === PRODUCT === -->
		</products>
	 <researchPrerequisite>PrecisionRifling</researchPrerequisite>
	<skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
  </RecipeDef>
  
