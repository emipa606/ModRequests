<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <BodyDef>
    <defName>Slime</defName>
    <label>slime</label>
    <corePart>
      <def>Torso</def>
      <customLabel>torso membrane</customLabel>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
      </groups>
      <parts>
        <li>
          <def>SlimeNeck</def>
		  <customLabel>neck membrane</customLabel>
          <coverage>0.075</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
			  <customLabel>head membrane</customLabel>
              <coverage>0.80</coverage>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
              </groups>
              <parts>
                <li>
                  <def>SlimeSkull</def>
				  <customLabel>slime core membrane</customLabel>
                  <coverage>0.18</coverage>
                  <depth>Inside</depth>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                  <parts>
                    <li>
                      <def>SlimeCore</def>
                      <coverage>0.8</coverage>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>left shoulder membrane</customLabel>
          <coverage>0.12</coverage>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>left arm membrane</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>left hand membrane</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Finger</def>
                      <customLabel>left pinky membrane</customLabel>
                      <coverage>0.06</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left ring finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left middle finger membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left index finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left thumb membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>right shoulder membrane</customLabel>
          <coverage>0.12</coverage>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>right arm membrane</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>right hand membrane</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Finger</def>
                      <customLabel>right pinky membrane</customLabel>
                      <coverage>0.06</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right ring finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right middle finger membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right index finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right thumb membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Waist</def>
		  <customLabel>waist membrane</customLabel>
          <coverage>0</coverage>
          <height>Bottom</height>
          <groups>
            <li>Waist</li>
          </groups>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left leg membrane</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>left foot membrane</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
              <parts>
                <li>
                  <def>Toe</def>
                  <customLabel>left little toe membrane</customLabel>
                  <coverage>0.06</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left fourth toe membrane</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left middle toe membrane</customLabel>
                  <coverage>0.08</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left second toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left big toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right leg membrane</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>right foot membrane</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
              <parts>
                <li>
                  <def>Toe</def>
                  <customLabel>right little toe membrane</customLabel>
                  <coverage>0.06</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right fourth toe membrane</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right middle toe membrane</customLabel>
                  <coverage>0.08</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right second toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right big toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>

 <!-- pet slime --> 
 
  <BodyDef>
    <defName>PetSlime</defName>
    <label>lesser slime</label>
 	 	  <corePart>
              <def>PetSlimeBody</def>
			  <height>Middle</height>
			  <depth>Outside</depth>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>SlimeSkull</def>
				  <customLabel>inner membrane</customLabel>
                  <coverage>0.18</coverage>
                  <depth>Inside</depth>
                  <parts>
                    <li>
                      <def>SlimeCore</def>
                      <coverage>0.8</coverage>
                    </li>
                  </parts>
                </li>
              </parts>
 		   </corePart>
  </BodyDef>
  
  <!--Muffalo Slime -->
  	<BodyDef>
		<defName>MuffaloSlimeBody</defName>
		<label>muffalo slime</label>
		<corePart>
			<def>Body</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<customLabel>body membrane</customLabel>
			<parts>
				<li>
					<def>SlimeNeck</def>
					<coverage>0.22</coverage>
					<customLabel>neck membrane</customLabel>
					<height>Top</height>
					<parts>
						<li>
							<def>Head</def>
							<coverage>0.75</coverage>
							<customLabel>head membrane</customLabel>
							<parts>
								<li>
									<def>SlimeSkull</def>
									<coverage>0.25</coverage>
									<customLabel>slime core membrane</customLabel>
									<depth>Inside</depth>
									<parts>
										<li>
											<def>SlimeCore</def>
											<coverage>0.7</coverage>
											<depth>Inside</depth>
										</li>
									</parts>
								</li>
							</parts>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>front left leg membrane</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<groups>
						<li>FrontLeftLeg</li>
					</groups>
					<parts>
						<li>
							<def>Hoof</def>
							<customLabel>front left hoof membrane</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>front right leg membrane</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<groups>
						<li>FrontRightLeg</li>
					</groups>
					<parts>
						<li>
							<def>Hoof</def>
							<customLabel>front right hoof membrane</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>rear left leg membrane</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Hoof</def>
							<customLabel>rear left hoof membrane</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>rear right leg membrane</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Hoof</def>
							<customLabel>rear right hoof membrane</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
			</parts>
		</corePart>
	</BodyDef>


</Defs>
