<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <BodyDef>
    <defName>SlimePersonBody</defName>
    <label>slime</label>
    <corePart>
      <def>Torso</def>
      <customLabel>torso membrane</customLabel>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
      </groups>
      <parts>
        <li>
          <def>SlimeNeck</def>
		  <customLabel>neck membrane</customLabel>
          <coverage>0.075</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
			  <customLabel>head membrane</customLabel>
              <coverage>0.80</coverage>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
              </groups>
              <parts>
                <li>
                  <def>SlimeSkull</def>
				  <customLabel>slime core membrane</customLabel>
                  <coverage>0.18</coverage>
                  <depth>Inside</depth>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Brain</def>
					  <customLabel>slime core</customLabel>
                      <coverage>0.8</coverage>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>left shoulder membrane</customLabel>
          <coverage>0.12</coverage>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>left arm membrane</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>left hand membrane</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Finger</def>
                      <customLabel>left pinky membrane</customLabel>
                      <coverage>0.06</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left ring finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left middle finger membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left index finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>left thumb membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>right shoulder membrane</customLabel>
          <coverage>0.12</coverage>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>right arm membrane</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>right hand membrane</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Finger</def>
                      <customLabel>right pinky membrane</customLabel>
                      <coverage>0.06</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right ring finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right middle finger membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right index finger membrane</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>Finger</def>
                      <customLabel>right thumb membrane</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>Hands</li>
                        <li>RightHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Waist</def>
		  <customLabel>waist membrane</customLabel>
          <coverage>0</coverage>
          <height>Bottom</height>
          <groups>
            <li>Waist</li>
          </groups>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left leg membrane</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>left foot membrane</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
              <parts>
                <li>
                  <def>Toe</def>
                  <customLabel>left little toe membrane</customLabel>
                  <coverage>0.06</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left fourth toe membrane</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left middle toe membrane</customLabel>
                  <coverage>0.08</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left second toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>left big toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right leg membrane</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>right foot membrane</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
              <parts>
                <li>
                  <def>Toe</def>
                  <customLabel>right little toe membrane</customLabel>
                  <coverage>0.06</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right fourth toe membrane</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right middle toe membrane</customLabel>
                  <coverage>0.08</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right second toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
                <li>
                  <def>Toe</def>
                  <customLabel>right big toe membrane</customLabel>
                  <coverage>0.09</coverage>
                  <groups>
                    <li>Feet</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>

</Defs>
