<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
  <defName>NamerFactionSlime</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->The [connecting_verb] [faction_title]</li>
		
		<li>faction_title->Slime People</li>
		<li>faction_title->Slime Tribe</li>
		<li>faction_title->Slime Collective</li>
		<li>faction_title->Slime Community</li>
		
		<li>connecting_verb->squishy</li>
		<li>connecting_verb->squished</li>
		<li>connecting_verb->squishing</li>
		<li>connecting_verb->wet</li>
			</rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerSettlementSlime</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[place_name]</li>
		<li>r_name->[place_type] of [place_name]</li>
		
		<li>place_name->Bubblepop</li>
		<li>place_name->Bloppop</li>
		<li>place_name->Squish</li>
		<li>place_name->Bloppopbubble</li>
		<li>place_name->Popbubblepop</li>
		<li>place_name->Blobpop</li>
		<li>place_name->Blobblobpopbubble</li>
		<li>place_name->Squishpopbubble</li>
		<li>place_name->Blobpopbubblepop</li>
		<li>place_name->Poppopbubblepopblop</li>
		<li>place_name->Bloppopblop</li>

		<li>place_type->Lake</li>
		<li>place_type->Tribe</li>
		<li>place_type->Village</li>

      </rulesStrings>
    </rulePack>
  </RulePackDef>


</Defs>
