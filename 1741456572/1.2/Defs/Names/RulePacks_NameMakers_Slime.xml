<Defs>

  <RulePackDef>
    <defName>NamerPersonSlime</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[Slimefirst] [Slimelast]</li>
        <li>name->[Slimefirst]</li>
        <li>name->[Slimelast]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>Slimefirst</keyword>
          <path>Slime_Names/NameFirst</path>
