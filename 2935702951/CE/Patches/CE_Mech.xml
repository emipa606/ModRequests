			<ArmorRating_Blunt>10</ArmorRating_Blunt>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="TF_Misc"]/statBases/ArmorRating_Sharp</xpath>
		<value>
			<ArmorRating_Sharp>12</ArmorRating_Sharp>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="TF_Misc"]/statBases</xpath>
		<value>
			<MeleeDodgeChance>0.23</MeleeDodgeChance>
			<MeleeCritChance>0.49</MeleeCritChance>
			<MeleeParryChance>0.48</MeleeParryChance>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="TF_Misc"]/tools</xpath>
		<value>
			<tools>
				<li Class="CombatExtended.ToolCE">
					<label>pneumatic hammer</label>
					<capacities>
						<li>Cut</li>
