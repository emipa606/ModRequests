<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ExGT UrbanCombat</li>
		</mods>

		<nomatch Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationSequence">
					<success>Always</success>
					<operations>
						<li Class="PatchOperationTest">
							<xpath>Defs/ThingDef[defName="Human"]/comps</xpath>
							<success>Invert</success>
						</li>
						<li Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="Human"]</xpath>
							<value>
								<comps />
							</value>
						</li>
					</operations>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/comps</xpath>
					<value>
						<li Class="WeaponUtilityII.CompProperties_PawnEquipmentGizmo"></li>
					</value>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/AlienRace.ThingDef_AlienRace</xpath>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationSequence">
								<success>Always</success>
								<operations>
									<li Class="PatchOperationTest">
										<xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
										<success>Invert</success>
									</li>
									<li Class="PatchOperationAdd">
										<xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]</xpath>
										<value>
											<comps />
										</value>
									</li>
								</operations>
							</li>

							<li Class="PatchOperationAdd">
								<xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
								<value>
									<li Class="WeaponUtilityII.CompProperties_PawnEquipmentGizmo"></li>
								</value>
							</li>
						</operations>
					</match>
				</li>
			</operations>
		</nomatch>

	</Operation>

    
</Patch>