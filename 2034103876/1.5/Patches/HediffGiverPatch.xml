<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Defs\Bodies\Bodies_Humanlike.xml -->

	<!-- Human -->
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffGiverSetDef[defName = "OrganicStandard" or defName = "Human"]/hediffGivers</xpath>
				<value>
				  <li Class="AvP.HediffGiver_StartWithXenomorphHediff">
					<hediff>AvP_HiddenXenomorphImpregnation</hediff>
					<partsToAffect>
					<li>Torso</li>
					</partsToAffect>
					<chance>5</chance>
				  </li>
				  <li Class="AvP.HediffGiver_StartWithXenomorphHediff">
					<hediff>AvP_HiddenNeomorphImpregnation</hediff>
					<partsToAffect>
					<li>Torso</li>
					</partsToAffect>
					<chance>5</chance>
				  </li>
				</value>
			</li>
		</operations>	
	</Operation>
</Patch>
