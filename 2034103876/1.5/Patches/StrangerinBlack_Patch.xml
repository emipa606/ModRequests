<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Allows other races to spawn for Stranger in Black if there is a PawnKindDef with StrangerinBlack with the same race as the one your playing as -->
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/IncidentDef[defName = "StrangerInBlackJoin"]/workerClass</xpath>
				<value><workerClass>AvP.IncidentWorker_WandererJoin</workerClass></value>
			</li>
		</operations>	
	</Operation>
</Patch>
