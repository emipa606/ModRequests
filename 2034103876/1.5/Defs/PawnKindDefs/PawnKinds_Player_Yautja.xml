<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef Abstract="True" Name="BaseYautjaPawnKind">
    <race>AvP_Alien_Yautja</race>
    <combatPower>40</combatPower>
	<initialWillRange>3~5</initialWillRange>
	<initialResistanceRange>17~27</initialResistanceRange>
    <isFighter>true</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
	    <lifeStages>
      <li>
        <bodyGraphicData>
          <drawSize>0.75</drawSize>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <drawSize>1</drawSize>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <backstoryFiltersOverride Inherit="false">
      <li>
        <categories>
          <li>AvP_Yautja_Standard</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
	<modExtensions>
	  <!--li Class="AlienRace.Info">
		<allowHumanBios>false</allowHumanBios>
		<maleGenderProbability>0.75</maleGenderProbability>
	  </li-->
	</modExtensions>
  </PawnKindDef>

  <PawnKindDef ParentName="BaseYautjaPawnKind">
    <defName>Colonist_AvP_Yautja</defName>
    <label>Yautja colonist</label>
    <defaultFactionType>AvP_Yautja_PlayerColony</defaultFactionType>
    <apparelTags>
		<li>YautjaGreaves</li>
		<li>YautjaWebbing</li>
    </apparelTags>
    <apparelMoney>
      <min>10000</min>
      <max>10000</max>
    </apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
  </PawnKindDef>
  
  <PawnKindDef ParentName="BaseYautjaPawnKind">
