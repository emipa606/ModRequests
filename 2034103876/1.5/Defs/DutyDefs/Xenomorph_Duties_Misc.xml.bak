							<!-- There Are No Drones! -->
							<li Class="ThinkNode_ConditionalPawnKindCountLesser">
							  <pawnKind>RRY_Xenomorph_Drone</pawnKind>
							  <pawnKindCount>1</pawnKindCount>
							  <subNodes>
							  
