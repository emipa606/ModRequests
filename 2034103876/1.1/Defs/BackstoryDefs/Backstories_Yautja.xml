			<amount>2</amount>
		</li>
		<li>
			<defName>Mining</defName>
			<amount>4</amount>
		</li>
		<li>
			<defName>Plants</defName>
			<amount>3</amount>
		</li>
		<li>
			<defName>Artistic</defName>
			<amount>2</amount>
		</li>
    </skillGains>
    <spawnCategories>
      <li>AvP_Yautja_Standard</li>
