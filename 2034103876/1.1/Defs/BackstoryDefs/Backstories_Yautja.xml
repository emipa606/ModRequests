    <bodyTypeFemale>Female</bodyTypeFemale>
    <slot>Adulthood</slot>
    <workDisables>
		
	</workDisables>
	<forcedTraits>
		<li>
			<defName>Industriousness</defName>
			<degree>1</degree>
			<chance>15</chance>
		</li>
		<li>
