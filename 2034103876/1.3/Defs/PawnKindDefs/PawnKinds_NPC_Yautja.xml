<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ============================ Yautja Base ========================== -->
	<PawnKindDef Name="AvP_YautjaBase" Abstract="True">
		<defaultFactionType>AvP_Yautja_BadBloodFaction</defaultFactionType>
		<race>AvP_Alien_Yautja</race>
		<backstoryFiltersOverride Inherit="false">
			<li>
				<categories>
					<li>AvP_Yautja_Standard</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<chemicalAddictionChance>0.05</chemicalAddictionChance>
		<baseRecruitDifficulty>1</baseRecruitDifficulty>
		<initialWillRange>3~5</initialWillRange>
		<initialResistanceRange>17~27</initialResistanceRange>
		<apparelAllowHeadgearChance>0.85</apparelAllowHeadgearChance>
		<apparelTags>
			<!--li>YautjaArmour</li>
			<li>YautjaWristblade</li>
			<li>YautjaHelmet</li-->
			<li>YautjaApparel</li>
			<li>YautjaWristblade</li>
		</apparelTags>
		<modExtensions>
			<!--li Class="AlienRace.Info">
			<allowHumanBios>false</allowHumanBios>
			<maleGenderProbability>0.75</maleGenderProbability>
			</li-->
		</modExtensions>
	</PawnKindDef>
	
<!-- ============================ Yautja Civilian Base ========================== -->
	<PawnKindDef Name="AvP_YautjaCivBase" ParentName="AvP_YautjaBase" Abstract="True">
		<combatPower>50</combatPower>
		<itemQuality>Normal</itemQuality>
		<isFighter>false</isFighter>
		<gearHealthRange>
			<min>0.2</min>
			<max>2.0</max>
		</gearHealthRange>
		<apparelMoney>
			<min>300</min>
			<max>900</max>
		</apparelMoney>
		<weaponMoney>
			<min>300</min>
			<max>750</max>
		</weaponMoney>
		<weaponTags>
		<li>YautjaCombistaff</li>
		<li>YautjaHuntingBow</li>
		<li>YautjaNeedler</li>
		</weaponTags>
		<techHediffsMoney>
			<min>200</min>
			<max>300</max>
		</techHediffsMoney>
		<techHediffsTags>
			<li>Simple</li>
		</techHediffsTags>
		<techHediffsChance>0.51</techHediffsChance>
		<invNutrition>2.55</invNutrition>
		<inventoryOptions>
			<skipChance>0.75</skipChance>
			<subOptionsChooseOne>
				<li>
					<choiceChance>90</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>
						<min>25</min>
						<max>70</max>
					</countRange>
				</li>
				<li>
					<choiceChance>10</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>
						<min>700</min>
						<max>1000</max>
					</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
	</PawnKindDef>
	 
