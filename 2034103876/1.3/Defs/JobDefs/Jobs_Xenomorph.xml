<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <JobDef>
    <defName>AvP_Job_Xenomorph_EnterHiveTunnel</defName>
    <driverClass>JobDriver_Xenomorph_EnterHiveTunnel</driverClass>
    <reportString>entering TargetA.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>AvP_Job_Xenomorph_LayEgg</defName>
    <driverClass>JobDriver_Xenomorph_LayEgg</driverClass>
    <reportString>laying egg.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>AvP_Job_Xenomorph_Kidnap</defName>
    <driverClass>JobDriver_Xenomorph_Kidnap</driverClass>
    <reportString>kidnapping TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <checkOverrideOnDamage>Never</checkOverrideOnDamage>
  </JobDef>
  
  <JobDef>
    <defName>AvP_Job_Xenomorph_PredalienImpregnate</defName>
    <driverClass>JobDriver_Xenomorph_Predalien_Impregnate</driverClass>
    <reportString>Impregnating TargetA.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>AvP_Job_Xenomorph_MaintainHive</defName>
    <driverClass>JobDriver_Xenomorph_Maintain</driverClass>
    <reportString>maintaining TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>AvP_Job_Xenomorph_Construct_Hive_Slime</defName>
    <driverClass>JobDriver_Xenomorph_ConstructHive_Slime</driverClass>
    <reportString>constructing hive slime.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

  <JobDef>
    <defName>AvP_Job_Xenomorph_Construct_Hive_Node</defName>
    <driverClass>JobDriver_Xenomorph_ConstructHive_Node</driverClass>
    <reportString>constructing hive node.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

  <JobDef>
    <defName>AvP_Job_Xenomorph_Construct_Hive_Wall</defName>
    <driverClass>JobDriver_Xenomorph_ConstructHive_Wall</driverClass>
    <reportString>constructing hive wall.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

  <JobDef>
    <defName>AvP_Job_Xenomorph_Construct_Hive_Roof</defName>
    <driverClass>JobDriver_Xenomorph_ConstructHive_Roof</driverClass>
    <reportString>constructing hive roof.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

  <JobDef>
    <defName>AvP_Job_Xenomorph_Hosthunting_AttackMelee</defName>
    <driverClass>JobDriver_Xenomorph_Hosthunting_AttackMelee</driverClass>
    <reportString>melee attacking TargetA.</reportString>
    <alwaysShowWeapon>true</alwaysShowWeapon>
    <casualInterruptible>false</casualInterruptible>
    <collideWithPawns>true</collideWithPawns>
  </JobDef>
  
  <JobDef>
		<defName>AvP_Xenomorph_Ingest</defName>
		<driverClass>JobDriver_Xenomorph_Ingest</driverClass>
		<reportString>consuming TargetA.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

</Defs>