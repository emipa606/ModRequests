  <ThingDef Class="AvP.ThingDef_CryptosleepCasket" ParentName="BuildingBase">
    <defName>AvP_XenomorphCryptosleepCasket</defName>
    <label>ancient cryptosleep casket</label>
    <description>This cryptosleep casket looks like it has been here for a very long time. Who knows what it might contain?</description>
    <thingClass>AvP.Building_XenomorphCryptosleepCasket</thingClass>
    <graphicData>
      <texPath>Things/Building/Ship/ShipCryptosleepCasket</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,2)</drawSize>
      <shadowData>
        <volume>(0.83, 0.3, 1.7)</volume>
      </shadowData>
    </graphicData>
    <minifiedDef>MinifiedThingNoBox</minifiedDef>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <blockWind>true</blockWind>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <Mass>50</Mass>
      <MaxHitPoints>250</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(1,2)</size>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(1,0,0)</interactionCellOffset>
    <costList>
      <Steel>180</Steel>
      <Uranium>5</Uranium>
    </costList>
    <killedLeavings>
      <ChunkSlagSteel>1</ChunkSlagSteel>
      <Steel>25</Steel>
      <Uranium>3</Uranium>
    </killedLeavings>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>2.66</explosiveRadius>
        <explosiveDamageType>Flame</explosiveDamageType>
      </li>
    </comps>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
  </ThingDef>

