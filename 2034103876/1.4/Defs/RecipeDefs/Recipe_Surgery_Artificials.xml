<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RecipeDef ParentName="SurgeryFlesh">
    <defName>AvP_RepairInhibitor</defName>
    <label>Attempt repairing a behavorial inhibitor</label>
    <description>Inhibitor repair</description>
    <workerClass>Recipe_InstallNaturalBodyPart</workerClass>
    <jobString>Attempting to repair a behavorial inhibitor</jobString>
    <workAmount>2200</workAmount>
    <removesHediff>AvP_Defective_Inhibitor</removesHediff>
    <successfullyRemovedHediffMessage>{0} has successfully repaired {1}'s behavorial inhibitor.</successfullyRemovedHediffMessage>
    <deathOnFailedSurgeryChance>2.0</deathOnFailedSurgeryChance>
    <surgerySuccessChanceFactor>1</surgerySuccessChanceFactor>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Manufactured</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Manufactured</li>
      </categories>
      <disallowedThingDefs>
        <li>ComponentIndustrial</li>
		<li>Wort</li>
		<li>Neutroamine</li>
		<li>Chemfuel</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
	<appliedOnFixedBodyParts>
      <li>AvP_BehavorialInhibitor</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>
 </Defs>