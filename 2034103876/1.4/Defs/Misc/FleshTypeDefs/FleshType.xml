    </genericWounds>
  </FleshTypeDef>
  
  <FleshTypeDef ParentName="AvP_Flesh_Base">
    <defName>AvP_SynthFlesh</defName>
    <ateDirect>AteInsectMeatDirect</ateDirect>
    <ateAsIngredient>AteInsectMeatAsIngredient</ateAsIngredient>
