<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef ParentName="BuildingBase">
    <defName>BoardGameBoxes</defName>
    <label>board game boxes</label>
    <description>A stack of board game boxes.</description>
    <graphicData>
      <texPath>Things/Building/Decor/BoardGameBoxes</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <fillPercent>0.15</fillPercent>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <designationCategory>Joy</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <socialPropernessMatters>true</socialPropernessMatters>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <building>
      <isEdifice>false</isEdifice>
  	  <canBuildNonEdificesUnder>true</canBuildNonEdificesUnder>
    </building>
    <clearBuildingArea>false</clearBuildingArea>
    <size>(1,1)</size>
    <researchPrerequisites>
      <li>TabletopGames</li>
    </researchPrerequisites>
    <stuffCategories>
      <li>Woody</li>
      <li>Fabric</li>
      <li>Leathery</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>10</costStuffCount>
    <costList>
      <WoodLog>10</WoodLog>
    </costList>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <MarketValue>350</MarketValue>
      <Mass>7.5</Mass>
      <WorkToBuild>5000</WorkToBuild>
    </statBases>
    <comps>
    </comps>
    <uiIconOffset>(0,0.2)</uiIconOffset>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>PlayerHandbook</defName>
