<Defs>
    <!--Revolver-->
    <SoundDef>
        <defName>Revolver_Begin</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Folder">
                        <clipFolderPath>Vanilla/Revolver/Begin</clipFolderPath>
                    </li>
                </grains>
                <volumeRange>20~20</volumeRange>
            </li>
        </subSounds>
    </SoundDef>
