<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="CrateBase">
    <defName>CrateStorage</defName>
    <label>storage crate</label>
    <description>...</description>
    <size>(1, 1)</size>
    <fillPercent>0.30</fillPercent>
    <graphicData>
      <texPath>CrateA</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1, 1)</drawSize>
      <shadowData>
        <volume>(1, 0.5, 1)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
    </statBases>  
    <hasInteractionCell>false</hasInteractionCell>
    <building>
      <openingEffect>HermeticCrateOpened</openingEffect>
    </building>
    <comps>
      <li Class="CompProperties_EmptyStateGraphic">
        <graphicData>
          <texPath>CrateA</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <drawSize>(1, 1)</drawSize>
        </graphicData>
      </li>
    </comps>
    <tickerType>Normal</tickerType>
  </ThingDef>

