<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=============== Neutroamine ====================-->
  
  <ThingDef ParentName="ResourceBase">
    <defName>Neutroamine</defName>
    <label>Neutroamine</label>
    <description>Eine synthetische Chemikalie. Neutramin als Inhaltsstoff.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Neutroamine</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>0.90</drawSize>
    </graphicData>
    <stackLimit>150</stackLimit>
    <statBases>
	  <WorkToMake>200</WorkToMake>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>6</MarketValue>
      <Mass>0.02</Mass>
      <Flammability>1.0</Flammability>
	  <DeteriorationRate>1.0</DeteriorationRate>
	</statBases>
	<recipeMaker>
	    <skillRequirements>
		<Cooking>1</Cooking>
		</skillRequirements>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Brewing</soundWorking>
    </recipeMaker>
	<costList>
	<Neutroaleaves>25</Neutroaleaves>
	</costList>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

</Defs>