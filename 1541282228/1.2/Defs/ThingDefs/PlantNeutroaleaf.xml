<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=========================== Neutroaleaf ==============================-->
  <ThingDef ParentName="PlantBase">
    <defName>PlantNeutroaleaf</defName>
    <label>Neutroaleaf</label>
    <description>Wächst schnell, die Blätter können in Neutramin verfeinert werden.</description>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Beauty>0</Beauty>
	  <Nutrition>0.05</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/NeutroaleafPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
      <ingestible>
      <preferability>DesperateOnly</preferability>
    </ingestible>
    <plant>
	    <fertilitySensitivity>1</fertilitySensitivity>
		<growDays>4.5</growDays>
		<dieIfLeafless>true</dieIfLeafless>
        <!--reproduces>true</reproduces!-->
        <harvestTag>Standard</harvestTag>
        <harvestedThingDef>Neutroaleaves</harvestedThingDef>
        <harvestYield>3</harvestYield>
	  <sowTags>
		<li>Ground</li>
		<li>Hydroponic</li>
	  </sowTags>
	  <sowMinSkill>12</sowMinSkill>
      <topWindExposure>0.1</topWindExposure>
      <!--visualSizeRange>
        <min>0.5</min>
        <max>1</max>
      </visualSizeRange-->
    </plant>
  </ThingDef>
</Defs>
  