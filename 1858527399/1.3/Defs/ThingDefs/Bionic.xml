<Defs>
    <!-- Bionic Heart -->
    <HediffDef ParentName="AddedBodyPartBase">
        <defName>AIHediffs_BionicHeart</defName>
        <label>armor-plated bionic heart</label>
        <labelNoun>an armor-plated bionic heart</labelNoun>
        <description>An installed armor-plated bionic heart.</description>
        <descriptionHyperlinks>
            <ThingDef>AIThings_BionicHeart</ThingDef>
        </descriptionHyperlinks>
        <spawnThingOnRemoved>AIThings_BionicHeart</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.15</partEfficiency>
            <betterThanNatural>true</betterThanNatural>
        </addedPartProps>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
                    <ArmorRating_Sharp>0.04</ArmorRating_Sharp>
                </statOffsets>
            </li>
        </stages>
    </HediffDef>

    <ThingDef ParentName="BodyPartBionicBase">
        <defName>AIThings_BionicHeart</defName>
        <label>armor-plated bionic heart</label>
        <description>An bionic eye augmented with armor plating.</description>
        <descriptionHyperlinks>
            <RecipeDef>AIRecipes_InstallBionicHeart</RecipeDef>
        </descriptionHyperlinks>
        <statBases>
            <Mass>5</Mass>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardHighFreq</li>
        </thingSetMakerTags>
        <costList>
            <Plasteel>3</Plasteel>
            <ComponentSpacer>4</ComponentSpacer>
            <BionicHeart>1</BionicHeart>
        </costList>

    </ThingDef>

    <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
        <defName>AIRecipes_InstallBionicHeart</defName>
        <label>install armor-plated bionic heart</label>
        <description>Install an armor-plated bionic heart</description>
        <descriptionHyperlinks>
            <ThingDef>AIThings_BionicHeart</ThingDef>
        </descriptionHyperlinks>
        <jobString>Installing armor-plated bionic heart.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_BionicHeart</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>AIThings_BionicHeart</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>
        <addsHediff>AIHediffs_BionicHeart</addsHediff>
    </RecipeDef>


