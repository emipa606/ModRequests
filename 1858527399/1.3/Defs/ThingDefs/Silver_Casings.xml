<Defs>
    <!-- Silver Heart Casing-->
    <HediffDef ParentName="ImplantHediffBase">
        <defName>AIHediffs_SilverHeartCasing</defName>
        <label>silver heart casing</label>
        <labelNoun>a silver heart casing</labelNoun>
        <description>An installed silver heart casing.</description>
        <descriptionHyperlinks>
            <ThingDef>AIThings_SilverHeartCasing</ThingDef>
        </descriptionHyperlinks>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Blunt>0.01</ArmorRating_Blunt>
                    <ArmorRating_Sharp>0.007</ArmorRating_Sharp>
                    <ImmunityGainSpeed>0.03</ImmunityGainSpeed>
                    <MedicalSurgerySuccessChance>0.01</MedicalSurgerySuccessChance>
                </statOffsets>
            </li>
        </stages>
        <spawnThingOnRemoved>AIThings_SilverHeartCasing</spawnThingOnRemoved>
    </HediffDef>

    <ThingDef ParentName="BodyPartProstheticMakeableBase">
        <defName>AIThings_SilverHeartCasing</defName>
        <label>silver heart casing</label>
        <description>A silver casing meant to protect a human heart.</description>
        <descriptionHyperlinks>
            <RecipeDef>AIRecipes_InstallSilverHeartCasing</RecipeDef>
        </descriptionHyperlinks>
        <costList>
            <Silver>4</Silver>
            <ComponentIndustrial>3</ComponentIndustrial>
        </costList>
        <statBases>
            <Mass>2</Mass>
        </statBases>
    </ThingDef>

    <RecipeDef ParentName="SurgeryInstallImplantBase">
        <defName>AIRecipes_InstallSilverHeartCasing</defName>
        <label>install silver heart casing</label>
        <description>Install a silver heart casing.</description>
        <descriptionHyperlinks>
            <ThingDef>AIThings_SilverHeartCasing</ThingDef>
        </descriptionHyperlinks>
        <jobString>Installing silver heart casing.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_SilverHeartCasing</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>AIThings_SilverHeartCasing</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>
        <addsHediff>AIHediffs_SilverHeartCasing</addsHediff>
