<?xml version="1.0" encoding="UTF-8"?>

<Defs>
    <RecipeDef ParentName="AISurgery_InstallArchotech">
        <defName>AISurgery_InstallArmoredArchotechHeart</defName>
        <label>install armor-plated archotech heart</label>
        <description>install an armor-plated archotech heart.</description>
        <jobString>Installing armor-plated archotech heart.</jobString>

        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_ArmoredArchotechHeart</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>

        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AIThings_ArmoredArchotechHeart</li>
            </thingDefs>
        </fixedIngredientFilter>

        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>

        <addsHediff>AIHediffs_ArmoredArchotechHeart</addsHediff>
    </RecipeDef>

