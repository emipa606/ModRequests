<?xml version="1.0" encoding="UTF-8" ?>

<Defs>
    <RecipeDef ParentName="AISurgery_Base">
        <defName>AISurgery_InstallArmoredBionicBrain</defName>
        <label>install armor-plated bionic brain</label>
        <description>install an armor-plated bionic brain.</description>
        <jobString>Installing armor-plated bionic brain.</jobString>

        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_ArmoredBionicBrain</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>

        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AIThings_ArmoredBionicBrain</li>
            </thingDefs>
        </fixedIngredientFilter>

        <appliedOnFixedBodyParts>
            <li>Brain</li>
        </appliedOnFixedBodyParts>

        <addsHediff>AIHediffs_ArmoredBionicBrain</addsHediff>
    </RecipeDef>

