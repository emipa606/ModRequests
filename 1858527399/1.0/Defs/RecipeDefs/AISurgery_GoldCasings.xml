<?xml version="1.0" encoding="UTF-8" ?>

<Defs>
    <RecipeDef ParentName="AISurgery_InstallCasing">
        <defName>AISurgery_InstallGoldHeartCasing</defName>
        <label>install gold heart casing</label>
        <description>install a gold heart casing.</description>
        <jobString>Installing gold heart casing.</jobString>

        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_GoldHeartCasing</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>

        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AIThings_GoldHeartCasing</li>
            </thingDefs>
        </fixedIngredientFilter>

        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>

        <addsHediff>AIHediffs_GoldHeartCasing</addsHediff>
    </RecipeDef>

