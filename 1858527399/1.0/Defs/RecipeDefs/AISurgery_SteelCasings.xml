<?xml version="1.0" encoding="UTF-8" ?>

<Defs>
    <RecipeDef ParentName="AISurgery_InstallCasing">
        <defName>AISurgery_InstallSteelHeartCasing</defName>
        <label>install steel heart casing</label>
        <description>install a steel heart casing.</description>
        <jobString>Installing steel heart casing.</jobString>

        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_SteelHeartCasing</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>

        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AIThings_SteelHeartCasing</li>
            </thingDefs>
        </fixedIngredientFilter>

        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>

        <addsHediff>AIHediffs_SteelHeartCasing</addsHediff>
    </RecipeDef>

