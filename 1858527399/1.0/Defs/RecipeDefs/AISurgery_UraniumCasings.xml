<?xml version="1.0" encoding="UTF-8" ?>

<Defs>
    <RecipeDef ParentName="AISurgery_InstallCasing">
        <defName>AISurgery_InstallUraniumHeartCasing</defName>
        <label>install uranium heart casing</label>
        <description>install a uranium heart casing.</description>
        <jobString>Installing uranium heart casing.</jobString>

        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIThings_UraniumHeartCasing</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>

        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AIThings_UraniumHeartCasing</li>
            </thingDefs>
        </fixedIngredientFilter>

        <appliedOnFixedBodyParts>
            <li>Heart</li>
        </appliedOnFixedBodyParts>

        <addsHediff>AIHediffs_UraniumHeartCasing</addsHediff>
    </RecipeDef>

