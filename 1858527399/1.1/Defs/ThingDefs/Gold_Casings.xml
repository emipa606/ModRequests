            <RecipeDef>AIRecipes_InstallGoldHeartCasing</RecipeDef>
        </descriptionHyperlinks>
        <costList>
            <Gold>4</Gold>
            <ComponentIndustrial>3</ComponentIndustrial>
        </costList>
        <statBases>
            <Mass>2</Mass>
        </statBases>
    </ThingDef>

