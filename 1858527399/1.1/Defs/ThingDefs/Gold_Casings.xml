<Defs>
    <!-- Gold Heart Casing-->
    <HediffDef ParentName="ImplantHediffBase">
        <defName>AIHediffs_GoldHeartCasing</defName>
        <label>gold heart casing</label>
        <labelNoun>a gold heart casing</labelNoun>
        <description>An installed gold heart casing.</description>
        <descriptionHyperlinks>
            <ThingDef>AIThings_GoldHeartCasing</ThingDef>
        </descriptionHyperlinks>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Blunt>0.012</ArmorRating_Blunt>
                    <ArmorRating_Sharp>0.008</ArmorRating_Sharp>
                    <ImmunityGainSpeed>-0.03</ImmunityGainSpeed>
                    <SocialImpact>0.01</SocialImpact>
                </statOffsets>
            </li>
        </stages>
        <spawnThingOnRemoved>AIThings_GoldHeartCasing</spawnThingOnRemoved>
    </HediffDef>

    <ThingDef ParentName="BodyPartProstheticMakeableBase">
        <defName>AIThings_GoldHeartCasing</defName>
        <label>gold heart casing</label>
        <description>A gold casing meant to protect a human heart.</description>
