<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RecipeDef ParentName="SurgeryFlesh">
    <defName>EVOR_Surgery_Primary_HighPincer</defName>
    <label>&lt;color=#00FFF7&gt;implant high pressure pincer&lt;/color&gt;</label>
    <description>Implant a high pressure pincer.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Implanting high pressure pincer.</jobString>
    <workAmount>3000</workAmount>
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
    <recipeUsers>
      <li>Human</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>EVOR_Item_HighPincer</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>EVOR_Item_HighPincer</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Shoulder</li>
    </appliedOnFixedBodyParts>
    <addsHediff>EVOR_Hediff_Primary_HighPincer</addsHediff>
    <descriptionHyperlinks><ThingDef>EVOR_Item_HighPincer</ThingDef></descriptionHyperlinks>
  </RecipeDef>
  
