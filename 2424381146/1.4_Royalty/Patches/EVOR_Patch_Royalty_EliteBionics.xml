<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>Elite Bionics Framework</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Sternum_NornCrown"]/comps/li[@Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust"]</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Sternum_NornCrown"]/comps</xpath>
						<value>
							<li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust">
								<linearAdjustment>5</linearAdjustment>
								<scaleAdjustment>0.2</scaleAdjustment>
							</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Sternum_ReactiveShimmerworms"]/comps/li[@Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust"]</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Sternum_ReactiveShimmerworms"]/comps</xpath>
						<value>
							<li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust">
								<linearAdjustment>5</linearAdjustment>
								<scaleAdjustment>0.2</scaleAdjustment>
							</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Torso_MucousGland"]/comps/li[@Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust"]</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/HediffDef[defName="EVOR_Hediff_Torso_MucousGland"]/comps</xpath>
						<value>
							<li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust">
								<linearAdjustment>5</linearAdjustment>
								<scaleAdjustment>0.2</scaleAdjustment>
							</li>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>