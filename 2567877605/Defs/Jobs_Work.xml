<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <JobDef>
    <defName>RecycleThisDestroy</defName>
    <driverClass>RecycleThis.JobDriver_DestroyThing</driverClass>
    <reportString>getting rid of TargetA.</reportString>
    <modExtensions>
      <li Class="RecycleThis.RecycleThisModExtension">
        <DestroySpeed>200</DestroySpeed>
      </li>
    </modExtensions>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>RecycleThisRecycle</defName>
    <driverClass>RecycleThis.JobDriver_RecycleThing</driverClass>
    <reportString>scrapping TargetA.</reportString>
    <modExtensions>
      <li Class="RecycleThis.RecycleThisModExtension">
        <RecycleSpeed>1600</RecycleSpeed> 
      </li>
    </modExtensions>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
</Defs>