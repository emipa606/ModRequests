<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <RecipeDef Name="SurgeryBone" ParentName="SurgeryFlesh" Abstract="True">
		<surgerySuccessChanceFactor>0.9</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0.02</deathOnFailedSurgeryChance>
        <recipeUsers>
            <li>Human</li>
        </recipeUsers>
    </RecipeDef>

<RecipeDef ParentName="SurgeryBone">
		<defName>InstallSpinalFusion</defName>
		<label>spinal fusion</label>
		<description>Fuses the spine.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Fusing the spine.</jobString>
		<workAmount>4000</workAmount>
		<ingredients>
			<li>
				<filter>
          <categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ArtificialBone</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
				<li>ArtificialBone</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Spine</li>
		</appliedOnFixedBodyParts>
		<addsHediff>SpinalFusion</addsHediff>
        <researchPrerequisite>Prosthetics</researchPrerequisite>
	</RecipeDef>
	
