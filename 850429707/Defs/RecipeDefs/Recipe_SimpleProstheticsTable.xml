<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
	<RecipeDef>
		<defName>MakeArtificialBone</defName>
		<label>Craft artificial bone</label>
		<description>Crafts an artifical bone from plasteel and steel.</description>
		<jobString>Crafting artifical bone.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>20000</workAmount>
		<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<products>
			<ArtificialBone>1</ArtificialBone>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<researchPrerequisite>Prosthetics</researchPrerequisite>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
			<Crafting>5</Crafting>
		</skillRequirements>
	</RecipeDef>
	
