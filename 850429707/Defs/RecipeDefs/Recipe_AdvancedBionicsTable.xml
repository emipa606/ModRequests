<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
<RecipeDef>
		<defName>MakeAdvancedBionicEye</defName>
		<label>Upgrade bionic eye</label>
		<description>Upgrades a bionic eye.</description>
		<jobString>Upgrading bionic part.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>40000</workAmount>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
      <li>
				<filter>
					<thingDefs>
						<li>Hyperweave</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
        <li>Hyperweave</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
      <Crafting>10</Crafting>
    </skillRequirements>
	</RecipeDef>

<RecipeDef>
		<defName>MakeAdvancedPowerArm</defName>
		<label>Upgrade power arm</label>
		<description>Upgrades a power arm. Highly lethal and effective in combat.\n\nDo not install on an unstable subject, as this might cause unwanted death and destruction.</description>
		<jobString>Upgrading bionic part.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>40000</workAmount>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<products>
			<AdvancedPowerArm>1</AdvancedPowerArm>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>PowerArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
      <li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Uranium</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>PowerArm</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
				<li>Uranium</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
      <Crafting>10</Crafting>
    </skillRequirements>
	</RecipeDef>

