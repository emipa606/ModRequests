<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<RecipeDef>
		<defName>MakePowerArm</defName>
		<label>craft power arm</label>
		<description>Attaching a power claw to a bionic arm is not as easy as it sounds, but damn is it awesome! Lethal and effective in combat.\n\nDo not install on an unstable subject, as this might cause unwanted death and destruction.</description>
		<jobString>Crafting bionic part.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>30000</workAmount>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<products>
			<PowerArm>1</PowerArm>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>PowerClaw</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicArm</li>
				<li>PowerClaw</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<researchPrerequisite>Bionics</researchPrerequisite>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
			<Crafting>7</Crafting>
		</skillRequirements>
	</RecipeDef>
	
