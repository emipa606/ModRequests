  </HediffDef>

  <HediffDef ParentName="addedPartSimple">
    <defName>SimpleProstheticArm</defName>
    <label>simple prosthetic arm</label>
    <labelNoun>a simple prosthetic arm</labelNoun>
    <defaultLabelColor>(102,178,255)</defaultLabelColor>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>8.2</power> <!-- Same as natural fist -->
            <cooldownTime>2</cooldownTime>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>SimpleProstheticArm</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.5</partEfficiency>
    </addedPartProps>
  </HediffDef>
  
  <HediffDef ParentName="addedPartSimple">
