<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   <HediffDef Name="addedPartSimple" Abstract="True">
      <hediffClass>Hediff_AddedPart</hediffClass>
      <isBad>false</isBad>
      <defaultLabelColor>(102,178,255)</defaultLabelColor>
   </HediffDef>

   <!-- Added body parts -->
   
   <HediffDef ParentName="addedPartSimple">
    <defName>PegLeg</defName>
    <label>peg leg</label>
    <labelNoun>a peg leg</labelNoun>
    <defaultLabelColor>(102,178,255)</defaultLabelColor>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.6</partEfficiency>
    </addedPartProps>
    <spawnThingOnRemoved>WoodLog</spawnThingOnRemoved>
  </HediffDef>

