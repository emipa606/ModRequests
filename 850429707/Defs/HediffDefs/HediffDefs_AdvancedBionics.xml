<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   
   <HediffDef Name="addedPartAdvanced" Abstract="True">
      <hediffClass>Hediff_AddedPart</hediffClass>
      <isBad>false</isBad>
      <defaultLabelColor>(188,39,242)</defaultLabelColor>
      <addedPartProps>
		<betterThanNatural>true</betterThanNatural>
      </addedPartProps>
   </HediffDef>
   
   <HediffDef Name="addedPartAdvancedImplant" Abstract="True">
      <hediffClass>Hediff_Implant</hediffClass>
      <defaultLabelColor>(188,39,242)</defaultLabelColor>
      <isBad>false</isBad>
      <addedPartProps>
		<betterThanNatural>true</betterThanNatural>
      </addedPartProps>
   </HediffDef>

   
   <!-- Added body parts -->
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedPowerArm</defName>
      <label>advanced power arm</label>
      <labelNoun>an advanced power arm</labelNoun>
      <spawnThingOnRemoved>AdvancedPowerArm</spawnThingOnRemoved>
      <addedPartProps>
         <partEfficiency>1</partEfficiency>
         <solid>true</solid>
         <isGoodWeapon>true</isGoodWeapon>
      </addedPartProps>
      <comps>
         <li Class="HediffCompProperties_VerbGiver">
            <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Cut</li>
            </capacities>
            <power>30</power>
            <cooldownTime>1.6</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
        </tools>
         </li>
      </comps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicArm</defName>
      <label>advanced bionic arm</label>
