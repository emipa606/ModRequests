<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   <HediffDef Name="addedPartSimple" Abstract="True">
      <hediffClass>Hediff_AddedPart</hediffClass>
      <isBad>false</isBad>
      <defaultLabelColor>(102,178,255)</defaultLabelColor>
      <priceImpact>true</priceImpact>
      <countsAsAddedPartOrImplant>true</countsAsAddedPartOrImplant>
   </HediffDef>

   <!-- Added body parts -->
   
   <HediffDef ParentName="AddedBodyPartBase">
    <defName>WoodenFoot</defName>
    <label>wooden foot</label>
    <labelNoun>a wooden foot</labelNoun>
    <description>An installed wooden foot.</description>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.60</partEfficiency>
    </addedPartProps>
    <spawnThingOnRemoved>WoodLog</spawnThingOnRemoved>
  </HediffDef>
  
  <HediffDef ParentName="addedPartSimple">
    <defName>CochlearImplant</defName>
    <label>cochlear implant</label>
    <labelNoun>a cochlear implant</labelNoun>
    <description>An installed cochlear implant</description>
    <defaultLabelColor>(102,178,255)</defaultLabelColor>
    <spawnThingOnRemoved>CochlearImplant</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>CochlearImplant</ThingDef></descriptionHyperlinks>
    <addedPartProps>
      <partEfficiency>0.65</partEfficiency>
    </addedPartProps>
  </HediffDef>
  
  <HediffDef ParentName="addedPartSimple">
    <defName>SimpleProstheticLeg</defName>
    <label>simple prosthetic leg</label>
    <labelNoun>a simple prosthetic leg</labelNoun>
    <description>An installed prosthetic leg</description>
    <defaultLabelColor>(102,178,255)</defaultLabelColor>
    <spawnThingOnRemoved>SimpleProstheticLeg</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>SimpleProstheticLeg</ThingDef></descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.8</partEfficiency>
    </addedPartProps>
  </HediffDef>

  <HediffDef ParentName="addedPartSimple">
    <defName>SimpleProstheticArm</defName>
    <label>simple prosthetic arm</label>
    <labelNoun>a simple prosthetic arm</labelNoun>
    <description>An installed prosthetic arm</description>
    <defaultLabelColor>(102,178,255)</defaultLabelColor>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>8.2</power> <!-- Same as natural fist -->
            <cooldownTime>2</cooldownTime>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>SimpleProstheticArm</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>SimpleProstheticArm</ThingDef></descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.8</partEfficiency>
    </addedPartProps>
  </HediffDef>
  
  <HediffDef ParentName="addedPartSimple">
    <defName>SpinalFusion</defName>
    <label>spinal fusion</label>
    <labelNoun>a fused spine</labelNoun>
    <description>A fused spine</description>
    <spawnThingOnRemoved>ArtificialBone</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>ArtificialBone</ThingDef></descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.7</partEfficiency>
    </addedPartProps>
  </HediffDef>
   
