<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   
   <HediffDef Name="addedPartAdvanced" Abstract="True">
      <hediffClass>Hediff_AddedPart</hediffClass>
      <isBad>false</isBad>
      <defaultLabelColor>(188,39,242)</defaultLabelColor>
      <addedPartProps>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <priceImpact>true</priceImpact>
    <countsAsAddedPartOrImplant>true</countsAsAddedPartOrImplant>
   </HediffDef>
   
   <HediffDef Name="addedPartAdvancedImplant" Abstract="True">
      <hediffClass>Hediff_Implant</hediffClass>
      <defaultLabelColor>(188,39,242)</defaultLabelColor>
      <isBad>false</isBad>
<addedPartProps>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <priceImpact>true</priceImpact>
    <countsAsAddedPartOrImplant>true</countsAsAddedPartOrImplant>
   </HediffDef>

   
   <!-- Added body parts -->
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedPowerArm</defName>
      <label>advanced power arm</label>
      <labelNoun>an advanced power arm</labelNoun>
      <description>An installed advanced power arm.</description>
      <spawnThingOnRemoved>AdvancedPowerArm</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedPowerArm</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1</partEfficiency>
         <solid>true</solid>
         <isGoodWeapon>true</isGoodWeapon>
      </addedPartProps>
      <comps>
         <li Class="HediffCompProperties_VerbGiver">
            <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Cut</li>
            </capacities>
            <power>30</power>
            <cooldownTime>1.6</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
        </tools>
         </li>
      </comps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicArm</defName>
      <label>advanced bionic arm</label>
      <labelNoun>an advanced bionic arm</labelNoun>
      <description>An installed advanced bionic arm.</description>
      <spawnThingOnRemoved>AdvancedBionicArm</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicArm</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.5</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
      <comps>
         <li Class="HediffCompProperties_VerbGiver">
            <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>14</power>
            <cooldownTime>2</cooldownTime>
            <soundMeleeHit>MeleeHit_BionicPunch</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_BionicPunch</soundMeleeMiss>
          </li>
        </tools>
         </li>
      </comps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicLeg</defName>
      <label>advanced bionic leg</label>
      <labelNoun>an advanced bionic leg</labelNoun>
      <description>An installed advanced bionic leg.</description>
      <spawnThingOnRemoved>AdvancedBionicLeg</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicLeg</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.5</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicHand</defName>
      <label>advanced bionic hand</label>
      <labelNoun>an advanced bionic hand</labelNoun>
      <description>An installed advanced bionic hand.</description>
      <spawnThingOnRemoved>AdvancedBionicHand</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicHand</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.3</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
      <comps>
         <li Class="HediffCompProperties_VerbGiver">
            <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>12</power>
            <cooldownTime>2</cooldownTime>
            <soundMeleeHit>MeleeHit_BionicPunch</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_BionicPunch</soundMeleeMiss>
          </li>
        </tools>
         </li>
      </comps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicFoot</defName>
      <label>advanced bionic foot</label>
      <labelNoun>an advanced bionic foot</labelNoun>
      <description>An installed advanced bionic foot.</description>
      <spawnThingOnRemoved>AdvancedBionicFoot</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicFoot</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.3</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicEar</defName>
      <label>advanced bionic ear</label>
      <description>An installed advanced bionic ear.</description>
      <spawnThingOnRemoved>AdvancedBionicEar</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicEar</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
      <stages>
         <li>
      <statOffsets>
               <NegotiationAbility>0.10</NegotiationAbility>
               <SocialImpact>0.10</SocialImpact>
               <TrainAnimalChance>0.10</TrainAnimalChance>
               <TameAnimalChance>0.10</TameAnimalChance>
            </statOffsets>
                     </li>
      </stages>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicEye</defName>
      <label>advanced bionic eye</label>
      <labelNoun>an advanced bionic eye</labelNoun>
      <description>An installed advanced bionic eye.</description>
      <spawnThingOnRemoved>AdvancedBionicEye</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicEye</ThingDef></descriptionHyperlinks>
      <woundAnchorRange>0</woundAnchorRange>
      <displayWound>true</displayWound>
      <addedPartProps>
         <partEfficiency>1.5</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
          <eyeGraphicSouth>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Pawn/Wounds/AdvancedEye_south</texPath>
    </eyeGraphicSouth>
    <eyeGraphicEast>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Pawn/Wounds/AdvancedEye_east</texPath>
    </eyeGraphicEast>
    <eyeGraphicScale>0.2</eyeGraphicScale>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
      <defName>AdvancedBionicSpine</defName>
      <label>advanced bionic spine</label>
      <labelNoun>an advanced bionic spine</labelNoun>
      <description>An installed advanced bionic spine.</description>
      <spawnThingOnRemoved>AdvancedBionicSpine</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>AdvancedBionicSpine</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.0</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
      <stages>
		<li>
         <capMods>
            <li>
               <capacity>Manipulation</capacity>
               <offset>0.20</offset>
            </li>
            <li>
               <capacity>Moving</capacity>
               <offset>0.20</offset>
            </li>
         </capMods>
		</li>	 
      </stages>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvancedImplant">
      <defName>ExoskeletonSuit</defName>
      <label>exoskeleton suit</label>
      <labelNoun>an exoskeleton suit</labelNoun>
      <description>An installed exoskeleton suit.</description>
      <spawnThingOnRemoved>ExoskeletonSuit</spawnThingOnRemoved>
      <descriptionHyperlinks><ThingDef>ExoskeletonSuit</ThingDef></descriptionHyperlinks>
      <addedPartProps>
         <partEfficiency>1.0</partEfficiency>
         <solid>true</solid>
      </addedPartProps>
      <stages>
		<li>
         <capMods>
            <li>
               <capacity>Manipulation</capacity>
               <offset>0.15</offset>
            </li>
            <li>
               <capacity>Moving</capacity>
               <offset>0.15</offset>
            </li>
         </capMods>
		</li>	 
      </stages>
   </HediffDef>
   
   <HediffDef ParentName="addedPartAdvanced">
    <defName>ArchotechEye</defName>
    <label>archotech eye</label>
