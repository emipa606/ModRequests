    <description>An installed archotech eye.</description>
    <spawnThingOnRemoved>ArchotechEye</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>ArchotechEye</ThingDef></descriptionHyperlinks>
    <woundAnchorRange>0</woundAnchorRange>
    <displayWound>true</displayWound>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.60</partEfficiency>
    </addedPartProps>
        <eyeGraphicSouth>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Pawn/Wounds/ArchotechEye_south</texPath>
    </eyeGraphicSouth>
    <eyeGraphicEast>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Pawn/Wounds/ArchotechEye_east</texPath>
    </eyeGraphicEast>
    <eyeGraphicScale>0.2</eyeGraphicScale>
  </HediffDef>

  <HediffDef ParentName="addedPartAdvanced">
