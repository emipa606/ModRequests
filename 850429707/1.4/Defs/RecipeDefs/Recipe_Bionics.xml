	<RecipeDef ParentName="SurgeryBionic">
		<defName>InstallPowerArm</defName>
		<label>install power arm</label>
		<description>Installs power arm.</description>
		<descriptionHyperlinks>
      <ThingDef>PowerArm</ThingDef>
      <HediffDef>PowerArm</HediffDef>
    </descriptionHyperlinks>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing power arm.</jobString>
		<workAmount>4000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>PowerArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>PowerArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Shoulder</li>
		</appliedOnFixedBodyParts>
		<addsHediff>PowerArm</addsHediff>
		<researchPrerequisite>Prosthetics</researchPrerequisite>
	</RecipeDef>
	
