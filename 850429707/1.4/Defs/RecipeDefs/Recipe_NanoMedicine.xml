<Defs>

	<RecipeDef Name="SurgeryNano" ParentName="SurgeryFlesh" Abstract="True">
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
		<skillRequirements>
            <Medicine>12</Medicine>
        </skillRequirements>
        		<surgerySuccessChanceFactor>1</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0.01</deathOnFailedSurgeryChance>
	</RecipeDef>
	
<RecipeDef ParentName="SurgeryNano">
		<defName>SurgeryRemoveFrailty</defName>
		<label>cure frailty</label>
		<description>Inject nanomachines to cure frailty.</description>
		<descriptionHyperlinks>
			<HediffDef>Frail</HediffDef>
		</descriptionHyperlinks>
		<jobString>Curing frailty.</jobString>
		<removesHediff>Frail</removesHediff>
		<successfullyRemovedHediffMessage>{0} has successfully cured {1}'s frailty.</successfullyRemovedHediffMessage>
		<workerClass>Recipe_RemoveHediff</workerClass>
		<workAmount>6000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
            <li>MedicineUltratech</li>
          </thingDefs>
				</filter>
				<count>8</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				  <li>MedicineUltratech</li>
        </thingDefs>
		</fixedIngredientFilter>
		<skillRequirements>
			<Medicine>14</Medicine>
		</skillRequirements>
		<appliedOnFixedBodyParts>
			<li>Torso</li> 
		</appliedOnFixedBodyParts>
		<researchPrerequisite>Nanomedicine</researchPrerequisite>
	</RecipeDef>
	
