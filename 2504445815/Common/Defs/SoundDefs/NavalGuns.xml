  <SoundDef>
    <defName>Sound_Single127mm</defName>  
    <context>MapOnly</context>
    <eventNames />  
    <maxSimultaneous>10</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Single127mm</clipFolderPath>
          </li>
        </grains>
        <pitchRange>
          <min>0.9</min>        
          <max>1.1</max>
        </pitchRange>       
        <volumeRange>
          <min>100</min>        
          <max>100</max>
        </volumeRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

