  <SoundDef>
    <defName>Sound_Flak38</defName>  
    <context>MapOnly</context>
    <eventNames />  
    <maxSimultaneous>30</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Flak38</clipFolderPath>
          </li>
        </grains>
        <pitchRange>
          <min>0.9</min>        
          <max>1.1</max>
        </pitchRange>       
        <volumeRange>
          <min>100</min>        
          <max>100</max>
        </volumeRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

