<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationTest">
        <xpath>Defs/ThingDef[defName="Human"]/comps</xpath>
        <success>Invert</success>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="Human"]</xpath>
        <value>
          <comps />
        </value>
      </li>
    </operations>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Human"]/comps</xpath>
    <value>
      <li Class="WeaponUtilityIII.CompProperties_PawnEquipmentGizmo"></li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Human"]/comps</xpath>
    <value>
      <li Class="MYDE_ExGTWorldWar.CompProperties_GiveDashGizmos_Pawn"></li>
    </value>
  </Operation>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/AlienRace.ThingDef_AlienRace</xpath>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationSequence">
          <success>Always</success>
          <operations>
            <li Class="PatchOperationTest">
              <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
              <success>Invert</success>
            </li>
            <li Class="PatchOperationAdd">
              <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]</xpath>
              <value>
                <comps />
              </value>
            </li>
          </operations>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
          <value>
            <li Class="WeaponUtilityIII.CompProperties_PawnEquipmentGizmo"></li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/AlienRace.ThingDef_AlienRace</xpath>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationSequence">
          <success>Always</success>
          <operations>
            <li Class="PatchOperationTest">
              <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
              <success>Invert</success>
            </li>
            <li Class="PatchOperationAdd">
              <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]</xpath>
              <value>
                <comps />
              </value>
            </li>
          </operations>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/AlienRace.ThingDef_AlienRace[race/intelligence="Humanlike"]/comps</xpath>
          <value>
            <li Class="MYDE_ExGTWorldWar.CompProperties_GiveDashGizmos_Pawn"></li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>