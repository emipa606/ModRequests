<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--衣服-->
  <ThingDef ParentName="ArmorMachineableBase">
    <defName>WW_Uniform</defName>
    <label>War Uniform</label>
    <description>A sturdy lined uniform</description>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Apparel/WW_Uniform</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.00</drawSize>
    </graphicData>
    <costList>
      <Cloth>50</Cloth>
      <Steel>50</Steel>
    </costList>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>600</MaxHitPoints>
      <WorkToMake>14000</WorkToMake>
      <Mass>8</Mass>
      <Flammability>0.3</Flammability>
      <ArmorRating_Sharp>0.40</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.20</ArmorRating_Blunt>
      <ArmorRating_Heat>0.4</ArmorRating_Heat>
      <Insulation_Cold>14.4</Insulation_Cold>
      <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.12</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/WW_Uniform</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>IndustrialAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  <!--腰带-->
  <ThingDef ParentName="ApparelBase">
    <defName>WW_BUCM</defName>
    <label>Burned umbilical cord potion</label>
    <description>Expired drugs found in abandoned factories can cause the injected person to develop burnt umbilical cord like wrists, causing their brain to become confused and feel pain. However, their blood can also quickly block the wound. These wrists can help the injected person walk and even attack</description>
    <thingClass>MYDE_ExGTWorldWar.WW_BUCM</thingClass>
    <graphicData>
      <texPath>Apparel/WW_BUCM</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tickerType>Normal</tickerType>
    <statBases>
     <MaxHitPoints>600</MaxHitPoints>
      <Mass>3</Mass>
      <WorkToMake>14000</WorkToMake>
      <Flammability>0.4</Flammability>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <costList>
      <Gold>8</Gold>
      <PsychoidLeaves>125</PsychoidLeaves>
      <ComponentIndustrial>5</ComponentIndustrial>
      <DevilstrandCloth>150</DevilstrandCloth>
      <Neutroamine>75</Neutroamine>
    </costList>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedBelt</unfinishedThingDef>
      <researchPrerequisite>MedicineProduction</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
      <useIngredientsForColor>false</useIngredientsForColor>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
      <displayPriority>300</displayPriority>
    </recipeMaker>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <wearPerDay>0</wearPerDay>
      <bodyPartGroups>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>ST_Turret</li>
      </tags>
      <wornGraphicPath>Apparel/BUCM/BUCM</wornGraphicPath>
      <developmentalStageFilter>Adult</developmentalStageFilter>
      <wornGraphicData>
        <renderUtilityAsPack>true</renderUtilityAsPack>
        <north>
          <offset>(0,-0.25)</offset>
        </north>
        <south>
          <offset>(0,-0.25)</offset>
        </south>
        <east>
          <offset>(0,-0.25)</offset>
        </east>
        <west>
          <offset>(0,-0.25)</offset>
        </west>
        <male>
          <scale>(0.4,0.4)</scale>
        </male>
        <female>
          <scale>(0.4,0.4)</scale>
        </female>
        <thin>
          <scale>(0.4,0.4)</scale>
        </thin>
        <hulk>
          <scale>(0.51,0.51)</scale>
        </hulk>
        <fat>
          <scale>(0.51,0.51)</scale>
        </fat>
      </wornGraphicData>
    </apparel>
    <allowedArchonexusCount>1</allowedArchonexusCount>
    <tradeTags>
      <li>Armor</li>
    </tradeTags>
  </ThingDef>
  <HediffDef>
    <defName>BurntTentacle</defName>
    <label>Burnt tentacle</label>
    <labelNoun>Burnt tentacle</labelNoun>
    <description>The injectors of this medication all develop strange, burnt smelling umbilical cord like wrist contacts, and become confused and constantly feel pain.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <maxSeverity>1</maxSeverity>
    <initialSeverity>1</initialSeverity>
    <isBad>false</isBad>
    <stages>
      <li>
        <painOffset>0.05</painOffset>
        <statFactors>
        <MeleeDamageFactor>1.5</MeleeDamageFactor>
        </statFactors>
        <label>Burnt tentacle</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.8</setMax>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>0.23</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
  <!--生成-->
  <ThingDef ParentName="EtherealThingBase">
    <defName>MYDE_WW_TheDead_A</defName>
    <label>break through the soil</label>
    <thingClass>MYDE_ExGTWorldWar.TheDead_A</thingClass>
