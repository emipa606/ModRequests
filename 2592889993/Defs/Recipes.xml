<Defs>

	<RecipeDef>
		<defName>CookMealSushi</defName>
		<label>prepare sushi</label>
		<description>Prepares a delicious bowl of sushi. (Requires any sort of fish meat, rice, and vegetables.)</description>
		<jobString>Preparing sushi.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RawFishMeat</li>
						<li>RawSquidMeat</li>
						<li>RawEelMeat</li>
						<li>JellyfishMeat</li>
						<li>PufferfishMeat</li>
						<li>SeaCucumberMeat</li>
						<li>RawShellfishMeat</li>
					</thingDefs>
				</filter>
				<count>0.25</count>
			</li>  
			<li>
				<filter>
					<thingDefs>
						<li>RawRice</li>
					</thingDefs>
				</filter>
				<count>0.25</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>0.15</count>
			</li>
		</ingredients>
		<products>
			<MealSushi>1</MealSushi>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawFishMeat</li>
				<li>RawSquidMeat</li>
				<li>RawEelMeat</li>
				<li>JellyfishMeat</li>
				<li>PufferfishMeat</li>
				<li>SeaCucumberMeat</li>
				<li>RawShellfishMeat</li>
				<li>RawRice</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>RawRice</li>
			</disallowedThingDefs>
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
  		<recipeUsers>			
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
	</RecipeDef>

