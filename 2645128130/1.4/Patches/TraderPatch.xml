<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- General Traders -->
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/TraderKindDef[defName = "Orbital_Exotic"]/stockGenerators</xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>OC_OddityStrongbox_Weapons</thingDef>
                <countRange>0~1</countRange>
            </li>
        </value>
    </Operation>
	
	<!-- Oddity Orbital Trader -->
	<Operation Class="PatchOperationAdd">
        <xpath>Defs/TraderKindDef[defName = "OC_Orbital_Oddity"]/stockGenerators</xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>OC_OddityStrongbox_Weapons</thingDef>
                <countRange>1~2</countRange>
            </li>
        </value>
    </Operation>
</Patch>