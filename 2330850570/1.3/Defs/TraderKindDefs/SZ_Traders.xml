<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <TraderKindDef>
    <defName>Base_SZ_Standard</defName>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>1500~4500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>3~10</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>200~300</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>500~600</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>350~450</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Gold</thingDef>
        <countRange>-20~150</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>500~1000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>15~30</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>300~700</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>ResourcesRaw</categoryDef>
        <thingDefCountRange>3~3</thingDefCountRange>
        <totalPriceRange>200~600</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories>
          <li>EggsFertilized</li>
        </excludedCategories>
        <thingDefCountRange>3~5</thingDefCountRange>
        <totalPriceRange>300~800</totalPriceRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
        </thingDefs>
      </li>
      
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>SZ_ChengYinPin</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>4~5</thingDefCountRange>
        <totalPriceRange>500~1200</totalPriceRange>
      </li>
      
      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>2</count>
            <chance>0.5</chance>
          </li>
          <li>
            <count>3</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>
      
      <!-- Weapons-->
      <li Class="StockGenerator_WeaponsRanged">
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <countRange>3~5</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>WeaponsMelee</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>1~3</countRange>
      </li>
      
      <!-- Apparel-->
      <li Class="StockGenerator_Clothes">
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <countRange>4~12</countRange>
      </li>
      
      <!-- Implants -->
      <!-- none -->
      
      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>0~3</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>

      <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_SingleDef">
        <thingDef>AnimusStone</thingDef>
        <countRange>-3~1</countRange> <!-- 25% chance of appearing -->
      </li>

      <!-- Slaves -->
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>-1~2</countRange>
      </li>

      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>3~4</kindCountRange>
        <countRange>2~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>



