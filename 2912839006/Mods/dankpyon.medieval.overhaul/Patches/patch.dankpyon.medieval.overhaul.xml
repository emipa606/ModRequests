<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- We want shiny gems -->
	<Operation Class="XmlExtensions.FindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<caseFalse/>
		<caseTrue>
			<Operation Class="XmlExtensions.ApplyPatch">
				<patchName>Coinage_InjectActions</patchName>
				<arguments>
					<li>ThingDef[@ParentName="DankPyon_GemBase"]</li>
					<li>ThingDef[</li>
				</arguments>
			</Operation>
		</caseTrue>
	</Operation>
	
	
</Patch>
		
		