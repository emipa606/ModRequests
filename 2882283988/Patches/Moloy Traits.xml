<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Moloy Traits</li>
		</mods>		

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
				<xpath>Defs/CM_Callouts.CalloutConstantByTraitDef[defName="CM_Callout_SpicyTraits"]/traits</xpath>
				<value>
					<li>MT_Commander</li>
					<li>MT_Tyrant</li>
				</value>
				</li>
				<li Class="PatchOperationAdd">
				<xpath>Defs/CM_Callouts.CalloutConstantByTraitDef[defName="CM_Callout_MildTraits"]/traits</xpath>
				<value>
					<li>MT_Charismatic</li>
					<li>MT_Luminary</li>
				</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
