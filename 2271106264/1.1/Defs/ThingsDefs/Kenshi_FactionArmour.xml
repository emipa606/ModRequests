	<ThingDef ParentName="ArmorHelmetMakeableBase">
		<defName>Kenshi_TinCan</defName>
		<equippedStatOffsets>
			<ShootingAccuracyPawn>-0.2</ShootingAccuracyPawn>
		</equippedStatOffsets>
