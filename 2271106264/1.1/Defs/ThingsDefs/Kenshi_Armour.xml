<Defs>
	<!-- HolyChestPlate -->
	<ThingDef ParentName="ArmorSmithableBase">
		<defName>Kenshi_HolyChestPlate</defName>
		<label>Holy Chest Plate</label>
		<description>It gives near perfect protection of the body, because it's so holy. Limb protection is not so great, but at least you'll have your organs.</description>
		<recipeMaker>
			<recipeUsers Inherit="false">
				<li>KenshiSmithingTable</li>
				<li>KenshiElectricSmithingTable</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<researchPrerequisite>PlateArmor</researchPrerequisite>
			<skillRequirements>
				<Crafting>7</Crafting>
			</skillRequirements>
		</recipeMaker>
		<graphicData>
			<texPath>Armor/HolyChestPlate</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>110</costStuffCount>
		<statBases>
			<WorkToMake>32000</WorkToMake>
			<MaxHitPoints>270</MaxHitPoints>
			<Mass>4</Mass>
			<StuffEffectMultiplierArmor>0.85</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>1.0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>10</EquipDelay>
		</statBases>
		<thingSetMakerTags>
			<li>RewardStandardLowFreq</li>
			<li>RewardStandardQualitySuper</li>
		</thingSetMakerTags>
		<equippedStatOffsets>
			<MoveSpeed>-0.3</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
			</bodyPartGroups>
			<wornGraphicPath>Armor/HolyChestPlate</wornGraphicPath>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>MedievalMilitary</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255,255,255,255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>
