<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Turbolaser Sound -->
   <SoundDef>
    <defName>Turbolaser_Shot</defName>
    <context>MapOnly</context>
    <eventNames />
           <maxVoices>6</maxVoices>
           <maxSimultaneous>2</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_clip">
            <clipPath>Turbolaser_turbofire/Turbolaser</clipPath>
          </li>
        </grains>
        <volumeRange>
          <min>120</min>
          <max>600</max>
        </volumeRange>
        <pitchRange>
          <min>0.935690</min>
          <max>1.055345</max>
        </pitchRange>
      </li>
    </subSounds>
 </SoundDef>
 
