<?xml version="1.0" encoding="UTF-8"?>
<Defs>


	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_SolidifyPsyfocus</defName>
		<label>solidify psyfocus</label>
		<description>Chanell psyfocus into the fabric of reality to solidify it in form of physical material. Scales up with physical sensitivity</description>
		<iconPath>Abilities/Archocaster/Power</iconPath>
		<abilityClass>VPEArchocaster.Ability_SolidifyPsyfocus</abilityClass>
		<castTime>250</castTime>
		<cooldownTime>45000</cooldownTime>
		<targetMode>Self</targetMode>
		<showUndrafted>true</showUndrafted>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>1</level>
				<order>1</order>
				<psyfocusCost>0</psyfocusCost>
				<entropyGain>40</entropyGain>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<staticPsycost>0.1</staticPsycost>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_UnlockArchocaster</defName>
		<label>unlock archocaster path</label>
		<description>Create a projection of caster's own neural activity to the target's brain to unlock the archocaster path.</description>
		<iconPath>Abilities/Archocaster/Focus</iconPath>
		<abilityClass>VPEArchocaster.Ability_UnlockArchocaster</abilityClass>
		<range>5</range>
		<castTime>1200</castTime>
		<cooldownTime>100000</cooldownTime>
		<targetMode>Pawn</targetMode>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>2</level>
				<order>1</order>
				<psyfocusCost>0.5</psyfocusCost>
				<entropyGain>40</entropyGain>
				<psychic>true</psychic>
				<prerequisites>
					<li>VPEAC_SolidifyPsyfocus</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<ofCasterFaction>True</ofCasterFaction>
				<targetMustBePsycaster>True</targetMustBePsycaster>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_PsyBlock</defName>
		<label>psy block</label>
		<description>Create a complex structure in a human brain blocking their psychic sensitivity. If used again the original block will resonate with the second, destroying each other</description>
		<iconPath>Abilities/Archocaster/Neuroquake</iconPath>
		<abilityClass>VPEArchocaster.Ability_PsyBlock</abilityClass>
		<range>4</range>
		<castTime>450</castTime>
		<cooldownTime>1500</cooldownTime>
		<targetMode>Pawn</targetMode>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<goodwillImpact>-10</goodwillImpact>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>2</level>
				<order>2</order>
				<psyfocusCost>0.25</psyfocusCost>
				<entropyGain>20</entropyGain>
				<prerequisites>
					<li>VPEAC_SolidifyPsyfocus</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_GiveLevelTo</defName>
		<label>give level to</label>
		<description>Sacrifice own psycasting capabilities to give other enough xp to get one psy-level. Only works if caster has enough xp.</description>
		<iconPath>Abilities/Archocaster/NeuralHeatDump</iconPath>
		<abilityClass>VPEArchocaster.Ability_GiveLevelTo</abilityClass>
		<range>2</range>
		<castTime>120</castTime>
		<targetMode>Pawn</targetMode>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>3</level>
				<order>1</order>
				<psyfocusCost>0.025</psyfocusCost>
				<entropyGain>45</entropyGain>
				<psychic>true</psychic>
				<prerequisites>
					<li>VPEAC_UnlockArchocaster</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<ofCasterFaction>True</ofCasterFaction>
				<targetMustBePsycaster>True</targetMustBePsycaster>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_GiveLevelFrom</defName>
		<label>give level from</label>
		<description>Sacrifice own psycasting capabilities to give other xp equal to own current level. Only works if caster has enough xp.</description>
		<iconPath>Abilities/Archocaster/NeuralHeatDump</iconPath>
		<abilityClass>VPEArchocaster.Ability_GiveLevelFrom</abilityClass>
		<range>2</range>
		<castTime>120</castTime>
		<targetMode>Pawn</targetMode>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>3</level>
				<order>2</order>
				<psyfocusCost>0.08</psyfocusCost>
				<entropyGain>75</entropyGain>
				<psychic>true</psychic>
				<prerequisites>
					<li>VPEAC_PsyBlock</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<ofCasterFaction>True</ofCasterFaction>
				<targetMustBePsycaster>True</targetMustBePsycaster>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_TransferPsycasts</defName>
		<label>transfer psycasts</label>
		<description>Shift the targets and own's perception of reality to trick quantum field into swapping all psycasting experience and skills. Overflows both with huge neural heat waves.</description>
		<iconPath>Abilities/Archocaster/LocationSwap</iconPath>
		<abilityClass>VPEArchocaster.Ability_TransferPsycasts</abilityClass>
		<range>2</range>
		<castTime>4000</castTime>
		<cooldownTime>500000</cooldownTime>
		<targetMode>Pawn</targetMode>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>4</level>
				<order>1</order>
				<psyfocusCost>1</psyfocusCost>
				<entropyGain>5</entropyGain>
				<psychic>true</psychic>
				<prerequisites>
					<li>VPEAC_GiveLevelTo</li>
					<li>VPEAC_GiveLevelFrom</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<ofCasterFaction>True</ofCasterFaction>
				<targetMustBePsycaster>True</targetMustBePsycaster>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>VPEAC_ResetPsycasts</defName>
		<label>reset psycasts</label>
		<description>By quickly creating and disrupting links to the psy-fiel in the target's brain, reset all their learned psycasts, without loosing actual experience. Of course such operation puts a huge strain on the person's mind, overloading it with neural heat. Another downside is that the caster must have at least the same psy level as their target.</description>
		<iconPath>Abilities/Archocaster/MindWipe</iconPath>
		<abilityClass>VPEArchocaster.Ability_ResetPsycasts</abilityClass>
		<range>3</range>
		<castTime>800</castTime>
		<cooldownTime>75000</cooldownTime>
		<targetMode>Pawn</targetMode>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEAC_Archocaster</path>
				<level>5</level>
				<order>1</order>
				<psyfocusCost>0.5</psyfocusCost>
				<entropyGain>90</entropyGain>
				<psychic>true</psychic>
				<prerequisites>
					<li>VPEAC_TransferPsycasts</li>
				</prerequisites>
			</li>
			<li Class="VPEArchocaster.AbilityExtension_TargetValidator">
				<ofCasterFaction>True</ofCasterFaction>
				<strongerPsycaster>True</strongerPsycaster>
				<targetMustBePsycaster>True</targetMustBePsycaster>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	
</Defs>