<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[1.0] Xenite Industries</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<success>Always</success>
					<xpath>Defs/RecipeDef[defName="XImakeCharcoal"]//li[text()="WoodLog"]</xpath>
					<value>
						<li>Owl_WoodLog</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>