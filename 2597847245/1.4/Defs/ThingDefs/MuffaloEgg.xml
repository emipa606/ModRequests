<Defs>
  <ThingDef Name="MuffaloEggBase" ParentName="EggFertBase" Abstract="True">
    <ingestible>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
    <techLevel>Spacer</techLevel>
    <graphicData>
      <drawSize>1.8</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>2000</WorkToMake>
      <MarketValue>1000</MarketValue>
    </statBases>
    <recipeMaker>
      <skillRequirements>
        <Medicine>15</Medicine>
      </skillRequirements>
      <researchPrerequisite>MuffaloMutation</researchPrerequisite>
      <recipeUsers Inherit="False">
        <li>EggLab</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>
  <ThingDef ParentName="MuffaloEggBase">
    <defName>EggGoldMuffaloFertilized</defName>
    <label>Gold muffalo egg (fert.)</label>
    <description>An fertilized egg containing a gold breed of muffalo.</description>
    <graphicData>
      <color>(193,145,79)</color>
    </graphicData>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>1</hatcherDaystoHatch>
        <hatcherPawn>GoldMuffalo</hatcherPawn>
      </li>
    </comps>
    <costList>
      <WoolMuffalo>100</WoolMuffalo>
      <MedicineUltratech>5</MedicineUltratech>
      <Gold>100</Gold>
    </costList>
  </ThingDef>
