<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- SmallBattery未導入時にWB_SmallWaterproofBatteryを削除します -->

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Small Battery</li>
    </mods>
    <nomatch Class="PatchOperationRemove">
      <xpath>/Defs/ThingDef[defName="WB_SmallWaterproofBattery"]</xpath>
    </nomatch>
  </Operation>

</Patch>
