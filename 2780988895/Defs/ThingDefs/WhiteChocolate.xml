      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>2.5</MarketValue>
      <Mass>0.075</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>8</DeteriorationRate>
      <Nutrition>0.125</Nutrition>
    </statBases>
    <thingCategories>
      <li>Foods</li>
    </thingCategories>
