<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <BodyDef>
    <defName>KenshiFauna_SkimmerLike</defName>
    <label>skimmer-like</label>
    <corePart>
      <def>Shell</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>Stomach</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>InsectHeart</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Pronotum</def>
          <coverage>0.18</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>InsectHead</def>
              <coverage>0.65</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Brain</def>
                  <coverage>0.2</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.15</coverage>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.15</coverage>
                </li>
                <li>
                  <def>Antenna</def>
                  <coverage>0.10</coverage>
                  <customLabel>left feather antenna</customLabel>
                </li>
                <li>
                  <def>Antenna</def>
                  <coverage>0.10</coverage>
                  <customLabel>middle feather antenna</customLabel>
                </li>
                <li>
                  <def>Antenna</def>
                  <coverage>0.10</coverage>
                  <customLabel>right feather antenna</customLabel>
                </li>				
                <li>
                  <def>InsectNostril</def>
                  <coverage>0.07</coverage>
                </li>
                <li>
                  <def>InsectMouth</def>
                  <coverage>0.10</coverage>
                  <groups>
                    <li>Mouth</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>front left leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
          <groups>
            <li>FrontLeftLeg</li>
          </groups>		  
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>front right leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
          <groups>
            <li>FrontRightLeg</li>
          </groups>		  
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>top left leg</customLabel>
          <coverage>0.06</coverage>
          <height>Middle</height>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>top right leg</customLabel>
          <coverage>0.06</coverage>
          <height>Middle</height>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>rear left leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>rear right leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
        </li>
      </parts>
    </corePart>
  </BodyDef>

