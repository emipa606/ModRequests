<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef ParentName="FloorBase">
    <renderPrecedence>230</renderPrecedence>
    <defName>KenshiFauna_FineNacreTile</defName>
    <label>fine nacre tile</label>
    <description>Exquisite tiles made from leviathan pearls, polished and laminated to preserve their gloss. While very slow to build, they are exceptionally beautiful, and will delight even the most posh of nobles.</description>
    <texturePath>Terrain/NacreFloor</texturePath>
    <uiIconPath>Terrain/NacreFloor_MenuIcon</uiIconPath>
    <pollutionTintColor>(0.95, 0.95, 0.93, 1)</pollutionTintColor>
    <isPaintable>false</isPaintable>
    <statBases>
      <WorkToBuild>5500</WorkToBuild>
      <Beauty>12</Beauty>
      <CleaningTimeFactor>0.8</CleaningTimeFactor>
    </statBases>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
    <tags>
      <li MayRequire="Ludeon.RimWorld.Royalty">FineFloor</li>
    </tags>
    <costList>
      <KenshiFauna_NacreBlock>10</KenshiFauna_NacreBlock>
    </costList>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
  </TerrainDef>
  
</Defs>