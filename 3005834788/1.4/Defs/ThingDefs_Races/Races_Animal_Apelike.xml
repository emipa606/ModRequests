<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>KenshiFauna_Gorillo</defName>
    <label>gorillo</label>
    <description>A large predatory reptilian covered in a layer of thick fur. The gorillo's name is owed to its unusual anatomy, more resembling of a gorilla than any of the Earth's reptiles. Vicious and territorial, they won't hesitate to pulverize anyone who dares to approach them.</description>
    <statBases>
      <MoveSpeed>4.6</MoveSpeed>
      <MarketValue>750</MarketValue>
      <ComfyTemperatureMin>-25</ComfyTemperatureMin>
      <ComfyTemperatureMax>40</ComfyTemperatureMax>
      <FilthRate>4</FilthRate>
    </statBases>
    <tools>
      <li>
				<label>left fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>16</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
			</li>
			<li>
				<label>right fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>16</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>22</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>12</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
    </tools>
    <race>
      <herdAnimal>false</herdAnimal>
      <body>KenshiFauna_ThreeFingeredApelike</body>
      <predator>true</predator>
      <maxPreyBodySize>3.5</maxPreyBodySize>
      <baseHungerRate>0.25</baseHungerRate>
      <baseBodySize>2.5</baseBodySize>
      <baseHealthScale>2.8</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Heavy</leatherDef>
      <wildness>0.85</wildness>
      <lifeExpectancy>32</lifeExpectancy>
      <trainability>Intermediate</trainability>
      <manhunterOnTameFailChance>1</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
      <gestationPeriodDays>12</gestationPeriodDays>
      <wildBiomes>
				<AridShrubland>0.6</AridShrubland>
        <TemperateForest>0.4</TemperateForest>
        <TemperateSwamp>0.2</TemperateSwamp>
			</wildBiomes>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.3</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.666</minAge>
          <soundWounded>KenshiFauna_Gorillo_Wounded</soundWounded>
          <soundDeath>KenshiFauna_Gorillo_Death</soundDeath>
          <soundCall>KenshiFauna_Gorillo_Call</soundCall>
          <soundAngry>KenshiFauna_Gorillo_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
      <headPosPerRotation>
        <li>(0.0, 0, 0.2)</li>
        <li>(0.23, 0, 0.11)</li>
        <li>(0, 0, 0)</li>
        <li>(-0.23, 0, 0.11)</li>
      </headPosPerRotation>
    </race>
    <tradeTags>
      <li>AnimalUncommon</li>
      <li>AnimalFighter</li>
    </tradeTags>
  </ThingDef>

