<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>Defs/TraderKindDef[defName="GR_OrbitalTrader"]</xpath>
			<value>
				<li Class="TraderGen.GenExtension">
					<specializations>
						<li MayRequire="BiomesTeam.BiomesCaverns">
							<def>TG_Caveworld</def>
							<commonality>5.0</commonality>
						</li>
						<li>
							<def>TG_Culture</def>
							<commonality>5.0</commonality>
						</li>
						<li>
							<def>TG_Drugs</def>
							<commonality>10.0</commonality>
						</li>
						<li>
							<def>TG_Medicine</def>
							<commonality>10.0</commonality>
						</li>
						<li>
							<def>TG_Tech</def>
							<commonality>10.0</commonality>
						</li>
						<li>
							<def>TG_Wealth</def>
							<commonality>10.0</commonality>
						</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">
							<def>TG_Xenogenetics</def>
							<commonality>5.0</commonality>
						</li>
					</specializations>
					<extraNameRules>TG_OrbitalNamerGenetics</extraNameRules>
				</li>
			</value>
		</match>
	</Operation>
</Patch>