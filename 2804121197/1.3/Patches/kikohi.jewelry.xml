<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Jewelry</li>
		</mods>
		<!-- Extra specialization for orbital traders. -->
		<match Class="PatchOperationAdd">
			<xpath>/Defs</xpath>
			<value>
				<TG.TraderSpecializationDef>
					<defName>TG_Jewelry</defName>
					<label>jewelry</label>
					<stockGens>
						<li Class="TG.StockGen.Jewelry">
							<thingDefCountRange>4~6</thingDefCountRange>
							<countRange>1</countRange>
						</li>
						<li Class="TG.StockGen.RandomStuffFromCategory">
							<stuffCategoryDef>Gemstones</stuffCategoryDef>
							<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
							<stuffCountRange>120~200</stuffCountRange>
							<thingDefCountRange>2~3</thingDefCountRange>
							<countRange>1</countRange>
						</li>
						<li Class="TG.StockGen.RandomStuffFromCategory">
							<stuffCategoryDef>Gemstones</stuffCategoryDef>
							<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
							<stuffCountRange>120~200</stuffCountRange>
							<thingDefCountRange>2~3</thingDefCountRange>
							<countRange>1</countRange>
						</li>
					</stockGens>
				</TG.TraderSpecializationDef>
			</value>
		</match>
	</Operation>
</Patch>
