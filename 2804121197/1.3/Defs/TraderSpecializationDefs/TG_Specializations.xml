			<li Class="StockGenerator_SingleDef">
				<thingDef>Meat_Human</thingDef>
				<totalPriceRange>500~1111</totalPriceRange>
			</li>
			<li Class="TG.StockGen.StuffFromThingDef">
				<availableThingDefs>
					<li>Leather_Human</li>
				</availableThingDefs>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
