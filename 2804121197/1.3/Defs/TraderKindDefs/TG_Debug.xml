<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Used for debug actions only. -->
	<TraderKindDef>
		<defName>TG_OrbitalDebug</defName>
		<label>debug ship</label>
		<orbital>true</orbital>
		<requestable>false</requestable>
		<commonality>0</commonality>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>2000~4000</countRange>
			</li>
		</stockGenerators>
		<modExtensions>
			<li Class="TG.GenExtension">
				<specializations>
					<li>
						<!-- This value will be overwritten by the GenerateOrbitalTrader debug action. -->
						<def>TG_Bionics</def>
						<commonality>10.0</commonality>
					</li>
				</specializations>
			</li>
		</modExtensions>
	</TraderKindDef>
</Defs>
