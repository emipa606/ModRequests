<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RulePackDef>
		<defName>TG_OrbitalNamerBulk</defName>
		<include>
			<li>NamerTraderGeneral</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=8)->[businessname] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[Letter][Letter] [traderkindbusinesstype]</li>
				<li>r_name(p=4)->[maybe_adjective] [NaturalObject] [traderkindbusinesstype]</li>
			</rulesStrings>
			<rulesFiles>
				<li>traderkindbusinesstype->Words/Nouns/TG_BulkBusinessTypes</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<RulePackDef>
		<defName>TG_OrbitalNamerCombat</defName>
		<include>
			<li>NamerTraderGeneral</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=8)->[businessname] [traderkindbusinesstype]</li>
				<li>r_name(p=8)->[weapon] [traderkindbusinesstype]</li>
				<li>r_name(p=4)->[AdjectiveBadass] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[AdjectiveBadass] [Animal] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[AdjectiveBadass] [Animal]</li>
				<li>r_name(p=2)->[AdjectiveBadass] [weapon] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[AdjectiveBadass] [weapon]</li>
			</rulesStrings>
			<rulesFiles>
				<li>traderkindbusinesstype->Words/Nouns/TG_CombatBusinessTypes</li>
				<li>weapon->Words/Nouns/Weapons</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<RulePackDef>
		<defName>TG_OrbitalNamerExotic</defName>
