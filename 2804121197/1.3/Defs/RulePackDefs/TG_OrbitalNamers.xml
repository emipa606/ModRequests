			<rulesStrings>
				<li>r_name(p=4)->[businessname] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[Letter][Letter] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[maybe_adjective] [PersonAlly] [traderkindbusinesstype]</li>
			</rulesStrings>
			<rulesFiles>
				<li>traderkindbusinesstype->Words/Nouns/TG_ExoticBusinessTypes</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<RulePackDef>
		<defName>TG_OrbitalNamerSlaver</defName>
		<include>
			<li>NamerTraderGeneral</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=6)->[PersonBadass] [traderkindbusinesstype]</li>
				<li>r_name(p=4)->[AdjectiveBadass] [traderkindbusinesstype]</li>
				<li>r_name(p=4)->[businessname] [traderkindbusinesstype]</li>
				<li>r_name(p=2)->[AdjectiveBadass] [AnimalBadass] [traderkindbusinesstype]</li>
			</rulesStrings>
			<rulesFiles>
				<li>traderkindbusinesstype->Words/Nouns/TG_SlaverBusinessTypes</li>
			</rulesFiles>
