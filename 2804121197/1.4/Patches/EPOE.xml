<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering</li>
			<li>Expanded Prosthetics and Organ Engineering - Forked</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraderKindDef[defName="ProsthesesTrader"]</xpath>
					<value>
						<li Class="TraderGen.GenExtension">
							<specializations>
								<li MayRequire="vanillaracesexpanded.android">
									<def>TG_Androids</def>
									<commonality>10.0</commonality>
								</li>
								<li>
									<def>TG_Drugs</def>
									<commonality>10.0</commonality>
								</li>
								<li>
									<def>TG_Hunting</def>
									<commonality>10.0</commonality>
								</li>
								<li>
									<def>TG_Medicine</def>
									<commonality>10.0</commonality>
								</li>
								<li>
									<def>TG_Organs</def>
									<commonality>10.0</commonality>
								</li>
								<li MayRequire="Ludeon.RimWorld.Royalty">
									<def>TG_Psycasting</def>
									<commonality>5.0</commonality>
								</li>
								<li MayRequire="VexedTrees980.RimRobots">
									<def>TG_Synthetics</def>
									<commonality>8.0</commonality>
								</li>
								<li>
									<def>TG_Wealth</def>
									<commonality>10.0</commonality>
								</li>
								<li MayRequire="Ludeon.RimWorld.Biotech">
									<def>TG_Xenogenetics</def>
									<commonality>5.0</commonality>
								</li>
							</specializations>
						</li>
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraderKindDef[defName="TextilesTrader"]</xpath>
					<value>
						<li Class="TraderGen.GenExtension">
							<specializations>
								<li>
									<def>TG_Biomaterials</def>
									<commonality>5.0</commonality>
								</li>
								<li>
									<def>TG_Cannibal</def>
									<commonality>6.0</commonality>
								</li>
								<li>
									<def>TG_Draper</def>
									<commonality>5.0</commonality>
								</li>
								<li>
									<def>TG_Hunting</def>
									<commonality>10.0</commonality>
								</li>
								<li>
									<def>TG_Husbandry</def>
									<commonality>15.0</commonality>
								</li>
								<li>
									<def>TG_Wealth</def>
									<commonality>10.0</commonality>
								</li>
							</specializations>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>