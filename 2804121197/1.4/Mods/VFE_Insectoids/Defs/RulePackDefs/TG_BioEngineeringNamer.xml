<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- New namer for bio-engineering suppliers. -->
	<RulePackDef>
		<defName>TG_OrbitalNamerBioEngineering</defName>
		<include>
			<li>NamerTraderGeneral</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=8)->[InsectoidName] [GeneticBusinessType]</li>
				<li>r_name(p=4)->[InsectoidName] [businesstype]</li>
				<li>r_name(p=8)->[InsectoidAdjective] [GeneticBusinessType]</li>
				<li>r_name(p=4)->[InsectoidAdjective] [businesstype]</li>
				<li>r_name(p=4)->[InsectoidAdjective] [InsectoidName] [GeneticBusinessType]</li>
				<li>r_name(p=2)->[InsectoidAdjective] [InsectoidName] [businesstype]</li>
			</rulesStrings>
			<rulesFiles>
				<li>GeneticBusinessType->Words/Nouns/TG_GeneticBusinessTypes</li>
				<li>InsectoidAdjective->Words/Insect</li>
				<li>InsectoidName->Words/Nouns/TG_InsectoidNames</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
</Defs>