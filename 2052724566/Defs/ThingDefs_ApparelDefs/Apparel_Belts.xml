<Defs>
  <!--============== Backpacks =================-->
  <!-- Civilian -->
  
  
      <!-- Military pack USMC -->
  
    <ThingDef ParentName="RHApparelMakeableBase_USMilitary">
    <defName>RNApparel_Backpack_Military_Coyote</defName>
    <label>backpack military (coyote)</label>
    <description>A military grade backpack, capable of carrying all kinds of essential equipment. It's designed for heavy and rough use.</description>
    <graphicData>
      <texPath>Things/Apparel/Display/USMCTan_MilitaryBackpack</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>90</costStuffCount>
	 <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
	  	  <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
	<colorPerStuff>
      <li>
        <stuff>Cloth</stuff>
        <color>(129, 97, 60)</color>
      </li>
    </colorPerStuff>
	<generateCommonality>0.35</generateCommonality>
    <thingCategories>
      <li>RHApparel_Belts</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToMake>2000</WorkToMake>
      <Mass>1.0</Mass>
      <StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <CarryingCapacity>60</CarryingCapacity> <!-- Now you've done it. You have officially annihilated balancing in all of gaming. I hope you are proud of what you've done here. -->
	  <MoveSpeed>-0.10</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
	  <careIfWornByCorpse>false</careIfWornByCorpse>
      <wornGraphicPath>Things/Apparel/Belt/USMCTan_MilitaryBackpack</wornGraphicPath>
<wornGraphicData>
  <renderUtilityAsPack>false</renderUtilityAsPack>
</wornGraphicData>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>RH_BeltsUSMC</li>
        <li>RH_Belts_USMCTan</li>
		<li>PassiveCamo_Desert_Med</li>
	  </tags>
	  <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
		 <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(64, 78, 44)</only>
        </li>
		<li>
          <weight>10</weight>
          <only>(61, 61, 61)</only>
        </li>
		<li>
          <weight>5</weight>
          <only>(99, 99, 99)</only>
        </li>		
		<li>
          <weight>5</weight>
          <only>(204, 172, 92)</only>
        </li>		
		<li>
          <weight>5</weight>
          <only>(122, 112, 82)</only>
        </li>
		<li>
          <weight>5</weight>
          <only>(99, 99, 99)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>      
  
