<Defs>

	<SoundDef>
		<defName>HP_Bolt_Gun_Sound</defName>
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>10</maxSimultaneous>  
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
					<clipFolderPath>Ranged/Bolt_Shot</clipFolderPath>
					</li>
				</grains>      
				<volumeRange>
					<min>25.0</min>        
					<max>30.0</max>
				</volumeRange>      
				<pitchRange>
					<min>0.9</min>        
					<max>1.0</max>
				</pitchRange>      
				<sustainLoop>false</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>	

