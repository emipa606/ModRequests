<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Wheelchair</defName>
		<label>Wheelchair</label>
		<description>A chair fitted with wheels for use as a means of transport by a person who is unable to walk as a result of illness, injury, or disability.</description>
		<graphicData>
			<texPath>Wheelchair</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>65</costStuffCount>
		<costList>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToMake>2800</WorkToMake>
			<Mass>1.6</Mass>
			<StuffEffectMultiplierArmor>0.05</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.5</EquipDelay>
		</statBases>
		<tickerType>Normal</tickerType>
        <comps>
            <li Class="Wheelchairs.WheelchairClassProperties">
                <compClass>Wheelchairs.WheelchairClass</compClass>
            </li>
        </comps>
		<thingCategories>
			<li>ApparelUtility</li>
		</thingCategories>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Wheelchair</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>Machining</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>TableMachining</li>
			</recipeUsers>
		</recipeMaker>

	</ThingDef>

</Defs>	