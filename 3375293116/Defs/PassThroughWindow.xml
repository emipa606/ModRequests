<?xml version="1.0" encoding="utf-8"?>



<Defs>





	<ThingDef ParentName="StorageShelfBase">
		<defName>PassThroughWindow</defName>
		<label>pass-through window</label>
		<description>A two-sided pass-through window for transferring items between rooms without entering or disrupting the other room's climate—perfect for moving raw food directly to the kitchen or cold storage. Act fast, though, perishable items left on the windowsill will deteriorate.</description>
		<thingClass>PassThroughWindow.Building_PassThroughWindow</thingClass>
		<graphicData>
			<texPath>Things/Building/Furniture/PassThroughWindow</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Steel>20</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>8</Mass>
			<WorkToBuild>500</WorkToBuild>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">5</StyleDominance>
		</statBases>
		<size>(1,1)</size>
		<designationHotKey>Misc12</designationHotKey>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<fillPercent>1</fillPercent>
		<coversFloor>true</coversFloor>
		<building>
			<fixedStorageSettings Inherit="False">
				<filter>
					<categories>
						<li>Foods</li>
						<li>Manufactured</li>
						<li>ResourcesRaw</li>
						<li>Items</li>
						<li>Weapons</li>
						<li>Apparel</li>
						<li>BodyParts</li>
						<li>Corpses</li>
					</categories>
				</filter>
			</fixedStorageSettings>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<blockLight>true</blockLight>
		<blockWeather>true</blockWeather>
		<castEdgeShadows>true</castEdgeShadows>
		<canOverlapZones>false</canOverlapZones>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<comps>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</ThingDef>








	
	
	
	
	
	
	
	
	
	
	
	
	
	
</Defs>
