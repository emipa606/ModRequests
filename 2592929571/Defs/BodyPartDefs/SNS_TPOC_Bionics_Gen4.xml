<Defs>

  <!-- Modified vanilla defs so no medicine or medical skill requirement -->
  
  <RecipeDef Name="SNSCESurgeryInstallBase" ParentName="SurgeryFlesh" Abstract="True">
    <workAmount>100</workAmount>
    <recipeUsers>
      <li>Human</li>
    </recipeUsers>
    <ingredients>
    </ingredients>
    <fixedIngredientFilter>
