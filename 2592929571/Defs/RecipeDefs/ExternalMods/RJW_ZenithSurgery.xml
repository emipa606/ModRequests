<Defs>

	<!-- Zenith Penis - Replaces Penis/Vagina -->
	<RecipeDef ParentName="SexReassignmentP">
		<defName>SNSCE_InstallBodyPart_ZenithPenis</defName>
		<label>install zenith penis</label>
		<description>Installs a zenith penis, replacing other bodyparts (such as another penis or a vagina) if present. \n\nDue to the inherent self-improving properties of Eternium, installing this implant requires no medical experience whatsoever and can be done without the usage of medicine.</description>
		<descriptionHyperlinks>
			<ThingDef>SNSCE_ZenithPenis</ThingDef>
			<HediffDef>SNSCE_ZenithPenis</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing zenith penis.</jobString>
		<workAmount>100</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SNSCE_ZenithPenis</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SNSCE_ZenithPenis</li>
			</thingDefs>
		</fixedIngredientFilter>
		<addsHediff>SNSCE_ZenithPenis</addsHediff>
		<anesthetize>false</anesthetize>
		<surgerySuccessChanceFactor>500</surgerySuccessChanceFactor>
		<skillRequirements>
			<Medicine>0</Medicine>
		</skillRequirements>
	</RecipeDef>

