<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TraderKindDef>
		<defName>GreenHouseTrader</defName>
		<label>greenhouse trader</label>
		<commonality>0.5</commonality>
		<orbital>true</orbital>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>
					<min>2000</min>
					<max>4000</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>WoodLog</thingDef>
				<countRange>
					<min>500</min>
					<max>4000</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>FoodRaw</categoryDef>
				<thingDefCountRange>
					<min>7</min>
					<max>25</max>
				</thingDefCountRange>
				<totalPriceRange>
					<min>150</min>
					<max>2000</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Seeds</tradeTag>
				<thingDefCountRange>
					<min>10</min>
					<max>20</max>
				</thingDefCountRange>
				<countRange>
					<min>50</min>
					<max>700</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Animals">
				<tradeTagsSell>
					<li>AnimalFarm</li>
					<li>AnimalPet</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>AnimalFarm</li>
					<li>AnimalPet</li>
				</tradeTagsBuy>
				<minWildness>0.60</minWildness>
				<kindCountRange>
					<min>0</min>
					<max>5</max>
				</kindCountRange>
				<countRange>
					<min>1</min>
					<max>4</max>
				</countRange>
			</li>
		</stockGenerators>	  
	</TraderKindDef>
	
