<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Vikings</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>
				<ThingDef ParentName="SeedBase">
					<defName>Seed_VCE_Blueberry</defName>
					<label>blueberry seeds</label>
					<graphicData>
						<texPath>Seeds/PlantsExpandedMore/VCE_Blueberry</texPath>
					</graphicData>
					<descriptionHyperlinks>
						<ThingDef>VCE_Blueberry</ThingDef>
					</descriptionHyperlinks>
					<modExtensions>
						<li Class="SeedsPleaseLite.Seed">
							<sources>
								<li>VCE_Blueberry</li>
							</sources>
						</li>
					</modExtensions>
				</ThingDef>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<!-- [Seeds Please: Lite] VCE_Barley(vanillaexpanded.vplantsemore)Autogenerated as Seed_VCE_Barley-->
						<ThingDef ParentName="SeedBase">
							<defName>Seed_VCE_Barley</defName>
							<label>barley seeds</label>
							<graphicData>
								<texPath>Seeds/PlantsExpandedMore/VCE_BarleyA</texPath>
							</graphicData>
							<descriptionHyperlinks>
								<ThingDef>VCE_Barley</ThingDef>
							</descriptionHyperlinks>
							<modExtensions>
								<li Class="SeedsPleaseLite.Seed">
									<extraChance>0.33</extraChance>
									<extractable>False</extractable>
									<sources>
										<li>VCE_Barley</li>
									</sources>
								</li>
							</modExtensions>
						</ThingDef>
						
						<!-- [Seeds Please: Lite] VCE_Sorghum(vanillaexpanded.vplantsemore)Autogenerated as Seed_VCE_Sorghum-->
						<ThingDef ParentName="SeedBase">
							<defName>Seed_VCE_Sorghum</defName>
							<label>sorghum seeds</label>
							<graphicData>
								<texPath>Seeds/PlantsExpandedMore/VCE_Sorghum</texPath>
							</graphicData>
							<descriptionHyperlinks>
								<ThingDef>VCE_Sorghum</ThingDef>
							</descriptionHyperlinks>
							<modExtensions>
								<li Class="SeedsPleaseLite.Seed">
									<sources>
										<li>VCE_Sorghum</li>
									</sources>
								</li>
							</modExtensions>
						</ThingDef>
						
						<!-- [Seeds Please: Lite] VCE_Rye(vanillaexpanded.vplantsemore)Autogenerated as Seed_VCE_Rye-->
						<ThingDef ParentName="SeedBase">
							<defName>Seed_VCE_Rye</defName>
							<label>rye seeds</label>
							<graphicData>
								<texPath>Seeds/PlantsExpandedMore/VCE_Rye</texPath>
							</graphicData>
							<descriptionHyperlinks>
								<ThingDef>VCE_Rye</ThingDef>
							</descriptionHyperlinks>
							<modExtensions>
								<li Class="SeedsPleaseLite.Seed">
									<sources>
										<li>VCE_Rye</li>
									</sources>
								</li>
							</modExtensions>
						</ThingDef>
						
						<!-- [Seeds Please: Lite] VCE_Oats(vanillaexpanded.vplantsemore)Autogenerated as Seed_VCE_Oats-->
						<ThingDef ParentName="SeedBase">
							<defName>Seed_VCE_Oats</defName>
							<label>oats seeds</label>
							<graphicData>
								<texPath>Seeds/PlantsExpandedMore/VCE_Oats</texPath>
							</graphicData>
							<descriptionHyperlinks>
								<ThingDef>VCE_Oats</ThingDef>
							</descriptionHyperlinks>
							<modExtensions>
								<li Class="SeedsPleaseLite.Seed">
									<sources>
										<li>VCE_Oats</li>
									</sources>
								</li>
							</modExtensions>
						</ThingDef>
					</value>
				</li>

				<li Class="PatchOperationReplace">
				
					<xpath>Defs/ThingDef[defName="VCE_Oats"]/plant/harvestedThingDef</xpath>
					<value>
						<harvestedThingDef>VCE_RawOats</harvestedThingDef>
					</value>
				</li>
			
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="VCE_Oats"]/modExtensions/li[secondaryOutput="VCE_RawOats"]/secondaryOutput</xpath>
					<value>
						<secondaryOutput>VCE_Flour</secondaryOutput>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>