<Defs>

	<ThingDef ParentName="PlantFoodRawBase">
		<defName>StinkerFungus</defName>
		<label>stinker fungus</label>
		<description>stinker fungus.</description>
		<possessionCount>10</possessionCount>
		<descriptionHyperlinks>
			<ThingDef>VPE_Plant_Stinker</ThingDef>
			<ThingDef>Seed_VPE_Plant_Stinker</ThingDef>
			<RecipeDef>RefineFungus</RecipeDef>
		</descriptionHyperlinks>
		<thingCategories>
			<li>UnrefinedFungus</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/RawFungus</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.1</MarketValue>
		</statBases>
		<ingestible>
			<foodType>Fungus</foodType>
		</ingestible>
		<ingredient>
			<mergeCompatibilityTags>
				<li MayRequire="Ludeon.RimWorld.Ideology">Fungus</li>
			</mergeCompatibilityTags>
		</ingredient>
		<butcherProducts>
			<RawFungus>8</RawFungus>
		</butcherProducts>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>

