<Defs>

    <ThingDef ParentName="PlantFoodRawBase">
        <defName>EdakuFungus</defName>
        <label>edaku fungus</label>
        <description>edaku fungus.</description>
        <possessionCount>10</possessionCount>
        <descriptionHyperlinks>
            <ThingDef>RG_Plant_Edaku</ThingDef>
            <ThingDef>Seed_RG_Plant_Edaku</ThingDef>
            <RecipeDef>RefineFungus</RecipeDef>
        </descriptionHyperlinks>
        <thingCategories>
            <li>UnrefinedFungus</li>
        </thingCategories>
        <graphicData>
            <texPath>Things/Item/Resource/PlantFoodRaw/RawFungus</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.1</MarketValue>
        </statBases>
        <ingestible>
            <foodType>Fungus</foodType>
        </ingestible>
        <ingredient>
            <mergeCompatibilityTags>
                <li MayRequire="Ludeon.RimWorld.Ideology">Fungus</li>
            </mergeCompatibilityTags>
        </ingredient>
        <butcherProducts>
            <RawFungus>8</RawFungus>
        </butcherProducts>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>30</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
    </ThingDef>

