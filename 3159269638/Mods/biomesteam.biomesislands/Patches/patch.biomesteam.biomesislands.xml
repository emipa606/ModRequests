<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>/Defs[not(ThingDef[defName="Seed_BICoconut"])]</xpath>
				<value>
					<ThingDef ParentName="SeedBase">
						<defName>Seed_BICoconut</defName>
						<label>coconut seeds</label>
						<modExtensions>
							<li Class="SeedsPleaseLite.Seed">
								<sources>
									<li>BiomesIslands_CoconutPalm</li>
								</sources>
							</li>
						</modExtensions>
						<graphicData>
							<texPath>Seeds/CoconutPalm</texPath>
						</graphicData>
					</ThingDef>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>