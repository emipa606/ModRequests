<Defs>
	<ThingDef ParentName="SeedBase">
		<defName>Seed_CMeatChicken</defName>
		<label>Synthetic chicken seeds</label>
		<modExtensions>
			<li Class="SeedsPleaseLite.Seed">
				<sources>
					<li>CMeatChicken</li>
				</sources>
				<extractable>False</extractable>
			</li>
		</modExtensions>
		<graphicData>
			<texPath>Seeds/CMeatChicken</texPath>
		</graphicData>
	</ThingDef>
	
