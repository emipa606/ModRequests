<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Plant_Grass"]</xpath>
		<value>
			<li Class="SeedsPleaseLite.Seedless" />
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="DankPyon_Plant_Mushrooms"]/plant/harvestedThingDef</xpath>
        <value>
            <harvestedThingDef>MushroomFungus</harvestedThingDef>
        </value>
    </Operation>

	<Operation Class="PatchOperationAdd">
        <xpath>Defs/RecipeDef[defName="RefineFungus"]/descriptionHyperlinks</xpath>
        <value>
            <ThingDef>MushroomFungus</ThingDef>
        </value>
    </Operation>
</Patch> 