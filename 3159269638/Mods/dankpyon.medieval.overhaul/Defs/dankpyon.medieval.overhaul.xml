<Defs>

	<ThingDef ParentName="PlantFoodRawBase">
		<defName>MushroomFungus</defName>
		<label>mushroom fungus</label>
		<description>mushroom fungus.</description>
		<possessionCount>10</possessionCount>
		<descriptionHyperlinks>
			<ThingDef>DankPyon_Plant_Mushrooms</ThingDef>
			<ThingDef>Seed_DankPyon_Mushrooms</ThingDef>
			<RecipeDef>RefineFungus</RecipeDef>
		</descriptionHyperlinks>
		<thingCategories>
			<li>UnrefinedFungus</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/RawFungus</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.1</MarketValue>
		</statBases>
		<ingestible>
			<foodType>Fungus</foodType>
		</ingestible>
		<ingredient>
			<mergeCompatibilityTags>
				<li MayRequire="Ludeon.RimWorld.Ideology">Fungus</li>
			</mergeCompatibilityTags>
		</ingredient>
		<butcherProducts>
			<RawFungus>8</RawFungus>
		</butcherProducts>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>

