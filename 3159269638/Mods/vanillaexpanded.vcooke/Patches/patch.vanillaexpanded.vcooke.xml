<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li MayRequire="vanillaexpanded.vmemese" Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>/Defs[not(ThingDef[defName="Seed_VCE_ChocolateBush"])]</xpath>
				<value>
					<!-- SeedsPlease :: VCE_ChocolateBush(vanillaexpanded.vmemese)Autogenerated as Seed_VCE_ChocolateBush-->
					<ThingDef ParentName="SeedBase">
						<defName>Seed_VCE_ChocolateBush</defName>
						<label>chocolate bush seeds</label>
						<descriptionHyperlinks>
							<ThingDef>VCE_ChocolateBush</ThingDef>
						</descriptionHyperlinks>
						<modExtensions>
							<li Class="SeedsPleaseLite.Seed">
								<sources>
									<li>VCE_ChocolateBush</li>
								</sources>
							</li>
						</modExtensions>
					</ThingDef>
				</value>
			</li>
			<li MayRequire="dismarzero.vgp.vgpvegetablegarden" Class="PatchOperationSequence">
				<operations>
					<!--Change seed names-->
					<li Class="PatchOperationReplace">
						<xpath>/Defs/ThingDef[defName="Seed_VCEWheat"]/label</xpath>
						<value><label>Spelt seeds</label></value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/ThingDef[defName="Seed_VCESugarcane"]/label</xpath>
						<value><label>Uba sugarcane seeds</label></value>
					</li>
				</operations>
			</li>
			<li MayRequire="crustypeanut.rc2.core" Class="PatchOperationSequence">
				<operations>
					<!--Change seed names-->
					<li Class="PatchOperationReplace">
						<xpath>/Defs/ThingDef[defName="Seed_VCEWheat"]/label</xpath>
						<value><label>Spelt seeds</label></value>
					</li>
				</operations>
			</li>
		</operations>
	</Operation>
</Patch>