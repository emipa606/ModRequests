<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!-- STANDARD SHUTTLE -->
    <ThingDef ParentName="CarryallSkyfallerBase">
        <defName>Carryall_imperial_shuttle_Leaving</defName>
        <label>imperial shuttle (leaving)</label>
        <thingClass>SRTS.SRTSLeaving</thingClass>
        <graphicData>
            <texPath>Shuttle/ImperialShuttle/ImperialShuttle</texPath>
            <drawSize>(7,5)</drawSize>
        </graphicData>
        <skyfaller>
            <shadowSize>(5,3.5)</shadowSize>
            <reversed>true</reversed>
            <anticipationSound>Carryall_Leaving</anticipationSound>
            <anticipationSoundTicks>-20</anticipationSoundTicks>
            <ticksToImpactRange>-15~-10</ticksToImpactRange>
            <moteSpawnTime>0.05</moteSpawnTime>
            <angleCurve>
                <points>
                    <li>(0,90)</li>
                    <li>(0.5,85)</li>
                    <li>(1,75)</li>
                </points>
            </angleCurve>
            <rotationCurve>
                <points>
                    <li>(0,90)</li>
                    <li>(0.2,90)</li>
                    <li>(0.35,95)</li>
                    <li>(1,95)</li>
                </points>
            </rotationCurve>
            <zPositionCurve>
                <points>
                    <li>(0,0)</li>
                    <li>(0.2,4)</li>
                </points>
            </zPositionCurve>
            <speedCurve>
                <points>
                    <li>(0,0)</li>
                    <li>(0.2,0.003)</li>
                    <li>(0.5,0.5)</li>
                </points>
            </speedCurve>
        </skyfaller>
    </ThingDef>

    <ThingDef ParentName="CarryallSkyfallerBase">
        <defName>Carryall_imperial_shuttle_Incoming</defName>
        <label>imperial shuttle (incoming)</label>
        <thingClass>SRTS.SRTSIncoming</thingClass>
        <graphicData>
            <texPath>Shuttle/ImperialShuttle/ImperialShuttle</texPath>
            <drawSize>(7,5)</drawSize>
        </graphicData>
        <skyfaller>
            <shadowSize>(5,3.5)</shadowSize>
            <anticipationSound>Carryall_Landing</anticipationSound>
            <anticipationSoundTicks>250</anticipationSoundTicks>
            <ticksToImpactRange>600</ticksToImpactRange>
            <angleCurve>
                <points>
                    <li>(0,-75)</li>
                    <li>(1,-90)</li>
                </points>
            </angleCurve>
            <rotationCurve>
                <points>
                    <li>(0,-95)</li>
                    <li>(0.85,-90)</li>
                    <li>(1,-90)</li>
                </points>
            </rotationCurve>
            <zPositionCurve>
                <points>
                    <li>(0.85,4)</li>
                    <li>(1,0)</li>
                </points>
            </zPositionCurve>
            <speedCurve>
                <points>
                    <li>(0.6,0.5)</li>
                    <li>(0.85,0.006)</li>
                    <li>(1,0)</li>
                </points>
            </speedCurve>
            <explosionRadius>0.025</explosionRadius>
            <explosionDamage>Lands_Carryall</explosionDamage>
            <explosionDamageFactor>1</explosionDamageFactor>
        </skyfaller>
    </ThingDef>

    <ThingDef>
        <defName>Carryall_imperial_shuttle_Active</defName>
        <label>imperial shuttle</label>
        <category>Item</category>
        <thingClass>ActiveDropPod</thingClass>
        <scatterableOnMapGen>false</scatterableOnMapGen>
        <tickerType>Normal</tickerType>
        <graphicData>
            <texPath>Shuttle/ImperialShuttle/ImperialShuttle</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(7,5)</drawSize>
        </graphicData>
        <altitudeLayer>Skyfaller</altitudeLayer>
        <blockWind>false</blockWind>
        <useHitPoints>true</useHitPoints>
        <statBases>
            <MaxHitPoints>600</MaxHitPoints>
            <Flammability>0.5</Flammability>
        </statBases>
        <selectable>true</selectable>
    </ThingDef>

