<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<TraderKindDef>
		<defName>Visitor_Outlander_Dust</defName>
		<label>trader</label>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FireDustPowder</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>IceDustPowder</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LightningDustPowder</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>GravityDustPowder</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>HardLightDustPowder</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>RWBY_PumpkinPetes</thingDef>
				<countRange>10~20</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>DustWeapons</categoryDef>
				<thingDefCountRange>0~1</thingDefCountRange>
				<countRange>1~1</countRange>
				<excludedThingDefs>
					<li>RWBY_Ember_Celica_Melee</li>
				</excludedThingDefs>
			</li>

			<!-- Resources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>50~250</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentIndustrial</thingDef>
				<countRange>-2~5</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>MedicineIndustrial</thingDef>
				<countRange>1~6</countRange>
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>Cloth</li>
					<li>Chocolate</li>
				</thingDefs>
			</li>

			<!-- Drugs -->
			<li Class="StockGenerator_Category">
				<categoryDef>Drugs</categoryDef>
				<thingDefCountRange>-1~1</thingDefCountRange>
				<totalPriceRange>100~300</totalPriceRange>
			</li>

			<!-- Techprints -->
			<li Class="StockGenerator_Techprints">
				<countChances>
					<li>
						<count>1</count>
						<chance>0.5</chance>
					</li>
				</countChances>
			</li>

			<!-- Apparel -->
			<li Class="StockGenerator_Clothes">
				<countRange>0~1</countRange>
			</li>

			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>

</Defs>
