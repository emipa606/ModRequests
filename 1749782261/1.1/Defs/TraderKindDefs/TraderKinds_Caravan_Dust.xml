<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<TraderKindDef>
		<defName>Caravan_Outlander_Dust</defName>
		<label>Dust trader</label>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>RWBY_PumpkinPetes</thingDef>
				<countRange>50~100</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FireDustCrystal</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>IceDustCrystal</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LightningDustCrystal</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>GravityDustCrystal</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>HardLightDustCrystal</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>750~1200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentIndustrial</thingDef>
				<countRange>30~40</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Steel</thingDef>
				<countRange>250~400</countRange>
			</li>
			
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>

	<TraderKindDef>
		<defName>Caravan_Outlander_Dust_CombatSupplier</defName>
		<label>Dust combat supplier</label>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FireDustPowder</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>IceDustPowder</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LightningDustPowder</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>GravityDustPowder</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>HardLightDustPowder</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>750~1200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentIndustrial</thingDef>
				<countRange>5~10</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>DustWeapons</categoryDef>
				<thingDefCountRange>5~7</thingDefCountRange>
				<countRange>1~1</countRange>
				<excludedThingDefs>
					<li>RWBY_Ember_Celica_Melee</li>
				</excludedThingDefs>
			</li>
			
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>

</Defs>
