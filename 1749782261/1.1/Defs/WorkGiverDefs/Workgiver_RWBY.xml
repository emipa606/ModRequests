<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<WorkGiverDef>
		<defName>DoBillsTableDustWorkshop</defName>
		<label>make things at Dust workshop</label>
		<giverClass>WorkGiver_DoBill</giverClass>
		<workType>Smithing</workType>
		<priorityInType>100</priorityInType>
		<fixedBillGiverDefs>
			<li>TableDustWorkshop</li>
		</fixedBillGiverDefs>
		<verb>work</verb>
		<gerund>working at</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<prioritizeSustains>true</prioritizeSustains>
	</WorkGiverDef>

	<WorkGiverDef>
		<defName>RWBY_GatherAmmunition</defName>
		<label>gather ammunition</label>
		<giverClass>RWBYRemnant.WorkGiver_GatherAmmunition</giverClass>
		<workType>BasicWorker</workType>
		<verb>haul</verb>
		<gerund>picking up</gerund>
		<canBeDoneWhileDrafted>false</canBeDoneWhileDrafted>
		<priorityInType>20</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

</Defs>