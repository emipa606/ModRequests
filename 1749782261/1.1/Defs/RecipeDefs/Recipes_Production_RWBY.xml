<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>MakeFireDustPowder</defName>
		<label>refine fire Dust crystal into powder</label>
		<description>Refining Dust crystals into powder is a dangerous process. Even a sneeze is enough to detonate a cloud of Dust.</description>
		<jobString>Making Dust powder from Dust crystal.</jobString>
		<workAmount>300</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>FireDustCrystal</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<FireDustPowder>10</FireDustPowder>
		</products>
		<researchPrerequisite>DustProcessing</researchPrerequisite>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FireDustCrystal</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

