<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef>
		<defName>RWBY_Thermos_Caffeine</defName>
		<label>Thermos caffeine</label>
		<description>An caffeine induced buff. Risk of overdose.</description>
		<initialSeverity>0.05</initialSeverity>
		<defaultLabelColor>(0.6, 0.5, 0.0)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<isBad>false</isBad>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-1.0</severityPerDay>
			</li>
		</comps>
		<stages>
			<li>
				<label>initial</label>
				<minSeverity>0.01</minSeverity>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>0.3</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<offset>0.2</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>high</label>
				<minSeverity>0.15</minSeverity>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>0.5</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<offset>0.35</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>blackout</label>
				<minSeverity>0.25</minSeverity>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.1</setMax>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>

