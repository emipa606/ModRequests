					<xpath>/Defs/StatDef[defName="TreeFellingSpeed"]</xpath>
						<value>
							<parts>
								<li Class="FP_RSLUM.StatPart_STR"> 
								<weight>0.5</weight>
								</li>
							</parts>
						</value>
				</nomatch>
				<match Class="PatchOperationAdd">
				<xpath>/Defs/StatDef[defName="TreeFellingSpeed"]/parts</xpath>
					<value>
						<li Class="FP_RSLUM.StatPart_STR"> 
								<weight>0.5</weight>
