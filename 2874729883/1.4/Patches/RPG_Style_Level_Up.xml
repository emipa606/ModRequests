						<li Class="FP_RSLUM.StatPart_STR"> 
								<weight>0.5</weight>
								</li>
					</value>
				</match>
			</li>
			<li Class="PatchOperationConditional">
				<xpath>/Defs/StatDef[defName="PlantHarvestingSpeed"]/parts</xpath>
