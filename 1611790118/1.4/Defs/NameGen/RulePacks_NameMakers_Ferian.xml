<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerFerian</defName>
    <rulePack>
      <rulesFiles>
        <li>malename->FerianNames/malename</li>
        <li>femalename->FerianNames/femalename</li>
        <li>lastname->FerianNames/lastname</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NamerPersonFerianMale</defName>
    <include>
      <li>NamerFerian</li>
    </include>
    <rulePack>
		<rulesStrings>
			<li>r_name->[malename] [lastname]</li>
		</rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonFerianFemale</defName>
    <include>
      <li>NamerFerian</li>
    </include>
    <rulePack>
		<rulesStrings>
			<li>r_name->[femalename] [lastname]</li>
		</rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>