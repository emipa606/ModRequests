<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonFerianMale</defName>
    <rulePack>
		<rulesStrings>
			<li>name->[malename] [lastname]</li>
		</rulesStrings>
		<rulesRaw>
			<li Class="Rule_File">
				<keyword>mainname</keyword>
				<path>FerianNames/mainname</path>
			</li>
			<li Class="Rule_File">
			  <keyword>lastname</keyword>
			  <path>FerianNames/lastname</path>
			</li>
			<li Class="Rule_File">
				<keyword>malename</keyword>
				<path>FerianNames/malename</path>
			</li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonFerianFemale</defName>
    <rulePack>
		<rulesStrings>
			<li>name->[femalename] [lastname]</li>
		</rulesStrings>
		<rulesRaw>
			<li Class="Rule_File">
				<keyword>mainname</keyword>
				<path>FerianNames/mainname</path>
			</li>
			<li Class="Rule_File">
			  <keyword>lastname</keyword>
			  <path>FerianNames/lastname</path>
			</li>
			<li Class="Rule_File">
			  <keyword>femalename</keyword>
			  <path>FerianNames/femalename</path>
			</li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>