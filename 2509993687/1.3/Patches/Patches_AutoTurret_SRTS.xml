<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>SRTS Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AutoTurret"]/comps</xpath>
					<value>      

  <li Class="CompProperties_Transporter">
    <restEffectiveness>0.8</restEffectiveness>
    <massCapacity>120</massCapacity>
  </li>
  
    <li Class="SRTS.CompProperties_LaunchableSRTS">
        <travelSpeed>12</travelSpeed> 
        <fuelPerTile>2.65</fuelPerTile> 
        <minPassengers>0</minPassengers>
        <maxPassengers>1</maxPassengers>
      	<spaceFaring>false</spaceFaring> 
      	<shuttleBayLanding>false</shuttleBayLanding>
      </li>
	
					</value>
				</li>
				
				

			</operations>
		</match>
	</Operation>
</Patch>