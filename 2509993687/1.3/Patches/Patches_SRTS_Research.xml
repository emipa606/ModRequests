<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>SRTS Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="Research_AutoTurret"]/prerequisites</xpath>
					<value>      

					<li>Research_SRTSSuperpod</li>
	
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>SRTS Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="Research_PlanetaryFortress"]/prerequisites</xpath>
					<value>      

					<li>Research_SRAFTek</li>
	
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>	
</Patch>