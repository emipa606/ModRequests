<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- =======================bases======================= -->

	<ThingDef Name="ResourceBaseGalaxy" Abstract="True">
		<thingClass>ThingWithComps</thingClass>
		<category>Item</category>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
		<stackLimit>75</stackLimit>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
	</ThingDef>

	<ThingDef ParentName="ResourceBaseGalaxy" Name="WoolBaseGalaxy" Abstract="True">
		<stackLimit>100</stackLimit>
		<graphicData>
			<texPath>Things/Item/Resource/Wool</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<MarketValue>2.7</MarketValue>
			<Mass>0.028</Mass>
			<Flammability>1.7</Flammability>
			<DeteriorationRate>3.4</DeteriorationRate>
			<StuffPower_Armor_Sharp>0.20</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.10</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.20</StuffPower_Armor_Heat>
		</statBases>
		<stuffProps>
			<statFactors>
				<Flammability>1.7</Flammability>
			</statFactors>
			<categories>
				<li>Fabric</li>
			</categories>
			<commonality>0.12</commonality>
		</stuffProps>
		<thingCategories>
			<li>Textiles</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Abstract="True" Name="OrganicProductBaseGalaxy" ParentName="ResourceBaseGalaxy">
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>10</DeteriorationRate>
			<Mass>0.03</Mass>
		</statBases>
		<tickerType>Rare</tickerType>
	</ThingDef>

	<ThingDef Abstract="True" Name="EggBaseGalaxy" ParentName="OrganicProductBaseGalaxy" >
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/EggBirdSmall</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<ingestible>
			<foodType>AnimalProduct</foodType>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<tasteThought>AteRawFood</tasteThought>
		</ingestible>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<Mass>0.15</Mass>
			<MaxHitPoints>20</MaxHitPoints>
			<DeteriorationRate>2</DeteriorationRate>
			<Flammability>0.7</Flammability>
			<Nutrition>0.25</Nutrition>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>15</daysToRotStart>
				<rotDestroys>true</rotDestroys>
				<disableIfHatcher>true</disableIfHatcher>
			</li>
		</comps>
	</ThingDef>

	<ThingDef Abstract="True" Name="EggUnfertBaseGalaxy" ParentName="EggBaseGalaxy" >
		<ingestible>
			<preferability>RawBad</preferability>
		</ingestible>
		<thingCategories>
			<li>EggsUnfertilized</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Abstract="True" Name="EggFertBaseGalaxy" ParentName="EggBaseGalaxy" >
		<tickerType>Normal</tickerType>
		<ingestible>
			<preferability>DesperateOnly</preferability>
		</ingestible>
		<thingCategories>
			<li>EggsFertilized</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_TemperatureRuinable">
				<minSafeTemperature>0</minSafeTemperature>
				<maxSafeTemperature>50</maxSafeTemperature>
				<progressPerDegreePerTick>0.00003</progressPerDegreePerTick>
			</li>
		</comps>
	</ThingDef>


	<!-- =======================wools======================= -->


	<ThingDef ParentName="WoolBaseGalaxy">
		<defName>FluffinWool</defName>
		<label>fluffin wool</label>
		<description>Soft, light and provides very good insulation.</description>
		<graphicData>
