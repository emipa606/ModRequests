<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>RH_NamerAllegianceOutpost</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[RH_AllegianceSpetsnazBase]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>RH_AllegianceSpetsnazBase</keyword>
          <path>NameBanks/Names_SpetsnazBaseGen</path>
        </li>        
      </rulesRaw>
    </rulePack>
  </RulePackDef> 
  
  <RulePackDef>
    <defName>Names_FactionNamer_AllegianceTeam</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[RH_CallSign_Allegiance]-[RH_CallSignNo_I][RH_CallSignNo_II]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>RH_CallSign_Allegiance</keyword>
          <path>NameBanks/Names_Callsigns_ActualCallsigns</path>
        </li>        
