        </li>		
		<li Class="Rule_File">
          <keyword>RH_CallSignNo_II</keyword>
          <path>NameBanks/Names_Callsign_Number_Single2</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>  
  
