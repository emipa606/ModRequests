<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- Orkoid Wildman Behavior Link -->
<Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>
	<li Class="PatchOperationAdd">
	  <xpath>/Defs/MeditationFocusDef[defName="Natural"]/requiredBackstoriesAny</xpath>
		<value>
		  <li>
			<categoryName>Ork_Weird_Child</categoryName>
			<slot>Childhood</slot>
		  </li>
		</value>
	</li>
	<li Class="PatchOperationAdd">
	  <xpath>/Defs/MeditationFocusDef[defName="Artistic"]/incompatibleBackstoriesAny</xpath>
		<value>
		  <li>
			<categoryName>Ork_Weird_Child</categoryName>
			<slot>Childhood</slot>
		  </li>
		</value>
	</li>
  </operations>
</Operation>
</Patch>
