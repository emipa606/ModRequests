<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <PawnKindDef Abstract="True" Name="OG_Ork_Player_Base">
    <race>OG_Alien_Ork</race>
    <combatPower>40</combatPower>
    <baseRecruitDifficulty>1.0</baseRecruitDifficulty>
    <isFighter>true</isFighter>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
	    <lifeStages>
      <li>
        <bodyGraphicData>
          <drawSize>0.75</drawSize>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <drawSize>1</drawSize>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <apparelTags>
      <li>OGOClothing</li>
	  <li>Cloak</li>
    </apparelTags>
	<modExtensions>
      <li Class="FeralOrkz.BackstoryExtension" />
	  <li Class="AlienRace.Info">
		<allowHumanBios>false</allowHumanBios>
		<maleGenderProbability>1</maleGenderProbability>
	  </li>
	</modExtensions>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>Ork_Standard</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
  </PawnKindDef>

  <PawnKindDef ParentName="OG_Ork_Player_Base">
    <defName>Tribesperson_OG_Ork</defName>
    <label>Ork Tribal</label>
    <defaultFactionType>OG_Ork_PlayerTribe</defaultFactionType>
    <apparelMoney>
      <min>500</min>
      <max>1200</max>
    </apparelMoney>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>Ork_Boy</li>
        </categories>
        <commonality>1</commonality>
      </li>
      <li>
        <categories>
          <li>Ork_Boy_Weird</li>
        </categories>
        <commonality>0.1</commonality>
      </li>
      <li>
        <categories>
          <li>Ork_Nob</li>
        </categories>
        <commonality>0.3</commonality>
      </li>
      <li>
        <categories>
          <li>Ork_Nob_Weird</li>
        </categories>
        <commonality>0.05</commonality>
      </li>
      <li>
        <categories>
          <li>Ork_Boss</li>
        </categories>
        <commonality>0.15</commonality>
      </li>
      <li>
        <categories>
          <li>Ork_Boss_Weird</li>
        </categories>
        <commonality>0.025</commonality>
      </li>
    </backstoryFiltersOverride>
    <apparelTags>
      <li>Neolithic</li>
    </apparelTags>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
  </PawnKindDef>

