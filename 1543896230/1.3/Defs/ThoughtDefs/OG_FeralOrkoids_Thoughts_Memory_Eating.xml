<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ======= Basic quality thoughts ========= -->
  
  <ThoughtDef>
    <defName>AteOrkMeal</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate ork meal</label>
        <description>I had to eat a disgusting, fungal meal. I know it keeps you alive, but nobody wants to swallow that glop.</description>
        <baseMoodEffect>-6</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

  <ThoughtDef>
    <defName>AteOrkFungusDirect</defName>
		<showBubble>true</showBubble>
		<icon>Things/Mote/ThoughtSymbol/Food</icon>
		<durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate ork fungus</label>
        <description>I ate some green, slimy fungus. It was, possibly, the most disgusting thing I've ever tasted.</description>
        <baseMoodEffect>-6</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

	<ThoughtDef>
		<defName>AteOrkFungusAsIngredient</defName>
		<showBubble>true</showBubble>
		<icon>Things/Mote/ThoughtSymbol/Food</icon>
		<durationDays>1</durationDays>
		<stages>
			<li>
				<label>ate cooked ork fungus</label>
				<description>I ate a meal cooked from green, slimy fungus. The seasoning partly covered the taste, but there were glands and fluid pockets in there that I can't identify.</description>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
  
  
