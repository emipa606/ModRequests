		</stuffCategories>
		<thingCategories>
			<li>Animal_Apparel</li>
		</thingCategories>
		<generateAllowChance>0</generateAllowChance>
		<apparel>
		<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Neck</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Animal/Apparel/Accessories/DogBow/dogbow</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
			</layers>
			<defaultOutfitTags>
				<li>Anything</li>
			</defaultOutfitTags>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
	</ThingDef>

