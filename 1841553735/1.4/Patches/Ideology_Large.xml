<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Ideology</li>
    </mods>
    <match Class="PatchOperationAdd">
        <xpath>/Defs/ThingSetMakerDef[defName="MapGen_PrisonCellStockpile2"]/root[@Class="ThingSetMaker_RandomOption"]/options</xpath>
        <value> 

			<li>
		    <weight>1</weight>
			  <thingSetMaker Class="ThingSetMaker_Count">
				<fixedParams>
				  <filter>
						<thingDefs>
							<li>Skull</li> 
						</thingDefs>
				  </filter>
				  <countRange>1~6</countRange>
				</fixedParams>
			  </thingSetMaker>
			</li>

		</value>
		</match>
	
	</Operation>
	
</Patch>