<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Rim War</li>
    </mods>
		<match Class="PatchOperationReplace">
        <xpath>/Defs/GenStepDef[defName="Settlement"]/genStep</xpath>		
			<value>
				<genStep Class="LargeFactionBase.GenStep_LargeFactionBase2">
				</genStep>
			</value>		
		</match>
		<nomatch Class="PatchOperationReplace">
        <xpath>/Defs/GenStepDef[defName="Settlement"]/genStep</xpath>		
			<value>
				<genStep Class="LargeFactionBase.GenStep_LargeFactionBase">
				</genStep>
			</value>		
		</nomatch>
</Operation>
	
</Patch>