      <ThingSetMakerDef>
    <defName>MapGen_PrisonCellStockpile2</defName>
    <debugParams>
      <totalMarketValueRange>
        <min>10</min>
        <max>10000</max>
      </totalMarketValueRange>
    </debugParams>
    <root Class="ThingSetMaker_RandomOption">
      <options>
	  
				<li>
          <weight>1</weight>  
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <filter>
                <categories>
                  <li>FoodMeals</li>
                </categories>
				<disallowedThingDefs>
                  <li>InsectJelly</li>
                </disallowedThingDefs>
                <disallowWorsePreferability>RawBad</disallowWorsePreferability>
                <disallowWithComp>CompHatcher</disallowWithComp>
                <disallowInedibleByHuman>true</disallowInedibleByHuman>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		
		<li>
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
	              <categories>
                  <li>BodyPartsNatural</li>
				  <li>BodyPartsProsthetic</li>
                </categories>
              </filter>
			  <countRange>2~6</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
		
		<li>
          <weight>1</weight>  
                <thingSetMaker Class="ThingSetMaker_MarketValue">
                  <fixedParams>
		<!-- <maxThingMarketValue>300</maxThingMarketValue> -->
              <filter>
					<thingDefs>
						<li>Leather_Human</li> 
					</thingDefs>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		
      </options>
    </root>
  </ThingSetMakerDef>

