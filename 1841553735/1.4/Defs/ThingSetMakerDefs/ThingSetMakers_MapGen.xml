<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingSetMakerDef>
      <defName>MapGen_DefaultStockpile2</defName>
    <debugParams> <!-- debug only! -->
      <totalMarketValueRange>
        <min>4000</min>
        <max>10000</max>
      </totalMarketValueRange>
    </debugParams>
    <root Class="ThingSetMaker_RandomOption">
      <fixedParams>
        <qualityGenerator>Reward</qualityGenerator>
      </fixedParams>
      <options>
	    <li>
          <weight>0.6</weight>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingDefs>
				  <li>MechSerumResurrector</li>
				  <li>MechSerumHealer</li>					  
	              </thingDefs>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		
        <li>
          <weight>0.8</weight>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingDefs>
				  <li>Apparel_PsychicInsanityLance</li>
				  <li>Apparel_PsychicShockLance</li>					  
	              </thingDefs>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>


 <ThingSetMakerDef>
    <defName>MapGen_DefaultStockpile3</defName>
    <debugParams>
      <countRange>
        <min>8</min>
        <max>8</max>
      </countRange>
      <totalMarketValueRange>
        <min>300</min>
        <max>10000</max>
      </totalMarketValueRange>
    </debugParams>
    <root Class="ThingSetMaker_RandomOption">
	  <fixedParams>
        <qualityGenerator>Reward</qualityGenerator>
      </fixedParams>
      <options>
        <li>
          <weight>0.2</weight>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>3000</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Weapons</li>
                </categories>
                <disallowedThingDefs>
                  <li>ElephantTusk</li>
                </disallowedThingDefs>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>0.2</weight>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>3500</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Apparel</li>
                </categories>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>0.5</weight>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>3000</maxThingMarketValue>
              <filter>
                <categories>
                  <li>ResourcesRaw</li>
                </categories>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		        <li>
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>3000</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Drugs</li>
                </categories>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		
        <li>
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_RandomGeneralGoods" />
        </li>
		
		
		        <li>
          <weight>1</weight>
		  
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <filter>
                <categories>
                  <li>Manufactured</li>
                  <li>FoodMeals</li>
                </categories>
                <thingSetMakerTagsToDisallow>
                  <li>RewardSpecial</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
		
		<li>
          <weight>4</weight>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Apparel_PowerArmor</li>
                  <li>Apparel_PlateArmor</li>
                  <li>Apparel_ShieldBelt</li>
				  <li>Apparel_ArmorRecon</li>	
				  <li>Apparel_PowerArmorHelmet</li>
				  <li>Apparel_ArmorHelmetRecon</li>		
	              </thingDefs>
	              <categories>
                  <li>Weapons</li>
                  <li>BuildingsArt</li>
                  <li>BodyPartsBionic</li>
                </categories>
              </filter>
            </fixedParams>
          </thingSetMaker>
		</li>
		
		<li>
					<weight>4</weight>
          <thingSetMaker Class="ThingSetMaker_Conditional_MakingFaction">
            <makingFaction MayRequire="Ludeon.RimWorld.Royalty">Empire</makingFaction>
            <requireNonNull>true</requireNonNull>

            <thingSetMaker Class="ThingSetMaker_Count">
              <fixedParams>
                <filter>
                  <thingSetMakerTagsToAllow>
                    <li>ImplantEmpireRoyal</li>
                    <li>WeaponBladelink</li>
                  </thingSetMakerTagsToAllow>
                  <thingSetMakerTagsToDisallow>
                    <li>RewardNonEmpireFactionsOnly</li>
                  </thingSetMakerTagsToDisallow>
                  <tradeTagsToAllow>
                    <li>PsychicWeapon</li>
                    <li>PsychicApparel</li>
                  </tradeTagsToAllow>
                </filter>
                <countRange>2~3</countRange>
              </fixedParams>
            </thingSetMaker>
          </thingSetMaker>
        </li>
				
      </options>
    </root>
  </ThingSetMakerDef>
  
