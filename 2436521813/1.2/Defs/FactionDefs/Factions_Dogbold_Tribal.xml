<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<FactionDef>
    <defName>DogboldTribe</defName>
    <label>canine kobold tribe</label>
    <description>A tribe of canine Kobolds.</description>
    <colorSpectrum>
      <li>(0.23, 1.0, 0.23)</li>
      <li>(0.45, 0.25, 0.66)</li>
      <li>(0.66, 0.45, 0.33)</li>
      <li>(0.35, 1, 0.66)</li>
    </colorSpectrum>
    <startingGoodwill>
      <min>-80</min>
      <max>100</max>
    </startingGoodwill>
    <naturalColonyGoodwill>
      <min>-80</min>
      <max>100</max>
    </naturalColonyGoodwill>
    <goodwillDailyGain>0.20</goodwillDailyGain>
    <goodwillDailyFall>0.40</goodwillDailyFall>
    <pawnSingular>canine kobold</pawnSingular>
    <pawnsPlural>canine kobolds</pawnsPlural>
    <listOrderPriority>20</listOrderPriority>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <canMakeRandomly>true</canMakeRandomly>
    <canStageAttacks>true</canStageAttacks>
    <factionIconPath>World/TribalDogbolds</factionIconPath>
    <geneticVariance>0.1</geneticVariance>
    <factionNameMaker>NamerFactionTribal</factionNameMaker>
    <settlementNameMaker>NamerSettlementTribal</settlementNameMaker>
    <pawnNameMaker>NamerPersonTribal</pawnNameMaker>
    <techLevel>Neolithic</techLevel>
    <backstoryCategories>
      <li>Tribal</li>
    </backstoryCategories>
    <leaderTitle>chieftain</leaderTitle>
    <hairTags>
      <li>Dogbold</li>
    </hairTags>
    <raidCommonalityFromPointsCurve>
      <points>
        <li>(40, 3)</li>
        <li>(500, 2)</li>
        <li>(1500, 1)</li>
        <li>(3500, 0.25)</li>
      </points>
    </raidCommonalityFromPointsCurve>
    <caravanTraderKinds>
      <li>Caravan_Neolithic_BulkGoods</li>
      <li>Caravan_Neolithic_WarMerchant</li>
      <li>Caravan_Neolithic_Slaver</li>
      <li>Caravan_Neolithic_ShamanMerchant</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_Neolithic_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Neolithic_Standard</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-40</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(800, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      
	  <li>
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <options>
          <DogboldTribalVillager>5</DogboldTribalVillager>
          <DogboldTribalWarrior>5</DogboldTribalWarrior>
          <DogboldTribalHunter>10</DogboldTribalHunter>
          <DogboldTribalChiefWarrior>1</DogboldTribalChiefWarrior>
        </options>
      </li>
	  
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <DogboldTribalVillager>10</DogboldTribalVillager>
          <DogboldTribalWarrior>5</DogboldTribalWarrior>
          <DogboldTribalHunter>5</DogboldTribalHunter>
          <DogboldTribalChiefWarrior>1</DogboldTribalChiefWarrior>
        </options>
      </li>
	  
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <DogboldTribalTrader>1</DogboldTribalTrader>
        </traders>
        <carriers>
          <Muffalo>6</Muffalo>
          <Dromedary>5</Dromedary>
          <Alpaca>2.5</Alpaca>
        </carriers>
        <guards>
          <DogboldTribalWarrior>5</DogboldTribalWarrior>
          <DogboldTribalHunter>10</DogboldTribalHunter>
        </guards>
      </li>
	  
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <DogboldTribalVillager>10</DogboldTribalVillager>
          <DogboldTribalWarrior>10</DogboldTribalWarrior>
          <DogboldTribalHunter>20</DogboldTribalHunter>
          <DogboldTribalChiefWarrior>2</DogboldTribalChiefWarrior>
        </options>
      </li>
    </pawnGroupMakers>
    <settlementTexturePath>World/DogboldCamp</settlementTexturePath>
  </FactionDef>
</Defs>