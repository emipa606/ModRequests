<Defs>

~Base~

<PawnKindDef Name="DogboldBase" Abstract="True">
        <race>Alien_Dogbold</race>
        <defaultFactionType>DogboldTribe</defaultFactionType>
        <baseRecruitDifficulty>0.70</baseRecruitDifficulty>
        <backstoryFilters>
			<li>
				<categories>
					<li>Tribal</li>
				</categories>
			</li>
		</backstoryFilters>
        <chemicalAddictionChance>0</chemicalAddictionChance>
        <invNutrition>2</invNutrition>
        <invFoodDef>Pemmican</invFoodDef>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <itemQuality>Normal</itemQuality>
        <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
        <maxGenerationAge>60</maxGenerationAge>
        <gearHealthRange>
            <min>0.5</min>
            <max>1.8</max>
		</gearHealthRange>
	<initialWillRange>0~0.25</initialWillRange>
    <initialResistanceRange>2~7</initialResistanceRange>
	</PawnKindDef>
	
	
