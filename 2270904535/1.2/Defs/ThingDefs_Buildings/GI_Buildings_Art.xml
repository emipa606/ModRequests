<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="SculptureBase">
    <defName>TitanSculpture</defName>
    <label>titan sculpture</label>
    <description>It is a sculpture modeled after a titan carrying the earth.</description>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/Art/TitanStatue</texPath>
	  <drawSize>(8,8)</drawSize>
	  <shadowData>
		<volume>(1.4, 1.3, 1.8)</volume>
      </shadowData>
    </graphicData>
    <fillPercent>0.5</fillPercent>
    <rotatable>false</rotatable>
	<size>(4,4)</size>
	<canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>900</MaxHitPoints>
      <Beauty>2800</Beauty>
	  <Mass>80</Mass>
      <WorkToMake>345000</WorkToMake>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>1600</costStuffCount>
  </ThingDef>


</Defs>