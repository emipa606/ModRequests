  <!-- flamer -->
  <ThingDef ParentName="BaseBullet">
    <defName>SM_Bullet_Flamer</defName>
    <label>fuel</label>
    <graphicData>
      <texPath>Things/Projectile/LauncherShot</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
      <color>(254,189,58)</color>
    </graphicData>
    <thingClass>Submunition.Projectile_Advanced</thingClass>
    <projectile>
      <speed>40</speed>
      <damageAmountBase>0</damageAmountBase>
      <damageDef>SM_Smoke_Silent</damageDef>
      <explosionRadius>-1</explosionRadius>
      <ai_IsIncendiary>true</ai_IsIncendiary>
      <explosionDelay>0</explosionDelay>
    </projectile>
    <modExtensions>
      <li Class="Submunition.DefAdvancedProjectileExtension">
        <spawnSpreadRadius>.25</spawnSpreadRadius>
        <spawnCount>1</spawnCount>
        <preImpactDetonation>9999</preImpactDetonation>
        <duration>9999</duration>
        <interval>3</interval>
        <secondaryDelay>10</secondaryDelay>
        <spawnThingDef>SM_Gas_Napalm</spawnThingDef>
        <spawner>true</spawner>
      </li>
    </modExtensions>
  </ThingDef>

