<Defs>

	<HediffDef Name="ObsoletePart" ParentName="PawnmorphPart" Abstract="true">
		<hediffClass>Pawnmorph.Hediffs.ObsoletePart</hediffClass>
	</HediffDef>

<!--These hediffs are currently unused but had a purpose in pervious versions of the mod and old saves may still contain them-->
	<HediffDef ParentName="ObsoletePart">
		<defName>EtherMuzzle</defName>
		<label>muzzle (depricated)</label>
		<description>You should not have this part, if you do, please use any one of the mutation items to get a more up-to-date mutation.</description>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.50</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.50</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>10.0</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
	</HediffDef>

