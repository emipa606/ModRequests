<?xml version="1.0" encoding="utf-8" ?>
<Plants>
<!-- 1티어 -->
    <ThingDef ParentName="FruitTreeBase">
    <defName>PlantPeachTree</defName>
    <label>peach tree</label>
    <description>Peach trees produce soft, fuzzy, sweet fruits called peaches. The peach tree will remain after harvesting and will start another cycle of peach production.</description>
    <graphicData>
      <texPath>Things/Plant/peachTree/peachTree</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shadowData>
        <volume>(0.3, 0.3, 0.3)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
    </statBases>
    <plant>
	  <sowMinSkill>2</sowMinSkill>
      <sowResearchPrerequisites>
        <li>T_FruitI</li>
      </sowResearchPrerequisites>
	  <visualSizeRange>
        <min>1.3</min>
        <max>2.8</max>
      </visualSizeRange>
      <topWindExposure>0.25</topWindExposure>
      <growDays>12</growDays>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <harvestWork>800</harvestWork>
      <harvestYield>24</harvestYield>
	  <harvestMinGrowth>0.80</harvestMinGrowth>
      <immatureGraphicPath>Things/Plant/AppleTree/appleTree</immatureGraphicPath>
      <harvestedThingDef>RawPeach</harvestedThingDef>
      <fertilityMin>0.6</fertilityMin>
    </plant>
  </ThingDef>
   
    <ThingDef ParentName="PlantBase">
    <defName>Plantgrape</defName>
    <label>grape vine</label>
    <description>Grape vines produce small, sweet berries that grow in clusters on a single branch. These fruits are called grapes.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
	  <Beauty>3</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/grapePlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.3, 0.3, 0.3)</volume>
      </shadowData>
    </graphicData>
    <fillPercent>0.09</fillPercent>
    <selectable>true</selectable>
    <pathCost>30</pathCost>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <foodType>Plant</foodType>
      <nutrition>0.25</nutrition>
    </ingestible>
    <plant>
	  <sowMinSkill>2</sowMinSkill>
      <sowResearchPrerequisites>
        <li>T_FruitI</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Field</li>
        <li>Hydroponic</li>
      </sowTags>
      <harvestWork>150</harvestWork>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawGrape</harvestedThingDef>
      <harvestYield>17</harvestYield>
      <fertilityMin>0.7</fertilityMin>
      <wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
      <wildClusterSizeRange>
        <min>1</min>
        <max>2</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <visualSizeRange>
        <min>0.25</min>
        <max>1.05</max>
      </visualSizeRange>
      <growDays>7.5</growDays>
      <topWindExposure>0.1</topWindExposure>
      <fertilitySensitivity>0.85</fertilitySensitivity>
    </plant>
  </ThingDef>

