<?xml version="1.0" encoding="utf-8"?>
<Plants>

  <ThingDef ParentName="PlantBase">
    <defName>PlantFoxtail_millet</defName>
    <label>Foxtail_millet plant</label>
    <description>Foxtail_millet grows fast but harvest number is few.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/susu</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.10</nutrition>
		</ingestible>
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<maxMeshCount>9</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<!-- 필요 재배스킬, 필요 연구, 토질민감도 -->
			<sowMinSkill>4</sowMinSkill>
			<sowResearchPrerequisites>
				<li>T_GrainI</li>
			</sowResearchPrerequisites>
			<fertilitySensitivity>0.65</fertilitySensitivity>
			<!-- 수확작물, 수확량, 수확가능 지대, 성장기간 -->
			<harvestedThingDef>RawFoxtail_millet</harvestedThingDef> 
			<harvestYield>16</harvestYield>
			<sowTags>
				<li>Field</li>
				<li>Hydroponic</li>
			</sowTags>
			<growDays>3.3</growDays>
			<topWindExposure>0.2</topWindExposure>
			<visualSizeRange>
				<min>0.3</min>
				<max>0.8</max>
			</visualSizeRange>
		</plant>
	</ThingDef>

