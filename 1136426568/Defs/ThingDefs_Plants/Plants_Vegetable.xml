<?xml version="1.0" encoding="utf-8"?>
<Plants>
  <ThingDef ParentName="PlantBase">
    <defName>PlantRadish</defName>
    <label>Radish plant</label>
    <description>Radish grows fast but harvest number is few.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Radish</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		    <fillPercent>0.09</fillPercent>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.10</nutrition>
		</ingestible>
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>

			<harvestTag>Standard</harvestTag>
			<!-- 필요 재배스킬, 필요 연구, 토질민감도 -->
			<sowMinSkill>4</sowMinSkill>
			<sowResearchPrerequisites>
				<li>T_VegetableI</li>
			</sowResearchPrerequisites>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<!-- 수확작물, 수확량, 수확가능 지대, 성장기간 -->
			<harvestedThingDef>Radish</harvestedThingDef> 
			<harvestYield>11</harvestYield>
			<sowTags>
				<li>Field</li>
				<li>Hydroponic</li>
			</sowTags>
			<growDays>4.4</growDays>
			<topWindExposure>0.15</topWindExposure>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.2</max>
			</visualSizeRange>
		</plant>
	</ThingDef>

