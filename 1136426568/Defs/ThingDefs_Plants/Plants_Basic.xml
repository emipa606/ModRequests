    <description>Proso millet plant is basic crop. It just provide a few food.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Plant_Proso_millet_Harvest</texPath>
