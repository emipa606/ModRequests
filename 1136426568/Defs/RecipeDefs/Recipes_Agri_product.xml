<Defs>

	<RecipeDef>
    <defName>GrindGrainflour</defName>
    <label>grind Grain</label>
    <description>Grind Grain and get Grainflour x10</description>
    <jobString>Grinding Flour.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
      <effectWorking>Cook</effectWorking>
      <workAmount>600</workAmount>
       <soundWorking>Recipe_CookMeal</soundWorking>
       <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
			<li>RawProso_millet</li>
			<li>RawWheat</li>
			<li>RawCorn</li>
			<li>RawRice</li>
			<li>RawPotatoes</li>
			<li>RawBarley</li>
			<li>RawSorghum</li>
			<li>RawFoxtail_millet</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
		<li>RawProso_millet</li>
        <li>RawWheat</li>
        <li>RawCorn</li>
        <li>RawRice</li>
        <li>RawPotatoes</li>
        <li>RawBarley</li>
        <li>RawSorghum</li>
        <li>RawFoxtail_millet</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Grainflour>10</Grainflour>
	  <Grain_Bran>2</Grain_Bran>
    </products>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>1</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Cooking</workSkill>
	<recipeUsers>
        <li>TableAgriculture</li>
    </recipeUsers>
  </RecipeDef>
 
