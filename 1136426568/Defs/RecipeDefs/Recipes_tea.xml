<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
	<defName>Makegraintea</defName>
	<label>make grain tea</label>
	<description>make grain tea.</description>
	<jobString>making grain tea.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>700</workAmount>
	<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
	<ingredients>
      <li>
        <filter>
          <thingDefs>
			<li>Grainflour</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
	        <li>
        <filter>
          <thingDefs>
			<li>SpicePowder</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
	        <li>
        <filter>
          <thingDefs>
			<li>Waterbowl</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>Grainflour</li>
			<li>SpicePowder</li>
			<li>Waterbowl</li>
          </thingDefs>
		</fixedIngredientFilter>
	<products>
      <graintea>1</graintea>
    </products>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
	<recipeUsers>
        <li>Electricteamachine</li>
    </recipeUsers>
  </RecipeDef>

