<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- 고기 -->

  <RecipeDef>
		<defName>cookBoiled_egg</defName>
		<label>cook Boiled egg.</label>
		<description>Cooks a Boiled egg.</description>
		<jobString>Cooking Boiled egg.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workAmount>300</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
		<li>
			<filter>
				<categories>
					<li>EggsUnfertilized</li>
				</categories>
			</filter>
		<count>2</count>
		</li>
			<li>
				<filter>
					<thingDefs>
						<li>Salt</li>
					</thingDefs>
				</filter>
			<count>2</count>
		</li>
	</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>EggsUnfertilized</li>
			</categories>
		  <thingDefs>
			<li>Salt</li>
          </thingDefs>
		</fixedIngredientFilter>
	<products>
      <Boiled_egg>1</Boiled_egg>
    </products>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>1</minLevel>
			</li>
		</skillRequirements>
	<recipeUsers>
		<li>CookingCampfire</li>
        <li>FueledStove</li>
		<li>ElectricStove</li>
    </recipeUsers>
  </RecipeDef>


