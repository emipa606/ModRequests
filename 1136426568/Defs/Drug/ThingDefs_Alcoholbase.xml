<?xml version="1.0" encoding="utf-8"?>
<Defs>
=
<!-- 1티어 FruitRaw-->

  <ThingDef ParentName="Farmproduct">
    <defName>RawMead</defName>
    <label>Raw Mead</label>
    <description>Raw Mead is ingridient for brewing Mead. You can make Mead by fermenting it in barrel.</description>
    <graphicData>
      <texPath>Things/Items/barrel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>12</MarketValue>
    </statBases>
    <stackLimit>75</stackLimit>
	<ingestible>
	    <foodType>Processed</foodType>
	    <preferability>DesperateOnly</preferability>
		<tasteThought>AteKibble</tasteThought>
        <nutrition>0.1</nutrition>
        <ingestEffect>EatVegetarian</ingestEffect>
        <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <thingCategories>
      <li>ProductfoodRaw</li>
    </thingCategories>
    <tickerType>Rare</tickerType>   
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>15</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

