<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

   <RecipeDef>
    <defName>MakeSoju</defName>
    <label>Make Soju x5</label>
    <description>Make Soju x5</description>
    <jobString>Making Soju.</jobString>
    <workSpeedStat>BrewingSpeed</workSpeedStat>
      <effectWorking>Cook</effectWorking>
      <workAmount>1500</workAmount>
       <soundWorking>Recipe_CookMeal</soundWorking>
       <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
			<li>Makkoli</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
			<li>Makkoli</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Soju>5</Soju>
    </products>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Cooking</workSkill>
	<recipeUsers>
        <li>distiller</li>
    </recipeUsers>
  </RecipeDef>

