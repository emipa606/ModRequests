<?xml version="1.0" encoding="utf-8"?>
<Defs>




  <ThingDef ParentName="RoughPlantBase">
    <defName>Grain_Bran</defName>
    <label>Grain bran</label>
    <description>Grain bran is by-product of making grain flour. It can be used to make compost.</description>
    <stackLimit>200</stackLimit>
    <graphicData>
      <texPath>Things/Items/Grain_Bran</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>0.3</MarketValue>
      <Mass>0.014</Mass>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
		<rotDestroys>true</rotDestroys>
	  </li>
    </comps>
    <thingCategories>
      <li>Foods</li>
    </thingCategories>
  </ThingDef>

