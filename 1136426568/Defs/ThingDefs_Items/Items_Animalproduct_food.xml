<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>
<!-- 새알 -->

  <ThingDef ParentName="MealRottable">
    <defName>Boiled_egg</defName>
    <label>Boiled egg [1Tier]</label>
    <description>A egg that is made by boilng on heat.</description>
    <graphicData>
      <texPath>Things/Items/Animal_product/Boiled_egg</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>75</stackLimit>
    <statBases>
      <DeteriorationRate>0.5</DeteriorationRate>
      <MarketValue>20</MarketValue>
      <Mass>0.4</Mass>
    </statBases>
    <ingestible>
		<preferability>MealSimple</preferability>
		<nutrition>0.55</nutrition>
		<tasteThought>AteEgg</tasteThought>
		<ingestEffect>EatVegetarian</ingestEffect>
		<ingestSound>Meal_Eat</ingestSound>
		<specialThoughtAsIngredient>AteEgg</specialThoughtAsIngredient>
    </ingestible>
  </ThingDef>  



<!-- 우유 -->

  <ThingDef ParentName="MealRottable">
    <defName>Butter</defName>
    <label>Butter [1Tier]</label>
    <description>A Butter that is made from milk.</description>
    <graphicData>
      <texPath>Things/Items/Animal_product/Butter</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>75</stackLimit>
    <statBases>
      <DeteriorationRate>0.5</DeteriorationRate>
      <MarketValue>30</MarketValue>
      <Mass>0.4</Mass>
    </statBases>
    <ingestible>
		<preferability>MealSimple</preferability>
		<nutrition>0.3</nutrition>
		<tasteThought>AteButter</tasteThought>
		<ingestEffect>EatVegetarian</ingestEffect>
		<ingestSound>Meal_Eat</ingestSound>
		<specialThoughtAsIngredient>AteButter</specialThoughtAsIngredient>
	  <outcomeDoers>
			<li Class="IngestionOutcomeDoer_GiveHediff">
				<hediffDef>Hedmilkthing</hediffDef>
					<severity>0.4</severity> <!--지속시간-->
			</li>
      </outcomeDoers>
    </ingestible>
  </ThingDef>  

