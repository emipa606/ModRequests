  <ThingDef ParentName="RawFruitBase">
    <defName>RawPeach</defName>
    <label>Peach</label>
    <description> Peach. Nice to eat, even when raw.</description>
    <graphicData>
      <texPath>Things/Items/peach</texPath>
    </graphicData>
    <statBases>
		<MarketValue>2.4</MarketValue>
	</statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>7</daysToRotStart>
