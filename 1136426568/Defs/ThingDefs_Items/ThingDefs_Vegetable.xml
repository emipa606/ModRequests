<!-- 1티어-->
  <ThingDef ParentName="PlantFoodRawBase2">
    <defName>Radish</defName>
    <label>Radish</label>
    <description>Raw Radish.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Items/Radish</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1</MarketValue>
    </statBases>
    <ingestible>
      <foodType>Seed</foodType>
	  <nutrition>0.02</nutrition>
    </ingestible>
    <thingCategories>
      <li>Vegetable</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
    </comps>
  </ThingDef>
 
