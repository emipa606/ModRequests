  <ThingDef ParentName="PlantFoodRawBase2">
    <defName>sugarcane</defName>
    <label>sugarcane</label>
    <description>sugarcane is used to make sugar</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Items/sugarcane</texPath>
    </graphicData>
    <statBases>
      <MarketValue>0.5</MarketValue>
    </statBases>
    <ingestible>
      <foodType>Seed</foodType>
	  <nutrition>0.01</nutrition>
    </ingestible>
    <thingCategories>
      <li>SpiceRaw</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>10</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
    </comps>
  </ThingDef>

