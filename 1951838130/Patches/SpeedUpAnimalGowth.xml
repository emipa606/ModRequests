<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	
	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<!-- change_juvenile_age -->
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef/race/lifeStageAges/li[2]/minAge</xpath>
				<value>
					<minAge>0.01</minAge>
				</value>
			</li>	
			<!-- change_adult_age -->
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef/race/lifeStageAges/li[3]/minAge</xpath>
				<value>
					<minAge>0.1</minAge>
				</value>
			</li>			
		</operations>
	</Operation>

</Patch>
