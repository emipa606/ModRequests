	<RulePackDef>
		<defName>LeaderTitleMaker_Kemet</defName>
		<rulePack>
			<rulesStrings>
				<li>r_leaderTitle(p=6)->[memeLeaderAdjective] [memeLeaderNoun]</li>
				<li>r_leaderTitle(p=3)->[cultureLeaderAdjective] [memeLeaderNoun]</li>
				<li>r_leaderTitle(p=1)->[memeLeaderAdjective] [cultureLeaderNoun]</li>
				<li>r_leaderTitle(p=1)->[cultureLeaderTitle]</li>
				<li>r_leaderTitle(p=1)->Red Crown</li>
				<li>r_leaderTitle(p=1)->[HorusName]</li>
				
				<li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>

				<li>cultureLeaderAdjective->great</li>
				<li>cultureLeaderAdjective->high</li>
				<li>cultureLeaderAdjective->first</li>
				<li>cultureLeaderAdjective->grand</li>
				<li>cultureLeaderAdjective->golden</li>

				<li>cultureLeaderNoun->pharaoh</li>
				<li>cultureLeaderNoun->horus</li>
				<li>cultureLeaderNoun->qore</li>
				
				<li>HorusName->[HorusTitle] of Amun</li>
				<li>HorusName->[HorusTitle] of Horus</li>
				<li>HorusName->[HorusTitle] of the Gods</li>
				<li>HorusName->Horus, the [HorusTitle]</li>
			</rulesStrings>
			<rulesFiles>
				<li>HorusTitle->Names/TitleHorus</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	
	
