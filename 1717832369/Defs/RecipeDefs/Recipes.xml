<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<RecipeDef>
		<defName>MakeBionicReproductiveOrgans</defName>
		<label>Craft bionic reproductive organs</label>
		<description>Crafts bionic reproductive organs from plasteel and components.</description>
		<jobString>Crafting bionic part.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>30000</workAmount>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<products>
			<BionicReproductiveOrgans>1</BionicReproductiveOrgans>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
		<researchPrerequisite>Bionics</researchPrerequisite>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
			<Crafting>7</Crafting>
		</skillRequirements>
	</RecipeDef>

<RecipeDef>
		<defName>MakeAdvancedBionicReproductiveOrgans</defName>
		<label>Upgrade bionic reproductive organs</label>
		<description>Upgrades bionic reproductive organs.</description>
		<jobString>Upgrading bionic part.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>RBSESound</soundWorking>
		<workAmount>40000</workAmount>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<products>
			<AdvancedBionicReproductiveOrgans>1</AdvancedBionicReproductiveOrgans>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicReproductiveOrgans</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
      <li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicReproductiveOrgans</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<skillRequirements>
      <Crafting>10</Crafting>
    </skillRequirements>
	</RecipeDef>
	

</Defs>