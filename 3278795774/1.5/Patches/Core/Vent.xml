<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Vanilla -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName = "Vent"]/graphicData/texPath</xpath>
		<value>
            <texPath>Things/Building/Misc/TempControl/Vent/MediVent</texPath>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Vent"]/graphicData</xpath>
		<value>
            <shaderType>CutoutComplex</shaderType>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Vent"]</xpath>
		<value>
			<stuffCategories>
			    <li>Woody</li>
				<li>Stony</li>
				<li>Metallic</li>
			</stuffCategories>
			<costStuffCount>20</costStuffCount>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName = "Vent"]/costList/Steel</xpath>
		<value>
			<DankPyon_IronIngot>10</DankPyon_IronIngot>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Vent"]/building</xpath>
		<value>
			<paintable>true</paintable>
		</value>
	</Operation>
	
</Patch>