<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "ToolCabinet"]</xpath>
		<value>
			<stuffCategories>
				<li>Woody</li>
			</stuffCategories>
			<costStuffCount>80</costStuffCount>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName = "ToolCabinet"]/costList/Steel</xpath>
		<value>
			<DankPyon_IronIngot>120</DankPyon_IronIngot>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "ToolCabinet"]/graphicData</xpath>
		<value>
			<shaderType>CutoutComplex</shaderType>
		</value>
	</Operation>
	
</Patch>