<?xml version="1.0" encoding="utf-8" ?>
<Patch>


 <!-- Basically this lets us circumvent the Stellarch title by adding our own list of titles so that we can force this faction to not have the Ascent quest -->	
<Operation Class="PatchOperationAdd">
		
<xpath>/Defs/RoyalTitleDef[defName="Freeholder"]</xpath> 	
<value>
			
    <tags>
      <li>EmpireTitle</li>
      <li>EmpireTwoTitle</li>
    </tags>   	
</value>
	
</Operation> 
	

