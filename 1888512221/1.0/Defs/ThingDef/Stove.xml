  <ThingDef ParentName="BuildingBase">
    <defName>Inputer_Hopper</defName>
    <label>Stuff Inputer Hopper</label>
    <description>Holds resources for use by machines like nutrient paste dispensers.</description>
    <thingClass>Building_Storage</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/Hopper</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.5</fillPercent>
    <pathCost>70</pathCost>
    <canOverlapZones>false</canOverlapZones>
    <building>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
		  <disallowedCategories>
			<li>PlantMatter</li>
		  </disallowedCategories>
		  <categories>
            <li>Weapons</li>
			<li>StoneChunks</li>
			<li>Chunks</li>
		    <li>ResourcesRaw</li>
			<li>Apparel</li>
			<li>Headgear</li>
		  </categories>
          <disallowedThingDefs>
            <li>WoodLog</li>
          </disallowedThingDefs>
	      <specialFiltersToDisallow>
            <li>AllowNonSmeltableWeapons</li>
          </specialFiltersToDisallow>
        </filter>
      </defaultStorageSettings>
    </building>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <castEdgeShadows>true</castEdgeShadows>
    <statBases>
      <WorkToBuild>300</WorkToBuild>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <Steel>15</Steel>
    </costList>
    <tickerType>Never</tickerType>
    <rotatable>true</rotatable>
    <neverMultiSelect>true</neverMultiSelect>
    <designationCategory>Production</designationCategory>
    <surfaceType>Item</surfaceType>
    <constructEffect>ConstructMetal</constructEffect>
    <researchPrerequisites>
      <li>Stove_basis</li>
    </researchPrerequisites>
    <uiIconPath>Things/Building/Production/Hopper_MenuIcon</uiIconPath>
    <uiIconScale>0.85</uiIconScale>
    <comps>
      <li Class="CompProperties_InputerHopper" />
    </comps>
  </ThingDef>


