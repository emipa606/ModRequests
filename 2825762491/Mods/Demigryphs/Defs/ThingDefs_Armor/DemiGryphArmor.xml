<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ApparelMakeableBase">
    <defName>pphhyy_Demigryph_leatherArmor</defName>
    <label>Demigryph Caparison</label>
    <description>Demigryph Caparison made from leather or fabrics, meant you show off heraldry and or deal with the temperature. (you need both pieces for it to look good and offer full protection)</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <!-- <li>DankPyon_TailorsBench</li> -->
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Apparel/DemigryphCaparison</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <!-- <shaderType>CutoutComplex</shaderType> -->
    </graphicData>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToMake>4500</WorkToMake>
      <Mass>5</Mass>
      <StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.55</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.55</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <costStuffCount>120</costStuffCount>
    <!-- <costList>
      <DankPyon_GoldOre>2</DankPyon_GoldOre>
    </costList> -->
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <!-- <li>Apparel</li> -->
      <li>Animal_Apparel</li>
    </thingCategories>
    <generateAllowChance>0</generateAllowChance>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Legs</li>
        <li>Arms</li>
        <li>FrontRightLeg</li>
        <li>FrontLeftLeg</li>
        <li>Tail</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/DemigryphCaparison</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>Animal</li>
        <li>AnimalCUTOUTCOMPLEX</li>
        <li>pphhyy_Demigryph</li>
        <li>pphhyy_CrimsonDemigryph</li>
        <li>pphhyy_CheetahDemigryph</li>
        <li>pphhyy_VultureDemigryph</li>
        <li>pphhyy_LeopardDemigryph</li>
        <li>pphhyy_PantherDemigryph</li>
        <li>pphhyy_TigerDemigryph</li>
        <li>pphhyy_Daermigryph</li>
        <li>pphhyy_JadeDemigryph</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      </apparel>
  </ThingDef>

