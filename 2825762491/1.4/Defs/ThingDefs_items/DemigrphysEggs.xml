<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="EggFertBase">
    <defName>EggDemigryphFertilized</defName>
    <label>Demigryph egg (fert.)</label>
    <description>A fertilized Demigryph egg. If all goes well, it should hatch into a baby Demigryph. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
		  <texPath>Animal/DemigryphEgg</texPath>
		  <drawSize>1</drawSize>
	</graphicData>
    <stackLimit>3</stackLimit>
    <statBases>
      <Nutrition>2.6</Nutrition>
      <MarketValue>100</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>10</hatcherDaystoHatch>
        <hatcherPawn>pphhyy_Demigryph</hatcherPawn>
      </li>
    </comps>
    <tradeTags>
		<li>Exotic</li>	
	</tradeTags>
  </ThingDef>

