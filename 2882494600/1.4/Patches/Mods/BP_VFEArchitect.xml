<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Architect</li>
        </mods>
            <match Class="PatchOperationFindMod">
                <mods>
                    <li>Biomes! Prehistoric</li>
                </mods>
                <match Class="PatchOperationSequence">
                <operations>
             
	
<li Class="PatchOperationAdd">
	<xpath>/Defs</xpath>
	<value>
 	
   <TerrainDef ParentName="VFEArch_WoodenParquetBase">
      <defName>VFEArch_WoodenParquetPrehistoric</defName>
      <label>prehistoric wooden parquet</label>
		<color>(205,133,40)</color>
      <costList Inherit="False">
         <LumberPrehistoric>12</LumberPrehistoric>
      </costList>
   </TerrainDef>  
   
   <TerrainDef ParentName="VFEArch_WoodenParquetBase">
      <defName>VFEArch_WoodenParquetOrnamental</defName>
      <label>ornamental wooden parquet</label>
		<color>(134,124,106)</color>
      <costList Inherit="False">
         <LumberOrnamental>12</LumberOrnamental>
      </costList>
   </TerrainDef> 
   
   <TerrainDef ParentName="VFEArch_WoodenParquetBase">
      <defName>VFEArch_WoodenParquetRedwood</defName>
      <label>redwood wooden parquet</label>
		<color>(102,41,20)</color>
      <costList Inherit="False">
         <LumberRedwood>12</LumberRedwood>
      </costList>
   </TerrainDef>  
   
    <TerrainDef ParentName="VFEArch_FineWoodenBase">
    	<defName>VFEArch_FineWoodenFloorPrehistoric</defName>
        <label>prehistoric fine wooden floor</label>
		<color>(205,133,40)</color>
        <costList>
            <LumberPrehistoric>20</LumberPrehistoric>
        </costList>
    </TerrainDef>
                  	
    <TerrainDef ParentName="VFEArch_FineWoodenBase">
        <defName>VFEArch_FineWoodenFloorOrnamental</defName>
        <label>ornamental fine wooden floor</label>
        <color>(134,124,106)</color>
    	<costList>
            <LumberOrnamental>20</LumberOrnamental>
        </costList>
    </TerrainDef>
    
     <TerrainDef ParentName="VFEArch_FineWoodenBase">
    	<defName>VFEArch_FineWoodenFloorRedwood</defName>
        <label>redwood fine wooden floor</label>
		<color>(102,41,20)</color>
        <costList>
            <LumberRedwood>20</LumberRedwood>
        </costList>
    </TerrainDef>
   

    </value>
</li>    

            </operations>
            </match>
		</match>    
    </Operation>

</Patch>