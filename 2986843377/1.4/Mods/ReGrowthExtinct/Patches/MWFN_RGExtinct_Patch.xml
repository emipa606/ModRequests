<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Extinct Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="GlobalUtility"]/rulePack/rulesFiles</xpath>
					<value>
						<li>RGExtinct->Words/Nouns/RGExtinct</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamerCommon"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[RGExtinct]</li>
						<li>MoreAnimals_name->[Color][RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct][BodyPart]</li>
						<li>MoreAnimals_name->[Color] [RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct] [BodyPart]</li>
						<li>subject(p=0.15)->[RGExtinct]</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamer_VTG"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[RGExtinct]</li>
						<li>MoreAnimals_name->[Color][RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct][BodyPart]</li>
						<li>MoreAnimals_name->[Color] [RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct] [BodyPart]</li>
						<li>subject(p=0.15)->[RGExtinct]</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamerNoColor_VTG"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct][BodyPart]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct] [BodyPart]</li>
						<li>subject(p=0.15)->[RGExtinct]</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamerNoAlphaAnimals_VTG"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[RGExtinct]</li>
						<li>MoreAnimals_name->[Color][RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct][BodyPart]</li>
						<li>MoreAnimals_name->[Color] [RGExtinct]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [RGExtinct]</li>
						<li>MoreAnimals_name->[RGExtinct] [BodyPart]</li>
						<li>subject(p=0.15)->[RGExtinct]</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>