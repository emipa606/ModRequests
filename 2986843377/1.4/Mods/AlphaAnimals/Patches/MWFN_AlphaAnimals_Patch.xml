<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="GlobalUtility"]/rulePack/rulesFiles</xpath>
					<value>
						<li>AlphaAnimals->Words/Nouns/AlphaAnimals</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamerCommon"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[AlphaAnimals]</li>
						<li>MoreAnimals_name->[Color][AlphaAnimals]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals][BodyPart]</li>
						<li>MoreAnimals_name->[Color] [AlphaAnimals]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals] [BodyPart]</li>
						<li>subject(p=0.15)->[AlphaAnimals]</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamer_VTG"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[AlphaAnimals]</li>
						<li>MoreAnimals_name->[Color][AlphaAnimals]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals][BodyPart]</li>
						<li>MoreAnimals_name->[Color] [AlphaAnimals]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals] [BodyPart]</li>
						<li>subject(p=0.15)->[AlphaAnimals]</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<success>Always</success>
					<xpath>/Defs/RulePackDef[defName="WorldFeatureNamerNoColor_VTG"]/rulePack/rulesStrings</xpath>
					<value>
						<li>MoreAnimals_name->[AlphaAnimals]</li>
						<li>MoreAnimals_name->[AdjectiveNatural][AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals][BodyPart]</li>
						<li>MoreAnimals_name->[AdjectiveNatural] [AlphaAnimals]</li>
						<li>MoreAnimals_name->[AlphaAnimals] [BodyPart]</li>
						<li>subject(p=0.15)->[AlphaAnimals]</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>