<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <StatCategoryDef>
    <defName>DTechprintingStatCat</defName>
    <label>Techprinting</label>
    <displayOrder>13</displayOrder>
	<displayAllByDefault>true</displayAllByDefault>
  </StatCategoryDef>

  <StatDef>
    <defName>DTechShardYield</defName>
	<workerClass>DTechprinting.StatWorker_ShardYield</workerClass>
    <label>techshard yield</label>
    <labelForFullStatList>techshard yield</labelForFullStatList>
    <description>The number of techshards that will be yielded when this item is analyzed at a techprinter.</description>
    <category>DTechprintingStatCat</category>
    <toStringStyle>Integer</toStringStyle>
	<formatString>{0} shards</formatString>
    <hideAtValue>0</hideAtValue>
	<!--<showIfUndefined>false</showIfUndefined>-->
    <displayPriorityInCategory>2</displayPriorityInCategory>
  </StatDef>
  
  <StatDef>
    <defName>DTechShardResearch</defName>
	<workerClass>DTechprinting.StatWorker_ShardTech</workerClass>
    <label>research project</label>
    <labelForFullStatList>techshard research project</labelForFullStatList>
    <description>The research project that will be unlocked or progress when the resulting techsards are applied.</description>
    <category>DTechprintingStatCat</category>
    <hideAtValue>-1</hideAtValue>
	<!--<showIfUndefined>false</showIfUndefined>-->
    <displayPriorityInCategory>1</displayPriorityInCategory>
  </StatDef>

</Defs>
