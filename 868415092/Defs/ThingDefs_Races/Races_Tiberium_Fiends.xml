<Defs>
	<ThingDef ParentName="AnimalThingBase">
		<defName>Thrimbo_TBI</defName>
		<label>Thrimbo</label>
		<description>A gigantic creature of unknown origin. The Thrumbo, but this one seems to have an Tiberium infection. Be careful, it might be easily enraged.</description>
		<statBases>
			<MoveSpeed>5.25</MoveSpeed>
			<ComfyTemperatureMin>-40</ComfyTemperatureMin>
			<MarketValue>6400</MarketValue>
			<MeatAmount>50</MeatAmount>
			<Mass>1500</Mass>
			<PainShockThreshold>1</PainShockThreshold>
			<!-- more than implied by body size -->
		</statBases>
		<verbs>
			<li>
				<verbClass>Verb_MeleeAttack</verbClass>
				<defaultCooldownTime>1.90</defaultCooldownTime>
				<meleeDamageBaseAmount>32</meleeDamageBaseAmount>
				<meleeDamageDef>Scratch</meleeDamageDef>
				<linkedBodyPartsGroup>HornAttackTool</linkedBodyPartsGroup>
			</li>
		</verbs>
		<race>
			<body>QuadrupedAnimalWithHoovesAndHorn</body>
			<fleshType>Mutated</fleshType>
			<bloodDef>FilthTibBloodG</bloodDef>
			<herdAnimal>true</herdAnimal>
			<predator>true</predator>
			<maxPreyBodySize>3</maxPreyBodySize>
			<baseBodySize>3.5</baseBodySize>
			<baseHungerRate>1.5</baseHungerRate>
			<baseHealthScale>15.0</baseHealthScale>
			<leatherColor>(200,255,200)</leatherColor>
			<leatherInsulation>1.35</leatherInsulation>
			<foodType>CarnivoreAnimalStrict</foodType>
			<meatLabel>Mutated Green Meat</meatLabel>
			<meatColor>(20,150,50)</meatColor>
			<leatherCommonalityFactor>0.1</leatherCommonalityFactor>
			<leatherMarketValueFactor>5</leatherMarketValueFactor>
			<leatherLabel>Thrimbo Fur</leatherLabel>
			<leatherStatFactors>
				<ArmorRating_Blunt>1.5</ArmorRating_Blunt>
				<ArmorRating_Sharp>4.0</ArmorRating_Sharp>
				<ArmorRating_Heat>6.0</ArmorRating_Heat>
			</leatherStatFactors>
			<trainableIntelligence>Advanced</trainableIntelligence>
			<gestationPeriodDays>80</gestationPeriodDays>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
			<wildness>0.985</wildness>
			<lifeExpectancy>130</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
					</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>6.0</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>25.0</minAge>
					<soundCall>Pawn_Thrumbo_Call</soundCall>
					<soundAngry>Pawn_Thrumbo_Angry</soundAngry>
					<soundWounded>Pawn_Thrumbo_Pain</soundWounded>
					<soundDeath>Pawn_Thrumbo_Death</soundDeath>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
	</ThingDef>

