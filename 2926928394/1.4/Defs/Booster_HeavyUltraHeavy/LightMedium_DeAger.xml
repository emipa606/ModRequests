<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--Building-->
	<ThingDef ParentName="MechUpgradePodBase_Heavy">
		<defName>MechanoidUpgradePod_HeavyUltraHeavy_Maintenance</defName>
		<building>
			<subcoreScannerOutputDef>Wastepack</subcoreScannerOutputDef>
			<subcoreScannerStartEffect>SubcoreSoftscanner_Start</subcoreScannerStartEffect>
			<subcoreScannerWorking>SubcoreSoftscanner_Working</subcoreScannerWorking>
			<subcoreScannerComplete>SubcoreSoftscanner_Complete</subcoreScannerComplete>
			<subcoreScannerHediff>BMU_MechBoost_Armor_HeavyUltraHeavy_Maintenance</subcoreScannerHediff>
			<subcoreScannerTicks>10000</subcoreScannerTicks>
			<!--4 hours-->
			<ai_chillDestination>false</ai_chillDestination>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
			<subcoreScannerFixedIngredients>
				<li>
					<filter>
						<thingDefs>
							<li>Plasteel</li>
						</thingDefs>
					</filter>
					<count>4</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Chemfuel</li>
						</thingDefs>
					</filter>
					<count>10</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>ComponentIndustrial</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Uranium</li>
						</thingDefs>
					</filter>
					<count>5</count>
				</li>
			</subcoreScannerFixedIngredients>
		</building>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_MechUpgraderExtraProps">
				<ForWeightClass>Heavy</ForWeightClass>
				<ForWeightClassSecondary>UltraHeavy</ForWeightClassSecondary>
				<WeightMultiplier>1</WeightMultiplier>
				<WeightMultiplierString>1</WeightMultiplierString>
				<!--affects stuff like the amount of wastepacks produced and....that's kinda it-->
				<ModeLabel>Maintenance</ModeLabel>
				<resetAge>true</resetAge>
				<OnlyOneLevel>true</OnlyOneLevel>
			</li>
		</comps>
	</ThingDef>
