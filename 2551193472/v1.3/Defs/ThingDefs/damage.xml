<!-- WP -->
  <DamageDef Name="WhitePhospher" ParentName="Flame">
    <defName>WhitePhospher</defName>
    <workerClass>Submunition.DamageWorker_Inc</workerClass>
    <label>burn</label>
    <hasForcefulImpact>false</hasForcefulImpact>
    <makesBlood>false</makesBlood>
