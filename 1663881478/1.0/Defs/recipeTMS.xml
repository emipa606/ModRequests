<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>CraftRuneone</defName>
		<label>Craft Apprentice's runestone</label>
		<description>cheap.</description>
		<jobString>Crafting Apprentice's runestone.</jobString>
        <workSkill>Crafting</workSkill>
        <effectWorking>BurnDrug</effectWorking>
        <soundWorking>Recipe_CookMeal</soundWorking>
		<workSpeedStat>SculptingSpeed</workSpeedStat>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MedicineHerbal</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>StoneBlocks</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<TMSArtifactone>1</TMSArtifactone>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MedicineHerbal</li>
				<li>Neutroamine</li>
			</thingDefs>
			<categories>
     			     <li>StoneBlocks</li>
   			</categories>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>TMS_Building_Base</li>
		</recipeUsers>
        <skillRequirements>
			<Intellectual>4</Intellectual>
			<Crafting>4</Crafting>
        </skillRequirements>
		<researchPrerequisite>To_my_sideone</researchPrerequisite>
	</RecipeDef>
	
	
