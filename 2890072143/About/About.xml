<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
	<name>No Refuelling</name>
	<author>_sashkaa</author>
	<packageId>sash.NoRefuelling</packageId>
	<supportedVersions>
		<li>1.3</li>
		<li>1.4</li>
	</supportedVersions>
	<description>Patches objects that need fuel to require 0 fuel per tick, effectively stopping them from consuming fuel.</description>
	<loadAfter>
		<li>Ludeon.Rimworld</li>
		<li>Ludeon.Rimworld.Royalty</li>
		<li>Ludeon.Rimworld.Ideology</li>
		<li>Ludeon.Rimworld.Biotech</li>
	</loadAfter>
	<incompatibleWith>
		<li>xqt.lagfreelampsrevamped</li>
		<li>xqt.fuellessboogaloo</li>
		<li>xQT.LagFreeLampsRevamped.Three</li>
	</incompatibleWith>
</ModMetaData>