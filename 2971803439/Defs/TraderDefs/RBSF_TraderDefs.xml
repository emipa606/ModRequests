<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<TraderKindDef>
		<defName>RBSF_Visitor_Bards</defName>
		<label>musical trader</label>
		<stockGenerators>
      
			<!-- Resources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>50~466</countRange>
			</li>
			
			<li Class="StockGenerator_SingleDef">
				<thingDef>Pemmican</thingDef>
				<countRange>40~80</countRange>
			</li>
			
			<li Class="StockGenerator_SingleDef">
				<thingDef>MedicineHerbal</thingDef>
				<countRange>1~4</countRange>
			</li>
      
			<!-- Drugs -->
			<li Class="StockGenerator_Category">
				<categoryDef>Drugs</categoryDef>
				<maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
				<thingDefCountRange>-1~1</thingDefCountRange>
				<totalPriceRange>200~400</totalPriceRange>
			</li>
      
			<!-- Techprints -->
			<li Class="StockGenerator_Techprints">
				<countChances>
					<li>
						<count>1</count>
						<chance>0.5</chance>
					</li>
				</countChances>
			</li>
      
			<!-- Weapons -->
			<li Class="StockGenerator_MarketValue">
				<maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
				<tradeTag>Instrument</tradeTag>
				<countRange>2~5</countRange>
			</li>
      
			<!-- Apparel -->
			<li MayRequire = "Rooboid.SatyrFaun.Expanded" Class="StockGenerator_MarketValue">
				<tradeTag>FaeClothing</tradeTag>
				<countRange>-1~5</countRange>
			</li>
      
			<!-- Implants -->
			<!-- none -->
      
			<!-- Buildings -->
			<li Class="StockGenerator_MarketValue">
				<tradeTag>MusicalInstrument</tradeTag>
				<maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
				<countRange>0~4</countRange>
			</li>
      
			<!-- Exotic -->
			<li Class="StockGenerator_Tag">
				<tradeTag>MusicSheet</tradeTag>
				<thingDefCountRange>1~2</thingDefCountRange>
				<countRange>-1~2</countRange>
			</li>
			
			<li Class="StockGenerator_SingleDef">
				<thingDef>RBSF_MelodicElegySheet</thingDef>
				<countRange>-8~1</countRange>
			</li>
      
			<!-- Slaves -->
			<!-- none -->
      
			<!-- Animals -->
			<!-- none -->
      
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple"/>
			<li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
			<li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
			<li Class="StockGenerator_BuyTradeTag"><tag>Instrument</tag></li>
			<li Class="StockGenerator_BuyTradeTag"><tag>Drugs</tag></li>
			<li Class="StockGenerator_BuyTradeTag"><tag>MusicSheet</tag></li>
      
		</stockGenerators>
	</TraderKindDef>

</Defs>