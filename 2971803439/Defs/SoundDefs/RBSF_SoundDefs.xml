<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--====================== Viruoso Ability Sounds ======================-->
	<!-- Lullaby -->
	
	<SoundDef>
		<defName>RBSF_LullabySound</defName>
		<context>MapOnly</context>
		<sustain>True</sustain>
		<sustainFadeoutTime>3</sustainFadeoutTime>
		<maxSimultaneous>1</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipPath>RBSF_VirtuosoSounds/RBSF_MelodicLullaby</clipPath>
					</li>
				</grains>
				<volumeRange>28</volumeRange>
				<distRange>5~35</distRange>
				<muteWhenPaused>True</muteWhenPaused>
				<sustainAttack>3</sustainAttack>
				<sustainSkipFirstAttack>False</sustainSkipFirstAttack>
			</li>
		</subSounds>
	</SoundDef>
	
