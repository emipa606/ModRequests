<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>False</enabled>
		<label>Disable Titanboa from spawning:</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/IncidentDef[defName="AP_IncentTitanboaSpawn"]/baseChance</xpath>
				<value>
					<baseChance>0</baseChance>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>