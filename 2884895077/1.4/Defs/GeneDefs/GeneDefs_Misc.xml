<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <GeneDef>
    <defName>CarryWeight_Increase</defName>
    <label>increased carry weight</label>
    <labelShortAdj>strong</labelShortAdj>
    <description>Carriers of this gene have strengthened muscle fibers in their back, allowing them to carry more things than the average person in their weight class.</description>
    <iconPath>UI/Gene_WeightUp</iconPath>
    <displayOrderInCategory>0</displayOrderInCategory>
    <statOffsets>
      <CarryingCapacity>20</CarryingCapacity>
    </statOffsets>
    <biostatMet>-1</biostatMet>
        <exclusionTags>
      <li>CarryWeight</li>
    </exclusionTags>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>strong</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

    <GeneDef>
    <defName>CarryWeight_Decrease</defName>
    <label>decreased carry weight</label>
    <labelShortAdj>weak</labelShortAdj>
    <description>Carriers of this gene have weakened muscle fibers in their back, causing them to carry fewer things than the average person in their weight class.</description>
    <iconPath>UI/Gene_WeightDown</iconPath>
    <displayOrderInCategory>0</displayOrderInCategory>
    <statOffsets>
      <CarryingCapacity>-20</CarryingCapacity>
    </statOffsets>
    <biostatMet>1</biostatMet>
        <exclusionTags>
      <li>CarryWeight</li>
    </exclusionTags>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>weak</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

</Defs>