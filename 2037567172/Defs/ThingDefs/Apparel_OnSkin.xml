  <ThingDef ParentName="ApparelBase">
    <defName>Oracle_Outfit</defName>
    <label>Oracle's Outfit</label>
    <description>Lookin Cool Oracle!</description>
    <thingClass>ShieldBelt</thingClass>
    <graphicData>
      <texPath>Things/Outfits/Oracle_Outfit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tickerType>Normal</tickerType>
    <statBases>
      <Mass>3</Mass>
      <WorkToMake>5000</WorkToMake>
	  <ArmorRating_Sharp>0.20</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.14</ArmorRating_Blunt>
      <ArmorRating_Heat>0.18</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
      <EquipDelay>5.0</EquipDelay>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <costList>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Cloth>100</Cloth>
    </costList>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedBelt</unfinishedThingDef>
      <researchPrerequisite>ShieldBelt</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
      <useIngredientsForColor>false</useIngredientsForColor>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <wearPerDay>0</wearPerDay>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
		<li>Shoulders</li>
        <li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Outfits/Oracle_Outfit</wornGraphicPath>
      <layers>
        <li>Shell</li>
		<li>Belt</li>
      </layers>
      <tags>
        <li>BeltDefense</li>
      </tags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0,1,0,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0,1,0,1)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3,1)</min>
          <only>(0,1,0,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0,1,0,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  
