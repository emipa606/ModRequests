<Defs>

  	<ThingDef ParentName="ApparelMakeableBase">
    <defName>Joker_Outfit</defName>
    <label>Joker's Outfit</label>
	<description>Lookin Cool Joker!</description>
    <recipeMaker>
      <researchPrerequisite>PersonaApparel</researchPrerequisite>
	  <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Outfits/Joker_Outfit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
	  <MaxHitPoints>150</MaxHitPoints>
	  <WorkToMake>5000</WorkToMake>
      <Mass>3.5</Mass>
      <ArmorRating_Sharp>0.80</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.54</ArmorRating_Blunt>
      <ArmorRating_Heat>0.68</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
      <EquipDelay>5.0</EquipDelay>
    </statBases>
	<costList>
      <Cloth>100</Cloth>
    </costList>
	<apparel>
	  <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
		<li>Shoulders</li>
        <li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Outfits/Joker_Outfit</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
	<thingSetMakerTags>
		<li>RewardSpecial</li>
	</thingSetMakerTags>
  </ThingDef>
  
