	<!-- Traps -->
	
	<!-- Frontier Barbed Wire -->
	
    <ThingDef parent="BuildingBase">
        <defName>FrontierBarbedWire</defName>
        <label>frontier barbed wire</label>
        <description>A basic defensive structure that slows down the enemy, and injure them in the process, perfect to prevent any hostiles from crossing your territory.</description>
        <thingClass>Building_TrapDamager</thingClass>
        <category>Building</category>
        <graphicData>
            <texPath>Things/Buildings/DefesiveStructures_Buildable/Traps/BarbWire/BarbWirePlacement_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <linkType>Basic</linkType>
            <linkFlags>
                <li>Custom6</li>
            </linkFlags>
        </graphicData>
        <costList>
            <Steel>3</Steel>
			<WoodLog>3</WoodLog>
        </costList>
        <statBases>
            <TrapMeleeDamage>10</TrapMeleeDamage>
            <TrapSpringChance>0.8</TrapSpringChance>
            <MaxHitPoints>100</MaxHitPoints>
            <Beauty>-10</Beauty>
            <WorkToBuild>50</WorkToBuild>
            <Flammability>1.0</Flammability>
        </statBases>
        <designationCategory>HuntersWorkshop</designationCategory>
        <uiIconPath>Things/Buildings/DefesiveStructures_Buildable/Traps/BarbWire/BarbWirePlacement_UI</uiIconPath>
        <drawerType>MapMeshAndRealTime</drawerType>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <pathCost>200</pathCost>
        <fillPercent>0.15</fillPercent>
        <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <rotatable>false</rotatable>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
        <coversFloor>false</coversFloor>
        <blockWind>false</blockWind>
        <holdsRoof>false</holdsRoof>
        <canOverlapZones>false</canOverlapZones>
        <castEdgeShadows>false</castEdgeShadows>
        <tickerType>Normal</tickerType>
        <building>
            <allowAutoroof>false</allowAutoroof>
            <isTrap>true</isTrap>
            <isFence>true</isFence>
            <isInert>true</isInert>
            <trapDestroyOnSpring>false</trapDestroyOnSpring>
            <trapDamageCategory>Sharp</trapDamageCategory>
            <ai_chillDestination>false</ai_chillDestination>
            <blueprintGraphicData>
                <texPath>Things/Buildings/DefesiveStructures_Buildable/Traps/BarbWire/BarbWirePlacement_Blueprint_Atlas</texPath>
                <linkType>Basic</linkType>
                <linkFlags>
                    <li>Custom6</li>
                </linkFlags>
            </blueprintGraphicData>
        </building>
        <researchPrerequisites>
            <li>FrontierDefenses</li>
        </researchPrerequisites>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <repairEffect>Repair</repairEffect>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    </ThingDef>
	
	<!-- Explosive Mine -->
	
	<ThingDef ParentName="TrapIEDBase">
      <defName>Trap_ExplosiveMine</defName>
      <label>frontier mine</label>
      <description>An explosive proximity mine which triggers a frag explosion when stepped by an enemy.</description>
      <graphicData>
        <texPath>Things/Buildings/DefesiveStructures_Buildable/Traps/BoobyTrap/ExplosiveMine_Standard</texPath>
      </graphicData>
      <costList>
        <Steel>50</Steel>
        <Chemfuel>50</Chemfuel>
      </costList>
	  <uiIconPath>Things/Buildings/DefesiveStructures_Buildable/Traps/BoobyTrap/ExplosiveMine_Standard</uiIconPath>
      <designationHotKey>Misc7</designationHotKey>
	  <researchPrerequisites>
        <li>FrontierDefenses</li>
      </researchPrerequisites>
	  <designationCategory>HuntersWorkshop</designationCategory>
      <comps>
        <li Class="CompProperties_Explosive">
          <explosiveRadius>3.9</explosiveRadius>
          <explosiveDamageType>ExplosionEX</explosiveDamageType>
          <startWickHitPointsPercent>0.2</startWickHitPointsPercent>
          <wickTicks>15</wickTicks>
          <startWickOnDamageTaken>
            <li>Bullet</li>
            <li>Arrow</li>
            <li>ArrowHighVelocity</li>
          </startWickOnDamageTaken>
        </li>
      </comps>
    <specialDisplayRadius>3.9</specialDisplayRadius>
    </ThingDef>
  
  <!-- Mannable Turrets -->
  
  <!-- Ballista Turret "Mannable" -->
  
  <ThingDef ParentName="BuildingBase">
    <defName>Turret_Mannable_Ballista</defName>
    <label>ballista turret "mannable"</label>
    <description>A stationary ballista bolt launcher mounted in a quad-pod, capable of firing ten bolts at high speeds that penetrates armor.</description>
    <thingClass>Building_TurretGun</thingClass>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Buildings/DefesiveStructures_Buildable/Turrets/TurretBase/MannableTurret_Base</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <rect>(0.38,0.2,0.56375,0.8)</rect>
        <cornerTL>Damage/Corner</cornerTL>
      </damageData>
      <shadowData>
        <offset>(-0.13,-0.87,-0.1)</offset>
        <volume>(0.5,0.4,1.05)</volume>
      </shadowData>
    </graphicData>
    <techLevel>Industrial</techLevel>
    <uiIconPath>Things/Buildings/DefesiveStructures_Buildable/Turrets/UI/Ballista_UI</uiIconPath>
    <uiIconScale>0.90</uiIconScale>
    <size>(1,2)</size>
    <altitudeLayer>Building</altitudeLayer>
    <stealable>false</stealable>
    <rotatable>true</rotatable>
    <costList>
      <Steel>150</Steel>
      <SimpleComponent>10</SimpleComponent>
      <WoodLog>75</WoodLog>
    </costList>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <Flammability>0.5</Flammability>
      <WorkToBuild>14000</WorkToBuild>
      <ShootingAccuracyTurret>1.2</ShootingAccuracyTurret>
    </statBases>
    <tickerType>Normal</tickerType>
    <comps>
      <li Class="CompProperties_Mannable">
        <manWorkType>Violent</manWorkType>
      </li>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Breakdownable" />
      <li Class="CompProperties_Refuelable">
        <fuelLabel>Shots until bolt replenishment</fuelLabel>
        <fuelGizmoLabel>Shots remaining</fuelGizmoLabel>
        <fuelFilter>
          <thingDefs>
            <li>BallistaBolts</li>
          </thingDefs>
        </fuelFilter>
        <fuelCapacity>10</fuelCapacity>
        <initialFuelPercent>1</initialFuelPercent>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <minimumFueledThreshold>1</minimumFueledThreshold>
        <fuelMultiplier>1</fuelMultiplier>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <outOfFuelMessage>Cannot shoot: Needs new bolts</outOfFuelMessage>
        <fuelIconPath>Things/Buildings/DefesiveStructures_Buildable/Turrets/UI/Reload/BallistaAmmo_UI</fuelIconPath>
      </li>
    </comps>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>
    <castEdgeShadows>true</castEdgeShadows>
    <passability>PassThroughOnly</passability>
    <pathCost>100</pathCost>
    <fillPercent>0.4</fillPercent>
    <hasTooltip>true</hasTooltip>
    <designationCategory>HuntersWorkshop</designationCategory>
    <building>
      <ai_combatDangerous>true</ai_combatDangerous>
      <turretGunDef>Gun_BallistaTurretz</turretGunDef>
      <turretBurstCooldownTime>0.7</turretBurstCooldownTime>
      <turretBurstWarmupTime>0.35</turretBurstWarmupTime>
    </building>
    <placeWorkers>
      <li>PlaceWorker_TurretTop</li>
      <li>PlaceWorker_ShowTurretRadius</li>
    </placeWorkers>
    <constructionSkillPrerequisite>7</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>BallistaTurret</li>
    </researchPrerequisites>
  </ThingDef>

  <ThingDef ParentName="BaseWeaponTurret">
    <defName>Gun_BallistaTurretz</defName>
    <label>manned ballista turret</label>
    <description>A mannable ballista made to be mounted on a turret.</description>
    <graphicData>
      <texPath>Things/Buildings/DefesiveStructures_Buildable/Turrets/TurretHead/BallistaTurret_Head</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>(1.5,1.5)</drawSize>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <destroyOnDrop>true</destroyOnDrop>
    <tradeability>None</tradeability>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <AccuracyTouch>0.60</AccuracyTouch>
