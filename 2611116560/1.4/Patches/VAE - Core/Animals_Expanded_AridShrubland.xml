<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Animals Expanded</li>
    </mods>

  <match Class="PatchOperationSequence">
    <success>Normal</success>
    <operations>

      <li Class="PatchOperationAdd">
        <xpath>Defs</xpath>
        <value>
          <ThingDef ParentName="DankPyon_HideBaseLarge">
          <defName>DankPyon_Hide_Coyote</defName>
          <label>large coyote hide</label>
          <graphicData>
                        <texPath>Resources/HeavyFurLarge</texPath>
            <color>(115,110,100)</color>
          </graphicData>
          <butcherProducts Inherit="False">
            <Leather_Plain>27</Leather_Plain>
          </butcherProducts>
          <stuffProps>
            <color>(115,110,100)</color>
          </stuffProps>
          </ThingDef>
        </value>
        </li>

                <li Class="PatchOperationAdd">
        <xpath>Defs</xpath>
        <value>
          <ThingDef ParentName="DankPyon_HideBaseHuge">
          <defName>DankPyon_Hide_Giraffe</defName>
          <label>huge giraffe hide</label>
          <graphicData>
            <color>(162,106,57)</color>
          </graphicData>
