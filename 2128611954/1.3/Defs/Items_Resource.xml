<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--organic alloy 有机合金-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_OrganicAlloy</defName>
    <label>organic alloy</label>
    <description>A special type of alloy made for ultratech.Withing good toughness and ductility can be used as an excellent fabric material.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/OrganicAlloy</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <MarketValue>6.8</MarketValue>
      <Mass>0.009</Mass>
      <Flammability>0.4</Flammability>
      <DeteriorationRate>1</DeteriorationRate>
      <StuffPower_Armor_Sharp>1.6</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.48</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>2.88</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>28</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>28</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <categories>
        <li>Fabric</li>
      </categories>
      <color>(231,66,70)</color>
      <commonality>0.1</commonality>
      <statFactors>
        <MaxHitPoints>2.0</MaxHitPoints>
        <DeteriorationRate>0.1</DeteriorationRate>
        <Flammability>0.1</Flammability>
        <Beauty>5</Beauty>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>Textiles</li>
    </thingCategories>
    <burnableByRecipe>true</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <minRewardCount>30</minRewardCount>
  </ThingDef>
  <!--neko ear 猫耳-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_NekoEar</defName>
    <label>neko ear</label>
    <description>A strange aerial. It can transform energy.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/NekoEar</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>25</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>45</MarketValue>
      <Mass>0.3</Mass>
      <Flammability>0.2</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  <!--neko component 猫娘组件-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_NekoComponent</defName>
    <label>neko component</label>
    <description>A precision device removed from a mechanical neko seems to be an important part of some kind.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/NekoComponent</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>100</MarketValue>
      <Mass>0.3</Mass>
      <Flammability>0.2</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  <!--neko core 猫娘核心-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_NekoCore</defName>
    <label>neko core</label>
    <description>The core parts found in large machinery are made of incomprehensible technology and can be used directly in some places.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/NekoCore</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>10</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>5000</MarketValue>
      <Mass>0.4</Mass>
      <Flammability>0.1</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <tradeability>Sellable</tradeability>
    <comps>
      <li Class="CompProperties_Usable">
        <useJob>UseItem</useJob>
        <useLabel>upgrade shield</useLabel>
      </li>
      <li Class="SZMechNeko.CompProperties_UseEffectNekoShieldProUpgrade">
        <sound>PsychicSoothePulserCast</sound>
      </li>
    </comps>
  </ThingDef>
  <!--energy cube 能量块-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_EnergyCube</defName>
    <label>energy cube</label>
    <description>A special crystal structure, this could storage much energy, but it also could be explosived.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/EnergyCube</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>60</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>45</MarketValue>
      <Mass>0.25</Mass>
      <Flammability>0.7</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tickerType>Normal</tickerType>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>2.9</explosiveRadius>
        <explosiveDamageType>Flame</explosiveDamageType>
        <explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
        <startWickHitPointsPercent>0.7</startWickHitPointsPercent>
        <wickTicks>30~60</wickTicks>
      </li>
    </comps>
  </ThingDef>
  <!--UL-Alloy 超轻合金-->
  <ThingDef ParentName="ResourceBase">
    <defName>SZMechNeko_ULAlloy</defName>
    <label>UL-Alloy</label>
    <description>A metal with a special crystal structure that is very light in weight and easy to process.</description>
    <graphicData>
      <texPath>AutomechNeko/Item/ULAlloy</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <useHitPoints>false</useHitPoints>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MarketValue>0.6</MarketValue>
      <Mass>0.005</Mass>
      <StuffPower_Armor_Sharp>0.6</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.3</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.5</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>4</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>4</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>1.0</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.8</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <stackLimit>1000</stackLimit>
    <smallVolume>true</smallVolume>
    <smeltable>true</smeltable>
    <deepCommonality>1.5</deepCommonality>
    <deepCountPerPortion>150</deepCountPerPortion>
    <deepLumpSizeRange>7~13</deepLumpSizeRange>
    <burnableByRecipe>false</burnableByRecipe>
    <stuffProps>
      <categories>
        <li>Metallic</li>
      </categories>
      <commonality>0.03</commonality>
      <constructEffect>ConstructMetal</constructEffect>
      <color>(224,224,224)</color>
      <soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
      <statFactors>
        <Mass>0.4</Mass>
        <MaxHitPoints>2</MaxHitPoints>
        <MarketValue>0.4</MarketValue>
        <Beauty>1.5</Beauty>
        <Flammability>0</Flammability>
        <WorkToMake>0.5</WorkToMake>
        <WorkToBuild>0.4</WorkToBuild>
        <DoorOpenSpeed>4.5</DoorOpenSpeed>
        <Comfort>1.2</Comfort>
        <WorkTableWorkSpeedFactor>1.1</WorkTableWorkSpeedFactor>
        <MeleeWeapon_CooldownMultiplier>0.6</MeleeWeapon_CooldownMultiplier>
        <ImmunityGainSpeedFactor>1.1</ImmunityGainSpeedFactor>
        <JoyGainFactor>2</JoyGainFactor>
      </statFactors>
      <statOffsets>
        <Beauty>6</Beauty>
        <Cleanliness>2</Cleanliness>
        <MedicalTendQualityOffset>0.05</MedicalTendQualityOffset>
      </statOffsets>
    </stuffProps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <generateCommonality>0.2</generateCommonality>
  </ThingDef>
  <TerrainDef ParentName="FloorBase">
    <defName>SZMechNeko_ULAlloyTile</defName>
    <label>UL-Alloy tile</label>
    <renderPrecedence>244</renderPrecedence>
    <description>The floor made of UL-alloy can keep clean and tidy automatically.</description>
    <texturePath>Terrain/Surfaces/GenericFloorTile</texturePath>
    <color>(0.87, 0.87, 0.87)</color>
    <statBases>
      <WorkToBuild>420</WorkToBuild>
      <Cleanliness>0.8</Cleanliness>
      <Beauty>4</Beauty>
    </statBases>
    <costList>
      <SZMechNeko_ULAlloy>20</SZMechNeko_ULAlloy>
      <Silver>6</Silver>
    </costList>
    <constructEffect>ConstructMetal</constructEffect>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>SterileMaterials</li>
    </researchPrerequisites>
    <extinguishesFire>true</extinguishesFire>
    <filthAcceptanceMask>
      <li>None</li>
    </filthAcceptanceMask>
    <tags>
      <li>FineFloor</li>
    </tags>
  </TerrainDef>
</Defs>