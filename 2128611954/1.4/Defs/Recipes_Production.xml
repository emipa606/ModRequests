<Defs>
  <RecipeDef>
    <defName>SZMechNeko_MakeOrganicAlloy</defName>
    <label>make organic alloy</label>
    <description>Using chemfuel with plasteel to mak organic alloy. (15x)</description>
    <jobString>synthetise organic alloy.</jobString>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <workAmount>900</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Plasteel</li>
        <li>Chemfuel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SZMechNeko_OrganicAlloy>15</SZMechNeko_OrganicAlloy>
    </products>
    <researchPrerequisite>SZMechNeko_PlastidResolving</researchPrerequisite>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
