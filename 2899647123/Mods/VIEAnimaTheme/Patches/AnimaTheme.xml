<?xml version="1.0" encoding="utf-8"?>
<Patch>
			<Operation Class="XmlExtensions.FindMod">
				<mods>
					<li>Vanilla Ideology Expanded - Anima Theme</li>
				</mods>
				<caseTrue>
					<!---/GUARANLEN TREE OPTIONS-->
					<!-- ANIMA NEXUS -->
					<!-- Only if [Vanilla Ideology Expanded - Anima Theme] is detected. -->
					<!-- Gauranlen tree => Anima Nexus tree. -->
					<!-- Size adjustment. -->
					<li Class="PatchOperationReplace" MayRequire="VanillaExpanded.VIEAT">
						<xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]/graphicData/drawSize</xpath>
						<value>
							<drawSize>1</drawSize>
						</value>
					</li>
					<!-- /ANIMA NEXUS -->
				</caseTrue>
			</Operation>
			</Patch>