<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>majestic.framework</modId>
		<key>toggleResize</key>
		<defaultValue>sizeVanilla</defaultValue>
		<apply>
			<Operation Class="XmlExtensions.PatchByCase">
				<value>{toggleResize}</value>
				<cases>
					<li>
						<value>sizeVanilla</value>
						<apply>
							<!--The stats that are being patched will appear in the settings menu in the same order listed here-->
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>saguaro_size</li>
									<!--Name of stat (no spaces)-->
									<li>Saguaro Cactus</li>
									<!--Label for settings menu-->
									<li>Plant_SaguaroCactus</li>
									<!--Defname of the StatDef-->
									<li>1.3</li>
									<!--Default min modifier-->
									<li>2.0</li>
									<!--Default max modifier-->
									<li>arid</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>drago_size</li>
									<li>Drago Tree</li>
									<li>Plant_TreeDrago</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>arid</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>willow_size</li>
									<li>Willow Tree</li>
									<li>Plant_TreeWillow</li>
									<li>1.3</li>
									<li>1.8</li>
									<li>swamp</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>cypress_size</li>
									<li>Cypress Tree</li>
									<li>Plant_TreeCypress</li>
									<li>1.5</li>
									<li>2.3</li>
									<li>swamp</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>maple_size</li>
									<li>Maple Tree</li>
									<li>Plant_TreeMaple</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>oak_size</li>
									<li>Oak Tree</li>
									<li>Plant_TreeOak</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>poplar_size</li>
									<li>Poplar Tree</li>
									<li>Plant_TreePoplar</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>pine_size</li>
									<li>Pine Tree</li>
									<li>Plant_TreePine</li>
									<li>1.3</li>
									<li>3.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>birch_size</li>
									<li>Birch Tree</li>
									<li>Plant_TreeBirch</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>teak_size</li>
									<li>Teak Tree</li>
									<li>Plant_TreeTeak</li>
									<li>1.3</li>
									<li>2.4</li>
									<li>tropical</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>cecropia_size</li>
									<li>Cecropia Tree</li>
									<li>Plant_TreeCecropia</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>tropical</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>palm_size</li>
									<li>Palm Tree</li>
									<li>Plant_TreePalm</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>tropical</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>bamboo_size</li>
									<li>Bamboo</li>
									<li>Plant_TreeBamboo</li>
									<li>1.3</li>
									<li>2.0</li>
									<li>tropical</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>agarilux_size</li>
									<li>Agarilux</li>
									<li>Agarilux</li>
									<li>0.9</li>
									<li>1.5</li>
									<li>cave</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.FindMod">
								<mods>
									<li>Ideology</li>
								</mods>
								<caseFalse />
								<caseTrue>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>timbershroom_size</li>
											<li>Timbershroom</li>
											<li>Plant_Timbershroom</li>
											<li>0.9</li>
											<li>1.5</li>
											<li>cave</li>
										</arguments>
									</Operation>
								</caseTrue>
							</Operation>
							<Operation Class="XmlExtensions.FindMod">
								<mods>
									<li>Biotech</li>
								</mods>
								<caseFalse />
								<caseTrue>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>graypine_size</li>
											<li>GrayPine Tree</li>
											<li>Plant_TreeGrayPine</li>
											<li>1.3</li>
											<li>3.0</li>
											<li>biotech</li>
										</arguments>
									</Operation>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>witchwood_size</li>
											<li>Witchwood Tree</li>
											<li>Plant_Witchwood</li>
											<li>1.3</li>
											<li>3.0</li>
											<li>biotech</li>
										</arguments>
									</Operation>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>ratpalm_size</li>
											<li>Rat Palm Tree</li>
											<li>Plant_RatPalm</li>
											<li>1.3</li>
											<li>3.0</li>
											<li>biotech</li>
										</arguments>
									</Operation>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>polux_size</li>
											<li>Pollux Tree</li>
											<li>Plant_TreePolux</li>
											<li>1.5</li>
											<li>2.1</li>
											<li>special</li>
										</arguments>
									</Operation>
								</caseTrue>
							</Operation>

							<Operation Class="XmlExtensions.FindMod">
								<mods>
									<li>Ideology</li>
								</mods>
								<caseFalse />
								<caseTrue>
									<Operation Class="XmlExtensions.FindMod">
										<mods>
											<li>Vanilla Ideology Expanded - Anima Theme</li>
										</mods>
										<caseTrue>
											<Operation Class="XmlExtensions.ApplyPatch">
												<patchName>MajesticFramework_SizePatch</patchName>
												<arguments>
													<li>gauranlen_size</li>
													<li>Anima Nexus Tree (VIE:Anima Theme)</li>
													<li>Plant_TreeGauranlen</li>
													<li>1.5</li>
													<li>2.1</li>
													<li>special</li>
												</arguments>
											</Operation>
										</caseTrue>
										<caseFalse>
											<Operation Class="XmlExtensions.ApplyPatch">
												<patchName>MajesticFramework_SizePatch</patchName>
												<arguments>
													<li>gauranlen_size</li>
													<li>Gauranlen Tree</li>
													<li>Plant_TreeGauranlen</li>
													<li>1.5</li>
													<li>2.1</li>
													<li>special</li>
												</arguments>
											</Operation>
										</caseFalse>
									</Operation>
								</caseTrue>
							</Operation>
							<Operation Class="XmlExtensions.FindMod">
								<mods>
									<li>Royalty</li>
								</mods>
								<caseFalse />
								<caseTrue>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>anima_size</li>
											<li>Anima Tree</li>
											<li>Plant_TreeAnima</li>
											<li>1.8</li>
											<li>2.5</li>
											<li>special</li>
										</arguments>
									</Operation>
								</caseTrue>
							</Operation>
						</apply>
					</li>
				</cases>
			</Operation>
		</apply>
	</Operation>
</Patch>
		
		