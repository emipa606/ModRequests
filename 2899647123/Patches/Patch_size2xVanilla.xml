									<li>oak_sizedouble</li>
									<li>Oak Tree</li>
									<li>Plant_TreeOak</li>
									<li>2.6</li>
									<li>4.0</li>
									<li>temperate</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>poplar_sizedouble</li>
									<li>Poplar Tree</li>
									<li>Plant_TreePoplar</li>
									<li>2.6</li>
									<li>4.0</li>
									<li>temperate</li>
								</arguments>
