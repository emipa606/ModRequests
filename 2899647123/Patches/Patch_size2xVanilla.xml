									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>ratpalm_sizedouble</li>
											<li>Rat Palm Tree</li>
											<li>Plant_RatPalm</li>
											<li>2.6</li>
											<li>6.0</li>
											<li>biotech</li>
										</arguments>
									</Operation>
									<Operation Class="XmlExtensions.ApplyPatch">
										<patchName>MajesticFramework_SizePatch</patchName>
										<arguments>
											<li>polux_sizedouble</li>
											<li>Pollux Tree</li>
											<li>Plant_TreePolux</li>
											<li>3.0</li>
											<li>4.2</li>
											<li>special</li>
										</arguments>
									</Operation>
								</caseTrue>
							</Operation>

							<Operation Class="XmlExtensions.FindMod">
								<mods>
									<li>Ideology</li>
								</mods>
								<caseFalse />
								<caseTrue>
									<Operation Class="XmlExtensions.FindMod">
										<mods>
											<li>Vanilla Ideology Expanded - Anima Theme</li>
										</mods>
										<caseTrue>
											<Operation Class="XmlExtensions.ApplyPatch">
												<patchName>MajesticFramework_SizePatch</patchName>
												<arguments>
													<li>gauranlen_sizedouble</li>
													<li>Anima Nexus Tree (VIE:Anima Theme)</li>
													<li>Plant_TreeGauranlen</li>
													<li>3.0</li>
													<li>4.2</li>
													<li>special</li>
												</arguments>
											</Operation>
										</caseTrue>
										<caseFalse>
											<Operation Class="XmlExtensions.ApplyPatch">
												<patchName>MajesticFramework_SizePatch</patchName>
												<arguments>
													<li>gauranlen_sizedouble</li>
													<li>Gauranlen Tree</li>
