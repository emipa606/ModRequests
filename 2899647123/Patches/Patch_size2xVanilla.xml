<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>majestic.framework</modId>
		<key>toggleResize</key>
		<defaultValue>sizeVanilla</defaultValue>
		<apply>
			<Operation Class="XmlExtensions.PatchByCase">
				<value>{toggleResize}</value>
				<cases>
					<li>
						<value>sizeVanilladouble</value>
						<apply>
							<!--The stats that are being patched will appear in the settings menu in the same order listed here-->
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>saguaro_sizedouble</li>
									<!--Name of stat (no spaces)-->
									<li>Saguaro Cactus</li>
									<!--Label for settings menu-->
									<li>Plant_SaguaroCactus</li>
									<!--Defname of the StatDef-->
									<li>2.6</li>
									<!--Default min modifier-->
									<li>4.0</li>
									<!--Default max modifier-->
									<li>arid</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
								<arguments>
									<li>drago_sizedouble</li>
									<li>Drago Tree</li>
									<li>Plant_TreeDrago</li>
									<li>2.6</li>
									<li>4.0</li>
									<li>arid</li>
								</arguments>
							</Operation>
							<Operation Class="XmlExtensions.ApplyPatch">
								<patchName>MajesticFramework_SizePatch</patchName>
