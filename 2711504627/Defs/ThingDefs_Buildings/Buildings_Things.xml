      <li>Neo_Learn_Animals</li>
      <li>Neo_Learn_Artistic</li>
      <li>Neo_Learn_Construction</li>
      <li>Neo_Learn_Cooking</li>
      <li>Neo_Learn_Crafting</li>
      <li>Neo_Learn_Growing</li>
      <li>Neo_Learn_Medicine</li>
      <li>Neo_Learn_Melee</li>
      <li>Neo_Learn_Mining</li>
      <li>Neo_Learn_Intellectual</li>
      <li>Neo_Learn_Shooting</li>
      <li>Neo_Learn_Social</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
	<comps>
	<li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>Neo_Board</li>
        </linkableFacilities>
      </li>
	</comps>
    <building>
