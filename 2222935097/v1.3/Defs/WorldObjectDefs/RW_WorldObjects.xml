<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <WorldObjectDef>
    <defName>RW_Warband_Caravan</defName>
    <label>warband_caravan</label>
    <description>A warband with pawns.</description>
    <worldObjectClass>RimWar.Planet.Warband_Caravan</worldObjectClass>
    <texture>World/WorldObjects/Caravan</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/Expanding/Caravan</expandingIconTexture>
    <expandingIconPriority>100</expandingIconPriority>
    <expandMore>true</expandMore>
    <allowCaravanIncidentsWhichGenerateMap>true</allowCaravanIncidentsWhichGenerateMap>
    <IncidentTargetTags>
      <li>Caravan</li>
    </IncidentTargetTags>
    <inspectorTabs>
      <li>WITab_Caravan_Health</li>
      <li>WITab_Caravan_Needs</li>
      <li>WITab_Caravan_Gear</li>
      <li>WITab_Caravan_Social</li>
      <li>WITab_Caravan_Items</li>
    </inspectorTabs>
  </WorldObjectDef>
  
  <WorldObjectDef>
    <defName>RW_CapitolBuilding</defName>
    <label>Capitol</label>
    <description>Marks a faction capitol</description>
    <worldObjectClass>RimWar.Planet.CapitolBuilding</worldObjectClass>
    <texture>World/capitol_star_se</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/capitol_star_s</expandingIconTexture>
    <expandingIconPriority>100</expandingIconPriority>
    <expandMore>true</expandMore>
	<selectable>false</selectable>
	<neverMultiSelect>true</neverMultiSelect>
    <inspectorTabs>
    </inspectorTabs>
  </WorldObjectDef>
  
  <WorldObjectDef Name="RW_Site">
    <defName>RW_Site</defName>
    <label>Interactive Site</label>
    <description>Interactive site.</description>
    <worldObjectClass>RimWar.Planet.RimWarSite</worldObjectClass>
    <texture>World/BattleSite</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/BattleSiteExpanded</expandingIconTexture>
    <expandingIconPriority>100</expandingIconPriority>
    <expandMore>true</expandMore>
	<selectable>true</selectable>
	<neverMultiSelect>true</neverMultiSelect>
	<comps>
      <li Class="WorldObjectCompProperties_FormCaravan" />
	  <li Class="WorldObjectCompProperties_EnterCooldown" />
    </comps>
    <inspectorTabs>
    </inspectorTabs>
  </WorldObjectDef>
  
