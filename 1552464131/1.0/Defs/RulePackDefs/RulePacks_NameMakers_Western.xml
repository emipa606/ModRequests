<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>NamerFactionWesternD9</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[wnamepref] [syl][syl][syl]</li>
		<li>r_name->[wnamepref] [syl][syl][syl]</li>
		<li>r_name->[wnamepref] [syl][syl][syl]</li>
		<li>r_name->[wnamepref] [syl][syl][syl]</li>
		<li>r_name->[wnamepref] [syl][syl]</li>
		<li>r_name->[wnamepref] [syl][syl][syl][syl]</li>
		<li>wnamepref->Confederation of</li>
		<li>wnamepref->Union of</li>
		<li>wnamepref->Republic of</li>
		<li>wnamepref->Association of</li>
		<li>wnamepref->Alliance of</li>
      </rulesStrings>
	  <rulesRaw>
	    <li Class="Rule_File">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_English</path>
        </li>
	  </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBaseWesternD9</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[positiveconcept]'s [terrainfeature]</li>
        <li>r_name->[person]'s [terrainfeature]</li>
        <li>r_name->[person]'s [community]</li>
        <li>r_name->[nearname]</li>
        <li>r_name->[wordname]</li>
        <li>r_name->[wordname]</li>
        <li>subname->[wordname]</li>
        <li>subname->[community]</li>
        <li>wordname->[syl][syl]</li>
        <li>wordname->[syl][townend]</li>
        <li>wordname->[syl][syl][townend]</li>
        <li>wordname->[terrainfeature][townend]</li>
        <li>townend->ville</li>
        <li>townend->town</li>
        <li>townend->ton</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>positiveconcept</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
        <li Class="Rule_File">
          <keyword>community</keyword>
          <path>Words/Nouns/Communities</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>person</keyword>
          <pathList>
            <li>Words/Nouns/People_Family</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_English</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
</Defs>