<Defs>
  <!--== NON-REGION-SPECIFIC ==-->
  <!-- Artistic child -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_ArtisticChild</defName>
    <title>Artistic child</title>
    <titleShort>artist</titleShort>
    <baseDescription>[PAWN_nameDef] was creatively gifted and spent much of [PAWN_possessive] childhood practicing art as often as possible.</baseDescription>
    <skillGains>
      <li>
        <defName>Artistic</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Caravan kid -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_CaravanKid</defName>
    <title>Caravan kid</title>
    <titleShort>caravan kid</titleShort>
    <baseDescription>[PAWN_nameDef] was the child of a traveling merchant. [PAWN_pronoun] spent much of [PAWN_possessive] childhood on the road and gained many practical skills, but didn't get much of a chance to make friends or study books.</baseDescription>
    <skillGains>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Cooking</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Child of the Twelve -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_ChildOfTheTwelve</defName>
    <title>Child of the Twelve</title>
    <titleShort>Twelveschild</titleShort>
    <baseDescription>[PAWN_nameDef] was left by [PAWN_possessive] parents at a sanctuary dedicated to one of the Twelve. [PAWN_pronoun] was raised by devotees, learned to perform rites and rituals, and helped pilgrims that came to visit.</baseDescription>
    <skillGains>
      <li>
        <defName>Social</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Chirurgeon's assistant -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_ChirurgeonsAssistant</defName>
    <title>Chirurgeon's assistant</title>
    <titleShort>assistant</titleShort>
    <baseDescription>[PAWN_nameDef] was raised by a busy chirurgeon and helped tend to patients from an early age. [PAWN_pronoun] assisted with procedures and helped tend to the infirm.</baseDescription>
    <skillGains>
      <li>
        <defName>Medicine</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Cooking</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Clumsy kid -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_ClumsyKid</defName>
    <title>Clumsy kid</title>
    <titleShort>clumsy</titleShort>
    <baseDescription>[PAWN_nameDef] was unusually uncoordinated as a child and was often ridiculed by [PAWN_possessive] peers for failing at even basic tasks. [PAWN_pronoun] preferred the companionship of animals and books instead.</baseDescription>
    <skillGains>
      <li>
        <defName>Intellectual</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Delinquent -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_Delinquent</defName>
    <title>Delinquent</title>
    <titleShort>delinquent</titleShort>
    <baseDescription>[PAWN_nameDef] was often out with friends instead of doing chores or any learning. [PAWN_pronoun] pretended to be an adventurer and got into a lot of mischief outside of home.</baseDescription>
    <skillGains>
      <li>
        <defName>Melee</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-2</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Farm child -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_FarmChild</defName>
    <title>Farm child</title>
    <titleShort>farm child</titleShort>
    <baseDescription>[PAWN_nameDef] grew up on a farm and helped with chores from an early age. [PAWN_pronoun] took care of the crops and livestock and even helped fix things around the home, but didn't get much opportunity to study higher knowledge.</baseDescription>
    <skillGains>
      <li>
        <defName>Plants</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-2</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Obedient child -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_ObedientChild</defName>
    <title>Obedient child</title>
    <titleShort>obedient</titleShort>
    <baseDescription>[PAWN_nameDef] obediently helped around the house as a child. [PAWN_pronoun] became very good at household chores and didn't care much for others of [PAWN_possessive] age.</baseDescription>
    <skillGains>
      <li>
        <defName>Cooking</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <!-- Voidtouched -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_Voidtouched</defName>
    <title>Voidtouched</title>
    <titleShort>voidtouched</titleShort>
    <baseDescription>[PAWN_nameDef] was touched by a voidsent horror as a child. [PAWN_pronoun] survived the ordeal, but some malingering effects never went away. [PAWN_pronoun] retreated to books and art to avoid the stigma of [PAWN_possessive] condition.</baseDescription>
    <skillGains>
      <li>
        <defName>Intellectual</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>-2</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>-2</amount>
      </li>
    </skillGains>
    <bodyTypeGlobal>Thin</bodyTypeGlobal>
  </AlienRace.BackstoryDef>
  <!-- War orphan -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseChildhoodBackstory">
    <defName>ARR_Childhood_WarOrphan</defName>
    <title>War orphan</title>
    <titleShort>orphan</titleShort>
    <baseDescription>[PAWN_nameDef] lost both of [PAWN_possessive] parents to the ravages of war. [PAWN_pronoun] grew up in an orphanage and was cared for by many different people throughout [PAWN_possessive] childhood.</baseDescription>
    <skillGains>
      <li>
        <defName>Cooking</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!--== GRIDANIA ==-->

  <!-- Amphitheatre stagehand -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseCivilianChildhoodBackstory">
    <defName>ARR_Childhood_AmphitheaterStagehand</defName>
    <title>Amphitheatre stagehand</title>
    <titleShort>stagehand</titleShort>
    <baseDescription>[PAWN_nameDef] helped at Mih Khetto's Amphitheatre from a young age. [PAWN_pronoun] cleaned, fetched things, and performed errands, but also spent every free moment watching the performers and studying [PAWN_possessive] arts.</baseDescription>
    <skillGains>
      <li>
        <defName>Artistic</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Archer trainee -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseFighterChildhoodBackstory">
    <defName>ARR_Childhood_ArcherTrainee</defName>
    <title>Archer trainee</title>
    <titleShort>trainee</titleShort>
    <baseDescription>[PAWN_nameDef] became a trainee for the Gods' Quiver from a young age. [PAWN_pronoun] learned to mend and maintain equipment, assisted the men and women of [PAWN_possessive] unit, and learned to shoot both longbows and shortbows and hunt.</baseDescription>
    <skillGains>
      <li>
        <defName>Shooting</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>-2</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Botanist's apprentice -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseCivilianChildhoodBackstory">
    <defName>ARR_Childhood_BotanistApprentice</defName>
    <title>Botanist's apprentice</title>
    <titleShort>apprentice</titleShort>
    <baseDescription>[PAWN_nameDef] was apprenticed to the Greatloam Growery as a child. [PAWN_pronoun] learned to care for and identify both grown crops as well as wild plants, as well as [PAWN_possessive] culinary and medicinal value.</baseDescription>
    <skillGains>
      <li>
        <defName>Plants</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Cooking</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Carpenter's apprentice -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseCivilianChildhoodBackstory">
    <defName>ARR_Childhood_CarpenterApprentice</defName>
    <title>Carpenter's apprentice</title>
    <titleShort>apprentice</titleShort>
    <baseDescription>[PAWN_nameDef] trained at the Oak Atrium from a young age. [PAWN_pronoun] learned to recognize good lumber from poor and learned everything they could from the journeymen and masters when [PAWN_pronoun] weren't busy doing chores.</baseDescription>
    <skillGains>
      <li>
        <defName>Construction</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Child of the Twelveswood -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseUniversalChildhoodBackstory">
    <defName>ARR_Childhood_ChildOfTheTwelveswood</defName>
    <title>Child of the Twelveswood</title>
    <titleShort>woodschild</titleShort>
    <baseDescription>[PAWN_nameDef] grew up in the Black Shroud. [PAWN_pronoun] was taught how to survive in the woods, to responsibly partake of the Twelveswood's bounty, and above all to respect the Elementals. [PAWN_pronoun] did not get much of a chance to meet many new people and had little time for books and other intellectual pursuits.</baseDescription>
    <skillGains>
      <li>
        <defName>Plants</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-2</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Flower child -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseCivilianChildhoodBackstory">
    <defName>ARR_Childhood_FlowerChild</defName>
    <title>Flower child</title>
    <titleShort>flower child</titleShort>
    <baseDescription>[PAWN_nameDef] was raised by florists in Gridania. As a child, [PAWN_pronoun] helped grow decorative flowers and medicinal herbs alike, and spent much time in the woods gathering materials as well</baseDescription>
    <skillGains>
      <li>
        <defName>Plants</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Melee</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Lancer trainee -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseFighterChildhoodBackstory">
    <defName>ARR_Childhood_LancerTrainee</defName>
    <title>Lancer trainee</title>
    <titleShort>trainee</titleShort>
    <baseDescription>[PAWN_nameDef] became a trainee for the Wailers from a young age. [PAWN_pronoun] learned to mend and maintain equipment, did chores around the barracks, and practiced with a spear whenever [PAWN_pronoun] could.</baseDescription>
    <skillGains>
      <li>
        <defName>Melee</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
    <forcedTraits>
      <li>
        <defName>Brawler</defName>
        <chance>50</chance>
      </li>
    </forcedTraits>
  </AlienRace.BackstoryDef>

  <!-- Leatherworker's apprentice -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseCivilianChildhoodBackstory">
    <defName>ARR_Childhood_LeatherworkerApprentice</defName>
    <title>Leatherworker's apprentice</title>
    <titleShort>apprentice</titleShort>
    <baseDescription>[PAWN_nameDef] became an apprentice at Atelier Fen-Yll from a young age. [PAWN_pronoun] mainly helped with menial chores, but learned how to appraise animal hides and turn them into apparel and armor over time.</baseDescription>
    <skillGains>
      <li>
        <defName>Crafting</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Melee</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Lost child -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseUniversalChildhoodBackstory">
    <defName>ARR_Childhood_LostChild</defName>
    <title>Lost child</title>
    <titleShort>lost child</titleShort>
    <baseDescription>[PAWN_nameDef] was either lost or abandoned in the Twelveswood as a child. Either by a miracle or the grace of the Elementals, [PAWN_pronoun] survived for several years before being discovered and adopted by hunters.</baseDescription>
    <skillGains>
      <li>
        <defName>Animals</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-2</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-2</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- The Budding One -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseUniversalChildhoodBackstory">
    <defName>ARR_Childhood_TheBuddingOne</defName>
    <title>The Budding One</title>
    <titleShort>bud</titleShort>
    <baseDescription>[PAWN_nameDef] was a lost child found by the sylphs of Moonspore Grove. [PAWN_pronoun] was eventually taken in by traders dealing with the sylphs, but [PAWN_possessive] time with the beast tribe affected [PAWN_possessive] upbringing.</baseDescription>
    <skillGains>
      <li>
        <defName>Plants</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!--== LIMSA LOMINSA ==-->

  <!-- Cabin boy / cabin girl -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseUniversalChildhoodBackstory">
    <defName>ARR_Childhood_CabinChild</defName>
    <title>Cabin boy</title>
    <titleShort>cabin boy</titleShort>
    <titleFemale>Cabin girl</titleFemale>
    <titleShortFemale>cabin girl</titleShortFemale>
    <baseDescription>[PAWN_nameDef] spent a great deal of [PAWN_possessive] childhood on board a ship. [PAWN_pronoun] helped the cook in the galley serve meals, delivered messages between officers, and even learned a little bit about sailing and navigation in between chores.</baseDescription>
    <skillGains>
      <li>
        <defName>Cooking</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>-1</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>-1</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>

  <!-- Midshipman -->
  <AlienRace.BackstoryDef ParentName="ARR_BaseUniversalChildhoodBackstory">
    <defName>ARR_Childhood_Midshipman</defName>
    <title>Midshipman</title>
