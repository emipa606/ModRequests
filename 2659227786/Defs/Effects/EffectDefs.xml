<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- rift tear sound effect -->
  <EffecterDef>
    <defName>ARR_RiftTearAudio</defName>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SoundTriggered</subEffecterClass>
        <soundDef>ARR_RiftTeleportIn</soundDef>
        <spawnLocType>OnSource</spawnLocType>
      </li>
    </children>
  </EffecterDef>

  <!-- rift tear effecter -->
  <EffecterDef>
    <defName>ARR_RiftTear</defName>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SprayerTriggered</subEffecterClass>
        <fleckDef>ARR_FleckRiftSparks</fleckDef>
        <burstCount>3~3</burstCount>
        <scale>2.7~3</scale>
        <rotation>0~359</rotation>
        <rotationRate>-180~-120</rotationRate>
        <spawnLocType>OnSource</spawnLocType>
      </li>
      <li>
        <subEffecterClass>SubEffecter_SprayerTriggered</subEffecterClass>
        <fleckDef>ARR_FleckRiftTear</fleckDef>
        <burstCount>3~3</burstCount>
        <scale>2.7~3</scale>
        <rotation>0~359</rotation>
        <rotationRate>-180~-120</rotationRate>
        <spawnLocType>OnSource</spawnLocType>
      </li>
    </children>
  </EffecterDef>

  <!-- attunement effecter -->
  <EffecterDef>
    <defName>ARR_EffecterAetheryteAttune</defName>
    <offsetTowardsTarget>0.0~1.0</offsetTowardsTarget>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SprayerContinuous</subEffecterClass>
        <fleckUsesAngleForVelocity>true</fleckUsesAngleForVelocity>
        <fleckDef>ARR_AetherGlow</fleckDef>
        <ticksBetweenMotes>2</ticksBetweenMotes>
        <angle>0~359</angle>
        <speed>0.0~0.1</speed>
        <scale>0.5~0.6</scale>
        <positionRadius>0.5</positionRadius>
        <positionLerpFactor>0.6</positionLerpFactor>
        <spawnLocType>BetweenPositions</spawnLocType>
      </li>
    </children>
  </EffecterDef>

  <!-- potion sparkle effecter -->
  <EffecterDef>
    <defName>ARR_Sparkles</defName>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SprayerTriggered</subEffecterClass>
        <fleckDef>ARR_FleckSparkle</fleckDef>
        <positionRadius>0.35</positionRadius>
        <positionOffset>(0,0,.7)</positionOffset>
        <burstCount>7~9</burstCount>
        <scale>0.2~0.4</scale>
        <speed>-1~-1.3</speed>
        <rotation>0~90</rotation>
        <rotationRate>-180~180</rotationRate>
        <spawnLocType>OnTarget</spawnLocType>
      </li>
    </children>
  </EffecterDef>

