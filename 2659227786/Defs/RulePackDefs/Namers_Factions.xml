<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Generic Faction faction name generator -->
  <RulePackDef>
    <defName>ARR_NamerFaction_Generic</defName>
    <include>
      <li>ARR_GenericFactionNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[ARRGenericFactionPeopleNames] of [ARRGenericFactionProperNames]</li>
        <li>r_name(p=2)->[ARRGenericFactionPeopleNames] of the [ARRGFProperTitles]</li>
        <li>r_name->[ARRGFProperNames]'s [ARRGenericFactionPeopleNames]</li>
        <li>r_name(p=2)->[ARRGFProperTitles]'s [ARRGenericFactionPeopleNames]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ARR_GenericFactionNameUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>ARRGFProperNames->ARimReborn/Names/Faction_Proper</li>
        <li>ARRGFProperTitles->ARimReborn/Names/Faction_Proper_Titles</li>
        <li>ARRGFPeopleNames->ARimReborn/Names/Faction_People</li>
      </rulesFiles>
      <rulesStrings>
        <li>ARRGenericFactionProperNames->[ARRGFProperNames]</li>
        <li>ARRGenericFactionProperTitles->[ARRGFProperTitles]</li>
        <li>ARRGenericFactionPeopleNames->[ARRGFPeopleNames]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>
