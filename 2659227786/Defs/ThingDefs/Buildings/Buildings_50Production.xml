<Defs>
  <!--==== PRODUCTION ====-->

  <!-- Aetheric Prep Table -->
  <ThingDef ParentName="BenchBase">
    <defName>ARR_AetherPrepTable</defName>
    <label>aetheric prep table</label>
    <description>A work table with containers, holders, and measuring, mixing, and heating equipment equally useful for preparing meals or synthesizing alchemical substances.\n\nThis workbench relies on the use of aether crystals to operate, and thus is only usable by someone capable of manipulating aether.</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>ARimReborn/Things/Buildings/AetherPrepTable</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(3.5,1.5)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <costStuffCount>75</costStuffCount>
    <costList>
      <Steel>25</Steel>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2400</WorkToBuild>
      <MaxHitPoints>150</MaxHitPoints>
      <Flammability>0.5</Flammability>
    </statBases>
    <researchPrerequisites>
      <li>ARR_AdvancedSynthesis</li>
    </researchPrerequisites>
    <size>(3,1)</size>
    <designationCategory>ARR_Aether</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <recipes>
      <li>ARR_CookMealSimple</li>
      <li>ARR_CookMealSimpleBulk</li>
      <li>ARR_CookMealFine</li>
      <li>ARR_CookMealFine_Veg</li>
      <li>ARR_CookMealFine_Meat</li>
      <li>ARR_CookMealFineBulk</li>
      <li>ARR_CookMealFineBulk_Veg</li>
      <li>ARR_CookMealFineBulk_Meat</li>
      <li>ARR_CookMealLavish</li>
      <li>ARR_CookMealLavish_Veg</li>
      <li>ARR_CookMealLavish_Meat</li>
      <li>ARR_CookMealLavishBulk</li>
      <li>ARR_CookMealLavishBulk_Veg</li>
      <li>ARR_CookMealLavishBulk_Meat</li>
      <li>ARR_CookTravelRationsWithCrystals</li>
      <li>ARR_CookTravelRationsBulkWithCrystals</li>
      <li>ARR_CraftAlchemicalMedicineWithCrystals</li>
      <li>ARR_CraftPotionWithCrystals</li>
      <li>ARR_CraftPotionHiWithCrystals</li>
      <li>ARR_CraftPotionVitalityWithCrystals</li>
      <li>ARR_CraftPotionStrengthWithCrystals</li>
      <li>ARR_CraftPotionDexterityWithCrystals</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
  </ThingDef>

