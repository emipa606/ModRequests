<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Defs>

  <RecipeDef>
    <defName>ARR_MakeComponentIndustrial</defName>
    <label>synthesize component</label>
    <description>Synthesize a component using aetheric machine tools. Using aetheric processes to build components is slower and more resource intensive than using electric fabrication benches.</description>
    <jobString>Making component.</jobString>
    <workSkill>Crafting</workSkill>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>6000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentIndustrial>1</ComponentIndustrial>
    </products>
    <recipeUsers>
      <li>ARR_AetherMachineTable</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
  </RecipeDef>

</Defs>