<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Fill capacitors with crystals -->
  <WorkGiverDef>
    <defName>ARR_FillCapacitor</defName>
    <label>fill crystals</label>
    <giverClass>ARimReborn.WorkGiver_FillCapacitor</giverClass>
    <workType>Hauling</workType>
    <verb>fill</verb>
    <gerund>filling</gerund>
    <priorityInType>55</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>

  <!-- Empty crystals from capacitors -->
  <WorkGiverDef>
    <defName>ARR_EmptyCapacitor</defName>
    <label>empty crystals</label>
    <giverClass>ARimReborn.WorkGiver_EmptyCapacitor</giverClass>
    <workType>Hauling</workType>
    <verb>empty</verb>
    <gerund>emptying</gerund>
    <priorityInType>50</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>

</Defs>
