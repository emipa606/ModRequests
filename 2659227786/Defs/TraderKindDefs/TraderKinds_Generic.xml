<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>ARR_Caravan_Generic_Farm</defName>
    <label>farm caravan</label>
    <stockGenerators>

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>750~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>100~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>100~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ARR_MealTravel</thingDef>
        <countRange>20~50</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>5~20</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>2~2</thingDefCountRange>
        <totalPriceRange>200~600</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories><li>EggsFertilized</li></excludedCategories>
        <thingDefCountRange>4~6</thingDefCountRange>
        <totalPriceRange>400~600</totalPriceRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
        </thingDefs>
      </li>

      <!-- Alchemy -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>ARR_SeedStock</thingDef>
        <countRange>0~1</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>ARRHerbs</categoryDef>
        <thingDefCountRange>0~2</thingDefCountRange>
        <totalPriceRange>100~300</totalPriceRange>
      </li>

      <!-- Drugs -->
      <!-- none -->

      <!-- Techprints -->
      <!-- none -->

      <!-- Weapons -->
      <!-- none -->

      <!-- Apparel -->
      <!-- none -->

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <!-- none -->

      <!-- Slaves -->
      <!-- none -->

      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>2~3</kindCountRange>
        <countRange>3~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>

    </stockGenerators>
  </TraderKindDef>

