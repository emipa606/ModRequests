<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>ARR_Visitor_Generic</defName>
    <label>trader</label>
    <stockGenerators>

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>50~250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ARR_MealTravel</thingDef>
        <countRange>5~10</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>1~4</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ARR_Potion</thingDef>
        <countRange>0~2</countRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>

      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <totalPriceRange>200~400</totalPriceRange>
      </li>

      <!-- Aether crystals -->
      <li Class="StockGenerator_Category">
        <categoryDef>ARRCrystals</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>50~200</totalPriceRange>
      </li>

      <!-- Techprints -->
      <!-- none -->

      <!-- Weapons -->
      <li Class="StockGenerator_WeaponsRanged">
        <weaponTag>ARR_RangedSimple</weaponTag>
        <countRange>0~1</countRange>
      </li>
      <li Class="StockGenerator_WeaponsMelee">
        <weaponTag>ARR_MeleeSimple</weaponTag>
        <countRange>0~1</countRange>
      </li>

      <!-- Apparel -->
      <li Class="StockGenerator_Clothes">
        <apparelTag>ARR_Generic</apparelTag>
        <countRange>0~1</countRange>
      </li>

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <!-- none -->

      <!-- Slaves -->
      <!-- none -->

      <!-- Animals -->
      <!-- none -->

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>

    </stockGenerators>
  </TraderKindDef>

</Defs>
