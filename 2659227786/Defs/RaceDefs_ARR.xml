<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Defs>
  <!-- Abstract base for all ARR races -->
  <AlienRace.ThingDef_AlienRace Name="ARR_Base" ParentName="HumanRace" Abstract="true">
    <alienRace>
      <generalSettings>
        <humanRecipeImport>true</humanRecipeImport>
        <alienPartGenerator>
          <borderScale>2.0</borderScale>
          <atlasScale>2</atlasScale>
        </alienPartGenerator>
      </generalSettings>

      <raceRestriction>
        <researchList>
          <li>
            <projects>
              <li>ARR_Synthesis</li>
              <li>ARR_BasicAlchemy</li>
              <li>ARR_CrystalSynthesis</li>
              <li>ARR_AethericDevices</li>
              <li>ARR_AdvancedSynthesis</li>
              <li>ARR_AdvancedAccumulators</li>
              <li>ARR_Carpentry</li>
              <li>ARR_Blacksmithing</li>
              <li>ARR_Armorsmithing</li>
              <li>ARR_Leatherworking</li>
              <li>ARR_Weaving</li>
              <li>ARR_BasicMagitek</li>
              <li>ARR_Aetheryte</li>
              <li>ARR_AethericTransmission</li>
              <li>ARR_Fabrication</li>
              <li>ARR_Firearms</li>
              <li>ARR_AethericElectricity</li>
              <li>ARR_GrowMistletoe</li>
              <li>ARR_GrowLavender</li>
              <li>ARR_GrowGilBun</li>
              <li>ARR_GrowBelladonna</li>
            </projects>
          </li>
        </researchList>
        <buildingList>
          <li>ARR_AetherPrepTable</li>
          <li>ARR_AetherCraftTable</li>
          <li>ARR_AetherCraftTableLinked</li>
          <li>ARR_AetherTailorTable</li>
          <li>ARR_AetherTailorTableLinked</li>
          <li>ARR_AetherMachineTable</li>
          <li>ARR_IceAccumulator</li>
          <li>ARR_LightningAccumulator</li>
          <li>ARR_WaterAccumulator</li>
          <li>ARR_WindAccumulator</li>
          <li>ARR_GeothermalAccumulator</li>
          <li>ARR_CelestialAccumulator</li>
          <li>ARR_ElectricalAccumulator</li>
          <li>ARR_LightningGenerator</li>
          <li>ARR_FloorLampGlade</li>
          <li>ARR_FloorLampRiviera</li>
          <li>ARR_AethericCooler</li>
          <li>ARR_AethericIceCooler</li>
          <li>ARR_AethericHeater</li>
          <li>ARR_AethericThermostat</li>
          <li>ARR_AethericLink</li>
          <li>ARR_Aetheryte</li>
        </buildingList>
        <recipeList>
          <li>ARR_CraftKnife</li>
          <li>ARR_CraftKnifeWithCrystals</li>
          <li>ARR_CraftFalchion</li>
          <li>ARR_CraftFalchionWithCrystals</li>
          <li>ARR_CraftZweihander</li>
          <li>ARR_CraftZweihanderWithCrystals</li>
          <li>ARR_CraftSpear</li>
          <li>ARR_CraftSpearWithCrystals</li>
          <li>ARR_CraftCompositeBow</li>
          <li>ARR_CraftCompositeBowWithCrystals</li>
          <li>ARR_CraftLongBow</li>
          <li>ARR_CraftLongBowWithCrystals</li>
          <li>ARR_CraftDoublePistol</li>
          <li>ARR_CraftCarbine</li>
        </recipeList>
      </raceRestriction>

      <compatibility>
        <isFlesh>true</isFlesh>
        <isSentient>true</isSentient>
        <hasBlood>true</hasBlood>
      </compatibility>
    </alienRace>

    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <foodType>OmnivoreHuman</foodType>
      <hediffGiverSets>
        <li>ARR_Aetheric</li>
      </hediffGiverSets>
    </race>
<!--    <recipes>-->
<!--      <li>ARR_AdministerPotion</li>-->
<!--    </recipes>-->
    <recipes />

  </AlienRace.ThingDef_AlienRace>
</Defs>
