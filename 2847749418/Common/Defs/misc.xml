<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ResourceBase">
		<defName>K_AnimaExtract</defName>
		<label>anima extract</label>
		<description>A concentrated extract from processed anima fibers.</description>
		<graphicData>
		  <texPath>Things/Resource/AnimaExtract</texPath>
		  <graphicClass>Graphic_StackCount</graphicClass>
		  <drawSize>0.90</drawSize>
		</graphicData>
		<stackLimit>75</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<statBases>
		  <MaxHitPoints>50</MaxHitPoints>
		  <MarketValue>17</MarketValue>
		  <Mass>0.02</Mass>
		  <Flammability>0.4</Flammability>
		  <DeteriorationRate>3.0</DeteriorationRate>
		</statBases>
		<thingCategories>
		  <li>Manufactured</li>
		</thingCategories>
	</ThingDef>
	
	
	<ThingDef ParentName="UnfinishedBase">
		<defName>K_UnfinishedStuff</defName>
		<label>unfinished stuff</label>
		<description>An unfinished mass of some kind.</description>
		<statBases>
		  <Flammability>0.25</Flammability>
		</statBases>
		<graphicData>
		  <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		  <color>(165,203,173)</color>
		  <drawSize>0.50</drawSize>
		</graphicData>
		<stuffCategories Inherit="false" />
	</ThingDef>
	

</Defs>