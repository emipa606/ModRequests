	<RecipeDef MayRequire="vanillaexpanded.vpsycastse">
		<defName>K_Make_Eltex</defName>
		<label>synthesize eltex</label>
		<description>Synthesizes a piece of Eltex.</description>
		<jobString>Synthesizing Eltex.</jobString>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>12000</workAmount>
		<unfinishedThingDef>K_UnfinishedStuff</unfinishedThingDef>
		<recipeUsers>
			<li>FabricationBench</li>
		</recipeUsers>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>K_AnimaExtract</li>
			  </thingDefs>
			</filter>
			<count>5</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>Chemfuel</li>
			  </thingDefs>
			</filter>
			<count>10</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>Uranium</li>
			  </thingDefs>
			</filter>
			<count>15</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>K_AnimaExtract</li>
			<li>Chemfuel</li>
			<li>Uranium</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <VPE_Eltex>1</VPE_Eltex>
		</products>
		<researchPrerequisite>K_SynthEltex</researchPrerequisite>
		<skillRequirements>
		  <Intellectual>8</Intellectual>
		  <Crafting>14</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
	</RecipeDef>
	
