<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>Make_K_AnimaExtract</defName>
		<label>make anima extract</label>
		<description>Processes anima fibers into a concentrated extract.</description>
		<jobString>Refining anima fibers.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>3000</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>AnimaFiber</li>
			  </thingDefs>
			</filter>
			<count>50</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>AnimaFiber</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<researchPrerequisites>
			<li>DrugProduction</li>
			<li>AnimaDrugs</li>
		</researchPrerequisites>
		<products>
		  <K_AnimaExtract>1</K_AnimaExtract>
		</products>
		<skillRequirements>
		  <Crafting>3</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
	</RecipeDef>
