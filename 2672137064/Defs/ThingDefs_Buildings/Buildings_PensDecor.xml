  <ThingDef ParentName="FurnitureBase">
    <defName>PensTreeOne</defName>
    <label>tree pot 1</label>
    <description>A tree in a pot. Nice to look at.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/pentree1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1.53)</drawSize>
      <drawOffset>(0,0.4,0.4)</drawOffset>
      </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.15</fillPercent>
    <pathCost>42</pathCost>
    <costList>
      <WoodLog>10</WoodLog>
    </costList>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <Mass>10</Mass>
      <WorkToBuild>1200</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>2</Beauty>
    </statBases>
    <rotatable>false</rotatable>
    <size>(1,1)</size>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
  </ThingDef>
