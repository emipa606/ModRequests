<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Toony Terrain Textures</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<success>Always</success>
					<xpath>Defs/TerrainDef[defName="Owl_PackedSand"]/texturePath</xpath>
					<value>
						<texturePath>Terrain/Surfaces/PackedSandToon</texturePath>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>