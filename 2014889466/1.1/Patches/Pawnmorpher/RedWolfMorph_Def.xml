<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Pawnmorpher</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>

				<Pawnmorph.MorphDef ParentName="CanineMorph">
					<defName>RedWolfMorph</defName>
					<label>redwolfmorph</label>
					<description>Like their wild counterparts, red wolf morphs are more slender and less fluffy than their grey wolf cousins.  Even still, despite their smaller stature and more timid disposition they still make capable fighters, especially in packs.</description>
					<race>Wolf_Red</race>
					<classification>Wolf</classification>
					<categories>
						<li>Temperate</li>
						<li>Combat</li>
						<li>Canid</li>
					</categories>
					<fullTransformation>PawnmorphRedWolfTF</fullTransformation>
					<partialTransformation>PawnmorphRedWolfTFPartial</partialTransformation>
					<raceSettings>
						<graphicsSettings>
							<skinColorOverride>(112,102,102)</skinColorOverride>
							<skinColorOverrideSecond>(168,142,125)</skinColorOverrideSecond>
							<hairColorOverride>(42,36,36)</hairColorOverride>
						</graphicsSettings>
					</raceSettings>
					<transformSettings>
						<transformationMemory>RedWolfTransformationMemory</transformationMemory>
						<revertedMemory>RedWolfTransformationMemory</revertedMemory>
					</transformSettings>
				</Pawnmorph.MorphDef>

			</value>
		</match>
	</Operation>
</Patch>
