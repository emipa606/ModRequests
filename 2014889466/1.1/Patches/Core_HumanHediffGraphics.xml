<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Pawnmorpher</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Jaw"]/offsets/north</xpath>
					<value>
						<north>
							<bodyTypes>
								<Male>(0,0.94)</Male>
								<Female>(0,0.94)</Female>
								<Thin>(0,0.94)</Thin>
								<Hulk>(0,0.94)</Hulk>
								<Fat>(0,0.94)</Fat>
							</bodyTypes>
							<crownTypes>
								<Average_Wide>(-0.015,0.005)</Average_Wide>
								<Average_Pointy>(0,0.005)</Average_Pointy>
								<Narrow_Normal>(0,-0.01)</Narrow_Normal>
								<Narrow_Wide>(0,-0.01)</Narrow_Wide>
								<Narrow_Pointy>(0,-0.01)</Narrow_Pointy>
							</crownTypes>
						</north>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Jaw"]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfMuzzle>Parts/WolfRed/RedWolf_Jaw</EtherRedWolfMuzzle>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear" and inFrontOfBody="true"]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfEar>Parts/WolfRed/RedWolf_Ear_Left</EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear" and inFrontOfBody="false"]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfEar>Parts/WolfRed/RedWolf_Ear_Left_Behind</EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear" and inFrontOfBody="true"]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfEar>Parts/WolfRed/RedWolf_Ear_Right</EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear" and inFrontOfBody="false"]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfEar>Parts/WolfRed/RedWolf_Ear_Right_Behind</EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Tail" and not(layerOffset="-0.29")]/hediffGraphics</xpath>
					<value>
						<EtherRedWolfTail>Parts/WolfRed/RedWolf_Tail</EtherRedWolfTail>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
