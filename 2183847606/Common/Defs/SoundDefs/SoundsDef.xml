<Defs>

  	<!-- =========== AK sounds ============ -->

  <SoundDef>
    <defName>AK</defName>  
    <context>MapOnly</context>
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Shot/AKshot</clipPath>
          </li>
        </grains>      
        <volumeRange>34.7826~34.7826</volumeRange>     
        <pitchRange>
          <min>0.936413</min>        
          <max>1.042391</max>
        </pitchRange>               
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
