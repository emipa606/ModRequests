<Defs>

  <!-- ==================== Assault rifle ==================== -->

  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>Rust_AssaultRifleAK</defName>
    <label>assault rifle</label>
    <description>The Assault Rifle is an accurate, powerful, and fully automatic rifle that fires 5.56 rifle rounds.</description>
	<thingCategories Inherit = "False">
        <li>rustweapons</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Weapons/AK</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
	  <recipeUsers>
		<li>FueledSmithy</li>
		<li>ElectricSmithy</li>
	  </recipeUsers>
      <researchPrerequisite>RUST_Handmade_Firearms3</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <weaponTags>
      <li>SimpleGun</li>
    </weaponTags>
    <statBases>
      <WorkToMake>42000</WorkToMake>
      <Mass>4</Mass>
      <AccuracyTouch>0.50</AccuracyTouch>
      <AccuracyShort>0.60</AccuracyShort>
      <AccuracyMedium>0.55</AccuracyMedium>
      <AccuracyLong>0.45</AccuracyLong>
      <RangedWeapon_Cooldown>1.90</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>35</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
	  <WoodLog>10</WoodLog>
    </costList>
    <techLevel>Medieval</techLevel>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_AssaultRifle</defaultProjectile>
        <warmupTime>1.1</warmupTime>
        <range>27</range>
		<burstShotCount>4</burstShotCount>
        <ticksBetweenBurstShots>8</ticksBetweenBurstShots>
        <soundCast>AK</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
