<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <SoundDef>
        <defName>SnapBack</defName>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <onCamera>True</onCamera>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>snapback</clipPath>
                    </li>
                </grains>
                <volumeRange>
                    <min>100</min>
                    <max>100</max>
                </volumeRange>
            </li>
        </subSounds>
    </SoundDef>

    <SoundDef>
        <defName>ApplySnap</defName>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <onCamera>True</onCamera>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>swish</clipPath>
                    </li>
                </grains>
                <volumeRange>
                    <min>100</min>
                    <max>100</max>
                </volumeRange>
            </li>
        </subSounds>
    </SoundDef>

</Defs>
