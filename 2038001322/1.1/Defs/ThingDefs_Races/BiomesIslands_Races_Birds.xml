<ThingDef ParentName="BigBirdThingBase">
    <defName>BiomesIslands_OmegaDodo</defName>
    <label>omegadodo</label>
    <description>The last dodo you'll ever want or need! Having been brought back from extinction by Urbworld scientists, this species of dodo has been genetically engineered to be far larger than the rest of its kind for the purposes of giving meat.</description>
    <statBases>
      <MoveSpeed>2.8</MoveSpeed>
      <MarketValue>300</MarketValue>
      <ComfyTemperatureMin>-8</ComfyTemperatureMin>
	  <ComfyTemperatureMax>50</ComfyTemperatureMax>
	  <LeatherAmount>25</LeatherAmount>
    </statBases>
    <race>
      <herdAnimal>false</herdAnimal>
      <baseBodySize>1.5</baseBodySize>
		<baseHealthScale>1.0</baseHealthScale>
		<baseHungerRate>0.70</baseHungerRate>
		<foodType>VegetarianRoughAnimal</foodType>
      <wildness>0.50</wildness>
      <trainability>Simple</trainability>
      <lifeExpectancy>40</lifeExpectancy>
      <manhunterOnTameFailChance>0.01</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0</manhunterOnDamageChance>
	  <nuzzleMtbHours>10</nuzzleMtbHours>
	  <nameOnTameChance>0.90</nameOnTameChance>
	  <petness>0.10</petness>
    </race>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>BiomesIslands_EggOmegaDodoFertilized</eggFertilizedDef>
        <eggUnfertilizedDef>BiomesIslands_EggOmegaDodoUnfertilized</eggUnfertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>4</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>1</eggProgressUnfertilizedMax>
        <eggCountRange>
		<min>1</min>
		<max>3</max>
		
		</eggCountRange>
      </li>
    </comps>
    <tools>
      <li>
        <label>claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.4</cooldownTime>
        <linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>beak</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>18</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>10</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <tradeTags>
      <li>AnimalUncommon</li>
      <li>AnimalPet</li>
    </tradeTags>
  </ThingDef>

