<Patch>

<!-- desert -->
<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Vanilla Animals Expanded — Desert</li>
	</mods>
	<match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd"> 
			<xpath>/Defs/BiomeDef[defName = "BiomesIslands_Atoll"]/wildAnimals</xpath>
			<value>
				<AEXP_Hyena>0.1</AEXP_Hyena>
				<AEXP_DesertTortoise>1.2</AEXP_DesertTortoise>
			</value>
		</li>
		<li Class="PatchOperationAdd"> 
			<xpath>/Defs/BiomeDef[defName = "BiomesIslands_DesertIsland"]/wildAnimals</xpath>
			<value>
				<AEXP_Hyena>0.6</AEXP_Hyena>
				<AEXP_DesertTortoise>0.9</AEXP_DesertTortoise>
				<AEXP_Lion>0.4</AEXP_Lion>
				<AEXP_GilaMonster>0.4</AEXP_GilaMonster>
