  <ThingDef ParentName="ResourceBase">
    <defName>DrugVents_DrugMix</defName>
    <label>hard drug mix</label>
    <description>A mixture of hard drugs. Explodes when damaged.</description>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <tickerType>Normal</tickerType>
    <stackLimit>50</stackLimit>
    <techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Resources/DrugMix</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>70</MarketValue>
      <Mass>0.2</Mass>
      <Flammability>1.25</Flammability>
      <DeteriorationRate>0.5</DeteriorationRate>
    </statBases>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <effectWorking>Cook</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workAmount>200</workAmount>
      <targetCountAdjustment>1</targetCountAdjustment>
      <workSkill>Crafting</workSkill>
      <researchPrerequisite>DrugVents_HardDrugs</researchPrerequisite>
      <recipeUsers>
        <li>DrugLab</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>1.9</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
        <startWickHitPointsPercent>0.2</startWickHitPointsPercent>
        <wickTicks>40~80</wickTicks>
      </li>
    </comps>
    <costList>
      <Neutroamine>2</Neutroamine>
      <PsychoidLeaves>20</PsychoidLeaves>
      <SmokeleafLeaves>10</SmokeleafLeaves>
    </costList>
  </ThingDef>

