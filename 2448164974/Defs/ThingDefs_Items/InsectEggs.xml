<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Megaspider -->
  
  <ThingDef ParentName="EggFertBase">
    <defName>EggMegaspiderFertilized</defName>
    <label>megaspider egg (fert.)</label>
    <description>A fertilized Megaspider egg. If all goes well, it should hatch into a baby megaspider. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
      <color>(154,125,94)</color>
	  <drawSize>1.1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>40.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>10</hatcherDaystoHatch>
        <hatcherPawn>Megaspider</hatcherPawn>
      </li>
		</comps>
  </ThingDef>

