    <orbital>true</orbital>
    <stockGenerators>

      <!-- Metals -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>
          <min>2000</min>
          <max>6000</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>VBE_Cigars</thingDef>
        <countRange>
          <min>100</min>
          <max>300</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>VBE_Cigarettes</thingDef>
        <countRange>
          <min>200</min>
          <max>600</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef" MayRequire="sarg.alphacrafts">
        <thingDef>AC_PipeTobacco</thingDef>
        <countRange>
          <min>50</min>
          <max>100</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>Milk</thingDef>
        <countRange>
          <min>500</min>
          <max>1000</max>
        </countRange>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Clothing</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
