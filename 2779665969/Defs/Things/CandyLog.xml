<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
 <ThingDef ParentName="ResourceVerbBase">
    <defName>CandyLog</defName>
    <label>red candy logs</label>
    <description>Sturdy candy harvested from a very weird tree. It makes for a decent building material and shouldn't taste too bad either if you break it up.</description>
    <graphicData>
      <texPath>CandyLog</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.5</MarketValue>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>0.4</Mass> 
      <Flammability>0.4</Flammability>
      <DeteriorationRate>0.4</DeteriorationRate>
      <StuffPower_Armor_Sharp>0.4</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.4</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.4</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>4</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>4</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>0.40</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.4</BluntDamageMultiplier>
    </statBases>
    <burnableByRecipe>true</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <minRewardCount>10</minRewardCount>
    <stuffProps>
      <stuffAdjective>candy</stuffAdjective>
      <categories>
        <li>Woody</li>
      </categories>
      <commonality>0.0001</commonality>
      <color>(161,0,0)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactStuff>BulletImpact_Wood</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statFactors>
        <MaxHitPoints>0.7</MaxHitPoints>
        <Beauty>4</Beauty>
        <Flammability>0.6</Flammability>
        <WorkToMake>0.85</WorkToMake>
        <WorkToBuild>0.85</WorkToBuild>
        <DoorOpenSpeed>1.25</DoorOpenSpeed>
      </statFactors>
      <canSuggestUseDefaultStuff>true</canSuggestUseDefaultStuff>
    </stuffProps>
    <isTechHediff>true</isTechHediff>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <weaponClasses>
      <li>Melee</li>
      <li>MeleeBlunt</li>
      <li>Neolithic</li>
    </weaponClasses>
    <tools>
      <li>
        <label>log</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <allowedArchonexusCount>150</allowedArchonexusCount>
  </ThingDef>
  
