<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Patch Synthetic Animals -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Synthetic Animals</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "SyntheticWildBoar"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_Plain</leatherDef>
      </value>
    </match>
</Operation>

