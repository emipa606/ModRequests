<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Change Rimcraft -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>[Ry]Rimcraft Animals</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "WoW_Animal_Mastiff"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_SoftFur</leatherDef>
      </value>
    </match>
</Operation>



</Patch>
