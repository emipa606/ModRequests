<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Change Fox Squirrel -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Erin's Fox Squirrel</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "FoxSquirrel"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_SoftFur</leatherDef>
      </value>
    </match>
</Operation>

</Patch>
