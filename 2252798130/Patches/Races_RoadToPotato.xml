<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Change Road to Potato Dogs -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>RhodesianRidgebacks</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "rtp_BorderCollie"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_Wolf</leatherDef>
      </value>
    </match>
</Operation>



<Operation Class="PatchOperationFindMod">
    <mods>
        <li>BorderCollies</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "rtp_RhodesianRidgeback"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_SoftFur</leatherDef>
      </value>
    </match>
</Operation>


</Patch>
