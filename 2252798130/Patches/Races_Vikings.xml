<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Change Fox Squirrel -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Factions Expanded - Vikings</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "VFEV_Wolfhound"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_Wolf</leatherDef>
      </value>
    </match>
</Operation>



<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Factions Expanded - Vikings</li>
    </mods>
    <match Class="PatchOperationReplace">
       <xpath>/Defs/ThingDef[defName = "VFEV_WoolyCow"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_Heavy</leatherDef>
      </value>
    </match>
</Operation>


</Patch>
