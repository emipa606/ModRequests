  <!-- ===== ATST Body ===== -->
  <BodyDef>
    <defName>SWFactions_ATSTHull</defName>
    <label>ATST</label>
    <corePart>
      <def>ATST_Frame</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>ATST_Neck</def>
          <coverage>0.2</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>ATST_Head</def>
              <coverage>0.80</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>SWFactions_Cockpit</def>
                  <coverage>0.80</coverage>
                  <depth>Inside</depth>
                </li>
              </parts>
            </li>
            <li>
              <def>SWFactions_Thermometer</def>
              <coverage>0.01</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.3</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>left foot</customLabel>
              <coverage>0.2</coverage>
              <groups>
                <li>LeftBlade</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.3</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>right foot</customLabel>
              <coverage>0.2</coverage>
              <groups>
                <li>RightBlade</li>
              </groups>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
