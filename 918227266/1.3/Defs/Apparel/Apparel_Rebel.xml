<Defs>
  <ThingDef ParentName="ArmorPJHelmetMakeableBase">
    <defName>PJ_RebHat</defName>
    <label>Rebel Alliance helmet</label>
    <description>The standard helmet for troops in the Old Republic, now often used by rebels.</description>
    <techLevel>Spacer</techLevel>
    <graphicData>
      <texPath>General/Helmet/RebelHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>16400</WorkToMake>
      <Mass>0.14</Mass>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.36</ArmorRating_Blunt>
      <ArmorRating_Heat>0.9</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <costList>
      <Steel>20</Steel>
    </costList>
    <equippedStatOffsets>
      <ShootingAccuracyPawn>0.02</ShootingAccuracyPawn>
      <AimingDelayFactor>-0.01</AimingDelayFactor>
      <NegotiationAbility>0.01</NegotiationAbility>
      <SocialImpact>0.05</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>General/Helmet/RebelHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <tags>
        <li>PJ_Rebel</li>
      </tags>
    </apparel>
  </ThingDef>
