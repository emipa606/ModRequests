<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <TraderKindDef>
    <defName>PJ_ImpTrader_Visitor</defName>
    <label>trader</label>
    <stockGenerators>
      <!-- === Adding credits for imperial traders === -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>PJ_ImperialCredit</thingDef>
        <countRange>500~1250</countRange>
      </li>
      <li Class="StockGenerator_BuySingleDef"><thingDef>PJ_ImperialCredit</thingDef></li>
      <!-- === Outlander_Visitor_Standard as of 1.4 10/24/2022 === -->

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>50~250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>-2~5</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSurvivalPack</thingDef>
        <countRange>3~6</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>1~6</countRange>
      </li>
      <li Class="StockGenerator_ReinforcedBarrels">
        <countRange>1~4</countRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>
      
      <!-- Drugs -->
      <!-- No drugs from Imperials-->

      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>1</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>
      
      <!-- Weapons -->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>WeaponRanged</tradeTag>
        <countRange>0~1</countRange>
      </li>

      <!-- Apparel -->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Clothing</tradeTag>
        <countRange>0~1</countRange>
      </li>

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->
      
      <!-- Exotic -->
      <!-- none -->
      
      <!-- Slaves -->
      <!-- none -->
      
      <!-- Animals -->
      <!-- none -->
      
      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
    </stockGenerators>
  </TraderKindDef>
</Defs>