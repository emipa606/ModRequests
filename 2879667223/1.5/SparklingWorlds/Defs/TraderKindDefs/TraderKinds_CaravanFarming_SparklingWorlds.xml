<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Caravan_FarmingSW</defName>
    <label>farming trader</label>
    <commonality>1</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>
          <min>8500</min>
          <max>12000</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
		<excludedCategories>
			<li>MeatRaw</li>
			<li>PlantFoodRaw</li>
			<li>EggsFertilized</li>
		</excludedCategories>
		<thingDefCountRange>
          <min>3</min>
          <max>8</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>300</min>
          <max>1500</max>
        </totalPriceRange>
      </li>
	  <li Class="StockGenerator_Category">
        <categoryDef>MeatRaw</categoryDef>
		<thingDefCountRange>
          <min>3</min>
          <max>7</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>400</min>
          <max>1200</max>
        </totalPriceRange>
      </li>
	  <li Class="StockGenerator_Category">
        <categoryDef>PlantFoodRaw</categoryDef>
		<thingDefCountRange>
          <min>2</min>
          <max>5</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>300</min>
          <max>1500</max>
        </totalPriceRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>Hay</thingDef>
        <countRange>
          <min>200</min>
          <max>2000</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_Category">
        <categoryDef>EggsFertilized</categoryDef>
		<thingDefCountRange>
          <min>-1</min>
          <max>4</max>
        </thingDefCountRange>
        <countRange>
          <min>6</min>
          <max>40</max>
        </countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodMeals</categoryDef>
        <thingDefCountRange>
          <min>2</min>
          <max>8</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>300</min>
          <max>1700</max>
        </totalPriceRange>
      </li>
	  <li Class="StockGenerator_Animals">
	  <checkTemperature>true</checkTemperature>
        <tradeTagsSell>
			<li>AnimalFarm</li>
			<li>AnimalPet</li>
        </tradeTagsSell>
		<tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <maxWildness>0.40</maxWildness>
        <kindCountRange>
          <min>2</min>
          <max>6</max>
        </kindCountRange>
        <countRange>
          <min>10</min>
          <max>26</max>
        </countRange>
      </li>
		<li Class="StockGenerator_BuyExpensiveSimple" />
		<li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Kibble</li>
          <li>Chocolate</li>
          <li>Pemmican</li>
        </thingDefs>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>