<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Caravan_Neolithic_BulkGoods</defName>
    <label>bulk goods trader</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>4500~12000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>300~600</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>100~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>250~750</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>10~100</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Dye</thingDef>
        <countRange>30~50</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>2~4</thingDefCountRange>
        <totalPriceRange>200~1200</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories><li>EggsFertilized</li></excludedCategories>
        <thingDefCountRange>4~8</thingDefCountRange>
        <totalPriceRange>400~600</totalPriceRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
        </thingDefs>
      </li>
      
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
      </li>
      
      <!-- Techprints -->
      <!-- none -->
      
      <!-- Weapons -->
      <!-- none -->
      
      <!-- Apparel -->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>BasicClothing</tradeTag>
        <countRange>7~15</countRange>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
      </li>
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Clothing</tradeTag>
        <countRange>3~5</countRange>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
      </li>

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <!-- none -->
      
      <!-- Slaves -->
      <!-- none -->
      
      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>2~8</kindCountRange>
        <countRange>3~10</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

