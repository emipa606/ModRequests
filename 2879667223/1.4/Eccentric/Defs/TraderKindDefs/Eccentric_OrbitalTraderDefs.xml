<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Eccentric_Orbital_MobileRefinery</defName>
    <label>mobile refinery</label>
    <orbital>true</orbital>
    <commonality>0.5</commonality>
    <stockGenerators>

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>60000~100000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>15~130</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentSpacer</thingDef>
        <countRange>1~12</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>500~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Plasteel</thingDef>
        <countRange>100~800</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Synthread</thingDef>
        <countRange>100~800</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>20~130</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Neutroamine</thingDef>
        <countRange>100~500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Chemfuel</thingDef>
        <countRange>200~1600</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealNutrientPaste</thingDef>
        <countRange>15~50</countRange>
      </li>

      <!-- Fuels -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Deuterium</thingDef>
        <countRange>600~2400</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Tritium</thingDef>
        <countRange>200~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Helium</thingDef>
        <countRange>200~1200</countRange>
      </li>

      <!-- Reactor Components -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_ReactorHullPlate</thingDef>
        <countRange>0~15</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_TFGM</thingDef>
        <countRange>0~3</countRange>
      </li>

      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <thingDefCountRange>2~4</thingDefCountRange>
        <totalPriceRange>1000~3000</totalPriceRange>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>

    </stockGenerators>
  </TraderKindDef>

  <!--======================================================================================-->

  <TraderKindDef>
    <defName>Eccentric_Orbital_FuelTanker</defName>
    <label>fuel tanker</label>
    <orbital>true</orbital>
    <stockGenerators>

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>60000~100000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>15~130</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Neutroamine</thingDef>
        <countRange>100~400</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Chemfuel</thingDef>
        <countRange>400~1200</countRange>
        <price>Cheap</price>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealNutrientPaste</thingDef>
        <countRange>15~50</countRange>
      </li>

      <!-- Fuels -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Deuterium</thingDef>
        <countRange>1600~3600</countRange>
        <price>Cheap</price>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Tritium</thingDef>
        <countRange>400~2600</countRange>
        <price>Cheap</price>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_Helium</thingDef>
        <countRange>400~2600</countRange>
        <price>Cheap</price>
      </li>

      <!-- Reactor Components -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_ReactorHullPlate</thingDef>
        <countRange>5~30</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Eccentric_TFGM</thingDef>
        <countRange>1~3</countRange>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>

    </stockGenerators>
  </TraderKindDef>

  <!--======================================================================================-->

</Defs>
