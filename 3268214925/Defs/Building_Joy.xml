<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef ParentName="FurnitureWithQualityBase"> 
    <defName>YF_MahjongTable_A</defName>
    <label>mahjong table A</label>
    <description>A specialized table designed for playing mahjong.</description>
    <graphicData>
      <texPath>Things/Joy/YF_MahjongTable_A/YF_MahjongTable_A</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1.4,1.4)</drawSize>
            <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
        <offset>(0.0, 0.0, -0.1)</offset>
      </shadowData>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
        <thingCategories>
      <li>BuildingsJoy</li>
    </thingCategories>
    <uiIconScale>0.9</uiIconScale> 
    <altitudeLayer>Building</altitudeLayer>
    <rotatable>true</rotatable> 
    <fillPercent>0.40</fillPercent> 
    <minTechLevelToBuild>Medieval</minTechLevelToBuild> 
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
      <paintable>true</paintable>
    </building>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints> 
      <WorkToBuild>8000</WorkToBuild> 
      <Mass>10</Mass> 
      <Flammability>1.0</Flammability> 
      <Beauty>4</Beauty>
      <JoyGainFactor>1</JoyGainFactor>
    </statBases>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>70</costStuffCount>
    <pathCost>30</pathCost> 
    <passability>PassThroughOnly</passability>
    <defaultPlacingRot>South</defaultPlacingRot>

        <tradeTags>
      <li>Television</li>
    </tradeTags>

    <designationCategory>Joy</designationCategory>
    <constructionSkillPrerequisite>2</constructionSkillPrerequisite> 
  </ThingDef>



  <ThingDef ParentName="FurnitureWithQualityBase"> 
    <defName>YF_MahjongTable_AA</defName>
    <label>automatic mahjong table A</label>
    <description>An innovative mahjong table equipped with automated features to streamline gameplay.</description>
    <graphicData>
      <texPath>Things/Joy/YF_MahjongTable_AA/YF_MahjongTable_AA</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1.5,1.5)</drawSize>
            <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
        <offset>(0.0, 0.0, -0.1)</offset>
      </shadowData>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
        <thingCategories>
      <li>BuildingsJoy</li>
    </thingCategories>
    <uiIconScale>0.95</uiIconScale> 
    <altitudeLayer>Building</altitudeLayer>
    <rotatable>true</rotatable> 
    <fillPercent>0.40</fillPercent> 
    <minTechLevelToBuild>Medieval</minTechLevelToBuild> 
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
      <paintable>true</paintable>
    </building>
    <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints> 
      <WorkToBuild>10000</WorkToBuild> 
      <Mass>14</Mass> 
      <Flammability>1.0</Flammability> 
      <Beauty>4</Beauty>
      <JoyGainFactor>1.4</JoyGainFactor>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
    <costList>
    <Steel>80</Steel>
    <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <pathCost>42</pathCost>
    <passability>PassThroughOnly</passability>
    <defaultPlacingRot>South</defaultPlacingRot>

<comps>
      <li Class="CompProperties_Power"> 
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>120</basePowerConsumption>
      </li>
      <li Class="CompProperties_Breakdownable" /> 
      <li Class="CompProperties_Flickable" />  

    </comps>
        <tradeTags>
      <li>Television</li>
    </tradeTags>

    <designationCategory>Joy</designationCategory>

    <constructionSkillPrerequisite>7</constructionSkillPrerequisite> 
  </ThingDef>





