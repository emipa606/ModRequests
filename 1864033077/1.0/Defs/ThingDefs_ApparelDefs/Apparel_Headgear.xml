 <!--========================================= Berets =============================================-->
 <!-- Patch -->
  <ThingDef ParentName="RHHatMakeableBase_British">
    <defName>RNApparel_Beret_BritishArmy</defName>
    <label>beret (British Military)</label>
    <description>A British Military beret that has a cap badge which identifies what regiment the wearer belongs to.</description>
    <graphicData>
      <texPath>Things/Apparel/Display/Hats/Beret_BritishArmy</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
	  <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>1200</WorkToMake>
      <Mass>0.07</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.12</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.40</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
	<equippedStatOffsets>
	  <PsychicSensitivity>-0.10</PsychicSensitivity>
	  <ShootingAccuracyPawn>0.02</ShootingAccuracyPawn>
	  <SocialImpact>0.05</SocialImpact>
	  <TradePriceImprovement>0.02</TradePriceImprovement>
	  <NegotiationAbility>0.02</NegotiationAbility>
    </equippedStatOffsets>
		<!--<generateCommonality>0.5</generateCommonality>-->
    <apparel>
	<!--<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>-->
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Hats/Beret_BritishArmy</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>RH_Hat_BritishMilitary</li>
		<li>RH_Hat_BritishArmy</li>
		<li>PassiveCamo_Colour_Set</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3,1)</min>
          <max>(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  
