<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Expanded - Ideology Patches</li>
		</mods>
		<match Class="PatchOperationRemove">
			<xpath>Defs/ThingDef[
				defName = "VFEM_DarkCandle" or
				defName = "VFEM_WallTorchDarkLamp"
			]/comps/li[@Class="CompProperties_Refuelable"]
			</xpath>
		</match>
	</Operation>
</Patch>