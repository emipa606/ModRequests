<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Strawberries</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="TRAHS_Elephant_Strawberry_Race"]/race/leatherDef</xpath>
                    <value>
                        <leatherDef>CNS_Strawberries_RawStrawberries</leatherDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="TRAHS_Elephant_Strawberry_Race"]/race/specificMeatDef</xpath>
                    <value>
                        <specificMeatDef>CNS_Strawberries_RawStrawberries</specificMeatDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="TRAHS_Muffalo_Strawberry_Race"]/race/leatherDef</xpath>
                    <value>
                        <leatherDef>CNS_Strawberries_RawStrawberries</leatherDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="TRAHS_Muffalo_Strawberry_Race"]/race/specificMeatDef</xpath>
                    <value>
                        <specificMeatDef>CNS_Strawberries_RawStrawberries</specificMeatDef>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>