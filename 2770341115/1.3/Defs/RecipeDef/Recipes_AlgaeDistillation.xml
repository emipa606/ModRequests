<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef MayRequire="Argon.VMEu">
    <defName>AC_MakeDarkOilFromAlgae</defName>
    <label>make dark oil from algae</label>
    <description>A long process that employs cooking and distilling algae through a slow evaporation system to obtain small amounts of dark oil.</description>
    <jobString>Making dark oil.</jobString>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workAmount>700</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <recipeUsers>
      <li>BiofuelRefinery</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AC_RawAlgae</li>
          </thingDefs>
        </filter>
        <count>45</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AC_RawAlgae</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <VMEu_DarkOil>10</VMEu_DarkOil>
    </products>
  </RecipeDef>
</Defs>