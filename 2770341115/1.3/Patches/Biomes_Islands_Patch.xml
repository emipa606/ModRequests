<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Biomes! Islands</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="BiomesIslands_Kelp"]/plant</xpath>
                    <value>
                        <harvestWork>650</harvestWork>
			            <harvestYield>4</harvestYield>
				        <harvestTag>Standard</harvestTag>
			            <harvestedThingDef>AC_RawAlgae</harvestedThingDef>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="BiomesIslands_Sargassum"]/plant</xpath>
                    <value>
                        <harvestWork>750</harvestWork>
			            <harvestYield>6</harvestYield>
				        <harvestTag>Standard</harvestTag>
			            <harvestedThingDef>AC_RawAlgae</harvestedThingDef>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="BiomesIslands_Seaweed"]/plant</xpath>
                    <value>
                        <harvestWork>600</harvestWork>
			            <harvestYield>3</harvestYield>
				        <harvestTag>Standard</harvestTag>
			            <harvestedThingDef>AC_RawAlgae</harvestedThingDef>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>