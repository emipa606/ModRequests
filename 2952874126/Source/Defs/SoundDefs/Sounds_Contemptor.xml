<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--==================== Lascannon =========================-->
  <SoundDef>
    <defName>LascannonSound</defName>
    <context>MapOnly</context>
    <eventNames />
    <maxSimultaneous>10</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>lascannon</clipPath>
          </li>
        </grains>
        <volumeRange>
          <min>65.0</min>
          <max>120.0</max>
        </volumeRange>
        <pitchRange>
          <min>0.8</min>
          <max>1.0</max>
        </pitchRange>
        <sustainLoop>false</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

