<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/HediffDef[defName="WoundInfection"]/comps</xpath>
    <value>
		<comps>
		  <li Class="HediffCompProperties_TendDuration">
			<baseTendDurationHours>12</baseTendDurationHours>
			<severityPerDayTended>-0.53</severityPerDayTended>
		  </li>
		  <li Class="HediffCompProperties_Immunizable">
			<severityPerDayNotImmune>0.72</severityPerDayNotImmune>
			<immunityPerDaySick>0.65</immunityPerDaySick>
			<severityPerDayImmune>-0.70</severityPerDayImmune>
			<immunityPerDayNotSick>-0.36</immunityPerDayNotSick>
		  </li>
		  <li Class="HediffCompProperties_Discoverable">
			<sendLetterWhenDiscovered>true</sendLetterWhenDiscovered>
		  </li>
		</comps>
    </value>
  </Operation>
</Patch>