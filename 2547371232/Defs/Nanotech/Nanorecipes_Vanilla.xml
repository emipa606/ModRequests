<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--  BASE -->

    <RecipeDef Name="NanobotRecipeBase" Abstract="True">
		<workAmount>10</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<description>Make something out of nanobots.</description>
		<jobString>Working with nanotechnology.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
            <li>Nanofabricator</li>
        </recipeUsers>
    </RecipeDef>

