<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Chemfuel Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="AP_WoodPipe"]/designationCategory</xpath>
                    <value>
                        <designationCategory>VCHE_PipeNetworks</designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/PipeSystem.PipeNetDef[defName="AP_WoodNetwork"]/designator/designationCategoryDef</xpath>
                    <value>
                        <designationCategoryDef>VCHE_PipeNetworks</designationCategoryDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="AP_WoodTank"]/designationCategory</xpath>
                    <value>
                        <designationCategory>VCHE_PipeNetworks</designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="AP_WoodDrain"]/designationCategory</xpath>
                    <value>
                        <designationCategory>VCHE_PipeNetworks</designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="AP_WoodPulper"]/designationCategory</xpath>
                    <value>
                        <designationCategory>VCHE_PipeNetworks</designationCategory>
                    </value>
                </li>

                <li Class="PatchOperationRemove">
                    <xpath>Defs/DesignationCategoryDef[defName="AP_WoodNetwork_Designation"]</xpath>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>