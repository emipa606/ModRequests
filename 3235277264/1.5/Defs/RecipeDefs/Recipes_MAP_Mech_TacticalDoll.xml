<Defs>
  <RecipeDef ParentName="HeavyMechanoidRecipe">
    <defName>MAP_Mech_AssaultTacticalDoll</defName>
    <label>gestate Assault Tactical Doll</label>
    <description>Gestate a Assault Tactical Doll mechanoid.</description>
    <gestationCycles>2</gestationCycles>
    <researchPrerequisite>AssaultTacticalDollResearch</researchPrerequisite>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreHigh</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <MAP_Mech_AssaultTacticalDoll>1</MAP_Mech_AssaultTacticalDoll>
    </products>
    <descriptionHyperlinks>
      <ThingDef>MAP_Mech_AssaultTacticalDoll</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>
