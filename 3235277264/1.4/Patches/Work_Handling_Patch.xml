<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!--add workgive-->
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/WorkGiverDef[defName="Slaughter"]/canBeDoneByMechs</xpath>
