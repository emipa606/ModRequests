<?xml version="1.0" encoding="utf-8" ?>

<Patch>
  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Brewing Expanded</li>
		</mods>
		
		<match Class="PatchOperationSequence">	
			<operations>
		
			<li Class="PatchOperationReplace">			
				<xpath>Defs/ThingDef[defName="JWL_Zolpispyrine"]/costList</xpath>
				<value>
					<costList>
						<Neutroamine>5</Neutroamine>
						<MedicineIndustrial>2</MedicineIndustrial>
						<VBE_RawCoffee>10</VBE_RawCoffee>
						<Silver>50</Silver>
					</costList>
				</value>
			</li>
		
			<li Class="PatchOperationReplace">			
				<xpath>Defs/ThingDef[defName="JWL_Toletram"]/costList</xpath>
				<value>
					<costList>
						<Neutroamine>5</Neutroamine>
						<MedicineIndustrial>2</MedicineIndustrial>
						<VBE_RawTobacco>10</VBE_RawTobacco>
						<Silver>50</Silver>
					</costList>
				</value>
			</li>
		
			<li Class="PatchOperationReplace">			
				<xpath>Defs/ThingDef[defName="JWL_Flexinuma"]/costList</xpath>
				<value>
					<costList>
						<Neutroamine>5</Neutroamine>
						<MedicineIndustrial>2</MedicineIndustrial>
						<VBE_RawTea>10</VBE_RawTea>
						<Silver>50</Silver>
					</costList>
				</value>
			</li>
			</operations>
		
		</match>
		
	</Operation>
</Patch>