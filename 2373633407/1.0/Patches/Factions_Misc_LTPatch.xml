<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <!-- ========== Patch - Adding Trading Caravans ========== -->
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/caravanTraderKinds</xpath>
        <value>
          <li>Caravan_Outlander_Farming</li>
          <li>Caravan_Outlander_Mining</li>
          <li>Caravan_Outlander_Textiles</li>
          <li>Caravan_Outlander_Medical</li>
          <li>Caravan_Outlander_Drugs</li>
          <li>Caravan_Outlander_Archeological</li>
        </value>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/FactionDef[@Name="TribeBase"]/caravanTraderKinds</xpath>
        <value>
          <li>Caravan_Neolithic_Farming</li>
          <li>Caravan_Neolithic_Mining</li>
          <li>Caravan_Neolithic_Textiles</li>
          <li>Caravan_Neolithic_Medical</li>
          <li>Caravan_Neolithic_Art</li>
          <li>Caravan_Neolithic_Odd</li>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>
