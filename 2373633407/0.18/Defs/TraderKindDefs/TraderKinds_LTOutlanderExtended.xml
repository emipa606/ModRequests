<Defs>
 
    <!-- ========== Trader - Outlander ========== -->


<TraderKindDef>
    <defName>Caravan_Outlander_Farming</defName>
    <label>farming goods trader</label>
    <commonality>2</commonality>
    <stockGenerators>
        <li Class="StockGenerator_SingleDef">
            <thingDef>Silver</thingDef>
            <countRange>
                <min>750</min>
                <max>1200</max>
            </countRange>
        </li>
        <li Class="StockGenerator_SingleDef">
            <thingDef>MealSurvivalPack</thingDef>
            <countRange>
                <min>10</min>
                <max>100</max>
            </countRange>
        </li>
        <li Class="StockGenerator_SingleDef">
            <thingDef>Hay</thingDef>
            <price>Cheap</price>
            <countRange>
                <min>240</min>
                <max>400</max>
            </countRange>
        </li>
        <li Class="StockGenerator_Category">
            <categoryDef>PlantMatter</categoryDef>
            <thingDefCountRange>
                <min>1</min>
                <max>2</max>
            </thingDefCountRange>
            <countRange>
                <min>50</min>
                <max>250</max>
            </countRange>
        </li>
        <li Class="StockGenerator_Category">
            <categoryDef>PlantFoodRaw</categoryDef>
            <price>Cheap</price>
            <thingDefCountRange>
                <min>3</min>
                <max>5</max>
            </thingDefCountRange>
            <countRange>
                <min>150</min>
                <max>500</max>
            </countRange>
        </li>
        <li Class="StockGenerator_Category">
            <categoryDef>MeatRaw</categoryDef>
            <excludedThingDefs>
                <li>Human_Meat</li>
            </excludedThingDefs>
            <thingDefCountRange>
                <min>0</min>
                <max>2</max>
            </thingDefCountRange>
            <countRange>
                <min>100</min>
                <max>200</max>
            </countRange>
        </li>
            <li Class="StockGenerator_Category">
                <categoryDef>AnimalProductRaw</categoryDef>
                <excludedCategories><li>EggsFertilized</li></excludedCategories>
                <excludedThingDefs><li>InsectJelly</li></excludedThingDefs>
                <thingDefCountRange>
                    <min>2</min>
                    <max>4</max>
                </thingDefCountRange>
            <countRange>
                <min>300</min>
                <max>800</max>
            </countRange>
        </li>
        <li Class="StockGenerator_Animals">
            <maxWildness>0.70</maxWildness>
                <tradeTags>
                    <li>StandardAnimal</li>
                </tradeTags>
                <kindCountRange>
                    <min>5</min>
                    <max>6</max>
                </kindCountRange>
                <countRange>
                    <min>10</min>
                    <max>15</max>
                </countRange>
            </li>
            <li Class="StockGenerator_BuyArt" />
            <li Class="StockGenerator_BuyWeirdOrganic" />
            <li Class="StockGenerator_MultiDef">
                <thingDefs>
                    <li>Kibble</li>
                    <li>Chocolate</li>
                    <li>Pemmican</li>
                </thingDefs>
            </li>
        <li Class="StockGenerator_Category">
            <categoryDef>ResourcesRaw</categoryDef>
            <excludedCategories><li>StoneBlocks</li></excludedCategories>
        </li>
            <li Class="StockGenerator_Category">
            <categoryDef>FoodRaw</categoryDef>
        </li>
        <li Class="StockGenerator_Category">
            <categoryDef>FoodMeals</categoryDef>
        </li>
        <li Class="StockGenerator_Animals">
            <tradeTags>
                <li>StandardAnimal</li>
                <li>BadassAnimal</li>
            </tradeTags>
        </li>
    </stockGenerators>
</TraderKindDef>




