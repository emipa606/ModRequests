		<description>Replicates 1 Mech serum neurotrainer into 2.</description>
		<jobString>Replicating mech serum neurotrainer.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MechSerumNeurotrainer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MechSerumNeurotrainer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<MechSerumNeurotrainer>2</MechSerumNeurotrainer>
		</products>
	</RecipeDef>
	
	<Def Name="10_RF_RR_DefInsertion" Abstract="True" />
	
