	<RecipeDef ParentName="MedicinalReplicatorBase">
		<defName>11_RF_MRShootingNeurotrainer</defName>
		<label>Replicate Neurotrainer (Shooting)</label>
		<description>Replicates 1 shooting neurotrainer into 2.</description>
		<jobString>Replicating shooting neurotrainer.</jobString>
		<ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>Neurotrainer_Shooting</li>
                </thingDefs>
              </filter>
                <count>1</count>
            </li>
		</ingredients>
		<fixedIngredientFilter>
            <thingDefs>
              <li>Neurotrainer_Shooting</li>
            </thingDefs>
		</fixedIngredientFilter>
		<products>
			<Neurotrainer_Shooting>2</Neurotrainer_Shooting>
		</products>
	</RecipeDef>
