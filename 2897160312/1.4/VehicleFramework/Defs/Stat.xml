<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<Vehicles.VehicleStatDef>
		<defName>BDsVehiclePowerRecharge</defName>
		<label>reactor power output</label>
		<description>The amount of power vehicle's reactors produce per tick.</description>
		<category>VehicleBasics</category>
		<defaultBaseValue>0</defaultBaseValue>
		<minValue>0</minValue>
		<toStringStyle>FloatOne</toStringStyle>
		<formatString>{0} WD/tick</formatString>
		<operationType>Average</operationType>
		<showIfUndefined>false</showIfUndefined>
		<displayPriorityInCategory>6000</displayPriorityInCategory>

		<modSettingsInfo>
			<settingsType>SliderFloat</settingsType>
			<roundDecimalPlaces>2</roundDecimalPlaces>
			<increment>0.25</increment>
		</modSettingsInfo>
	</Vehicles.VehicleStatDef>

	<Vehicles.VehicleStatDef>
		<defName>BDsVehiclePowerCapacitor</defName>
		<label>capacitor capacity</label>
		<description>How much power the vehicular capacitor can store</description>
		<category>VehicleBasics</category>
		<defaultBaseValue>0</defaultBaseValue>
		<minValue>0</minValue>
		<toStringStyle>FloatOne</toStringStyle>
		<formatString>{0} WD</formatString>
		<operationType>Average</operationType>
		<showIfUndefined>false</showIfUndefined>
		<displayPriorityInCategory>6000</displayPriorityInCategory>

		<modSettingsInfo>
			<settingsType>IntegerBox</settingsType>
		</modSettingsInfo>
	</Vehicles.VehicleStatDef>

	<Vehicles.VehicleStatDef>
		<defName>BDsVehicleGeneratorFuelUseage</defName>
		<label>generator efficiency</label>
		<description>How much power the vehicular generator generate per unit of fuel</description>
		<category>VehicleBasics</category>
		<defaultBaseValue>0</defaultBaseValue>
		<minValue>0</minValue>
		<toStringStyle>FloatMaxThree</toStringStyle>
		<formatString>{0} WD/unit</formatString>
		<operationType>Average</operationType>
		<showIfUndefined>false</showIfUndefined>
		<displayPriorityInCategory>6000</displayPriorityInCategory>

		<modSettingsInfo>
			<settingsType>SliderFloat</settingsType>
			<roundDecimalPlaces>2</roundDecimalPlaces>
			<increment>0.001</increment>
		</modSettingsInfo>
	</Vehicles.VehicleStatDef>
</Defs>
