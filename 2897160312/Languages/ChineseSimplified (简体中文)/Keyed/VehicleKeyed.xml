  <AblativeShieldRecharge>充能速率: {0} HP/秒</AblativeShieldRecharge>
	<!-- EN: Recharge delay after taking damage: {0} second -->
  <AblativeShieldRechargeDelay>受击后充能间隔: {0} 秒</AblativeShieldRechargeDelay>
	<!-- EN: Initial charge when regen from breaking: {0} -->
