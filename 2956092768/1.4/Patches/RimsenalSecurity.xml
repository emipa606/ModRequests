<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	<mods>
		<li>Rimsenal - Security Pack</li>
	</mods>
	<match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Turret_ShardSentry"]/comps</xpath>
				<value>
					<li Class="GTM.Comp_GTM_Base"/>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Turret_WaveEmitter"]/comps</xpath>
				<value>
					<li Class="GTM.Comp_GTM_Base"/>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Turret_SuvTurret"]/comps</xpath>
				<value>
					<li Class="GTM.Comp_GTM_Base"/>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="RocketTurret"]/comps</xpath>
				<value>
					<li Class="GTM.Comp_GTM_Base"/>
				</value>
			</li>
			
		</operations>
	</match>
  </Operation>

</Patch>