<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef Name="UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <selectable>true</selectable>
    <tradeability>None</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <!-- For CompForbiddable -->
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <DeteriorationRate>2.0</DeteriorationRate>
      <Beauty>-4</Beauty>
      <Mass>1</Mass>
      <!-- ingredients are added to this value -->
    </statBases>
    <comps>
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  <ThingDef ParentName="UnfinishedBase">
    <defName>PJ_UnfinishedCrystal</defName>
    <label>unfinished crystal</label>
    <graphicData>
      <texPath>Items/CrystalXX</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  <ThingDef ParentName="UnfinishedBase">
    <defName>PJ_UnfinishedRedC</defName>
    <label>unfinished infusion</label>
    <graphicData>
      <texPath>Items/CrystalXX</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories />
  </ThingDef>
</Defs>