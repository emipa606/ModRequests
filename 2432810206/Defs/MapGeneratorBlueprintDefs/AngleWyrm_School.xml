<?xml version="1.0" encoding="utf-8" ?>
<!-- 
    School
    Mod: Leadership https://steamcommunity.com/sharedfiles/filedetails/?id=1565942758
    Note: Unique status of teacher's podium doesn't appear to cause problems when duplicated
-->
<Defs>
    <!-- 
        Old school 
    -->
	<MapGenerator.MapGeneratorBlueprintDef Name="AngleWyrm_School">
		<defName>AngleWyrm_School</defName>
		<chance>20</chance>
		<size>9,11</size>
		<canHaveHoles>true</canHaveHoles>
		<createTrigger>true</createTrigger>
    
        <!-- Walls, Doors and Furniture -->
		<buildingData>
            wwwwwwwww
            w.c...g.w
            w...p...w
            wss...ssw
            w.ts.s.tw
            wss...ssw
            w.......w
            wss...ssw
            w.ts.s.tw
            wss...ssw
            wwwwdwwww                        
		</buildingData>
        <buildingLegend>
            <li>
                <key>w</key>
                <value>Wall</value>
            </li>
            <li>
                <key>d</key>
                <value>Door</value>
            </li>
            <li>
                <key>t</key>
                <value>Table1x2c</value> <!-- 1x2 table -->
            </li>
            <li>
                <key>s</key>
                <value>Stool</value>
            </li>
            <li>
                <key>c</key>        <!-- Leadership teacher's chalkboard -->
                <value>ChalkboardCL</value>
            </li>
            <li>
                <key>g</key>        <!-- Leadership teacher's globe -->
                <value>GlobeCL</value>
            </li>
			<li>
				<key>p</key>         <!-- Leadership teacher's podium -->
				<value>TeachingSpot</value>
			</li>
        </buildingLegend>
		<rotationLegend>
			<li>
				<key>t</key>     <!-- 1x2 table -->
				<value>3</value> <!-- 0-North, 1-East, 2-South, 3-West -->
			</li>
            <li>
                <key>p</key>   <!-- Teacher's podium -->
                <value>2</value> <!-- 0-North, 1-East, 2-South, 3-West -->
            </li>
			<li>
				<key>c</key>     <!-- Teacher's chalkboard -->
				<value>0</value> <!-- 0-North, 1-East, 2-South, 3-West -->
			</li>
			<li>
				<key>g</key>     <!-- Teacher's globe -->
				<value>0</value> <!-- 0-North, 1-East, 2-South, 3-West -->
			</li>
		</rotationLegend>

        <!-- Flooring -->
		<floorData>
            wwwwwwwww
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wfffffffw
            wwwwfwwww 
		</floorData>
		<floorLegend>
			<li>
				<key>f</key>
				<value>WoodPlankFloor</value>
			</li>
		</floorLegend>
    </MapGenerator.MapGeneratorBlueprintDef>
</Defs>