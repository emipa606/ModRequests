<?xml version="1.0" encoding="utf-8"?>

<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
	<xpath>/Defs/ThingDef[defName="AP_TumbleWeed_Race"]</xpath>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<success>Always</success>
			<xpath>/Defs/ThingDef[defName="AP_TumbleWeed_Race"]/race/meatLabel</xpath>
			<value>
				<!-- Meat and leather is at zero, this is just for cool flavor -->
				<specificMeatDef>WoodLog</specificMeatDef>
				<leatherDef>WoodLog</leatherDef>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
	<xpath>/Defs/ThingDef[defName="AP_TumbleRoot_Race"]</xpath>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<success>Always</success>
			<xpath>/Defs/ThingDef[defName="AP_TumbleRoot_Race"]/race/meatLabel</xpath>
			<value>
				<!-- Meat and leather is at zero, this is just for cool flavor -->
				<specificMeatDef>MedicineHerbal</specificMeatDef>
				<leatherDef>WoodLog</leatherDef>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
	
</Patch>