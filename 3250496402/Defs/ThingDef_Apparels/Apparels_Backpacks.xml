<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ApparelBase">
    <defName>Apparel_EnforcerBackPackMelee</defName>
    <label>melee enforcer backpack</label>
    <description>A state of the art, multi purpose backpack worn by the enforcers. The backpack has a carry capacity of 350, along with a built-in shield bubble and a built-in jump pack suitable for close quarter combat.</description>
    <techLevel>Spacer</techLevel>
	<thingClass>Kraltech_Industries.Apparel_ShieldBubble</thingClass>
    <comps>
      <li Class="CompProperties_CauseHediff_Apparel">
        <hediff>MeleeBackpack</hediff>
        <part>Torso</part>
      </li>
    </comps>
	<tickerType>Normal</tickerType>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	  <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
	  <researchPrerequisite>EnforcerProtectiveGears</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
	  <skillRequirements>
        <Crafting>15</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>KraltechFabBench</li>
      </recipeUsers>
    </recipeMaker>
    <costList>
      <SuperSteel>250</SuperSteel>
      <ComponentSpacer>20</ComponentSpacer>
      <MechFussionCore>150</MechFussionCore>
    </costList>
    <graphicData>
      <texPath>Things/ArmorsAndClothes/Kral's_Protectives/KraltechFactionApparels/EnforcerUtility/Packs/EnforcerMultiPack</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>14000</WorkToMake>
	  <MarketValue>7500</MarketValue>
      <Mass>3</Mass>
      <Flammability>0</Flammability>
      <EquipDelay>2</EquipDelay>
	  <ArmorRating_Sharp>2.55</ArmorRating_Sharp>
      <ArmorRating_Blunt>2.35</ArmorRating_Blunt>
      <ArmorRating_Heat>4.7</ArmorRating_Heat>
	  <EnergyShieldRechargeRate>1.7</EnergyShieldRechargeRate>
      <EnergyShieldEnergyMax>20.00</EnergyShieldEnergyMax>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
	<equippedStatOffsets>	
    </equippedStatOffsets>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <scoreOffset>4</scoreOffset>
      <wornGraphicPath>Things/ArmorsAndClothes/Kral's_Protectives/KraltechFactionApparels/EnforcerUtility/Packs/EnforcerMultiPack</wornGraphicPath>
      <wornGraphicData>
        <renderUtilityAsPack>true</renderUtilityAsPack>
        <east>
          <offset>(-0.35,-0.05)</offset>
          <thin><offset>(0.05,0)</offset></thin>
          <hulk><offset>(-0.1,0)</offset></hulk>
          <fat><offset>(-0.15,0)</offset></fat>
        </east>
        <west>
          <offset>(0.35,-0.05)</offset>
          <thin><offset>(-0.05,0)</offset></thin>
          <hulk><offset>(0.1,0)</offset></hulk>
          <fat><offset>(0.15,0)</offset></fat>
        </west>
        <male>  <scale>(0.75,0.75)</scale></male>
        <female><scale>(0.75,0.75)</scale></female>
        <thin>  <scale>(0.7,0.7)</scale></thin>
        <hulk>  <scale>(0.9,0.9)</scale></hulk>
        <fat>   <scale>(0.9,0.9)</scale></fat>

      </wornGraphicData>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <wearPerDay>0</wearPerDay>
      <bodyPartGroups>
        <li>Waist</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>EnforcerBackPack</li>
      </tags>
    </apparel>
    <tradeTags>
    </tradeTags>
    <allowedArchonexusCount>1</allowedArchonexusCount>
  </ThingDef>
  
