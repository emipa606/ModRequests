<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef ParentName="ResourceBase" Name="Strongbox" Abstract="True">
    <thingClass>ThingWithComps</thingClass>
    <stackLimit>1</stackLimit>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>8.0</Mass>
      <Flammability>0</Flammability>
    </statBases>
	<tradeTags>
      <li>StrongBoxz</li>
    </tradeTags>
  </ThingDef>

  <ThingDef ParentName="Strongbox">
    <defName>Strongbox_Basic</defName>
    <label>strong box beginners pack</label>
    <description>A premium, ultra compact, secured container which contains one of a few basic gears needed to survive the rim.</description>
    <statBases>
      <MarketValue>7500</MarketValue>
    </statBases>
    <graphicData>
      <texPath>Things/StrongBox/StrongBox1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<thingCategories>
      <li>ItemsMisc</li>
    </thingCategories>
	<killedLeavings>
      <!--<Gun_HomelanderRevolver>1</Gun_HomelanderRevolver>-->
	  <Apparel_SleevelessTrenchCoat>1</Apparel_SleevelessTrenchCoat>
	  <HeadGear_MinersGasmask>1</HeadGear_MinersGasmask>
	  <FootWear_StriderShoesYellow>1</FootWear_StriderShoesYellow>
	  <Apparel_TravelerJumpsuit>1</Apparel_TravelerJumpsuit>
      <Handwear_TacticalGloves>1</Handwear_TacticalGloves>
    </killedLeavings>
    <comps>
      <li Class="CompProperties_Usable">
        <useJob>UseItem</useJob>
        <useLabel>Open Strongbox</useLabel>
		<useDuration>650</useDuration>
      </li>
      <li Class="CompProperties_UseEffectDestroySelf">
        <delayTicks>0</delayTicks>
        <spawnKilledLeavings>true</spawnKilledLeavings>
      </li>
    </comps>
	<tickerType>Normal</tickerType>
    <filthLeaving>Filth_MachineBits</filthLeaving>
  </ThingDef>
  
