<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantBase">
    <defName>PlantCCPWildflowers</defName>
    <label>wildflowers</label>
    <description>Mixed cultivated wildflowers, very pretty.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>25</Beauty>
	  <Nutrition>0</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Wildflowers</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
	<ingestible>
	  <preferability>NeverForNutrition</preferability>
    </ingestible>
    <plant>
	  <fertilitySensitivity>1</fertilitySensitivity>
	  <growDays>2.5</growDays>
	  <sowTags>
        <li>Ground</li>
      </sowTags>
      <fertilityMin>0.05</fertilityMin>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>0.4~0.6</visualSizeRange>
      <topWindExposure>0.3</topWindExposure>
    </plant>
  </ThingDef>
  
</Defs>