<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputPath>..\..\1.5\Assemblies</OutputPath>
    <TargetFramework>net48</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <DebugType>None</DebugType>
    <LangVersion>latest</LangVersion>
    <PackageId>$(AssemblyName)</PackageId>
    <Product>$(AssemblyName)</Product>
  </PropertyGroup>
	<PropertyGroup>
		<PublicizeAll>true</PublicizeAll>
		<BaseOutputPath>bin\</BaseOutputPath>
		<Company>UpsilonSigmaEta</Company>
	</PropertyGroup>
	<ItemGroup>
		<DoNotPublicize Include="Assembly-CSharp:Verse.AI.ThinkNode_Conditional.Satisfied"></DoNotPublicize>
	</ItemGroup>
  <ItemGroup>
    <PackageReference Include="Krafs.Publicizer" Version="2.2.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Krafs.Rimworld.Ref" Version="1.5.4033-beta" />
    <PackageReference Include="Lib.Harmony" Version="2.3.1.1" />
  </ItemGroup>
</Project>
