          <thingDefs>
            <li>AMW_MechanoidPlates</li>
          </thingDefs>
        </filter>
        <count>250</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AMW_MechanoidPlates</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Plasteel>250</Plasteel>
	  <Steel>125</Steel>
    </products>
  </RecipeDef>
  
