   <RecipeDef>
    <defName>AMW_MakeAGShell</defName>
    <label>make antigrain shell</label>
    <description>Use undetonated antigrain capsules and nanite packs to produce antigrain warheads.</description>
    <jobString>Working with salvager.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>10000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AMW_MechanitePack</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>AMW_AntigranFragments</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
	    <li>AMW_AntigranFragments</li>
        <li>AMW_MechanitePack</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Shell_AntigrainWarhead>1</Shell_AntigrainWarhead>
    </products>
  </RecipeDef>
  
