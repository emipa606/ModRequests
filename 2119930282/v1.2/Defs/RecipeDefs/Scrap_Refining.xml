<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
    <defName>AMW_SalvageComponents</defName>
    <label>scrap components</label>
    <description>Scrap mechanoid components to make some regular and advanced components.</description>
    <jobString>Working with salvager.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>5000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AMW_MechanoidComponents</li>
          </thingDefs>
        </filter>
        <count>125</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AMW_MechanoidComponents</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentIndustrial>10</ComponentIndustrial>
	  <ComponentSpacer>5</ComponentSpacer>
    </products>
  </RecipeDef>
  
  <RecipeDef>
    <defName>AMW_SalvagePlates</defName>
