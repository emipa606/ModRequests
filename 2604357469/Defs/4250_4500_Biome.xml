  <BiomeDef>
    <defName>4250_4500_Biome</defName>
    <label>4250m - 4500m</label>
    <description>Terrain between 4250m and 4500m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>4250</minElevation>
	  <maxElevation>4500</maxElevation>
