<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 4500m - 4750m ==================== -->
  <BiomeDef>
    <defName>4500_4750_Biome</defName>
    <label>4500m - 4500m</label>
    <description>Terrain between 4500m and 4750m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>4500</minElevation>
	  <maxElevation>4750</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/4500_4750</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
