<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 1000m - 1250m ==================== -->

  <BiomeDef>
    <defName>1000_1250_Biome</defName>
    <label>1000m - 1250m</label>
    <description>Terrain between 1000m and 1250m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>1000</minElevation>
	  <maxElevation>1250</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/1000_1250</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
