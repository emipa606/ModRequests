<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 250m - 500m ==================== -->

  <BiomeDef>
    <defName>250_500_Biome</defName>
    <label>250m - 500m</label>
    <description>Terrain between 250m and 500m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>250</minElevation>
	  <maxElevation>500</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/250_500</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
