<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 3000m - 3250m ==================== -->
  <BiomeDef>
    <defName>3000_3250_Biome</defName>
    <label>3000m - 3250m</label>
    <description>Terrain between 3000m and 3250m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>3000</minElevation>
	  <maxElevation>3250</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/3000_3250</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
