<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 1750m - 2000m ==================== -->

  <BiomeDef>
    <defName>1750_2000_Biome</defName>
    <label>1750m - 2000m</label>
    <description>Terrain between 1750m and 2000m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>1750</minElevation>
	  <maxElevation>2000</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/1750_2000</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
