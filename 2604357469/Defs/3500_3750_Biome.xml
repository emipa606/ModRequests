<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 3500m - 3750m ==================== -->
  <BiomeDef>
    <defName>3500_3750_Biome</defName>
    <label>3500m - 3750m</label>
    <description>Terrain between 3500m and 3750m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>3500</minElevation>
	  <maxElevation>3750</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/3500_3750</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
