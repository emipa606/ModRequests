<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 4750 - 5000m ==================== -->
  <BiomeDef>
    <defName>4750_5000_Biome</defName>
    <label>4750m - 5000m</label>
    <description>Terrain between 4750m and 5000m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>4750</minElevation>
	  <maxElevation>5000</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/4750_5000</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
