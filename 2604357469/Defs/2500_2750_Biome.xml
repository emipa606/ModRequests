<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 2500m - 2750m ==================== -->
  <BiomeDef>
    <defName>2500_2750_Biome</defName>
    <label>2500m - 2750m</label>
    <description>Terrain between 2500m and 2750m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>2500</minElevation>
	  <maxElevation>2750</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/2500_2750</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
