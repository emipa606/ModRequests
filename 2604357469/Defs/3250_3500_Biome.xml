<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 3250m - 3500m ==================== -->
  <BiomeDef>
    <defName>3250_3500_Biome</defName>
    <label>3250m - 3500m</label>
    <description>Terrain between 3250m and 3500m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>3250</minElevation>
	  <maxElevation>3500</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/3250_3500</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
