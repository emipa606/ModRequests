<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 750m - 1000m ==================== -->
<!-- I'm beggining to regret this idea. -->

  <BiomeDef>
    <defName>750_1000_Biome</defName>
    <label>750m - 1000m</label>
    <description>Terrain between 750m and 1000m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>750</minElevation>
	  <maxElevation>1000</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/750_1000</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
