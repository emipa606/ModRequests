<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 2750m - 3000m ==================== -->
  <BiomeDef>
    <defName>2750_3000_Biome</defName>
    <label>2750m - 3000m</label>
    <description>Terrain between 2750m and 3000m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>2750</minElevation>
	  <maxElevation>3000</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/2750_3000</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
