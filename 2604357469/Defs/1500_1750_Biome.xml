<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 1500m - 1750m ==================== -->

  <BiomeDef>
    <defName>1500_1750_Biome</defName>
    <label>1500m - 1750m</label>
    <description>Terrain between 1500m and 1750m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>1500</minElevation>
	  <maxElevation>1750</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/1500_1750</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
