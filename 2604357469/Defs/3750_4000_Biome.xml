<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 3750m - 4000m ==================== -->
  <BiomeDef>
    <defName>3750_4000_Biome</defName>
    <label>3750m - 4000m</label>
    <description>Terrain between 3750m and 4000m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>3750</minElevation>
	  <maxElevation>4000</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/3750_4000</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
