<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 2000m - 2250m ==================== -->

  <BiomeDef>
    <defName>2000_2250_Biome</defName>
    <label>2000m - 2250m</label>
    <description>Terrain between 2000m and 2250m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>2000</minElevation>
	  <maxElevation>2250</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/2000_2250</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
