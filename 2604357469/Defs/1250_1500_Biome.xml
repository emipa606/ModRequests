<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 1250m - 1500m ==================== -->

  <BiomeDef>
    <defName>1250_1500_Biome</defName>
    <label>1250m - 1500m</label>
    <description>Terrain between 1250m and 1500m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>1250</minElevation>
	  <maxElevation>1500</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/1250_1500</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
