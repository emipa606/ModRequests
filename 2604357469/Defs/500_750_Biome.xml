<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 500m - 750m ==================== -->

  <BiomeDef>
    <defName>500_750_Biome</defName>
    <label>500m - 750m</label>
    <description>Terrain between 500m and 750m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>500</minElevation>
	  <maxElevation>750</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/500_750</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
