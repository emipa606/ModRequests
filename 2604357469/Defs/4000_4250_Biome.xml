<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ==================== 4000m - 4250m ==================== -->
  <BiomeDef>
    <defName>4000_4250_Biome</defName>
    <label>4000m - 4250m</label>
    <description>Terrain between 4000m and 4250m.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	
	<modExtensions>
      <li Class="BiomesKit.BiomesKitControls">
	  <allowOnLand>True</allowOnLand>
	  <minElevation>4000</minElevation>
	  <maxElevation>4250</maxElevation>
      </li>
	</modExtensions>
	
    <canBuildBase>false</canBuildBase>
    <allowRoads>false</allowRoads>
    <allowRivers>false</allowRivers>
    <hasVirtualPlants>false</hasVirtualPlants>
    <texture>Biomes/4000_4250</texture>
    <impassable>true</impassable>
  </BiomeDef>

</Defs>
