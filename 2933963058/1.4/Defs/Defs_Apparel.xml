<?xml version="1.0" encoding="utf-8" ?>
<Defs>

		<!-- School_Thai_A -->

  <ThingDef ParentName="ApparelMakeableBase">
		<defName>High_School_Uniform_Thai_Girl_A</defName>
		<label>High School Uniform Thai Girl</label>
		<description>"High School Uniform Thai Girl."</description>
		<tradeability>Sellable</tradeability>
		<generateCommonality>0</generateCommonality>
		<generateAllowChance>0</generateAllowChance>
	<graphicData>
      <texPath>Things/Apparel/High_School_Uniform_Thai_A/High_School_Uniform_Thai_Girl_Logo</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
		<Mass>0.3</Mass>
		<MaxHitPoints>200</MaxHitPoints>
		<WorkToMake>4000</WorkToMake>
		<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
		<StuffEffectMultiplierInsulation_Cold>2.0</StuffEffectMultiplierInsulation_Cold>
		<StuffEffectMultiplierInsulation_Heat>2.0</StuffEffectMultiplierInsulation_Heat>
		<EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
	    <GlobalLearningFactor>0.3</GlobalLearningFactor>
		<PainShockThreshold>0.15</PainShockThreshold>
	</equippedStatOffsets>
    
	<costStuffCount>90</costStuffCount>
	<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
    <apparel>
		<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
			</bodyPartGroups>
		<wornGraphicPath>Things/Apparel/High_School_Uniform_Thai_A/High_School_Uniform_Thai_Girl</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
		<layers>
			<li>OnSkin</li>
		</layers>
		<tags>
			<li>School_Thai</li>
		</tags>
		<defaultOutfitTags>
			<li>Worker</li>
		</defaultOutfitTags>
	</apparel>
  </ThingDef>
  
