	<backstoryFiltersOverride>
            <li>
                <categories>
                    <li>CrystalloidRen</li>
                </categories>
            </li>
	</backstoryFiltersOverride>
	<maxGenerationAge>300</maxGenerationAge>
    <apparelTags>
      <li>IndustrialBasic</li>
	  <li>CrystalloidBasic</li>
    </apparelTags>
	<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelMoney>
      <min>380</min>
      <max>630</max>
    </apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
  </PawnKindDef>
  
  <PawnKindDef ParentName="BaseCrystalloidPlayerPawnKind">
    <defName>Valkyrie</defName>
