<Defs>

  <PawnKindDef>
    <defName>CrystalloidSlave</defName>
    <label>slave</label>
    <combatPower>25</combatPower>
    <race>Alien_Crystalloid</race>
    <baseRecruitDifficulty>0.60</baseRecruitDifficulty>
    <backstoryFiltersOverride>
            <li>
                <categories>
                    <li>CrystalloidRen</li>
                </categories>
            </li>
	</backstoryFiltersOverride>
    <defaultFactionType>OutlanderCivil</defaultFactionType>
    <itemQuality>Shoddy</itemQuality>
    <backstoryCryptosleepCommonality>0.1</backstoryCryptosleepCommonality>
    <isFighter>false</isFighter>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
	<maxGenerationAge>40</maxGenerationAge>
    <gearHealthRange>
      <min>0.2</min>
      <max>0.8</max>
    </gearHealthRange>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelMoney>
      <min>150</min>
      <max>300</max>
    </apparelMoney>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <techHediffsMoney>
      <min>0</min>
      <max>35</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
	  <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.01</techHediffsChance>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
  </PawnKindDef>
  
