<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef Name="CrystalOutlanderBase" Abstract="True">
    <race>Alien_Crystalloid</race>
    <defaultFactionType>OutlanderCivil</defaultFactionType>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
	<maxGenerationAge>90</maxGenerationAge>
    <invNutrition>2.55</invNutrition>
    <apparelTags>
      <li>IndustrialBasic</li>
      <li>IndustrialAdvanced</li>
    </apparelTags>
    <inventoryOptions>
      <skipChance>0.70</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>90</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>30~100</countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
	<initialWillRange>1~5</initialWillRange>
    <initialResistanceRange>12~29</initialResistanceRange>
