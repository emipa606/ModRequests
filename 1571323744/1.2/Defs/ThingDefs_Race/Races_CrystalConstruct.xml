<Defs>

  <ThingDef ParentName="BaseCrystal">
    <defName>Crystal_Prism</defName>
    <label>Prism</label>
    <description>A simple crystalline psionic construct. It is actively connected to a psionic network that dictates its actions. It is a light combat construct that leviates above ground and fies proton streams towards enemies.</description>
    <statBases>
      <MoveSpeed>4.2</MoveSpeed>
	  <PainShockThreshold>0.99</PainShockThreshold>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.45</ArmorRating_Sharp>
      <MeatAmount>0</MeatAmount>
    </statBases>
	<tools>
      <li>
        <label>shell</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2.2</cooldownTime>
        <linkedBodyPartsGroup>OuterShellTool</linkedBodyPartsGroup>
		<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>18</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
	</tools>
    <race>
	  <intelligence>ToolUser</intelligence>
      <thinkTreeMain>Mechanoid</thinkTreeMain>
      <body>CrystalPrism</body>
	  <useMeatFrom>Human</useMeatFrom>
	  <lifeExpectancy>2750</lifeExpectancy>
      <baseBodySize>0.7</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
	  <canBePredatorPrey>false</canBePredatorPrey>
      <lifeStageAges>
        <li>
          <def>CrystalFullyFormed</def>
          <minAge>0</minAge>
          <soundWounded>Pawn_Prism_Wounded</soundWounded>
          <soundDeath>Pawn_Prism_Death</soundDeath>
          <soundCall>Pawn_Prism_Call</soundCall>
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>1000</min>
        <max>2000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
	  <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(200,10)</li>
          <li>(1100,4)</li>
          <li>(1700,3)</li>
          <li>(2000,2)</li>
          <li>(2500,1)</li>
          <li>(2750,0)</li>
        </points>
      </ageGenerationCurve>
	  <hediffGiverSets>
	    <li>CrystalConstruct</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>Euthanize</li>
    </recipes>
	<butcherProducts>
	  <Crystal>10</Crystal>
    </butcherProducts>
  </ThingDef>
  
