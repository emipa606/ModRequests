  <!-- ====================================== Combat Fleece =========================================== -->
  
  	 <ThingDef ParentName="RHApparelMakeableBase_RHFactions">
    <defName>RNApparel_combats_Softshell_COA_Grey</defName>
    <description>Custom made combat softshell for operators. Designed to be used for both Arctic and Urban conditions.
	
Has a grey softshell jacket with desert DPM camo trousers.</description>
    <label>combat softshell (grey)</label>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/Display/COA_Softshell_DDPM</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>160</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>RHApparel_Combat</li>
    </thingCategories>
    <statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToMake>9000</WorkToMake>
      <Mass>0.60</Mass>
      <StuffEffectMultiplierArmor>0.3</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>2.05</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.20</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
	
	<equippedStatOffsets>
	  <ShootingAccuracyPawn>0.02</ShootingAccuracyPawn>
	  <WorkSpeedGlobal>0.20</WorkSpeedGlobal>
	  <ForagedNutritionPerDay>0.25</ForagedNutritionPerDay>
    </equippedStatOffsets>
	
		<apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
		<li>Shoulders</li>
        <li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Onskin/COA_Softshell_DDPM</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <!--<li>IndustrialAdvanced</li>-->
		<li>PassiveCamo_Desert_High</li>
		<li>RH_Onskin_Coalition</li>
		<!--<li>IndustrialBasic</li>-->
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
	  <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
	<colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
  
