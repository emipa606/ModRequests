      <ComponentSpacer>5</ComponentSpacer>
    </products>
    <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
    <skillRequirements>
      <Crafting>12</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<recipeUsers>
		<li>FabricationBench</li>
	</recipeUsers>
  </RecipeDef>

  <RecipeDef>
    <defName>Make10Patchleather</defName>
    <label>make patchleather x10</label>
    <description>Combine various types of leather to create weak patchleather.</description>
    <jobString>Making patchleather.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>9000</workAmount>
    <allowMixingIngredients>true</allowMixingIngredients>
    <targetCountAdjustment>50</targetCountAdjustment>
    <recipeUsers>
      <li>ElectricTailoringBench</li>
      <li>HandTailoringBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Leathers</li>
          </categories>
          <disallowedThingDefs>
            <li>Leather_Thrumbo</li>
          </disallowedThingDefs>
        </filter>
        <count>500</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Leathers</li>
      </categories>
      <disallowedThingDefs>
        <li>Leather_Patch</li>
        <li>Leather_Human</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <Leather_Patch>500</Leather_Patch>
    </products>
    <workSkill>Crafting</workSkill>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
  </RecipeDef>

  <RecipeDef ParentName="MakeStoneBlocksBase">
    <defName>Make3StoneBlocksAny</defName>
    <label>make any stone blocks x3</label>
    <description>Cut rock chunks into usable stone blocks.</description>
    <workerCounterClass>RecipeWorkerCounter_MakeStoneBlocks</workerCounterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>StoneChunks</li>
          </categories>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <specialProducts>
      <li>Butchery</li>
    </specialProducts>
    <recipeUsers>
      <li>ElectricTailoringBench</li>
    </recipeUsers>
  </RecipeDef>

  <RecipeDef>
    <defName>Extract3MetalFromSlag</defName>
    <label>smelt metal from slag x3</label>
    <description>Use heat and strong electromagnets to extract useful metal from slag chunks.</description>
    <jobString>Smelting metal from slag.</jobString>
    <workAmount>1000</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ChunkSlagSteel</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <!-- Switched to standard products so we can do "do until you have X"
    <specialProducts>
      <li>Smelted</li>
    </specialProducts>-->
    <products>
      <Steel>45</Steel>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ChunkSlagSteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <recipeUsers>
      <li>ElectricSmelter</li>
    </recipeUsers>
  </RecipeDef>


</Defs>