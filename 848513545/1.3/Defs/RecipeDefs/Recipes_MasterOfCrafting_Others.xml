	<RecipeDef>
		<defName>Make5ChemfuelFromWood</defName>
		<label>make chemfuel from wood x5</label>
		<description>Make a batch of chemfuel by gasifying wood logs.</description>
		<jobString>Refining chemfuel from wood.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>10000</workAmount>
		<workSkill>Intellectual</workSkill>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>350</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Chemfuel>175</Chemfuel>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef>
		<defName>Make5ChemfuelFromOrganics</defName>
		<label>make chemfuel from organic x5</label>
		<description>Make a batch of chemfuel by extracting and condensing wood gas from logs.</description>
		<jobString>Refining chemfuel from organics.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>12500</workAmount>
		<workSkill>Intellectual</workSkill>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Foods</li>
						<li>PlantMatter</li>
					</categories>
					<disallowedThingDefs>
						<li>Hay</li>
					</disallowedThingDefs>
				</filter>
				<count>17.5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Foods</li>
				<li>PlantMatter</li>
			</categories>
			<disallowedThingDefs>
				<li>Hay</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FoodRaw</li>
			</categories>
			<disallowedCategories>
				<li>MeatRaw</li>
				<li>AnimalProductRaw</li>
			</disallowedCategories>
		</defaultIngredientFilter>
		<products>
			<Chemfuel>175</Chemfuel>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
	</RecipeDef>
	
	<RecipeDef>
    <defName>Make5Component</defName>
    <label>make component x5</label>
    <description>Make components.</description>
    <jobString>Making components.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>25000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>60</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentIndustrial>5</ComponentIndustrial>
    </products>
		<skillRequirements>
			<Crafting>12</Crafting>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
	<recipeUsers>
		<li>FabricationBench</li>
	</recipeUsers>
  </RecipeDef>

  <RecipeDef>
    <defName>Make5AdvancedComponent</defName>
    <label>make advanced component x5</label>
    <description>Make advanced components.</description>
    <jobString>Making advanced components.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>50000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>1.5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ComponentIndustrial</li>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Gold</li>
      </thingDefs>
    </fixedIngredientFilter>
