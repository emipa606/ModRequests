<Defs>
	
	<RecipeDef>
		<defName>Make10Shell_HighExplosive</defName>
		<label>Make high-explosive shell x10</label>
		<description>Make high-explosive shells.</description>
		<jobString>Making high-explosive shells.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <targetCountAdjustment>5</targetCountAdjustment>
        <workSkill>Crafting</workSkill>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<workAmount>8000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>150</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
				<li>Chemfuel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Shell_HighExplosive>10</Shell_HighExplosive>
		</products>
		<recipeUsers>
			<li>TableMachining</li>
		</recipeUsers>
	</RecipeDef>
	
