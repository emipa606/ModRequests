<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <RecipeDef>
    <defName>Make_PotatoMush</defName>
    <label>make potato mush</label>
    <description>Make potato mush, similar to mashed potatoes.</description>
    <jobString>Making potato mush.</jobString>
    <workAmount>700</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>RawPotatoes</li>
               </thingDefs>
            </filter>
            <count>3.5</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
        <thingDefs>
          <li>RawPotatoes</li>
        </thingDefs>
      </fixedIngredientFilter>
    <products>
      <PotatoMush>17</PotatoMush>
    </products>
    <workSkill>Cooking</workSkill>
  </RecipeDef>

</Defs>