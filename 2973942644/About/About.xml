<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ModMetaData>
  <name>AssignedWorkbenches</name>
  <author>Maarx</author>
  <url>https://github.com/maarxx/AssignedWorkbenches/</url>
  <description>
The mod adds the 'Set Owner' Gizmo (like from a Bed) onto all Workbenches.

Pawns will not work on the bench unless they are assigned to it.
  </description>
  <packageId>maarx.assignedworkbenches</packageId>
  <supportedVersions>
    <li>1.4</li>
  </supportedVersions>
  <modDependencies>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
  </modDependencies>
  <loadAfter>
    <li>brrainz.harmony</li>
  </loadAfter>
</ModMetaData>
