<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonGardevoir</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[GardevoirFirst] [GardevoirLast]</li>
        <li>name->[GardevoirFirst] '[Nickname]' [GardevoirLast]</li>
      </rulesStrings>
	<rulesRaw>
		<li Class="Rule_File">
      <keyword>GardevoirFirst</keyword>
      <path>Gardevoir/GardevoirNames/GardevoirFirst</path>
    </li>
		<li Class="Rule_File">
      <keyword>GardevoirLast</keyword>
      <path>Gardevoir/GardevoirNames/GardevoirLast</path>
    </li>
		<li Class="Rule_File">
      <keyword>Nickname</keyword>
      <path>Nicknames/Nickname</path>
    </li>
  </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
