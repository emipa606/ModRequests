    </backstoryCategories>
    <humanlikeFaction>true</humanlikeFaction>
    <hidden>true</hidden>
    <autoFlee>False</autoFlee>
    <rescueesCanJoin>true</rescueesCanJoin>
    <hairTags>
      <li>Urban</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
        <li>Hyperweave</li>
      </thingDefs>
    </apparelStuffFilter>
