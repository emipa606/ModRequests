<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThoughtDef>
    <defName>DevDesigner_BannerInspiration</defName>
    <workerClass>VanillaFurnitureExpanded.ThoughtWorker_ThoughtFromNearbyThingDef</workerClass>
    <effectMultiplyingStat>PsychicSensitivity</effectMultiplyingStat>
    <stages>
      <li>
        <label>inspired by banner</label>
        <description>Displaying our colors gives me strength!</description>
        <baseMoodEffect>5</baseMoodEffect>
      </li>
    </stages>
    <modExtensions>
      <li Class="VanillaFurnitureExpanded.ThoughtGiverByProximityDefExtension">
        <ThingToGiveThought>DevDesigner_BloodBanner</ThingToGiveThought>
        <DistanceToGiveThought>25</DistanceToGiveThought>
      </li>
    </modExtensions>
  </ThoughtDef>

</Defs>
