<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef Name="ClutterStructureBuildingBase" Abstract="True">
		<category>Building</category>
		<thingClass>Building</thingClass>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<selectable>true</selectable>
		<drawerType>MapMeshAndRealTime</drawerType>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<repairEffect>Repair</repairEffect>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<statBases>
			<SellPriceFactor>0.70</SellPriceFactor>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="ClutterStructureBuildingBase">
		<defName>ClutterFence</defName>
		<label>Small Fence</label>
		<description>To stop those pesky creatures eating from your storage.</description>
		<graphicData>
			<linkType>Basic</linkType>
			<texPath>Clutter/Structure/Fence</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<linkFlags>
				<li>Custom5</li>
			</linkFlags>
		</graphicData>
		<uiIconPath>Clutter/Structure/FenceTestico</uiIconPath>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0.0</Flammability>
		</statBases>
		<costList>
			<Steel>5</Steel>
		</costList>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<altitudeLayer>Building</altitudeLayer>
		<pathCost>75</pathCost>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<costStuffCount>10</costStuffCount>
		<holdsRoof>false</holdsRoof>
		<selectable>true</selectable>
		<designationCategory>Structure</designationCategory>
		<researchPrerequisites>
			<li>UtilityStructures</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="ClutterStructureBuildingBase">
		<defName>ClutterConcreteFenceA</defName>
		<label>Concrete Fence</label>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
			<linkType>Basic</linkType>
			<texPath>Clutter/Structure/ConcreteFence</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<linkFlags>
				<li>Custom6</li>
			</linkFlags>
		</graphicData>
		<description>High fence</description>
		<uiIconPath>Clutter/Structure/ConcreteFenceIco</uiIconPath>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>200</WorkToBuild>
			<Flammability>0.0</Flammability>
		</statBases>
		<costList>
			<Steel>10</Steel>
		</costList>
		<costStuffCount>20</costStuffCount>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.69</fillPercent>
		<coversFloor>true</coversFloor>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<holdsRoof>false</holdsRoof>
		<designationCategory>Structure</designationCategory>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
		<building>
			<isInert>true</isInert>
		</building>
		<researchPrerequisites>
			<li>WallReinforcment</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>