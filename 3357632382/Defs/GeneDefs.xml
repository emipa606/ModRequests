    </descriptionHyperlinks>
    <biostatCpx>5</biostatCpx><!--复杂度-->
    <biostatMet>0</biostatMet><!--代谢效率-->
    <biostatArc>5</biostatArc><!--超凡胶囊-->
    <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
    <dontMindRawFood>true</dontMindRawFood>
    <statOffsets>
      <ShootingAccuracyPawn>-5</ShootingAccuracyPawn>
      <EatingSpeed>1</EatingSpeed>
    </statOffsets>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>isopoda</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>isopoda</symbol></li>
      </suffixSymbols>
    </symbolPack>
		<forcedHeadTypes>
			<li>Gene_Isopoda_Head</li>
		</forcedHeadTypes>
    <forcedHair>Gene_Isopoda_HeadHair</forcedHair>
    <hairColorOverride>(255,255,255)</hairColorOverride>
  </GeneDef>

  
	<HeadTypeDef ParentName="AverageBase">
