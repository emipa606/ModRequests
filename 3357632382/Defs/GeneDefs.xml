  <GeneDef>
    <defName>Gene_Isopoda_Head</defName>
    <label>isopoda head</label>
    <description>Carriers of this gene have discharge organs in their heads that can release strong electrical currents.</description>
    <iconPath>Things/Pawn/Humanlike/Heads/IsopodaHead/IsopodaHeadBase</iconPath>
    <displayCategory>Cosmetic_Body</displayCategory>
    <marketValueFactor>1</marketValueFactor>
    <geneClass>Isopoda_Geneline.Gene_IsopodaHead</geneClass>
    <abilities>
      <li>VFEI2_LightningAttackSmall</li>
    </abilities>
    <selectionWeight>0</selectionWeight>
