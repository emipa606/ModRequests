<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<AbilityDef>
		<defName>VFEI2_WideTunnels60S</defName>
		<label>breacher charge</label>
		<aiCanUse>true</aiCanUse>
		<displayGizmoWhileUndrafted>true</displayGizmoWhileUndrafted>
		<disableGizmoWhileUndrafted>False</disableGizmoWhileUndrafted>
		<casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
		<hostile>true</hostile>
		<description>Insectoid charges towards the target, dealing significant damage to anything on the path. Will destroy any sort of defenses with ease.</description>
		<iconPath>UI/Abilities/VFE_BreacherCharge</iconPath>
		<cooldownTicksRange>2500</cooldownTicksRange>
		<verbProperties>
			<verbClass>VFEInsectoids.Verb_CastAbilityWideTunnels</verbClass>
			<violent>true</violent>
			<forceNormalTimeSpeed>false</forceNormalTimeSpeed>
			<requireLineOfSight>false</requireLineOfSight>
			<soundCast>VFEI2_Insect_Jump</soundCast>
			<soundLanding>Longjump_Land</soundLanding>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
				<canTargetPawns>true</canTargetPawns>
				<canTargetBuildings>true</canTargetBuildings>
			</targetParams>
			<label>wide tunnels</label>
			<warmupTime>0.1</warmupTime>
			<range>20.9</range>
			<minRange>1.9</minRange>
		</verbProperties>
		<jobDef>CastJump</jobDef>
	</AbilityDef>

	<AbilityDef>
		<defName>VFEI2_ShootElectrictick</defName>
		<label>thrown electrictick</label>
		<aiCanUse>true</aiCanUse>
		<displayGizmoWhileUndrafted>true</displayGizmoWhileUndrafted>
		<disableGizmoWhileUndrafted>False</disableGizmoWhileUndrafted>
		<casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
		<hostile>true</hostile>
		<cooldownTicksRange>1</cooldownTicksRange>
		<description>Consume 20% of the food to throw the Electrictick to the target location. If the food is insufficient, it will not be usable.</description>
		<iconPath>Things/Pawn/Animal/ShootElectrictickIcon</iconPath>
		<verbProperties>
			<verbClass>Isopoda_Geneline.Verb_Launch_Nurtrition</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<range>20.9</range>
			<warmupTime>3</warmupTime>
			<soundCast>ThrowGrenade</soundCast>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
			<defaultProjectile>Proj_SW_Electrictick</defaultProjectile>
			<rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
		</verbProperties>
		<comps>
			<li>
				<compClass>Isopoda_Geneline.Ability_FoodNeed</compClass>
			</li>
		</comps>
	</AbilityDef>

