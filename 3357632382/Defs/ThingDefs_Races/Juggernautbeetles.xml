<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="VFEI2_BaseInsect">
        <defName>SW_Juggernautbeetles</defName>
        <label>Juggernautbeetles</label>
        <description>A giant bioengineered insectoid. Designed to destroy the mechanoid. The thick shell covering its entire body enables it to charge against the fierce firepower of the mechanoid's artillery and tear apart any mechanoid's armor with its strong front paws equipped with discharge organs.</description>
        <uiIconPath>Things/Pawn/Animal/SW_PraetorianExecutioner/SW_PraetorianExecutionerB_east</uiIconPath>
        <statBases>
            <MoveSpeed>4.00</MoveSpeed>
            <Flammability>0</Flammability>
            <ComfyTemperatureMin>-40</ComfyTemperatureMin>
            <ComfyTemperatureMax>160</ComfyTemperatureMax>
            <MarketValue>1500</MarketValue>
            <ArmorRating_Sharp>0.80</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.65</ArmorRating_Blunt>
            <ArmorRating_Heat>2</ArmorRating_Heat>
            <LeatherAmount>30</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>20</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
            </li>
            <li>
                <label>head claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>20</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <comps>
            <li Class="AnimalBehaviours.CompProperties_InitialAbility">
                <initialAbility>VFEI2_WideTunnels60S</initialAbility>
            </li>
        </comps>
        <race>
            <renderTree>VFEI2_Unarmored</renderTree>
            <body>BeetleLikeWithClaw</body>
            <baseHungerRate>0.25</baseHungerRate>
            <baseBodySize>3.00</baseBodySize>
            <baseHealthScale>6.00</baseHealthScale>

            <useMeatFrom>Megaspider</useMeatFrom>
            <wildness>0.4</wildness>
            <lifeExpectancy>6</lifeExpectancy>
            <trainability>Intermediate</trainability>
            <leatherDef>VFEI2_Chitin</leatherDef>
            <lifeStageAges>
                <li>
                    <def>EusocialInsectLarva</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>EusocialInsectJuvenile</def>
                    <minAge>0.03</minAge>
                </li>
                <li>
                    <def>EusocialInsectAdult</def>
                    <minAge>0.2</minAge>
                    <soundWounded>Pawn_Megascarab_Wounded</soundWounded>
                    <soundDeath>Pawn_Megascarab_Death</soundDeath>
                    <soundCall>Pawn_Megascarab_Call</soundCall>
                    <soundAngry>Pawn_Megascarab_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
    </ThingDef>
