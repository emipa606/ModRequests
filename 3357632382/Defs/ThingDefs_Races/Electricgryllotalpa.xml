<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="VFEI2_BaseInsect">
        <defName>SW_Electricgryllotalpa</defName>
        <label>Electricgryllotalpa</label>
        <description>A large bioengineered insectoid. Designed to destroy the mechanoid. The discharge organs are highly developed and capable of releasing focused arcs to attack distant enemies.Focused arcs can cause double damage to the mechanoid.</description>
        <uiIconPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_AcidSpittingSpiderB_east</uiIconPath>
        <statBases>
            <MoveSpeed>3.75</MoveSpeed>
            <Flammability>0</Flammability>
            <ComfyTemperatureMin>-40</ComfyTemperatureMin>
            <ComfyTemperatureMax>160</ComfyTemperatureMax>
            <MarketValue>500</MarketValue>
            <ArmorRating_Sharp>0.4</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
            <ArmorRating_Heat>2</ArmorRating_Heat>
            <LeatherAmount>25</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>15</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
            </li>
            <li>
                <label>head claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>15</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <renderTree>VFEI2_Unarmored</renderTree>
            <body>BeetleLikeWithClaw</body>
            <baseHungerRate>0.25</baseHungerRate>
            <baseBodySize>1.50</baseBodySize>
            <baseHealthScale>2.5</baseHealthScale>

            <useMeatFrom>Megaspider</useMeatFrom>
            <wildness>0.4</wildness>
            <lifeExpectancy>6</lifeExpectancy>
            <trainability>Advanced</trainability>
            <leatherDef>VFEI2_Chitin</leatherDef>
            <lifeStageAges>
                <li>
                    <def>EusocialInsectLarva</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>EusocialInsectJuvenile</def>
                    <minAge>0.015</minAge>
                </li>
                <li>
                    <def>EusocialInsectAdult</def>
                    <minAge>0.1</minAge>
                    <soundWounded>Pawn_Megascarab_Wounded</soundWounded>
                    <soundDeath>Pawn_Megascarab_Death</soundDeath>
                    <soundCall>Pawn_Megascarab_Call</soundCall>
                    <soundAngry>Pawn_Megascarab_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
        <verbs>
            <li>
                <verbClass>Verb_LaunchProjectile</verbClass>
                <hasStandardCommand>true</hasStandardCommand>
                <range>20</range>
                <warmupTime>3</warmupTime>
                <soundCast>Isopoda_LightningAttackSmall</soundCast>
                <forcedMissRadius>1</forcedMissRadius>
                <targetParams>
                    <canTargetLocations>true</canTargetLocations>
                </targetParams>
                <defaultProjectile>Prj_SW_Electricgryllotalpa</defaultProjectile>
                <rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
            </li>
        </verbs>
    </ThingDef>
    <ThingDef>
        <defName>Prj_SW_Electricgryllotalpa</defName>
        <label>focused arcs</label>
        <thingClass>Projectile_Explosive</thingClass>
        <category>Projectile</category>
        <tickerType>Normal</tickerType>
        <altitudeLayer>Projectile</altitudeLayer>
        <useHitPoints>False</useHitPoints>
        <neverMultiSelect>True</neverMultiSelect>
        <graphicData>
            <texPath>Things/Projectile/Prj_SW_Electricgryllotalpa</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <color>(147, 193, 232)</color>
            <drawSize>5</drawSize>
        </graphicData>
        <projectile>
            <damageDef>SW_FalmeWithEMP</damageDef>
            <screenShakeFactor>0</screenShakeFactor>
            <damageAmountBase>7.5</damageAmountBase>
            <armorPenetrationBase>1</armorPenetrationBase>
            <explosionRadius>1.9</explosionRadius>
            <speed>60</speed>
        </projectile>
    </ThingDef>
    <PawnKindDef ParentName="AnimalKindBase">
        <defName>SW_Electricgryllotalpa</defName>
        <label>Electricgryllotalpa</label>
        <race>SW_Electricgryllotalpa</race>
        <combatPower>150</combatPower>
        <canArriveManhunter>false</canArriveManhunter>
        <ecoSystemWeight>0.50</ecoSystemWeight>
        <defaultFactionType>Insect</defaultFactionType>
        <aiAvoidCover>false</aiAvoidCover>
        <defendPointRadius>0</defendPointRadius>
        <lifeStages>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_AcidSpittingSpiderA</texPath>
                    <drawSize>1.5</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_Dessicated_AcidSpittingSpider</texPath>
                    <drawSize>1.5</drawSize>
                </dessicatedBodyGraphicData>
            </li>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_AcidSpittingSpiderA</texPath>
                    <drawSize>2</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_Dessicated_AcidSpittingSpider</texPath>
                    <drawSize>2</drawSize>
                </dessicatedBodyGraphicData>
            </li>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_AcidSpittingSpiderA</texPath>
                    <drawSize>2.75</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_AcidSpittingSpider/SW_Dessicated_AcidSpittingSpider</texPath>
                    <drawSize>2.75</drawSize>
                </dessicatedBodyGraphicData>
            </li>
        </lifeStages>
    </PawnKindDef>

</Defs>