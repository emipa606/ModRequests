<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="VFEI2_BaseInsect">
        <defName>SW_Electricfish</defName>
        <label>Electricfish</label>
        <description>A medium bioengineered insectoid. Designed to destroy the mechanoid. The electricfish is the lowest caste of the isopoda hive, responsible for most of the resource gathering and work.</description>
        <uiIconPath>Things/Pawn/Animal/SW_LaborBug/SW_LaborBugB_east</uiIconPath>
        <statBases>
            <MoveSpeed>4.00</MoveSpeed>
            <Flammability>0</Flammability>
            <ComfyTemperatureMin>-25</ComfyTemperatureMin>
            <ComfyTemperatureMax>120</ComfyTemperatureMax>
            <MarketValue>200</MarketValue>
            <ArmorRating_Sharp>0.60</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
            <ArmorRating_Heat>2</ArmorRating_Heat>
            <LeatherAmount>25</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>10</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.60</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
            </li>
            <li>
                <label>head claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>10</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.60</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <renderTree>VFEI2_Unarmored</renderTree>
            <body>BeetleLikeWithClaw</body>
            <baseHungerRate>0.25</baseHungerRate>
            <baseBodySize>1.0</baseBodySize>
            <baseHealthScale>2.0</baseHealthScale>

            <useMeatFrom>Megaspider</useMeatFrom>
            <wildness>0.4</wildness>
            <lifeExpectancy>6</lifeExpectancy>
            <trainability>Advanced</trainability>
            <leatherDef>VFEI2_Chitin</leatherDef>
            <lifeStageAges>
                <li>
                    <def>EusocialInsectLarva</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>EusocialInsectJuvenile</def>
                    <minAge>0.03</minAge>
                </li>
                <li>
                    <def>EusocialInsectAdult</def>
                    <minAge>0.2</minAge>
                    <soundWounded>Pawn_Megascarab_Wounded</soundWounded>
                    <soundDeath>Pawn_Megascarab_Death</soundDeath>
                    <soundCall>Pawn_Megascarab_Call</soundCall>
                    <soundAngry>Pawn_Megascarab_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
    </ThingDef>
    <PawnKindDef ParentName="AnimalKindBase">
        <defName>SW_Electricfish</defName>
        <label>Electricfish</label>
        <race>SW_Electricfish</race>
        <combatPower>75</combatPower>
        <canArriveManhunter>false</canArriveManhunter>
        <ecoSystemWeight>0.50</ecoSystemWeight>
        <defaultFactionType>Insect</defaultFactionType>
        <aiAvoidCover>false</aiAvoidCover>
        <defendPointRadius>0</defendPointRadius>
        <lifeStages>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_LaborBugA</texPath>
                    <drawSize>1.5</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_Dessicated_LaborBug</texPath>
                    <drawSize>1.5</drawSize>
                </dessicatedBodyGraphicData>
            </li>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_LaborBugA</texPath>
                    <drawSize>2</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_Dessicated_LaborBug</texPath>
                    <drawSize>2</drawSize>
                </dessicatedBodyGraphicData>
            </li>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_LaborBugA</texPath>
                    <drawSize>2.75</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_LaborBug/SW_Dessicated_LaborBug</texPath>
                    <drawSize>2.75</drawSize>
                </dessicatedBodyGraphicData>
            </li>
        </lifeStages>
    </PawnKindDef>
</Defs>