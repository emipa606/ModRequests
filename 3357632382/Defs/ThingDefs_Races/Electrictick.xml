<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="VFEI2_BaseInsect">
        <defName>SW_Electrictick</defName>
        <label>Electrictick</label>
        <description>A medium bioengineered insectoid. Designed to destroy the mechanoid. The growth rate is extremely rapid, but as a result, the lifespan is very short. The discharge organ occupies more than half of the body tissue and releases a strong electric current upon death.</description>
        <statBases>
            <MoveSpeed>6.00</MoveSpeed>
            <Flammability>0</Flammability>
            <ComfyTemperatureMin>-20</ComfyTemperatureMin>
            <ComfyTemperatureMax>120</ComfyTemperatureMax>
            <MarketValue>100</MarketValue>
            <ArmorRating_Sharp>0.40</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
            <ArmorRating_Heat>2</ArmorRating_Heat>
            <LeatherAmount>10</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>5</power>
                <cooldownTime>1</cooldownTime>
                <armorPenetration>0.60</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
            </li>
            <li>
                <label>head claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>5</power>
                <cooldownTime>1</cooldownTime>
                <armorPenetration>0.60</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <thinkTreeMain>ThinkTree_SW_Electrictick</thinkTreeMain>
            <thinkTreeConstant>AnimalConstant</thinkTreeConstant>
            <renderTree>VFEI2_Unarmored</renderTree>
            <body>BeetleLikeWithClaw</body>
            <baseHungerRate>0.25</baseHungerRate>
            <baseBodySize>0.25</baseBodySize>
            <baseHealthScale>0.25</baseHealthScale>
            <useMeatFrom>Megaspider</useMeatFrom>
            <wildness>0.4</wildness>
            <lifeExpectancy>10</lifeExpectancy>
            <trainability>Intermediate</trainability>
            <leatherDef>VFEI2_Chitin</leatherDef>
            <lifeStageAges>
                <li>
                    <def>EusocialInsectAdult</def>
                    <soundWounded>Pawn_Megascarab_Wounded</soundWounded>
                    <soundDeath>Pawn_Megascarab_Death</soundDeath>
                    <soundCall>Pawn_Megascarab_Call</soundCall>
                    <soundAngry>Pawn_Megascarab_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
        <comps>
            <li Class="CompProperties_MechPowerCell">
                <totalPowerTicks>2500</totalPowerTicks>
                <labelOverride>Remaining Life</labelOverride>
                <tooltipOverride>This creature has an extremely short lifespan and will soon die.</tooltipOverride>
            </li>
            <li Class="CompProperties_Explosive">
                <explosiveRadius>1.9</explosiveRadius>
                <damageAmountBase>10</damageAmountBase>
                <armorPenetrationBase>1.0</armorPenetrationBase>
                <destroyThingOnExplosionSize>1</destroyThingOnExplosionSize>
                <explosiveDamageType>SW_FalmeWithEMP</explosiveDamageType>
                <explodeOnKilled>true</explodeOnKilled>
                <chanceNeverExplodeFromDamage>1</chanceNeverExplodeFromDamage>
            </li>
            <li>
                <compClass>Isopoda_Geneline.CompNoCorpse</compClass>
            </li>
        </comps>
    </ThingDef>
    <PawnKindDef ParentName="AnimalKindBase">
        <defName>SW_Electrictick</defName>
        <label>Electrictick</label>
        <race>SW_Electrictick</race>
        <combatPower>40</combatPower>
        <canArriveManhunter>false</canArriveManhunter>
        <ecoSystemWeight>0.50</ecoSystemWeight>
        <defaultFactionType>Insect</defaultFactionType>
        <forceNoDeathNotification>true</forceNoDeathNotification>
        <aiAvoidCover>true</aiAvoidCover>
        <defendPointRadius>0</defendPointRadius>
        <forceDeathOnDowned>true</forceDeathOnDowned>
        <lifeStages>
            <li>
                <bodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_GrenadeBug/SW_GrenadeBug</texPath>
                    <drawSize>1.5</drawSize>
                    <color>(66,73,81)</color>
                    <shaderType>CutoutComplex</shaderType>
                </bodyGraphicData>
                <dessicatedBodyGraphicData>
                    <texPath>Things/Pawn/Animal/SW_GrenadeBug/SW_Dessicated_EngineerBug</texPath>
                    <drawSize>1.5</drawSize>
                </dessicatedBodyGraphicData>
            </li>
        </lifeStages>
    </PawnKindDef>

    <ThinkTreeDef>
        <defName>ThinkTree_SW_Electrictick</defName>
        <thinkRoot Class="ThinkNode_Priority">
            <subNodes>
                <!-- Despawned -->
                <li Class="ThinkNode_Subtree">
                    <treeDef>Despawned</treeDef>
                </li>

                <!-- Downed -->
                <li Class="ThinkNode_Subtree">
                    <treeDef>Downed</treeDef>
                </li>

                <li Class="JobGiver_AIFightEnemies">
                    <targetAcquireRadius>65</targetAcquireRadius>
                    <targetKeepRadius>72</targetKeepRadius>
                    <allowTurrets>true</allowTurrets>
                </li>


                <!-- Idle -->
                <li Class="ThinkNode_Tagger">
                    <tagToGive>Idle</tagToGive>
                    <subNodes>
                        <li Class="JobGiver_WanderAnywhere">
                            <maxDanger>Deadly</maxDanger>
                        </li>
                    </subNodes>
                </li>

                <!-- Idle error -->
                <li Class="JobGiver_IdleError"/>
            </subNodes>
        </thinkRoot>
    </ThinkTreeDef>
</Defs>