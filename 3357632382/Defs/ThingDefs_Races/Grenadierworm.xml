<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="VFEI2_BaseInsect">
        <defName>SW_Grenadierworm</defName>
        <label>Grenadierworm</label>
        <description>A giant bioengineered insectoid. Designed to destroy the mechanoid. There are many electrictick eggs stored in the body, which release pheromones during combat to quickly hatch and then spray electricticks to attack distant enemies.</description>
        <uiIconPath>Things/Pawn/Animal/SW_EngineerBug/SW_EngineerBugB_east</uiIconPath>
        <statBases>
            <MoveSpeed>3.75</MoveSpeed>
            <Flammability>0</Flammability>
            <ComfyTemperatureMin>-40</ComfyTemperatureMin>
            <ComfyTemperatureMax>160</ComfyTemperatureMax>
            <MarketValue>650</MarketValue>
            <ArmorRating_Sharp>0.60</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
            <ArmorRating_Heat>2</ArmorRating_Heat>
            <LeatherAmount>30</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>20</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup></li>
            <li>
                <label>head claw</label>
                <capacities>
                    <li>SW_FlameCut</li>
                </capacities>
                <power>20</power>
                <cooldownTime>1.5</cooldownTime>
                <armorPenetration>0.80</armorPenetration>
                <extraMeleeDamages>
                    <li>
                        <def>EMP</def>
                        <amount>20</amount>
                        <chance>1</chance>
                    </li>
                </extraMeleeDamages>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor></li>
        </tools>
        <comps>
            <li Class="AnimalBehaviours.CompProperties_InitialAbility">
                <initialAbility>VFEI2_ShootElectrictick</initialAbility>
            </li>
        </comps>
        <race>
            <deathAction Class="Isopoda_Geneline.DeathActionProperties_SW_Grenadierworm"/>
            <renderTree>VFEI2_Unarmored</renderTree>
            <body>BeetleLikeWithClaw</body>
            <baseHungerRate>0.25</baseHungerRate>
            <baseBodySize>2.50</baseBodySize>
            <baseHealthScale>4.50</baseHealthScale>
            <useMeatFrom>Megaspider</useMeatFrom>
            <wildness>0.4</wildness>
            <lifeExpectancy>5</lifeExpectancy>
            <trainability>Advanced</trainability>
            <leatherDef>VFEI2_Chitin</leatherDef>
            <lifeStageAges>
                <li>
                    <def>EusocialInsectLarva</def>
                    <minAge>0</minAge></li>
                <li>
                    <def>EusocialInsectJuvenile</def>
                    <minAge>0.03</minAge></li>
                <li>
                    <def>EusocialInsectAdult</def>
                    <minAge>0.2</minAge>
                    <soundWounded>Pawn_Megascarab_Wounded</soundWounded>
                    <soundDeath>Pawn_Megascarab_Death</soundDeath>
                    <soundCall>Pawn_Megascarab_Call</soundCall>
                    <soundAngry>Pawn_Megascarab_Angry</soundAngry></li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
    </ThingDef>
    
    <ThingDef ParentName="BaseGrenadeProjectile">
    <defName>Proj_SW_Electrictick</defName>
    <label>Electrictick</label>
    <graphicData>
        <texPath>Things/Pawn/Animal/SW_GrenadeBug/SW_GrenadeBug_east</texPath>
        <graphicClass>Graphic_Single</graphicClass>
        <drawSize>1.5</drawSize>
    </graphicData>
    <thingClass>Isopoda_Geneline.Projectile_SpawnPawn</thingClass>
    <projectile>
        <speed>30</speed>
        <flyOverhead>true</flyOverhead>
        <explosionRadius>0</explosionRadius>
        <damageDef>Stun</damageDef>
        <shadowSize>0.6</shadowSize>
        <arcHeightFactor>3</arcHeightFactor>
        <screenShakeFactor>0</screenShakeFactor>
    </projectile>
    </ThingDef>
    
    <PawnKindDef ParentName="AnimalKindBase">
    <defName>SW_Grenadierworm</defName>
    <label>Grenadierworm</label>
    <race>SW_Grenadierworm</race>
    <combatPower>300</combatPower>
    <canArriveManhunter>false</canArriveManhunter>
    <ecoSystemWeight>0.50</ecoSystemWeight>
    <defaultFactionType>Insect</defaultFactionType>
    <aiAvoidCover>false</aiAvoidCover>
    <defendPointRadius>0</defendPointRadius>
    <lifeStages>
        <li>
            <bodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_EngineerBugA</texPath>
                <drawSize>1.5</drawSize>
                <color>(66,73,81)</color>
                <shaderType>CutoutComplex</shaderType>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_Dessicated_EngineerBug</texPath>
                <drawSize>1.5</drawSize>
            </dessicatedBodyGraphicData>
        </li>
        <li>
            <bodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_EngineerBugA</texPath>
                <drawSize>2.25</drawSize>
                <color>(66,73,81)</color>
                <shaderType>CutoutComplex</shaderType>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_Dessicated_EngineerBug</texPath>
                <drawSize>2.25</drawSize>
            </dessicatedBodyGraphicData>
        </li>
        <li>
            <bodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_EngineerBugA</texPath>
                <drawSize>3</drawSize>
                <color>(66,73,81)</color>
                <shaderType>CutoutComplex</shaderType>
            </bodyGraphicData>
            <dessicatedBodyGraphicData>
                <texPath>Things/Pawn/Animal/SW_EngineerBug/SW_Dessicated_EngineerBug</texPath>
                <drawSize>3</drawSize>
            </dessicatedBodyGraphicData>
        </li>
    </lifeStages>
    </PawnKindDef>
</Defs>