<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <VanillaRacesExpandedInsector.GenelineGeneDef ParentName="VRE_MutationGene">
    <defName>Gene_IsopodaMutation_Head</defName>
    <label>isopoda head</label>
    <description>Carriers of this gene have discharge organs in their heads that can release strong electrical currents.</description>
    <unlockable>true</unlockable>
    <iconPath>Things/Pawn/Humanlike/Heads/IsopodaHead/IsopodaHeadBase</iconPath>
    <marketValueFactor>1</marketValueFactor>
    <geneClass>Isopoda_Geneline.Gene_IsopodaHead</geneClass>
    <displayOrderInCategory>9999</displayOrderInCategory>
    <abilities>
      <li>VFEI2_LightningAttackSmall</li>
    </abilities>
    <selectionWeight>0</selectionWeight>
    <descriptionHyperlinks>
      <AbilityDef>VFEI2_LightningAttackSmall</AbilityDef>
    </descriptionHyperlinks>
    <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
    <dontMindRawFood>true</dontMindRawFood>
    <statOffsets>
      <ShootingAccuracyPawn>-5</ShootingAccuracyPawn>
      <EatingSpeed>1</EatingSpeed>
    </statOffsets>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>isopoda</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>isopoda</symbol></li>
      </suffixSymbols>
    </symbolPack>
		<forcedHeadTypes>
			<li>Gene_Isopoda_Head</li>
		</forcedHeadTypes>
    <exclusionTags>
      <li>Antennas</li>
      <li>Antenna</li>
      <li>AG_Antennae</li>
    </exclusionTags>
    <forcedHair>Gene_Isopoda_HeadHair</forcedHair>
    <hairColorOverride>(255,255,255)</hairColorOverride>
      <modExtensions>
          <li Class="VanillaGenesExpanded.GeneExtension">
              <backgroundPathEndogenes>UI/Icons/Genes/VRE_MutationBG</backgroundPathEndogenes>
              <backgroundPathXenogenes>UI/Icons/Genes/VRE_MutationBG</backgroundPathXenogenes>
          </li>
      </modExtensions>
  </VanillaRacesExpandedInsector.GenelineGeneDef>

  <VanillaRacesExpandedInsector.GenelineGeneDef ParentName="VRE_EvolutionGene">
    <defName>Gene_IsopodaEvolution_Body</defName>
    <label>isopoda body</label>
    <description>Carriers of this gene have thick shells and claws with discharge organs, and can produce Electrictick in their bodies.</description>
    <unlockable>true</unlockable>
    <iconPath>Things/Pawn/Humanlike/Bodies/IsopodaBodyIcon</iconPath>
    <geneClass>Isopoda_Geneline.Gene_IsopodaBody</geneClass>
    <abilities>
      <li>VFEI2_ShootElectrictickSlow</li>
    </abilities>
    <marketValueFactor>1</marketValueFactor>
    <selectionWeight>0</selectionWeight>
		<skinIsHairColor>false</skinIsHairColor>
		<tattoosVisible>false</tattoosVisible>
    <statOffsets>
      <ArmorRating_Sharp>0.30</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.15</ArmorRating_Blunt>
      <ArmorRating_Heat>2</ArmorRating_Heat>
    </statOffsets>
    <statFactors>
      <Flammability>0</Flammability>
    </statFactors>
		<modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathEndogenes>UI/Icons/Genes/VRE_EvolutionBG</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icons/Genes/VRE_EvolutionBG</backgroundPathXenogenes>
            </li>
		</modExtensions>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>isopoda</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>isopoda</symbol></li>
      </suffixSymbols>
    </symbolPack>
      <skinColorOverride>(255,255,255)</skinColorOverride>
    <exclusionTags>
      <li>SkinColorOverride</li>
    </exclusionTags>
  </VanillaRacesExpandedInsector.GenelineGeneDef>

</Defs>