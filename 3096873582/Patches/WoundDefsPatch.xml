        <countToAffect>1</countToAffect>
        <partsToAffect>
            <li>Head</li>
        </partsToAffect>
        <ageFractionChanceCurve>
          <!-- 
            0-10 0
            10-15 +10
            15-20 -10
            20+ 0
           -->
            <points>
              <li>(0.00, 0.0)</li>
              <li>(0.1, 0.0)</li>
              <li>(0.125, 0.10)</li>
              <li>(0.2, -0.07)</li>
              <li>(0.3, -0.001)</li>
              <li>(0.35, 0)</li>
              <li>(0.4, 0)</li>

