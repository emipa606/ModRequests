<?xml version="1.0" encoding="utf-8"?>
<Defs>
<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>WolfMan</defName>
    <label>wolf man</label>
    
    <description>A man with the head of a wolf.</description>
        
    <statBases>
      <PsychicSensitivity>1.0</PsychicSensitivity>
      <ToxicSensitivity>1.0</ToxicSensitivity>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
      <ComfyTemperatureMax>32</ComfyTemperatureMax>
      <LeatherAmount>30</LeatherAmount>
      <MarketValue>1000</MarketValue>
      <MoveSpeed>4.61</MoveSpeed>
      <Flammability>1.0</Flammability>
      <CarryingCapacity>75</CarryingCapacity>
      <MentalBreakThreshold>0.35</MentalBreakThreshold>
      <ResearchSpeed>1.0</ResearchSpeed>
      <MiningSpeed>1.0</MiningSpeed>
      <SocialImpact>1.1</SocialImpact>
      <PlantWorkSpeed>1.0</PlantWorkSpeed>
      <PlantHarvestYield>1.0</PlantHarvestYield>
      <ConstructionSpeed>1.0</ConstructionSpeed>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.15</ArmorRating_Sharp>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>

<alienRace>

<generalSettings>
    <humanRecipeImport>true</humanRecipeImport>
    <alienPartGenerator>
    <alienbodytypes>
    </alienbodytypes>
    <useGenderedHeads>false</useGenderedHeads>
    <alienskincolorgen Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>10</weight>
                    <only>RGBA(1.0,1.0,1.0,1)</only>
                </li>
            </options>
    </alienskincolorgen>
    </alienPartGenerator>
    
    <factionRelations>              
    </factionRelations>

    <maleGenderProbability>0.5</maleGenderProbability>

    <chemicalSettings>
    </chemicalSettings>
    

    <forcedRaceTraitEntries>
