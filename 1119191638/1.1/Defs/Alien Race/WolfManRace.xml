                    <defName>Kind</defName>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>NaturalMood</defName>
		    <degree>2</degree>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>Brawler</defName>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>Nerves</defName>
		    <degree>2</degree>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>Bloodlust</defName>
                    <chance>20</chance>
                </li>
    </forcedRaceTraitEntries>


</generalSettings>    


<graphicPaths>
