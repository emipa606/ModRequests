<?xml version="1.0" encoding="utf-8"?>
<Defs>
<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>CamelMan</defName>
    <label>camel man</label>
    
    <description>A man with the head of a camel.</description>
        
    <statBases>
      <PsychicSensitivity>1.0</PsychicSensitivity>
      <ComfyTemperatureMin>12</ComfyTemperatureMin>
      <ComfyTemperatureMax>52</ComfyTemperatureMax>
      <LeatherAmount>30</LeatherAmount>
      <MarketValue>1000</MarketValue>
      <MoveSpeed>4.61</MoveSpeed>
      <Flammability>1.0</Flammability>
      <CarryingCapacity>75</CarryingCapacity>
      <MentalBreakThreshold>0.35</MentalBreakThreshold>
      <ResearchSpeed>1.0</ResearchSpeed>
      <MiningSpeed>1.0</MiningSpeed>
      <SocialImpact>1.0</SocialImpact>
      <PlantWorkSpeed>1.0</PlantWorkSpeed>
      <PlantHarvestYield>1.0</PlantHarvestYield>
      <ConstructionSpeed>1.0</ConstructionSpeed>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.15</ArmorRating_Sharp>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>

<alienRace>

<generalSettings>
    <humanRecipeImport>true</humanRecipeImport>
    <alienPartGenerator>
        <colorChannels Inherit="False">
	    <li>
                <name>skin</name>
                <first Class="AlienRace.ColorGenerator_SkinColorMelanin">
                    <minMelanin>0</minMelanin>
                    <maxMelanin>0</maxMelanin>
                </first>
                <second Class="AlienRace.ColorGenerator_CustomAlienChannel">
                    <colorChannel>skin_1</colorChannel>
                </second>
           </li>
	</colorChannels>
	<bodyTypes Inherit = "False">
  	    <li>Male</li>
  	    <li>Female</li>
  	    <li>Thin</li>
  	    <li>Hulk</li>
  	    <li>Fat</li>
	</bodyTypes>
        <headTypes Inherit="False">
            <li>Male_AverageNormal</li>
            <li>Female_AverageNormal</li>
        </headTypes>
    </alienPartGenerator>
    
    <factionRelations>
	<li>
                    <factions>
                       <li>CamelManTribe</li>
                    </factions>
                    <goodwill>
                       <min>100</min>
                       <max>100</max>
                    </goodwill>
        </li>              
    </factionRelations>

    <maleGenderProbability>0.5</maleGenderProbability>

    <chemicalSettings>
    </chemicalSettings>
    

    <forcedRaceTraitEntries>
                <li>
                    <defName>NaturalMood</defName>
                    <degree>2</degree>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>Industriousness</defName>
                    <degree>2</degree>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>SpeedOffset</defName>
                    <degree>2</degree>
                    <chance>20</chance>
                </li>
                <li>
                    <defName>Greedy</defName>
                    <chance>20</chance>
                </li>
    </forcedRaceTraitEntries>


</generalSettings>    


<graphicPaths>
	<body>Beastmen/CamelMan/Body/</body>
