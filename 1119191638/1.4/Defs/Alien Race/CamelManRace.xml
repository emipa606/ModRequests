        <path>Beastmen/CamelMan/Head</path>
        <headtypeGraphics>
            <Male_AverageNormal>Beastmen/CamelMan/Head/Average_Normal</Male_AverageNormal>
            <Female_AverageNormal>Beastmen/CamelMan/Head/Average_Normal</Female_AverageNormal>
