		<defName>RT_SoundElectricBolt</defName>
		<context>MapOnly</context>
		<eventNames />
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Projectile/ElectricBolt</clipFolderPath>
					</li>
				</grains>

				<pitchRange>
					<min>0.9770588</min>
					<max>1.137647</max>
				</pitchRange>
				<distRange>
					<min>0</min>
					<max>50.40025</max>
				</distRange>
				<sustainLoop>False</sustainLoop>
