<?xml version="1.0" encoding="utf-8"?>
<Defs>


	<!-- ======================== Bantee Metroid ======================== -->


  <!-- General sounds it makes while being passive -->

	<SoundDef>
		<defName>RT_Bantee_Call</defName>
		<context>MapOnly</context>
		<eventNames />
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Pawns/Bantee/Call</clipFolderPath>
					</li>
				</grains>
				<volumeRange>
					<min>15.42672</min>
					<max>14.7734</max>
				</volumeRange>
				<pitchRange>
					<min>0.9770588</min>
					<max>1.137647</max>
				</pitchRange>
				<distRange>
					<min>0</min>
					<max>50.40025</max>
				</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>

  <!-- Angry noises, when it is aggressive -->

	<SoundDef>
		<defName>RT_Bantee_Angry</defName>
		<context>MapOnly</context>
		<eventNames />
		<maxVoices>2</maxVoices>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Pawns/Bantee/Call</clipFolderPath>
					</li>
				</grains>
				<volumeRange>
					<min>25</min>
					<max>25</max>
				</volumeRange>
				<pitchRange>
					<min>0.9541176</min>
					<max>1.137647</max>
				</pitchRange>
				<distRange>
					<min>0</min>
					<max>70</max>
				</distRange>
				<sustainLoop>False</sustainLoop>
				<repeatMode>NeverTwice</repeatMode>
			</li>
		</subSounds>
	</SoundDef>

