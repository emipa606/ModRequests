<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- GOLD PAN-->
  <WorkGiverDef>
    <defName>DoBillsUseGR_Pan</defName>
    <label>do bills with gold pan</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Mining</workType>
    <priorityInType>120</priorityInType>
    <fixedBillGiverDefs>
      <li>GR_Pan</li>
    </fixedBillGiverDefs>
    <verb>use</verb>
    <gerund>using</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

