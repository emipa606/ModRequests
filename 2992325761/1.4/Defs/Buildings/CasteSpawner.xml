		<size>(5,5)</size>
		<designationCategory>HumanHive</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<hasInteractionCell>False</hasInteractionCell>
		<surfaceType>Item</surfaceType>
