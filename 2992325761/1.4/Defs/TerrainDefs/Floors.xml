<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<TerrainDef ParentName="FloorBase">
		<defName>HiveOrganicFlooring</defName>
		<label>organic matter carpeting</label>
		<description>Floor made from organic matter. It is warm and wet underfoot.</description>
		<texturePath>Terrain/Floors/HiveFloor</texturePath>
		<color>(220,105,105)</color>
		<edgeType>FadeRough</edgeType>
		<renderPrecedence>70</renderPrecedence>
		<isPaintable>false</isPaintable>
		<statBases>
			<Beauty>0</Beauty>
			<WorkToBuild>200</WorkToBuild>
			<CleaningTimeFactor>1.3</CleaningTimeFactor>
		</statBases>
		<costList>
			<HiveOrganicMatter>2</HiveOrganicMatter>
		</costList>
		<researchPrerequisites>
			<li>Hive_OrganicSynthesis</li>
		</researchPrerequisites>
		<uiOrder>2020</uiOrder>
	</TerrainDef>
</Defs>