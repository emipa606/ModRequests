    <beardMeshSize>(1.5, 1.5)</beardMeshSize>
  </HeadTypeDef>

  <HeadTypeDef Name="NarrowBase" Abstract="True">
    <hairMeshSize>(1.3, 1.5)</hairMeshSize>
    <beardMeshSize>(1.5, 1.5)</beardMeshSize>
