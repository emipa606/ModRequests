<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RulePackDef>
		<defName>NamerPersonKobold</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[KoboldName] [KoboldNameSurfix]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>KoboldName</keyword>
					<path>NameBanks/name</path>
				</li>
				<li Class="Rule_File">
					<keyword>KoboldNameSurfix</keyword>
					<path>NameBanks/surfix</path>
				</li>
				<li Class="Rule_File">
					<keyword>KoboldNamePrefix</keyword>
					<path>NameBanks/prefix</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	

	<RulePackDef>
		<defName>NamerFactionKobold</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[Tribe] Tribe</li>
				<li>name->[Tribe] Clan</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>KoboldName</keyword>
					<path>NameBanks/Koboldname</path>
				</li>
				<li Class="Rule_File">
					<keyword>KoboldNameSurfix</keyword>
					<path>NameBanks/Koboldsurfix</path>
				</li>
				
				<li Class="Rule_File">
					<keyword>Tribe</keyword>
					<path>NameBanks/KoboldTribe</path>
				</li>
				
				<li Class="Rule_File">
					<keyword>color</keyword>
					<path>Words/Nouns/Colors</path>
				</li>
				<li Class="Rule_File">
					<keyword>Bodyparts</keyword>
					<path>Words/Nouns/Bodyparts</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
</Defs>