            </li>
            <li>
                <key>Shooting</key>
                <value>3</value>
            </li>
            <li>
                <key>Social</key>
                <value>2</value>
            </li>
        </skillGains>
        <spawnCategories>
            <li>KoboldBackstory</li>
        </spawnCategories>
    </AlienRace.AlienBackstoryDef>


