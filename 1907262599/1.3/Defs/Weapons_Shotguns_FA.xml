	<ThingDef Name="COBRAYT" ParentName="BaseMakeableGun_RA">
		<defName>Gun_CobrayTerminator_FA</defName>
		<label>Cobray Terminator</label>
		<description>The Cobray Terminator is a blow-foward single shot shotgun made in the 1980s. It's notorious for being very bad.</description>
		<graphicData>
			<texPath>Weapon/Shotgun/CobrayT</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.5</drawSize>
		</graphicData>
			<techLevel>Spacer</techLevel>
			<soundInteract>Interact_Shotgun</soundInteract>
		<recipeMaker>
			<researchPrerequisite>Gunsmithing</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<statBases>
			<WorkToMake>7500</WorkToMake>
			<Mass>4.6</Mass>
			<AccuracyTouch>0.85</AccuracyTouch>
			<AccuracyShort>0.35</AccuracyShort>
			<AccuracyMedium>0.10</AccuracyMedium>
			<AccuracyLong>0.1</AccuracyLong>
			<RangedWeapon_Cooldown>2.1</RangedWeapon_Cooldown>
		</statBases>
		<weaponTags>
			<li>SimpleGun</li>
		</weaponTags> <thingSetMakerTags> <li>RewardStandardHighFreq</li> </thingSetMakerTags> <generateCommonality>0.1</generateCommonality>
			<costList>
				<Steel>80</Steel>
				<ComponentIndustrial>3</ComponentIndustrial>
			</costList>
		<verbs>
		<li>
			<verbClass>Verb_Shoot</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>Bullet_12Gauge_FA</defaultProjectile>
			<warmupTime>1.7</warmupTime>
			<range>15.0</range>
			<burstShotCount>1</burstShotCount>
			<ticksBetweenBurstShots>0</ticksBetweenBurstShots>
			<soundCast>Shot_Shotgun</soundCast>
			<soundCastTail>GunTail_Medium</soundCastTail>
			<muzzleFlashScale>9</muzzleFlashScale>
		</li>
		</verbs>
		<tools>
		<li>
			<label>stock</label>
			<capacities>
			<li>Blunt</li>
			</capacities>
			<power>9</power>
			<cooldownTime>2.0</cooldownTime>
		</li>
		</tools>
		<comps>
			<li>
			<compClass>CompOversizedWeapon.CompOversizedWeapon</compClass>
			</li>
			</comps>
	</ThingDef>

