  	<!-- Knight -->
  <ThingDef ParentName="BaseMechanoidWalker">
    <defName>Mech_Knight</defName>
    <label>Knight</label>
    <description>The frontilne tank of a mechanoid, upgraded version of scyther with lower overall dps but tankier in health, equipped with a shield instead of left scythe for more tanking opportunity, aswel as equipped with personal shield bubble for intercepting incoming projectiles.</description>
    <tools>
      <li>
        <label>shield</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Shield</linkedBodyPartsGroup>
        <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
      </li>
      <li>
        <label>blade</label>
        <capacities>
          <li>Cut</li>
		  <li>Stab</li>
        </capacities>
        <power>20</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Blade</linkedBodyPartsGroup>
        <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>Knight</body>
      <baseHealthScale>1.32</baseHealthScale>
      <soundMeleeHitPawn>Pawn_Melee_MechanoidSlash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_MechanoidSlash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_MechanoidSlash_Miss</soundMeleeMiss>
      <detritusLeavings>
        <li>
          <def>ChunkMechanoidSlag</def>
          <texOverrideIndex>18</texOverrideIndex>
          <spawnChance>0.1</spawnChance>
        </li>
        <li>
          <def>ChunkMechanoidSlag</def>
          <texOverrideIndex>15</texOverrideIndex>
          <spawnChance>0.1</spawnChance>
        </li>
      </detritusLeavings>
    </race>
    <comps>
      <li Class="CompProperties_Shield" />
    </comps>
    <statBases>
      <BandwidthCost MayRequire="Ludeon.Rimworld.Biotech">2</BandwidthCost>
	  <ControlTakingTime>18</ControlTakingTime>
	  <EnergyShieldRechargeRate>0.5</EnergyShieldRechargeRate>
