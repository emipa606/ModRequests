<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Advanced bionic Rodian antenna -->

	<HediffDef ParentName="addedPartAdvanced">
		<defName>StarWarsRaces_AdvancedBionicRodianAntenna</defName>
		<label>advanced bionic Rodian antenna</label>
		<labelNoun>an advanced bionic Rodian antenna</labelNoun>
		<description>An installed advanced bionic Rodian antenna.</description>
		<descriptionHyperlinks><ThingDef>StarWarsRaces_AdvancedBionicRodianAntenna</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>StarWarsRaces_AdvancedBionicRodianAntenna</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.35</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>

	<ThingDef ParentName="EPIA_UpgradeBodyPartAdvancedBionicBase">
		<defName>StarWarsRaces_AdvancedBionicRodianAntenna</defName>
		<label>advanced bionic Rodian antenna</label>
		<description>An advanced artificial Rodian antenna. A collection of invisible subdermal microphones capture sound and transmit it to a biogel nerve interface that links directly with the auditory nerve. It's sensitive enough to hear a cat before you see it, can't be damaged by loud noise, and automatically tunes itself to pick out meaningful sounds in noisy environments.</description>
		<descriptionHyperlinks><RecipeDef>StarWarsRaces_InstallAdvancedBionicRodianAntenna</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>15</Plasteel>
			<ComponentSpacer>7</ComponentSpacer>
		</costList>
		<statBases>
		  <Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="EPIA_RecipeUpgradeAdvanced">
		<defName>EPIA_CreateStarWarsRaces_AdvancedBionicRodianAntenna</defName>
		<label>assemble advanced bionic Rodian antenna</label>
		<description>Upgrade bionic Rodian antenna into advanced bionic Rodian antenna.</description>
		<descriptionHyperlinks><ThingDef>StarWarsRaces_AdvancedBionicRodianAntenna</ThingDef></descriptionHyperlinks>
		<jobString>Upgrading bionic Rodian antenna.</jobString>
		<products>
			<StarWarsRaces_AdvancedBionicRodianAntenna>1</StarWarsRaces_AdvancedBionicRodianAntenna>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>StarWarsRaces_BionicRodianAntenna</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>StarWarsRaces_BionicRodianAntenna</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

    <RecipeDef ParentName="EPIASurgeryAdvancedBionic">
        <defName>StarWarsRaces_InstallAdvancedBionicRodianAntenna</defName>
        <label>install advanced bionic Rodian antenna</label>
        <description>Installs an advanced bionic Rodian antenna.</description>
        <descriptionHyperlinks>
			<ThingDef>StarWarsRaces_AdvancedBionicRodianAntenna</ThingDef>
			<HediffDef>StarWarsRaces_AdvancedBionicRodianAntenna</HediffDef>
		</descriptionHyperlinks>
        <jobString>Installing advanced bionic Rodian antenna.</jobString>
        <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>StarWarsRaces_AdvancedBionicRodianAntenna</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>StarWarsRaces_AdvancedBionicRodianAntenna</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>StarWarsRaces_LeftRodianAntenna</li>
            <li>StarWarsRaces_RightRodianAntenna</li>
        </appliedOnFixedBodyParts>
        <addsHediff>StarWarsRaces_AdvancedBionicRodianAntenna</addsHediff>
    </RecipeDef>

