<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Bionic Rodian antenna -->

	<HediffDef ParentName="AddedBodyPartBase">
		<defName>StarWarsRaces_BionicRodianAntenna</defName>
		<label>bionic Rodian antenna</label>
		<labelNoun>a bionic Rodian antenna</labelNoun>
		<description>An installed bionic Rodian antenna.</description>
		<descriptionHyperlinks><ThingDef>StarWarsRaces_BionicRodianAntenna</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>StarWarsRaces_BionicRodianAntenna</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>

	<ThingDef ParentName="BodyPartBionicBase">
		<defName>StarWarsRaces_BionicRodianAntenna</defName>
		<label>bionic Rodian antenna</label>
		<description>An advanced artificial Rodian antenna. A collection of invisible subdermal microphones capture sound and transmit it to a biogel nerve interface that links directly with the auditory nerve. It's sensitive enough to hear a cat before you see it, can't be damaged by loud noise, and automatically tunes itself to pick out meaningful sounds in noisy environments.</description>
		<descriptionHyperlinks><RecipeDef>StarWarsRaces_InstallBionicRodianAntenna</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>15</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<statBases>
		  <Mass>0.3</Mass>
		</statBases>
	</ThingDef>

    <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
        <defName>StarWarsRaces_InstallBionicRodianAntenna</defName>
        <label>install bionic Rodian antenna</label>
        <description>Installs an bionic Rodian antenna.</description>
        <descriptionHyperlinks>
			<ThingDef>StarWarsRaces_BionicRodianAntenna</ThingDef>
			<HediffDef>StarWarsRaces_BionicRodianAntenna</HediffDef>
		</descriptionHyperlinks>
        <jobString>Installing bionic Rodian antenna.</jobString>
        <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>StarWarsRaces_BionicRodianAntenna</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>StarWarsRaces_BionicRodianAntenna</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>StarWarsRaces_LeftRodianAntenna</li>
            <li>StarWarsRaces_RightRodianAntenna</li>
        </appliedOnFixedBodyParts>
        <addsHediff>StarWarsRaces_BionicRodianAntenna</addsHediff>
    </RecipeDef>

