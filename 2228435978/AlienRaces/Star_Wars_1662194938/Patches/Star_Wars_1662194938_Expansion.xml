<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering - Forked</li>
			<li>Expanded Prosthetics, Implants and Augmentations</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="EPOE_OrganicOptimizing"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>StarWarsRaces_LeftRodianAntenna</li>
						<li>StarWarsRaces_RightRodianAntenna</li>
						<li>StarWarsRaces_Togruta_Montral</li>
						<li>StarWarsRaces_Left_Lekku</li>
						<li>StarWarsRaces_Right_Lekku</li>
					</value>
				</li>
			</operations>
		</match>
		<nomatch Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>Defs/HediffDef[defName="StarWarsRaces_AdvancedBionicRodianAntenna" and @ParentName="addedPartAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="StarWarsRaces_AdvancedBionicRodianAntenna" and @ParentName="EPIA_UpgradeBodyPartAdvancedBionicBase"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="EPIA_CreateStarWarsRaces_AdvancedBionicRodianAntenna" and @ParentName="EPIA_RecipeUpgradeAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="StarWarsRaces_InstallAdvancedBionicRodianAntenna" and @ParentName="EPIASurgeryAdvancedBionic"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/HediffDef[defName="StarWarsRaces_AdvancedBionicTogrutaMontral" and @ParentName="addedPartAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="StarWarsRaces_AdvancedBionicTogrutaMontral" and @ParentName="EPIA_UpgradeBodyPartAdvancedBionicBase"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="EPIA_CreateStarWarsRaces_AdvancedBionicTogrutaMontral" and @ParentName="EPIA_RecipeUpgradeAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="StarWarsRaces_InstallAdvancedBionicTogrutaMontral" and @ParentName="EPIASurgeryAdvancedBionic"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/HediffDef[defName="StarWarsRaces_AdvancedBionicTwilekLekku" and @ParentName="addedPartAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="StarWarsRaces_AdvancedBionicTwilekLekku" and @ParentName="EPIA_UpgradeBodyPartAdvancedBionicBase"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="EPIA_CreateStarWarsRaces_AdvancedBionicTwilekLekku" and @ParentName="EPIA_RecipeUpgradeAdvanced"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="StarWarsRaces_InstallAdvancedBionicTwilekLekku" and @ParentName="EPIASurgeryAdvancedBionic"]</xpath>
				</li>
			</operations>
		</nomatch>
	</Operation>

</Patch>