<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering - Forked</li>
			<li>Expanded Prosthetics, Implants and Augmentations</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="EPOE_OrganicOptimizing"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>ShoulderManto</li>
						<li>InsectLeg</li>
						<li>HearingAppendix</li>
						<li>EyeSet</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="EPOE_InstinctOptimizingEyes"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>EyeSet</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="EPOE_InstinctOptimizingHands"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>Pincer</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="InstallTacticalCorneaImplant"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>EyeSet</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="InstallMuscleStimulatorArms"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>ShoulderManto</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="InstallMuscleStimulatorLegs"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>InsectLeg</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>