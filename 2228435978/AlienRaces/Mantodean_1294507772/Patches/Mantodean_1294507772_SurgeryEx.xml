<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Mantodean"]/alienRace/generalSettings/humanRecipeImport</xpath>
				<success>Invert</success>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Mantodean"]/alienRace/generalSettings</xpath>
				<value>
					<humanRecipeImport>true</humanRecipeImport>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>