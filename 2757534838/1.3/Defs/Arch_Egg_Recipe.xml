<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
      <defName>CreateArchoegg</defName>
      <label>Create Archotech egg</label>
      <description>Create an egg of Archotech Human.</description>
      
<jobString>Making Archotech Spawner egg</jobString>  

      <allowMixingIngredients>false</allowMixingIngredients>
	  <skillRequirements>
		<Crafting>20</Crafting>
      </skillRequirements>
      <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>ArchotechScrap</li>
				</thingDefs>
			</filter>
			<count>400</count>
		</li>
	  </ingredients>
      <products>
         <ArchFarming_FEgg>1</ArchFarming_FEgg>
      </products>
      <workAmount>10000</workAmount>
      <workSkill>Crafting</workSkill>
    <effectWorking>Repair</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
	<recipeUsers>
		<li>ArchBench</li>
	
     </recipeUsers>
	</RecipeDef>

</Defs>