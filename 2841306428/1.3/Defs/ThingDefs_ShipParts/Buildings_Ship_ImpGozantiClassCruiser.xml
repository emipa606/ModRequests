  <ThingDef ParentName="CEC_ShipPartImpGozanti">
    <defName>Ship_ImpGozantiClassCruiser_Disk</defName>
    <label>gozanti class cruiser (surveillance) disk</label>
    <graphicData>
      <texPath>ShipParts/ImpGozantiClassCruiserDisk</texPath>
      <drawSize>(13,13)</drawSize>
    </graphicData>
    <tradeability>None</tradeability>
    <size>(13,13)</size>
    <costList>
      <O21_OR_Durasteel>150</O21_OR_Durasteel>
      <O21_OR_ComponentHypertech>6</O21_OR_ComponentHypertech>
      <Gold>70</Gold>
      <AIPersonaCore>1</AIPersonaCore>
    </costList>
    <statBases>
      <MaxHitPoints>33800</MaxHitPoints>
      <WorkToBuild>52000</WorkToBuild>
    </statBases>
    <rotatable>false</rotatable>
    <altitudeLayer>WorldClipper</altitudeLayer>
    <comps>
      <li Class="CompProperties_ShipPart"/>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>1000</basePowerConsumption>
        <transmitsPower>false</transmitsPower>
      </li>
      <li Class="CompProperties_LongRangeMineralScannerSpaceAI"/>
      <li Class="CompProperties_ShipHeat">
        <compClass>CompShipHeat</compClass>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_BuildingConsciousness">
        <holoHediff>SoSHologramMachine</holoHediff>
        <holoWeapon>HologramLaser</holoWeapon>
        <holoWeaponMelee>HologramSword</holoWeaponMelee>
        <holoShield>HologramShield</holoShield>
      </li>
    </comps>
  </ThingDef>

  <!--Engine-->
  <ThingDef ParentName="CEC_ShipEngineBase">
    <defName>Ship_ImpGozantiClassCruiser_Engine</defName>
    <label>gozanti class cruiser (imperial) engine</label>
    <description>...</description>
    <graphicData>
      <texPath>ShipParts/ImpGozantiClassCruiserEngineBlock</texPath>
      <drawSize>(3,9)</drawSize>
    </graphicData>
    <size>(3,1)</size>
    <designatorDropdown>CEC_IGCC_Dropdown</designatorDropdown>
    <uiIconScale>0.5</uiIconScale>
    <researchPrerequisites>
      <li>CEC_IGCC_Parts</li>
    </researchPrerequisites>
    <costList>
      <O21_OR_Durasteel>200</O21_OR_Durasteel>
      <O21_OR_ComponentHypertech>2</O21_OR_ComponentHypertech>
    </costList>
    <statBases>
      <MaxHitPoints>2000</MaxHitPoints>
      <WorkToBuild>22000</WorkToBuild>
    </statBases>
    <comps>
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>1000.0</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <fuelFilter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>false</showFuelGizmo>
        <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
        <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
      <li Class="CompProperties_EngineTrail">
        <thrust>2</thrust>
      </li>
    </comps>
  </ThingDef>
  
