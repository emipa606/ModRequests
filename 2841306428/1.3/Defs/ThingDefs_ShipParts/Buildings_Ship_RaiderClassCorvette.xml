	<ThingDef ParentName="CEC_ShipEngineBase">
		<defName>Ship_RaiderClassCorvette_Engine</defName>
		<label>raider-class corvette engine</label>
    <description>...</description>
		<graphicData>
			<texPath>ShipParts3/RaiderClassCorvetteEngine</texPath>
			<drawSize>(9,2)</drawSize>
		</graphicData>
    <size>(9,2)</size>
