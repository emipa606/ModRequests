<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="OR_ApparelFabricatedBase">
		<defName>CEC_Rebreather</defName>
		<label>rebreather</label>
		<description>A rebreather designed to keep the user alive in outer space. Includes small thrusters for movement.</description>
		<techLevel>Ultra</techLevel>
		<tickerType>Normal</tickerType>
		<recipeMaker>
			<researchPrerequisite>CEC_Basics</researchPrerequisite>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
		</recipeMaker>
		<graphicData>
			<texPath>Apparel/Rebreather_north</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.90</drawSize>
			<color>(255, 255, 255)</color>
		</graphicData>
		<statBases>
			<WorkToMake>12000</WorkToMake>
			<MaxHitPoints>300</MaxHitPoints>
			<Mass>4</Mass>
			<Flammability>0.3</Flammability>
			<EquipDelay>6</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.25</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelUtility</li>
		</thingCategories>
		<costList>
			<O21_OR_Durasteel>85</O21_OR_Durasteel>
			<O21_OR_ComponentHypertech>3</O21_OR_ComponentHypertech>
		</costList>
		<apparel>
			<scoreOffset>4</scoreOffset>
			<wornGraphicPath>Apparel/Rebreather</wornGraphicPath>
			<wornGraphicData>
				<renderUtilityAsPack>true</renderUtilityAsPack>
				<east>
					<offset>(-0.3,0.1)</offset>
					<thin>
						<offset>(0.05,0.1)</offset>
					</thin>
					<hulk>
						<offset>(-0.1,0.15)</offset>
					</hulk>
					<fat>
						<offset>(-0.15,0.1)</offset>
					</fat>
				</east>
				<west>
					<offset>(0.35,0.1)</offset>
					<thin>
						<offset>(-0.05,0.1)</offset>
					</thin>
					<hulk>
						<offset>(0.1,0.1)</offset>
					</hulk>
					<fat>
						<offset>(0.15,0.1)</offset>
					</fat>
				</west>
				<male>
					<scale>(0.75,0.75)</scale>
				</male>
				<female>
					<scale>(0.75,0.75)</scale>
				</female>
				<thin>
					<scale>(0.7,0.7)</scale>
				</thin>
				<hulk>
					<scale>(0.9,0.9)</scale>
				</hulk>
				<fat>
					<scale>(0.9,0.9)</scale>
				</fat>
			</wornGraphicData>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<careIfDamaged>false</careIfDamaged>
			<wearPerDay>0</wearPerDay>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<layers>
				<li>Shell</li>
        <li>Overhead</li>
        <li>Belt</li>
			</layers>
			<tags>
				<li>EVA</li>
			</tags>
			<defaultOutfitTags Inherit="False">
			</defaultOutfitTags>
		</apparel>
	</ThingDef>

