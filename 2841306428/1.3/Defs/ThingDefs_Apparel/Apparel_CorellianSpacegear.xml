  <ThingDef ParentName="OR_UniformBodyFabricatedBase">
    <defName>CEC_Bodysuit_Warm</defName>
    <label>warm durafiber bodysuit</label>
    <description>A warm bodysuit worn by many across the galaxy, its warmig properties along with the tight fit and flexibility make it a popular choice for wearing under full sets of armour for space combat and often included as part of a uniform in space military.</description>
    <graphicData>
      <texPath>OuterRim/Apparel/CloneTrooper/PhaseTwo/Blank/Undersuit/Apparel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <recipeMaker>
      <researchPrerequisite>CEC_Basics</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <statBases>
      <Insulation_Cold>45</Insulation_Cold>
    </statBases>
    <apparel>
      <tags Inherit="False">
        <li>ORBodysuit_Warm</li>
      </tags>
      <wornGraphicPath>OuterRim/Apparel/CloneTrooper/PhaseTwo/Blank/Undersuit/Apparel</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <defaultOutfitTags Inherit="False">
	    </defaultOutfitTags>
    </apparel>
    <costList>
      <O21_OR_Durafiber>85</O21_OR_Durafiber>
    </costList>
  </ThingDef>

  <ThingDef ParentName="OR_ApparelFabricatedBase">
    <defName>CEC_SimpleEVA_Suit</defName>
    <label>simple EVA suit</label>
    <description>A airtight suit designed to keep the occupant alive in outer space.  Provides resistance to toxic airborne particulates. Airtight helmet and rebreather needed.</description>
    <graphicData>
      <texPath>Apparel/SimpleEVASuit/Apparel_Male_north</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>1.20</drawSize>
    </graphicData>
    <recipeMaker>
      <researchPrerequisite>CEC_Basics</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <statBases>
      <Mass>0.5</Mass>
      <WorkToMake>8000</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <ArmorRating_Sharp>0.28</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.20</ArmorRating_Blunt>
      <ArmorRating_Heat>0.15</ArmorRating_Heat>
      <Insulation_Cold>50</Insulation_Cold>
      <Insulation_Heat>25</Insulation_Heat>
      <EquipDelay>6</EquipDelay>
    </statBases>
    <apparel>
      <layers>
        <li>Middle</li>
      </layers>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Arms</li>
        <li>Hands</li>
        <li>Legs</li>
        <li>Feet</li>
      </bodyPartGroups>
      <tags Inherit="False">
        <li>SimpleEVA</li>
      </tags>
      <wornGraphicPath>Apparel/SimpleEVASuit/Apparel</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <defaultOutfitTags Inherit="False">
      </defaultOutfitTags>
    </apparel>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
	  <costStuffCount>45</costStuffCount>
    <costList>
      <O21_OR_Durafiber>15</O21_OR_Durafiber>
    </costList>
    <equippedStatOffsets>
      <ToxicSensitivity>-0.4</ToxicSensitivity>
    </equippedStatOffsets>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="OR_HelmetFabricatedBase">
    <defName>CEC_SimpleEVA_Helmet</defName>
    <label>simple EVA helmet</label>
    <description>A airtight helmet designed to keep the occupant alive in outer space. Provides resistance to toxic airborne particulates.</description>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>CEC_Basics</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Apparel/SimpleEVASuit/Helmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costList Inherit="False">
      <O21_OR_Durasteel>30</O21_OR_Durasteel>
      <O21_OR_ComponentHypertech>1</O21_OR_ComponentHypertech>
    </costList>
    <statBases>
      <Mass>1</Mass>
      <WorkToMake>3000</WorkToMake>
      <MaxHitPoints>150</MaxHitPoints>
      <ArmorRating_Sharp>0.43</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.47</ArmorRating_Blunt>
      <ArmorRating_Heat>0.71</ArmorRating_Heat>
      <Insulation_Cold>50</Insulation_Cold>
      <Insulation_Heat>25</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.04</MoveSpeed>
      <ToxicSensitivity>-0.6</ToxicSensitivity>
    </equippedStatOffsets>
    <generateCommonality>0</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/SimpleEVASuit/Helmet</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>EVA</li>
        <li>SimpleEVA</li>
      </tags>
      <defaultOutfitTags Inherit="False">
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

</Defs>