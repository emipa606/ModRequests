<!-- ==================================== SSTHD Trooper Helmet Base =======================================-->

  <ThingDef Name="SSTHDApparelArmorTrooperHelmetBase" ParentName="SSTHDArmorHelmetMakeableBase" Abstract="True">
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>PoweredArmor</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/SSTHD/Display/SSTHD_TrooperHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <WorkToMake>15750</WorkToMake>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>1</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.95</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.5</ArmorRating_Blunt>
      <ArmorRating_Heat>0.5</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <costList>
      <ComponentSpacer>1</ComponentSpacer>
      <Plasteel>30</Plasteel>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/SSTHD/Headgear/SSTHD_TrooperHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
    <comps>
    </comps>
  </ThingDef>

  <ThingDef ParentName="SSTHDApparelArmorTrooperHelmetBase">
    <defName>SSTHD_TrooperHelmet</defName>
    <label>Standard M-3 Trooper Helmet</label>
    <description>The M-3 helmet is a type of protective gear used by the Federation. It is mainly worn by the Mobile Infantry and ground-side members of the Fleet. It has an integrated radio headset built into the right side of the helmet which is activated by pushing a large black button on the outside. This version has no insignia and is used by the mobile infantry</description>
    <apparel>
      <tags>
        <li>SpacerMilitary</li>
        <li>RoyalCombatGear</li>
      </tags>
    </apparel>
  </ThingDef>
  
