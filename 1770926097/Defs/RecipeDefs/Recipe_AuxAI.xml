<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef ParentName="RecipeSyntheticOrgan">
		<defName>CreateAuxAIConstruction</defName>
		<label>Craft auxiliary AI (constructing)</label>
		<description>Creates a construction auxiliary AI. Needs gold and energized components for highest quality circuitry.</description>
		<jobString>Creating synthetic organ.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<ConstructorCore>1</ConstructorCore>
		</products>
	</RecipeDef>
