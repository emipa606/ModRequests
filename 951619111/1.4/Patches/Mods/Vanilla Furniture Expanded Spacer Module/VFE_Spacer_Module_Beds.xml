<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<!-- Vanilla Furniture Expanded Spacer Module -->
	
	<Operation Class="PatchOperationFindMod">
		
    	<mods>
        	<li>Vanilla Furniture Expanded - Spacer Module</li>
    	</mods>
		
    	<match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
			
			<!-- Illuminated End Table -->
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "Table_IlluminatedEndTable"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>10</basePowerConsumption>
				</value>
			</li>
			
			<!-- Illuminated Dresser -->
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "Table_IlluminatedDresser"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>10</basePowerConsumption>
				</value>
			</li>
			
        </operations>
    	</match>
	
	</Operation>

</Patch>