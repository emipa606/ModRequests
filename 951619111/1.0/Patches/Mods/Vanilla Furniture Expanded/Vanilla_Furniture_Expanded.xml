<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<!-- Vanilla Furniture Expanded -->
	
	<Operation Class="PatchOperationFindMod">
		
    	<mods>
        	<li>Vanilla Furniture Expanded</li>
    	</mods>
		
    	<match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
			
			<!-- Radio Industrial -->
			
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName = "Radio_Industrial"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>40</basePowerConsumption>
				</value>
			</li>
			
			<!-- Radio Spacer -->
			
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName = "Radio_Spacer"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>65</basePowerConsumption>
				</value>
			</li>
			
			<!-- Arcade Machine -->
			
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName = "Joy_Arcade"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>40</basePowerConsumption>
				</value>
			</li>
			
			<!-- Inudstrial Computer -->
			
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName = "Joy_InudstrialComputer"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>40</basePowerConsumption>
				</value>
			</li>
			
			<!-- Modern Computer -->
			
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName = "Joy_ModernComputer"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>60</basePowerConsumption>
				</value>
			</li>
			
        </operations>
    	</match>
	
	</Operation>

</Patch>