<Patch>

<!-- VVE Patch -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Vehicles Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

        <!-- costList -->
				<li Class="PatchOperationReplace">
				<xpath>Defs/Vehicles.VehicleBuildDef[defName="BA_EinheitsPKW_sena_Blueprint"]/costList</xpath>
					<value>
						<costList>
							<VVE_EngineBlock>1</VVE_EngineBlock>
              <VVE_CarAlternator>2</VVE_CarAlternator>
              <VVE_CarExhaust>2</VVE_CarExhaust>
              <VVE_CarSuspension>8</VVE_CarSuspension>
              <VVE_CarWheel>4</VVE_CarWheel>
              <Steel>380</Steel>
              <ComponentIndustrial>6</ComponentIndustrial>
              <MedicineIndustrial>10</MedicineIndustrial>
						</costList>
					</value>
				</li>

        <!-- researchPrerequisites -->
				<li Class="PatchOperationReplace">
				<xpath>Defs/Vehicles.VehicleBuildDef[defName="BA_EinheitsPKW_sena_Blueprint"]/researchPrerequisites</xpath>
					<value>
						<researchPrerequisites><li>VVE_ComplexVehicles</li></researchPrerequisites>
					</value>
				</li>

        <!-- designationCategory -->
				<!-- <li Class="PatchOperationReplace">
				<xpath>Defs/Vehicles.VehicleBuildDef[defName="BA_EinheitsPKW_sena_Blueprint"]/designationCategory</xpath>
					<value>
						<designationCategory>VVE_Vehicles</designationCategory>
					</value>
				</li> -->

			</operations>
		</match>
	</Operation>

</Patch>