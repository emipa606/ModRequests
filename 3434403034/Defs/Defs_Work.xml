<?xml version="1.0" encoding="utf-8"?>
<Defs>



	<ThingCategoryDef>
		<defName>BuildingsPrisonerRestraints</defName>
		<label>prisoner restraints</label>
		<parent>Buildings</parent>
	</ThingCategoryDef>
	
	
	
	<WorkGiverDef>
		<defName>Warden_TakeToTortureBed</defName>
		<label>put prisoners in resistance restraints</label>
		<giverClass>ResistanceRestraintsMod.WorkGiver_Warden_TakeToTortureBed</giverClass>
		<workType>Warden</workType>
		<verb>put resistance restraints on</verb>
		<gerund>putting resistance restraints on</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

	<WorkGiverDef>
		<defName>Warden_Warden_RemoveImmobility</defName>
		<label>remove prisoners from resistance restraints</label>
		<giverClass>ResistanceRestraintsMod.WorkGiver_Warden_RemoveImmobility</giverClass>
		<workType>Warden</workType>
		<verb>remove resistance restraints from</verb>
		<gerund>removing resistance restraints from</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

	<WorkGiverDef>
		<defName>Warden_Warden_StripPawnTortureBed</defName>
		<label>strip pawn in resistance restraints</label>
		<giverClass>ResistanceRestraintsMod.WorkGiver_Warden_StripPawnTortureBed</giverClass>
		<workType>Warden</workType>
		<verb>strip pawn in resistance restraints</verb>
		<gerund>stripping pawn in resistance restraints</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>





	<JobDef>
		<defName>SilkCircuit_RemoveImmobility</defName>
		<driverClass>ResistanceRestraintsMod.JobDriver_RemoveImmobility</driverClass>
		<reportString>Removing immobility from TargetA.</reportString>
		<playerInterruptible>true</playerInterruptible>
	</JobDef>


	
	
	
	
	














</Defs>