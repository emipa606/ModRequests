<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>AncientJeep_Blueprint</defName>
		<label>Humvee</label>
		<description>A military transport vehicle. Could hold upto 1100kg cargo and 5 passengers including gunner seat.</description>
		<designationCategory>VF_Vehicles</designationCategory>
		
		<graphicData>
			<texPath>Vehicles/AncientJeep/AncientJeep</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(5,5)</drawSize>
		</graphicData>
		
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>9500</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		
		<size>(3,5)</size>
		
		<researchPrerequisites>
			<li>Machining</li>
			<li>PrecisionRifling</li>
		</researchPrerequisites>
		
		<costList>
			<HumveeEngine>1</HumveeEngine>
			<Steel>450</Steel>
			<ComponentIndustrial>8</ComponentIndustrial>
		</costList>
		
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		
		<thingToSpawn>AncientJeep</thingToSpawn>
		
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>
</Defs>