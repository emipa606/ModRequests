<?xml version="1.0" encoding="utf-8" ?>

<Defs>	
	<Vehicles.VehicleDef ParentName="BaseVehiclePawn">
		<defName>AncientJeep</defName>
		<label>Humvee</label>
		<description>A military transport vehicle. Could hold upto 1100kg cargo and 5 passengers including exposed gunner seat.</description>
		<graphicData>
			<texPath>Vehicles/AncientJeep/AncientJeep</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(5,5)</drawSize>
			<color>(240, 240, 240)</color>
			<colorTwo>(131, 140, 119)</colorTwo>
			<colorThree>(220, 200, 140)</colorThree>
		</graphicData>
		
		<fillPercent>1</fillPercent>
		
		<buildDef>AncientJeep_Blueprint</buildDef>
		
		<size>(3,5)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Combat</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		
		<soundSustainersOnEvent>
			<li>
				<key>(IgnitionOn, IgnitionOff)</key>
				<value>HumveeIdle</value>
			</li>
		</soundSustainersOnEvent>
		<soundOneShotsOnEvent Inherit = "False">
			<li>
				<key>PawnEntered</key>
				<value>HumveeDoor</value>
			</li>
			<li>
				<key>PawnExited</key>
				<value>HumveeDoor</value>
			</li>
			<li>
				<key>IgnitionOn</key>
				<value>HumveeStartup</value>
			</li>
			<li>
				<key>IgnitionOff</key>
				<value>HumveeOff</value>
			</li>
		</soundOneShotsOnEvent>

		<statBases>
			<ArmorRating_Blunt>0.44</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.78</ArmorRating_Sharp>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<Flammability>0.5</Flammability>
			<MarketValue>2580</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>700</Mass>
			<MoveSpeed>9</MoveSpeed>
			<CargoCapacity>1100</CargoCapacity>
			<RepairRate>0.5</RepairRate>
			<AccelerationRate MayRequire="oskarpotocki.vanillavehiclesexpanded">0.06</AccelerationRate>
		</vehicleStats>
		
		<race>
			<baseBodySize>4</baseBodySize>
		</race>
		
		<properties>
			<customRoadCosts AssignDefaults = "0.35"/>
		
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
				</li>
				<li>
					<key>gunner</key>
					<label>Gunner</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					<exposed>true</exposed>
					
					<hitbox>
						<side>Body</side>
					</hitbox>
					
					<handlingTypes>Turret</handlingTypes>
					<turretIds>
						<li>mainTurret</li>
					</turretIds>
				
					<pawnRenderer>
						<layer>1</layer>
						<layerNorth>1</layerNorth>
						<layerSouth>1</layerSouth>
						<angleNorth>0</angleNorth>
						<angleEast>0</angleEast>
						<angleNorthEast>0</angleNorthEast>
						<angleSouthEast>0</angleSouthEast>
						<drawOffsetNorth>(0,0,0.6)</drawOffsetNorth>
						<drawOffsetNorthEast>(0,0,0.7)</drawOffsetNorthEast>
						<drawOffsetNorthWest>(0,0,0.7)</drawOffsetNorthWest>
						<drawOffsetSouth>(0,0,1.4)</drawOffsetSouth>
						<drawOffsetSouthEast>(-0.4,0,1.2)</drawOffsetSouthEast>
						<drawOffsetSouthWest>(0.4,0,1.2)</drawOffsetSouthWest>
						<drawOffsetEast>(-0.3,0,0.7)</drawOffsetEast>
					</pawnRenderer>
				</li>
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>3</slots>
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>75</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>External</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<radius>3</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, 0)</li>
						<li>(1, 0)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftWheel</key>
				<label>Front Left Wheel</label>
				<health>50</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightWheel</key>
				<label>Front Right Wheel</label>
				<health>50</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackLeftWheel</key>
				<label>Back Left Wheel</label>
				<health>50</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightWheel</key>
				<label>Back Right Wheel</label>
				<health>50</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftSuspension</key>
				<label>Front Left Suspension</label>
				<health>30</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightSuspension</key>
				<label>Front Right Suspension</label>
				<health>30</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackLeftSuspension</key>
				<label>Back Left Suspension</label>
				<health>30</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightSuspension</key>
				<label>Back Right Suspension</label>
				<health>30</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>Gearbox</key>
				<label>Gearbox</label>
				<health>45</health>
				<depth>Internal</depth>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontArmorPlating</key>
				<label>Front Armor Plating</label>
				<health>100</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0.72</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.38</ArmorRating_Sharp>
				</armor>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>LeftArmorPlating</key>
				<label>Left Armor Plating</label>
				<health>80</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0.72</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.38</ArmorRating_Sharp>
				</armor>
				
				<hitbox>
					<side>Left</side>
				</hitbox>
			</li>
			<li>
				<key>RightArmorPlating</key>
				<label>Right Armor Plating</label>
				<health>80</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0.72</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.38</ArmorRating_Sharp>
				</armor>
				
				<hitbox>
					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>Chemtank</key>
				<label>Chemtank</label>
				<health>40</health>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, 0)</li>
						<li>(1, 0)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>FrontBumper</key>
				<label>Front Bumper</label>
				<health>20</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>Upholstery</key>
				<label>Upholstery</label>
				<health>30</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Dashboard</key>
				<label>Dashboard</label>
				<health>45</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>SteeringWheel</key>
				<label>Steering Wheel</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Alternator</key>
				<label>Alternator</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Roof</key>
				<label>Roof</label>
				<health>85</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0.72</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.38</ArmorRating_Sharp>
				</armor>
			</li>
		</components>

		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<fuelConsumptionRate>20.5</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.8</fuelConsumptionWorldMultiplier>
				<fuelCapacity>100</fuelCapacity>
			</li>
			<li Class="Vehicles.CompProperties_VehicleTurrets">
				<turrets>
					<li>
						<turretDef>AncientJeep_MainTurret</turretDef>
						
						<renderProperties>
							<north>(0, 1.05)</north>
							<northWest>(-0.4, 0.8)</northWest>
							<northEast>(0.4, 0.8)</northEast>
							<south>(0, 0.3)</south>
							<southEast>(0.2, 0.3)</southEast>
							<southWest>(-0.2, 0.3)</southWest>
							<east>(0.45, 0.52)</east>
						</renderProperties>
						
						<gizmoLabel>Main Turret</gizmoLabel>
						
						<angleRestricted>(315,45)</angleRestricted>
						<aimPieOffset>(0, 0.65)</aimPieOffset>
						
						<key>mainTurret</key>
					</li>
				</turrets>
			</li>
		</comps>
	</Vehicles.VehicleDef>
</Defs>