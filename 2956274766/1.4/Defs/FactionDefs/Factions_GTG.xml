<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef Name="GTGFactionBase" Abstract="True">
    <autoFlee>false</autoFlee>
    <categoryTag>Outlander</categoryTag>
    <settlementGenerationWeight>1.5</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxCountAtGameStart>1</maxCountAtGameStart>
    <canSiege>false</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>department manager</leaderTitle>
    <leaderForceGenerateNewPawn>true</leaderForceGenerateNewPawn>
    <factionNameMaker>NamerFactionOutlander</factionNameMaker>
    <settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
    <pawnSingular>citizen</pawnSingular>
    <pawnsPlural>citizens</pawnsPlural>
    <allowedCultures>
      <li>Astropolitan</li>
    </allowedCultures>
    <disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_Animist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
    </disallowedMemes>
    <requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Transhumanist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">HumanPrimacy</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">HighLife</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Supremacist</li>
    </requiredMemes>
    <disallowedPrecepts>
      <!--Slavery-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Disapproved</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Horrible</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Abhorrent</li>
      <!--OrganUse-->
      <li MayRequire="Ludeon.RimWorld.Ideology">OrganUse_Abhorrent</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">OrganUse_HorribleSellOK</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">OrganUse_HorribleNoSell</li>
      <!--Execution-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Execution_Abhorrent</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Execution_DontCare</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Execution_HorribleIfInnocent</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Execution_RespectedIfGuilty</li>
      <!--Cannibalism-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Cannibalism_Abhorrent</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Cannibalism_Disapproved</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Cannibalism_Horrible</li>
      <!--Corpses-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Corpses_Ugly</li>
      <!--Apostasy-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Apostasy_Horrible</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Apostasy_Disapproved</li>
      <!--InsectMeatEating-->
      <li MayRequire="Ludeon.RimWorld.Ideology">InsectMeatEating_Despised_Classic</li>
      <!--Research-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_None</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_ExtremelySlow</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_VerySlow</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_Slow</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_Normal</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Research_Fast</li>
      <!--Lovin-->
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_Horrible</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_Prohibited</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_SpouseOnly_Mild</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_SpouseOnly_Moderate</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_SpouseOnly_Strict</li>
    </disallowedPrecepts>
    <structureMemeWeights>
      <Structure_Archist MayRequire="Ludeon.RimWorld.Ideology">1</Structure_Archist>
    </structureMemeWeights>
    <xenotypeSet>
      <xenotypeChances>
        <Eternals MayRequire="Ludeon.RimWorld.Biotech">0.025</Eternals>
        <Titans MayRequire="Ludeon.RimWorld.Biotech">0.1</Titans>
        <Hussar MayRequire="Ludeon.RimWorld.Biotech">0.05</Hussar>
        <Dirtmole MayRequire="Ludeon.RimWorld.Biotech">0.05</Dirtmole>
        <Genie MayRequire="Ludeon.RimWorld.Biotech">0.025</Genie>
        <Neanderthal MayRequire="Ludeon.RimWorld.Biotech">0.025</Neanderthal>
      </xenotypeChances>
    </xenotypeSet>
    <techLevel>Ultra</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>ImperialCommon</li>
        </categories>
        <commonality>0.2</commonality>
      </li>
      <li>
        <categories>
          <li>Outlander</li>
        </categories>
        <commonality>0.6</commonality>
      </li>
      <li>
        <categories>
          <li>Pirate</li>
        </categories>
        <commonality>0.2</commonality>
      </li>
    </backstoryFilters>
    <allowedArrivalTemperatureRange>-100~100</allowedArrivalTemperatureRange>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <colorSpectrum>
      <li>(1, 1, 1)</li>
    </colorSpectrum>
    <raidCommonalityFromPointsCurve>
      <points>
        <li>(0, 1)</li>
        <!--Constant 1 at all points levels-->
      </points>
    </raidCommonalityFromPointsCurve>
    <raidLootMaker>GTGFactionLootMaker</raidLootMaker>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(1500,500)</li>
        <!-- Can always use relatively strong pawns, because empire doesn't really have weak ones -->
        <li>(2500, 1501)</li>
        <li>(4500, 2501)</li>
        <li>(4501, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <maxConfigurableAtWorldCreation>1</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>1500</configurationListOrderPriority>
    <rescueesCanJoin>true</rescueesCanJoin>
    <raidLootValueFromPointsCurve>
      <points>
        <li>(35,     30)</li>
        <li>(100,   180)</li>
        <li>(1000,  800)</li>
        <li>(2000, 1500)</li>
        <li>(4000, 2000)</li>
      </points>
    </raidLootValueFromPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <Soldier>2</Soldier>
          <EliteSoldierGuard>2</EliteSoldierGuard>
          <SworderGuard>2</SworderGuard>
          <Trader>2</Trader>
        </options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <Soldier>10</Soldier>
          <EliteSoldierFighter>10</EliteSoldierFighter>
        </options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <Soldier>20</Soldier>
          <EliteSoldierFighter>20</EliteSoldierFighter>
        </options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <SworderFighter>20</SworderFighter>
        </options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <Soldier>10</Soldier>
          <EliteSoldierFighter>10</EliteSoldierFighter>
          <SworderFighter>5</SworderFighter>
          <Trader>1</Trader>
          <Executor>1</Executor>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <Trader>1</Trader>
        </traders>
        <carriers>
          <SyntheticAnimal_Colossus>10</SyntheticAnimal_Colossus>
        </carriers>
        <guards>
          <EliteSoldierGuard>10</EliteSoldierGuard>
          <SworderGuard>5</SworderGuard>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <Soldier>2</Soldier>
          <EliteSoldierGuard>2</EliteSoldierGuard>
          <SworderGuard>2</SworderGuard>
          <Trader>2</Trader>
        </options>
      </li>
    </pawnGroupMakers>
    <modExtensions>
      <li Class="VFECore.FactionDefExtension" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
        <startingGoodwillByFactionDefs>
          <PlayerColony>0~100</PlayerColony>
        </startingGoodwillByFactionDefs>
      </li>
    </modExtensions>
  </FactionDef>
  <ThingSetMakerDef>
    <defName>GTGFactionLootMaker</defName>
    <root Class="ThingSetMaker_MarketValue">
      <fixedParams>
        <filter>
          <thingDefs>
            <li>Gold</li>
            <li>MedicineUltratech</li>
            <li>Uranium</li>
          </thingDefs>
        </filter>
      </fixedParams>
    </root>
  </ThingSetMakerDef>
  <FactionDef ParentName="GTGFactionBase">
    <defName>GTGCivilianIndustrial</defName>
    <label>civilianIndustrial conglomerate</label>
    <fixedName>civilianIndustrial conglomerate</fixedName>
    <description>This faction includes the food caravans that sell various raw and cooked foods;the animal caravans that sell various animals;and the natural textiles caravans that sell various natural textiles.</description>
    <factionIconPath>Icon/GTGCivilianIndustrial</factionIconPath>
    <caravanTraderKinds>
      <li>Caravan_GTGCivilianIndustrial_Foods</li>
      <li>Caravan_GTGCivilianIndustrial_VanillaAnimals</li>
      <li>Caravan_GTGCivilianIndustrial_AllTypeAnimals</li>
      <li>Caravan_GTGCivilianIndustrial_NaturalTextiles</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_GTGCivilianIndustrial</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>FactionBase_GTGCivilianIndustrial</li>
    </baseTraderKinds>
  </FactionDef>
  <FactionDef ParentName="GTGFactionBase">
    <defName>GTGIndustrial</defName>
    <label>industrial complex</label>
    <fixedName>industrial complex</fixedName>
    <description>This faction includes the buildings caravans that sell various buildings;the industrial resource caravans that sell components,advanced components,steel,bricks and other industrial resources;and the artificial textiles caravans that sell various artificial textiles.</description>
    <factionIconPath>Icon/GTGIndustrial</factionIconPath>
    <caravanTraderKinds>
      <li>Caravan_GTGIndustrial_Buildings</li>
      <li>Caravan_GTGIndustrial_IndustrialResources</li>
      <li>Caravan_GTGIndustrial_ArtificialTextiles</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_GTGIndustrial</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>FactionBase_GTGIndustrial</li>
    </baseTraderKinds>
  </FactionDef>
  <FactionDef ParentName="GTGFactionBase">
    <defName>GTGMilitary</defName>
    <label>military corporations</label>
    <fixedName>military corporations</fixedName>
    <description>This faction includes the weapons&amp;shells caravans that sell various weapons and shells;the apparel&amp;utility caravans that sell apparel and utility;the medical supplies caravans that sell drugs,body parts and medicine;and the slave caravans that sell slaves of various factions and races.</description>
    <factionIconPath>Icon/GTGMilitary</factionIconPath>
    <caravanTraderKinds>
      <li>Caravan_GTGMilitary_Weapons_Shells</li>
      <li>Caravan_GTGMilitary_Apparel_Utility</li>
      <li>Caravan_GTGMilitary_MedicalSupplies</li>
      <li>Caravan_GTGMilitary_Slaves</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_GTGMilitary</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>FactionBase_GTGMilitary</li>
    </baseTraderKinds>
  </FactionDef>
  <FactionDef ParentName="GTGFactionBase">
    <defName>GTGBlackMarket</defName>
    <label>galactic blackMarket</label>
    <fixedName>galactic blackMarket</fixedName>
    <description>This faction includes the black market buyer caravans,which will buy anything;the black market exotic goods caravans that sell various exotic goods;the ultra technology caravans that sell neuro trainers,techprints,persona core and mech serum;and the Biotech caravans(require Biotech DLC) that sell various biotech items.</description>
    <factionIconPath>Icon/GTGBlackMarket</factionIconPath>
    <caravanTraderKinds>
      <li>Caravan_GTGBlackMarket_Buyer</li>
      <li>Caravan_GTGBlackMarket_ExoticGoods</li>
      <li>Caravan_GTGBlackMarket_UltraTechnology</li>
      <li MayRequire="Ludeon.RimWorld.Biotech">Caravan_GTGBlackMarket_Biotech</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_GTGBlackMarket</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>FactionBase_GTGBlackMarket</li>
    </baseTraderKinds>
  </FactionDef>
</Defs>