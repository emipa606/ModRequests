using System;
using RimWorld;
using Verse;

namespace davcin {
    public class SortBySkillConstruction : TransferableComparer {
        public override int Compare(Transferable lhs, Transferable rhs) {
            return davcin.SortBy.CompareSkill(SkillDefOf.Construction, ref lhs, ref rhs);
        }
    }
}
