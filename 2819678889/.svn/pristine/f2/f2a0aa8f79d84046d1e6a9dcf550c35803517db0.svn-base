using System;
using RimWorld;
using Verse;

namespace davcin {
    public class SortBySkillMelee : TransferableComparer {
        public override int Compare(Transferable lhs, Transferable rhs) {
            return davcin.SortBy.CompareSkill(SkillDefOf.Melee, lhs, rhs, true);
        }
    }
}
