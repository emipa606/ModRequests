<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<FactionDef ParentName="TribeBase">
		<defName>AztecCivil</defName>
		<label>civil Aztec tribe</label>
		<pawnSingular>tribesman</pawnSingular>
		<pawnsPlural>tribespeople</pawnsPlural>
		<categoryTag>Tribal</categoryTag>
		<listOrderPriority>20</listOrderPriority>
		<settlementGenerationWeight>1</settlementGenerationWeight>
		<requiredCountAtGameStart>1</requiredCountAtGameStart>
		<canMakeRandomly>true</canMakeRandomly>
		<canStageAttacks>true</canStageAttacks>
		<techLevel>Neolithic</techLevel>
		<factionNameMaker>NamerFactionTribal</factionNameMaker>
		<settlementNameMaker>NamerSettlementAztec</settlementNameMaker>
		<allowedCultures Inherit="false"><li>AztecCulture</li></allowedCultures>
		<structureMemeWeights Inherit="false">
			<Structure_OriginAztec>4</Structure_OriginAztec>
		</structureMemeWeights>
		<maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
		<configurationListOrderPriority>30</configurationListOrderPriority>
		<leaderTitle>tlatoani</leaderTitle>
		<colorSpectrum>
			<li>(0.85, 0.75, 0.37)</li>
			<li>(0.94, 0.61, 0.06)</li>
		</colorSpectrum>
		<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Supremacist</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">PainIsVirtue</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Raider</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
		</disallowedMemes>
		<requiredMemes>
			<li MayRequire="VanillaExpanded.VMemesE">VME_Trader</li>
		</requiredMemes>
		<factionIconPath>UI/StyleCategories/StyleAztec</factionIconPath>
		<modExtensions>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VFECore.FactionDefExtension">			
				<allowedBiomes>
					<li>AridShrubland</li>
					<li>Desert</li>
					<li>TemperateForest</li>
					<li>TemperateSwamp</li>
					<li>TropicalRainforest</li>
					<li>TropicalSwamp</li>
					
					<li MayRequire="sarg.alphabiomes">AB_TarPits</li>
					<li MayRequire="sarg.alphabiomes">AB_IdyllicMeadows</li>
					<li MayRequire="sarg.alphabiomes">AB_MiasmicMangrove</li>
					
					<li MayRequire="Mlie.AdvancedBiomes">Savanna</li>
					<li MayRequire="Mlie.AdvancedBiomes">Wetland</li>
					<li MayRequire="Mlie.AdvancedBiomes">Volcano</li>
					
					<li MayRequire="BiomesTeam.BiomesIslands">BiomesIslands_TropicalIsland</li>
					<li MayRequire="BiomesTeam.BiomesIslands">BiomesIslands_DesertIsland</li>
					<li MayRequire="BiomesTeam.BiomesIslands">BiomesIslands_TemperateIsland</li>
					
					<li MayRequire="BiomesTeam.Oasis">BMT_ChromaticOasis</li>
					
					<li MayRequire="zylle.MoreVanillaBiomes">ZBiome_Badlands</li>
					<li MayRequire="zylle.MoreVanillaBiomes">ZBiome_CoastalDunes</li>
					<li MayRequire="zylle.MoreVanillaBiomes">ZBiome_DesertOasis</li>
					<li MayRequire="zylle.MoreVanillaBiomes">ZBiome_Grasslands</li>
					<li MayRequire="zylle.MoreVanillaBiomes">ZBiome_Marsh</li>

					<li MayRequire="WindowsXP.RealisticPlanets">RRP_Grassland</li>
					<li MayRequire="WindowsXP.RealisticPlanets">RRP_Savanna</li>
					<li MayRequire="WindowsXP.RealisticPlanets">RRP_Steppes</li>
					<li MayRequire="WindowsXP.RealisticPlanets">RRP_TemperateDesert</li>
					<li MayRequire="WindowsXP.RealisticPlanets">RRP_Oasis</li>
				</allowedBiomes>
			</li>
		</modExtensions>
	</FactionDef>
	
