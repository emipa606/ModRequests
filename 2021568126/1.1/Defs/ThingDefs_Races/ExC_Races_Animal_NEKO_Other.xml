  <!--================================ ねこThingDef ===================================-->
  <!--================================ 首狩りうさぎ =====================================-->
  <ThingDef ParentName="ExC_CatBase">
	<defName>ExC_NEKO_Hare</defName>
	<label>hare</label>
	<description>A hare whose whole body is covered with white hair.</description>
    <statBases>
      <ArmorRating_Blunt>0</ArmorRating_Blunt>
      <ArmorRating_Sharp>0</ArmorRating_Sharp>
      <ArmorRating_Heat>0</ArmorRating_Heat>
      <ToxicSensitivity>0</ToxicSensitivity>
      <PsychicSensitivity>0</PsychicSensitivity>
      <MoveSpeed>6.9</MoveSpeed>
      <ComfyTemperatureMin>-30</ComfyTemperatureMin>
    </statBases>
    <race>
      <baseBodySize>0.8</baseBodySize>
      <manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
      <wildness>0.25</wildness>
      <baseHealthScale>1.0</baseHealthScale>
      <predator>true</predator>
      <wildBiomes>
        <SeaIce>0.001</SeaIce>
        <IceSheet>0.001</IceSheet>
        <Tundra>0.001</Tundra>
        <BorealForest>0.001</BorealForest>
        <ColdBog>0.001</ColdBog>
        <TemperateForest>0.001</TemperateForest>
        <TropicalRainforest>0.001</TropicalRainforest>
        <AridShrubland>0.001</AridShrubland>
        <TemperateSwamp>0.001</TemperateSwamp>
        <TropicalSwamp>0.001</TropicalSwamp>
        <Desert>0.001</Desert>
        <ExtremeDesert>0.001</ExtremeDesert>
      </wildBiomes>
	</race>
    <tools>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>45.0</power>
        <cooldownTime>10</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.8</chanceFactor>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>400</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>20</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>200</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>4</power>
        <cooldownTime>0.5</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>200</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
  </ThingDef>



