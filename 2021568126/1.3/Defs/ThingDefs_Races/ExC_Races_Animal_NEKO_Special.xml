    <combatPower>5000</combatPower>
    <canArriveManhunter>false</canArriveManhunter>
    <ecoSystemWeight>3</ecoSystemWeight>
    <wildGroupSize>
      <min>1</min>
      <max>2</max>
    </wildGroupSize>
    <minGenerationAge>0.5</minGenerationAge>
    <maxGenerationAge>10</maxGenerationAge>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/NEKO/Frost</texPath>
          <drawSize>0.8</drawSize>
