  <ThingDef ParentName="ExC_Strong_CatBase" Class="ExtendTheCat.ThingDef_SpecialCat">
	<defName>ExC_NEKO_Tiger</defName>
	<label>tiger</label>
	<description>A cat with yellow and black stripes. Because it is very ferocious, attention is necessary.</description>
    <thingClass>ExtendTheCat.ExC_SleepBody</thingClass>
	<SleepBodyPath>Things/Pawn/Animal/NEKO/TigerSleep</SleepBodyPath>
	<statBases>
	  <HungerRateMultiplier>1.25</HungerRateMultiplier>
	</statBases>
    <race>
      <baseHungerRate>1.0</baseHungerRate>
    </race>
    <comps>
      <li Class="CompProperties_Shearable">
        <woolDef>ExC_Tiger_Wool</woolDef>
