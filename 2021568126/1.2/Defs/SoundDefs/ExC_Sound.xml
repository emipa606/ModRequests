<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--======================= ねこの祝福 =======================-->
	<SoundDef>
		<defName>ExC_CatBellSound</defName>
		<sustain>False</sustain>
		<eventNames />
		<maxVoices>2</maxVoices>
		<maxSimultaneous>1</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<onCamera>True</onCamera>
				<grains>
			<li Class="AudioGrain_Clip">
				<clipPath>CatBell</clipPath>
			</li>
			</grains>
			<volumeRange>
				<min>25</min>
				<max>25</max>
			</volumeRange>
			<sustainLoop>False</sustainLoop>
			<sustainLoopDurationRange>
				<min>0</min>
				<max>0</max>
			</sustainLoopDurationRange>
			<sustainAttack>5</sustainAttack>
			<sustainRelease>5</sustainRelease>
		</li>
		</subSounds>
	</SoundDef>

