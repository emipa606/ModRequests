  <ThingDef ParentName="ExC_Rare_CatBase" Class="ExtendTheCat.ThingDef_SpecialCat">
	<defName>ExC_NEKO_Chiken</defName>
	<label>chiken</label>
	<description>It is a Chicken. It is a very useful cat that lays chicken eggs and cat eggs.</description>
    <thingClass>ExtendTheCat.ExC_SleepBody</thingClass>
	<SleepBodyPath>Things/Pawn/Animal/Other/ChikenSleep</SleepBodyPath>
    <statBases>
	  <LeatherAmount>0</LeatherAmount>
	  <MeatAmount>50</MeatAmount>
    </statBases>
	<race>
      <meatLabel>chiken</meatLabel>
      <gestationPeriodDays>12</gestationPeriodDays>
	  <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.7, 1)</li>
          <li>(2.0, 0)</li>
        </points>
	  </litterSizeCurve>
      <useMeatFrom/>
	</race>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggUnfertilizedDef>EggChickenUnfertilized</eggUnfertilizedDef>
        <eggFertilizedDef>EggChickenFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>1</eggLayIntervalDays>
        <eggCountRange>5</eggCountRange>
        <eggLayFemaleOnly>false</eggLayFemaleOnly>
      </li>
      <li Class="ExtendTheCat.ExC_CompProperties_Passive">
		<CommandLabel>lay eggs (Passive)</CommandLabel>
		<CommandDesk>lay cat eggs.</CommandDesk>
		<CommandIconPath>Icon/Command/CatEgg</CommandIconPath>
      </li>
      <li Class="CompExhale.CompProperties_Exhale">
        <Product>ExC_CatEgg</Product>
        <ExhaleTick>420000</ExhaleTick>
        <Quantity>1</Quantity>
		<ExtraExhaleText>Create cat egg {0}%</ExtraExhaleText>
      </li>
    </comps>
    <tradeTags>
      <li>AnimalFarm</li>
    </tradeTags>
  </ThingDef>

  <ThingDef ParentName="AnimalThingBase">
	<defName>ExC_NEKO_Cow</defName>
	<label>cow</label>
	<description>It is a cow. Very useful and very cute!</description>
    <statBases>
		<ComfyTemperatureMin>-20</ComfyTemperatureMin>
		<ComfyTemperatureMax>60</ComfyTemperatureMax>
		<MeatAmount>300</MeatAmount>
		<LeatherAmount>150</LeatherAmount>
		<ArmorRating_Blunt>0.20</ArmorRating_Blunt>
		<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
		<ArmorRating_Heat>0.3</ArmorRating_Heat>
		<ToxicEnvironmentResistance>0.6</ToxicEnvironmentResistance>
		<PsychicSensitivity>0.6</PsychicSensitivity>
		<Flammability>0.50</Flammability>
		<Mass>10</Mass>
        <MoveSpeed>3.2</MoveSpeed>
        <MarketValue>1000</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Milkable">
        <milkDef>Milk</milkDef>
        <milkIntervalDays>1</milkIntervalDays>
        <milkAmount>50</milkAmount>
		<milkFemaleOnly>false</milkFemaleOnly>
      </li>
    </comps>
	<tools>
      <li>
        <label>left hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>16</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>16</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
      </li>
    </tools>
    <race>
      <body>QuadrupedAnimalWithHooves</body>
      <packAnimal>true</packAnimal>
      <petness>0.95</petness>
      <baseHungerRate>0.3</baseHungerRate>
      <leatherDef>Leather_Plain</leatherDef>
      <baseHealthScale>4.0</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <gestationPeriodDays>30</gestationPeriodDays>
      <trainability>Advanced</trainability>
      <wildness>0.05</wildness>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
      <herdAnimal>true</herdAnimal>
	  <herdMigrationAllowed>false</herdMigrationAllowed>
	  <nuzzleMtbHours>5</nuzzleMtbHours>
      <lifeExpectancy>50</lifeExpectancy>
      <baseBodySize>2.5</baseBodySize>
      <meatLabel>beef</meatLabel>
      <useMeatFrom/>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.3</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_Cat_Injured</soundWounded>
          <soundDeath>Pawn_Cat_Death</soundDeath>
          <soundCall>Pawn_Cat_Call</soundCall>
          <soundAngry>Pawn_Cat_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
      <wildBiomes>
        <SeaIce>0.0005</SeaIce>
        <IceSheet>0.0005</IceSheet>
        <Tundra>0.0005</Tundra>
        <BorealForest>0.0005</BorealForest>
        <TemperateForest>0.0005</TemperateForest>
        <TropicalRainforest>0.0005</TropicalRainforest>
        <AridShrubland>0.0005</AridShrubland>
        <Desert>0.0005</Desert>
        <ExtremeDesert>0.0005</ExtremeDesert>
        <TemperateSwamp>0.0005</TemperateSwamp>
        <ColdBog>0.0005</ColdBog>
        <TropicalSwamp>0.0005</TropicalSwamp>
      </wildBiomes>
    </race>
    <tradeTags>
      <li>AnimalFarm</li>
    </tradeTags>
  </ThingDef>

