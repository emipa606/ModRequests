<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--=====================================================================-->
  <!--=============================== Egg  ================================-->
  <!--=====================================================================-->

  <ThingDef ParentName="OrganicProductBase">
    <defName>ExC_CatEgg</defName>
    <label>cat egg</label>
    <description>Egg from a cat.</description>
    <graphicData>
      <texPath>Things/Item/Resource/CatEgg</texPath>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>400</MarketValue>
      <Mass>2</Mass>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <tickerType>Normal</tickerType>
    <stackLimit>1</stackLimit>
    <comps>
      <li Class="ExtendTheCat.ExC_CompProperties_CatEgg">
        <hatchingTick>240000</hatchingTick>
        <ExtraTextMessage>Hatching time {0} days</ExtraTextMessage>
        <hatchingList>
          <li>
            <catName>Cat</catName>
            <weight>28</weight>
          </li>
          <li>
            <catName>NormalCat</catName>
            <weight>65</weight>
          </li>
          <li>
            <catName>ExC_NEKO_SCP</catName>
            <weight>5</weight>
          </li>
          <li>
            <catName>ExC_NEKO_Dragon</catName>
            <weight>2</weight>
          </li>
        </hatchingList>
      </li>
    </comps>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
  </ThingDef>
</Defs>