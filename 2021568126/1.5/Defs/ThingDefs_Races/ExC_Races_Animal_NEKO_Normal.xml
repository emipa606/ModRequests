<Defs>

  <!--================================ ねこThingDef ===================================-->
  <!--================================ 普通のねこ =====================================-->
  <ThingDef ParentName="ExC_CatBase" Abstract="True" Name="ExC_Normal_CatBase">
    <label>Normal CatBase</label>
    <description>普通のねこです。</description>
    <tools>
      <li>
        <label>cute little teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>23.6</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.5</chanceFactor>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>18</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>4</power>
        <cooldownTime>0.5</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
  </ThingDef>


