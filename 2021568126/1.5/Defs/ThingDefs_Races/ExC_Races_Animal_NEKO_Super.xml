    <comps>
      <li Class="ExtendTheCat.ExC_CompProperties_Initial">
        <initHediff>ExC_Hediff_Fly</initHediff>
      </li>
      <li Class="ExtendTheCat.ExC_CompProperties_Passive">
		<CommandLabel>Fly (Passive)</CommandLabel>
		<CommandDesk>This creature flies in the sky. Disables the effects of terrain-based movement delays.</CommandDesk>
		<CommandIconPath>Icon/Command/Fly</CommandIconPath>
      </li>
      <li Class="ExtendTheCat.ExC_CompProperties_ActionCommand">
		<CommandName>ExC_Comp_CallRain</CommandName>
		<CommandLabel>Call rain</CommandLabel>
		<CommandDesk>Call the rain clouds and make it rain.</CommandDesk>
		<CommandIconPath>Icon/Command/CallRain</CommandIconPath>
		<CoolDown>150000</CoolDown>
	  </li>
	  <li Class="ExtendTheCat.ExC_CompProperties_PositionCommand">
		<CommandName>ExC_Comp_Punishment</CommandName>
		<CommandLabel>Punishment</CommandLabel>
		<CommandDesk>Drops numerous thunderbolts from the sky to destroy its enemies.</CommandDesk>
		<CommandIconPath>Icon/Command/Punishment</CommandIconPath>
		<Radius>100</Radius>
		<CoolDown>10000</CoolDown>
	  </li>
	  <li Class="ExtendTheCat.ExC_CompProperties_TargetCommand">
		<CommandName>ExC_Comp_DragonBreath</CommandName>
		<CommandLabel>Dragon Breath</CommandLabel>
		<CommandDesk>Spits flames at its enemies.</CommandDesk>
		<CommandIconPath>Icon/Command/DragonBreath</CommandIconPath>
		<CoolDown>2500</CoolDown>
	  </li>
    </comps>
  </ThingDef>

