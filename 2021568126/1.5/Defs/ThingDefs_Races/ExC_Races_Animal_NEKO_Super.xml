  <ThingDef ParentName="ExC_CatBase" Abstract="True" Name="ExC_Super_CatBase">
    <statBases>
      <ArmorRating_Blunt>0.7</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.65</ArmorRating_Sharp>
      <ArmorRating_Heat>1.0</ArmorRating_Heat>
      <ToxicEnvironmentResistance>0.2</ToxicEnvironmentResistance>
      <PsychicSensitivity>0.2</PsychicSensitivity>
      <Flammability>0.01</Flammability>
      <MoveSpeed>6.2</MoveSpeed>
      <MarketValue>1200</MarketValue>
    </statBases>
    <race>
      <baseBodySize>3.5</baseBodySize>
      <manhunterOnTameFailChance>0.30</manhunterOnTameFailChance>
      <wildness>0.50</wildness>
      <baseHealthScale>14.0</baseHealthScale>
      <predator>true</predator>
      <wildBiomes>
        <SeaIce>0.3</SeaIce>
        <IceSheet>0.001</IceSheet>
        <Tundra>0.001</Tundra>
        <BorealForest>0.001</BorealForest>
        <TemperateForest>0.001</TemperateForest>
        <TropicalRainforest>0.001</TropicalRainforest>
        <AridShrubland>0.001</AridShrubland>
        <Desert>0.001</Desert>
        <ExtremeDesert>0.3</ExtremeDesert>
        <TemperateSwamp>0.001</TemperateSwamp>
        <ColdBog>0.001</ColdBog>
        <TropicalSwamp>0.001</TropicalSwamp>
      </wildBiomes>
	</race>
    <damageMultipliers>
      <li>
        <damageDef>Stun</damageDef>
        <multiplier>0.3</multiplier>
      </li>
    </damageMultipliers>
    <tools>
      <li>
        <label>cute little teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>40</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>40</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>30</power>
        <cooldownTime>1.35</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>30</power>
        <cooldownTime>1.35</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
  </ThingDef>

  <!--================================ 個別設定      =====================================-->
  <!--================================ 個別設定-すごい =====================================-->
  <ThingDef ParentName="ExC_Super_CatBase" Class="ExtendTheCat.ThingDef_SpecialCat">
	<defName>ExC_NEKO_RedLion</defName>
	<label>Red Lion</label>
	<description>A cat with a flaming red mane. It is terribly fierce, its strength is also suitable for dragons.</description>
    <thingClass>ExtendTheCat.ExC_SleepBody</thingClass>
	<SleepBodyPath>Things/Pawn/Animal/NEKO/RedLionSleep</SleepBodyPath>
    <race>
      <renderTree>ExC_RenderTreeDef_Cat</renderTree>
    </race>
	<statBases>
	  <RestRateMultiplier>0.2</RestRateMultiplier>
	</statBases>
    <comps>
      <li Class="CompProperties_Shearable">
        <woolDef>ExC_RedLion_Wool</woolDef>
        <shearIntervalDays>15</shearIntervalDays>
        <woolAmount>40</woolAmount>
      </li>
      <li Class="ExtendTheCat.ExC_CompProperties_ActionCommand">
		<CommandName>ExC_Comp_KingsRoar</CommandName>
		<CommandLabel>Kings Roar</CommandLabel>
		<CommandDesk>The cat intimidate their enemies with a hair-raising, terrifying roar.
The cat becomes sleepy.</CommandDesk>
		<CommandIconPath>Icon/Command/KingsRoar</CommandIconPath>
	  </li>
	  <li Class="ExtendTheCat.ExC_CompProperties_PositionCommand">
		<CommandName>ExC_Comp_Jump</CommandName>
		<CommandLabel>Jump</CommandLabel>
		<CommandDesk>The cat will leap at the target with quick movements.
The cat becomes sleepy.</CommandDesk>
		<CommandIconPath>Icon/Command/Jump</CommandIconPath>
	  </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="ExC_Super_CatBase" Class="ExtendTheCat.ThingDef_SpecialCat">
	<defName>ExC_NEKO_WhiteTiger</defName>
	<label>White Tiger</label>
	<description>A cat with a beautiful coat of white and black stripes. It is terribly fierce, its strength is also suitable for dragons.</description>
    <thingClass>ExtendTheCat.ExC_SleepBody</thingClass>
	<SleepBodyPath>Things/Pawn/Animal/NEKO/WhiteTigerSleep</SleepBodyPath>
<!--
	<statBases>
	  <HungerRateMultiplier>2.5</HungerRateMultiplier>
	</statBases>
-->
    <race>
      <renderTree>ExC_RenderTreeDef_Cat</renderTree>
      <baseHungerRate>1.0</baseHungerRate>
    </race>
    <comps>
      <li Class="CompProperties_Shearable">
        <woolDef>ExC_WhiteTiger_Wool</woolDef>
        <shearIntervalDays>15</shearIntervalDays>
        <woolAmount>40</woolAmount>
      </li>
      <li Class="ExtendTheCat.ExC_CompProperties_ActionCommand">
		<CommandName>ExC_Comp_TigerDance</CommandName>
		<CommandLabel>Tiger dance</CommandLabel>
		<CommandDesk>The cat runs around at a furious pace, attacking the enemies around it.
Cat will get hungry.</CommandDesk>
		<CommandIconPath>Icon/Command/TigerDance</CommandIconPath>
	  </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="ExC_Super_CatBase">
