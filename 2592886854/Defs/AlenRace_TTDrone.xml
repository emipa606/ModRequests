<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
		<defName>NTTDrone</defName>
		<label>think tank</label>
		<description>Small drone constructed of simple materials built in short time. This device is equipped with a basic AI that allows it to research, but is unable to do much else.</description>
		<uiIconPath>Things/Pawn/Humanlike/UI/IconHuman</uiIconPath>
		<comps>
			<li Class="Androids.CompProperties_EnergyTracker">
				<canHibernate>true</canHibernate>
				<hibernationJob>ChJDroidHibernate</hibernationJob>
				<maxEnergy>1</maxEnergy>
				<ticksSpentCharging>600</ticksSpentCharging>
			</li>
		</comps>
		<alienRace>
			<generalSettings>
				<maleGenderProbability>1</maleGenderProbability>
				<immuneToAge>true</immuneToAge>
				<disallowedTraits>
					<li>
						<defName>DrugDesire</defName>
					</li>
					<li>
						<defName>NaturalMood</defName>
					</li>
					<li>
						<defName>Nerves</defName>
					</li>
					<li>
						<defName>Neurotic</defName>
					</li>
					<li>
						<defName>PsychicSensitivity</defName>
					</li>
					<li>
						<defName>Cannibal</defName>
					</li>
					<li>
						<defName>TooSmart</defName>
					</li>
					<li>
						<defName>BodyPurist</defName>
					</li>
					<li>
						<defName>Greedy</defName>
					</li>
					<li>
						<defName>Jealous</defName>
					</li>
					<li>
						<defName>DislikesMen</defName>
					</li>
					<li>
						<defName>DislikesWomen</defName>
					</li>
					<li>
						<defName>Xenophobia</defName>
					</li>
					<li>
						<defName>AnnoyingVoice</defName>
					</li>
					<li>
						<defName>CreepyBreathing</defName>
					</li>
					<li>
						<defName>Pyromaniac</defName>
					</li>
					<li>
						<defName>Wimp</defName>
					</li>
				</disallowedTraits>
				<immuneToXenophobia>true</immuneToXenophobia>
				<alienPartGenerator>
					<alienbodytypes>
						<li>Thin</li>
					</alienbodytypes>
					<colorChannels>
						<li>
							<name>skin</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>10</weight>
										<only>RGBA(0.94,0.94,0.94,1)</only>
									</li>
								</options>
							</first>
						</li>
					</colorChannels>
				</alienPartGenerator>
			</generalSettings>
			<graphicPaths>
				<li>
					<body>TTBot/</body>
				</li>
			</graphicPaths>
			<styleSettings>
				<li>
					<key>HairDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
				<li>
					<key>TattooDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
				<li>
					<key>BeardDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
			</styleSettings>
			<raceRestriction>
				<onlyUseRaceRestrictedApparel>true</onlyUseRaceRestrictedApparel>
				<whiteApparelList>
					<li>ChjAndroid_ChemfuelGenerator</li>
				</whiteApparelList>
			</raceRestriction>
			<thoughtSettings>
				<cannotReceiveThoughtsAtAll>true</cannotReceiveThoughtsAtAll>
				<canStillReceiveThoughts>
					<li>ChJDroidAlways</li>
				</canStillReceiveThoughts>
				<butcherThoughtGeneral>
					<thought>NSalvageRobotCorpse</thought>
					<knowThought>NKnowSalvageRobotCorpse</knowThought>
				</butcherThoughtGeneral>
			</thoughtSettings>
			<relationSettings>
				<relationChanceModifierChild>0</relationChanceModifierChild>
				<relationChanceModifierExLover>0</relationChanceModifierExLover>
				<relationChanceModifierExSpouse>0</relationChanceModifierExSpouse>
				<relationChanceModifierFiance>0</relationChanceModifierFiance>
				<relationChanceModifierLover>0</relationChanceModifierLover>
				<relationChanceModifierParent>0</relationChanceModifierParent>
				<relationChanceModifierSibling>0</relationChanceModifierSibling>
				<relationChanceModifierSpouse>0</relationChanceModifierSpouse>
			</relationSettings>
		</alienRace>
		<statBases>
			<MarketValue>500</MarketValue>
			<MoveSpeed>4</MoveSpeed>
			<Flammability>0.25</Flammability>
			<ComfyTemperatureMin>-85</ComfyTemperatureMin>
			<ComfyTemperatureMax>120</ComfyTemperatureMax>
			<ToxicSensitivity>0</ToxicSensitivity>
			<PsychicSensitivity>0</PsychicSensitivity>
			<PainShockThreshold>5</PainShockThreshold>
			<MentalBreakThreshold>0</MentalBreakThreshold>
			<SocialImpact>0</SocialImpact>
			<GlobalLearningFactor>0</GlobalLearningFactor>
			<ImmunityGainSpeed>10</ImmunityGainSpeed>
			<WorkSpeedGlobal>1.5</WorkSpeedGlobal>
			<LeatherAmount>0</LeatherAmount>
			<MeatAmount>0</MeatAmount>
			<ArmorRating_Sharp>0.05</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.0</ArmorRating_Blunt>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<CarryingCapacity>60</CarryingCapacity>
			<Mass>15.0</Mass>
		</statBases>
		<tools>
			<li>
				<label>tool strike</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.6</cooldownTime>
				<linkedBodyPartsGroup>DroneHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>22</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
		</tools>
		<race>
			<thinkTreeMain>Humanlike</thinkTreeMain>
			<thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
			<intelligence>Humanlike</intelligence>
			<fleshType>ChJDroid</fleshType>
			<makesFootprints>false</makesFootprints>
			<lifeExpectancy>1000</lifeExpectancy>
			<baseHungerRate>0</baseHungerRate>
			<leatherDef>Leather_ChJAndroid</leatherDef>
			<nameCategory>HumanStandard</nameCategory>
			<body>DroneBody</body>
			<bloodDef>ChjFilthCoolant</bloodDef>
			<deathActionWorkerClass>Androids.DeathActionWorker_Droid</deathActionWorkerClass>
			<baseBodySize>0.75</baseBodySize>
			<baseHealthScale>1</baseHealthScale>
			<foodType>None</foodType>
			<useMeatFrom>ChjAndroid</useMeatFrom>
			<gestationPeriodDays>1</gestationPeriodDays>
			<canBePredatorPrey>false</canBePredatorPrey>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(1.01, 0.02)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeStageAges>
				<li>
					<def>HumanlikeAdult</def>
					<minAge>0</minAge>
					<soundWounded>Pawn_Mech_Scyther_Wounded</soundWounded>
					<soundDeath>Pawn_Mech_Scyther_Death</soundDeath>
					<soundCall>Pawn_Mech_Scyther_Call</soundCall>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_MechanoidBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_MechanoidBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_MechanoidBash_Miss</soundMeleeMiss>
			<specialShadowData>
				<volume>(0.3, 0.8, 0.4)</volume>
				<offset>(0,0,-0.3)</offset>
			</specialShadowData>
			<ageGenerationCurve>
				<points>
					<li>(14,0)</li>
					<li>(16,100)</li>
					<li>(50,100)</li>
					<li>(60,30)</li>
					<li>(70,18)</li>
					<li>(80,10)</li>
					<li>(90,3)</li>
					<li>(100,0)</li>
				</points>
			</ageGenerationCurve>
			<hediffGiverSets>
				<li>ChjAndroidStandard</li>
				<li>ChjDroid</li>
			</hediffGiverSets>
		</race>
		<recipes>
			<li>AdministerMechSerumHealer</li>
			<li>ChJAndroidRepairKit</li>
			<li>ChJAndroidShutDownDroid</li>
		</recipes>
		<butcherProducts>
			<Steel>10</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</butcherProducts>
		<modExtensions>
			<li Class="Androids.MechanicalPawnProperties">
				<noSkillLoss>true</noSkillLoss>
				<canSocialize>false</canSocialize>
				<colonyCaresIfDead>false</colonyCaresIfDead>
			</li>
			<li Class="Androids.DroidSpawnProperties">
				<defaultSkillLevel>0</defaultSkillLevel>
				<skills>
					<li>
						<def>Intellectual</def>
						<level>10</level>
					</li>
				</skills>
				<backstory>Backstory_TT</backstory>
				<bodyType>Thin</bodyType>
			</li>
		</modExtensions>
	</AlienRace.ThingDef_AlienRace>
</Defs>