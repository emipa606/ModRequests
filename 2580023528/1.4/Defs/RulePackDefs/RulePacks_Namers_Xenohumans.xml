<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerThrumboian</defName>
    <rulePack>
      <rulesFiles>
        <li>MaleNames->ThrumboianNames/MaleNames</li>
        <li>FemaleNames->ThrumboianNames/FemaleNames</li>
        <li>LastNames->ThrumboianNames/LastNames</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonThrumboianMale</defName>
    <include>
      <li>NamerThrumboian</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[MaleNames] [LastNames]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonThrumboianFemale</defName>
    <include>
      <li>NamerThrumboian</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[FemaleNames] [LastNames]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>
