    <!--PLASTEEL-->
    <RecipeDef>
        <defName>Mawy_MakePlasteel</defName>
        <label>Refine plasteel</label>
        <description>Refine plasteel from steel and chemfuel.</description>
        <jobString>Refining plasteel.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Smelt</effectWorking>
        <soundWorking>Recipe_Smelt</soundWorking>
        <workAmount>5000</workAmount>
        <recipeUsers>
            <li>ElectricSmelter</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>20</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Chemfuel</li>
                    </thingDefs>
                </filter>
                <count>40</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
                <li>Chemfuel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <Plasteel>20</Plasteel>
        </products>
        <researchPrerequisite>Mawy_Plasteel</researchPrerequisite>
        <skillRequirements>
            <Crafting>10</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
    </RecipeDef>
