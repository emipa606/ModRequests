<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	 <ThingDef ParentName="VTDoorBase">
		<defName>VTautoDoor</defName>
		<label>Automatic Vault Door</label>
		<description>Divides rooms. A fast and automatic door.</description>
		<thingClass>Building_Door</thingClass>
		<statBases>
			<WorkToBuild>1500</WorkToBuild>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/DoorVT_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/DoorVT_MenuIcon</uiIconPath>
		<costList>
		<Steel>50</Steel>
		<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<designationHotKey>Misc12</designationHotKey>
		<building>
		<canPlaceOverWall>true</canPlaceOverWall>
			<blueprintGraphicData>
				<texPath>Things/Building/Doors/DoorVT_blueMover</texPath>
			</blueprintGraphicData>
		</building>	
		<comps>
		<li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>50</basePowerConsumption>
		</li>
		<li Class="CompProperties_Breakdownable"/>
		</comps>
		<designationCategory>VaultItems</designationCategory>
		<researchPrerequisites>
		<li>Vaultbasics</li>
		<li>Autodoors</li>
		</researchPrerequisites>
		<designatorDropdown>Vaultdoors</designatorDropdown>
	</ThingDef>
	
