<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="VaultArtableFurnitureBase">
    <defName>vault_sofa</defName>
    <label>Vault Sofa</label>
    <description>A comfortable seat to relax and sit!</description>
	<size>(2,1)</size>
	<graphicData>
			<drawSize>(2.2,2.2)</drawSize>
			<texPath>Things/Building/Furniture/Vaultsofa</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
	</graphicData>
	<uiIconScale>0.5</uiIconScale>
    <altitudeLayer>Building</altitudeLayer>
	<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <rotatable>true</rotatable>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>8</Mass>
      <Flammability>1.0</Flammability>
      <JoyGainFactor>1.3</JoyGainFactor>
    </statBases>
    <stuffCategories>
		<li>Fabric</li>
		<li>Leathery</li>
	</stuffCategories>
	<costStuffCount>100</costStuffCount>
    <defaultPlacingRot>South</defaultPlacingRot>
    <researchPrerequisites>
			<li>VaultFurniture</li>
	</researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <building>
		<isSittable>true</isSittable>
	</building>
   
    <pathCost>10</pathCost>
    <designationCategory>VaultItems</designationCategory>
    <comps>
      <li Class="CompProperties_Flickable"/>
    </comps>
    <tradeability>None</tradeability>
  </ThingDef>

	
</Defs>
