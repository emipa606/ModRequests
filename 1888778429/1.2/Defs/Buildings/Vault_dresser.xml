<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ArtableFurnitureBase">
		<defName>VaultWardrobeA</defName>
		<label>Vault Locker</label>
		<description>A locker for storing clothes.</description>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
			<texPath>Things/Building/Furniture/Vault_DresserA</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rectN>(0.25,0.234375,0.5,0.28125)</rectN>
				<rectS>(0.25,0.234375,0.5,0.28125)</rectS>
				<rectE>(0.28125,0.25,0.453125,0.25)</rectE>
				<rectW>(0.28125,0.25,0.453125,0.25)</rectW>
			</damageData>
		</graphicData>
		<uiIconScale>0.65</uiIconScale>
		<uiIconOffset>(0, 0.15)</uiIconOffset>
		<altitudeLayer>Pawn</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>1</fillPercent>
		<minifiedDef>MinifiedThing</minifiedDef>
		<castEdgeShadows>true</castEdgeShadows>
		<costList>
			<Steel>10</Steel>
		</costList>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>120</costStuffCount>
		<statBases>
			<Beauty>2</Beauty>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>8</Mass>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(1,1)</size>
		<defaultPlacingRot>South</defaultPlacingRot>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<defaultStorageSettings>
				<priority>Important</priority>
				<filter>
					<categories>
						<li>Apparel</li>
					</categories>
				</filter>
			</defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<designationHotKey>Misc5</designationHotKey>
		<staticSunShadowHeight>0.15</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<designationCategory>VaultItems</designationCategory>
	
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<comps>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<Comfort>0.02</Comfort>
				</statOffsets>
				<maxSimultaneous>1</maxSimultaneous>
				<maxDistance>10</maxDistance>
			</li>
		</comps>
	</ThingDef>	
