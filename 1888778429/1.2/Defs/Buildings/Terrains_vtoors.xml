	<TerrainDef ParentName="FloorBase">
		<defName>vtfloor</defName>
		<label>Smooth Vault Floor Tiles</label>
		<renderPrecedence>300</renderPrecedence>
		<description>A thick metal floor built for keeping out ground radiation. It is fairly pleasing to the eyes, and can be cleaned with ease.</description>
		<texturePath>Things/Building/Floors/Vtfloor</texturePath>
		<statBases>
			<Beauty>1</Beauty>
			<Cleanliness>0.01</Cleanliness>
			<WorkToBuild>900</WorkToBuild>
		</statBases>
		<costList>
			<Steel>3</Steel>
		</costList>
		<constructEffect>ConstructMetal</constructEffect>
		<designationCategory>VaultItems</designationCategory>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designatorDropdown>vtfloor</designatorDropdown>
	</TerrainDef>
	
