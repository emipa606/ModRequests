<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="VaultArtableFurnitureBase">
		<defName>Vtarmchair</defName>
		<label>Armchair</label>
		<description>A comfortable and attractive chair. Can be used at tables, work stations, and elsewhere.</description>
		<graphicData>
			<texPath>Things/Building/Furniture/vtarmchair</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rectN>(0.25,0.234375,0.5,0.28125)</rectN>
				<rectS>(0.25,0.234375,0.5,0.28125)</rectS>
				<rectE>(0.28125,0.25,0.453125,0.25)</rectE>
				<rectW>(0.28125,0.25,0.453125,0.25)</rectW>
			</damageData>
			<shadowData>
				<volume>(0.23, 0.26, 0.23)</volume>
			</shadowData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>170</MaxHitPoints>
			<WorkToBuild>7000</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>8</Beauty>
			<Comfort>0.75</Comfort>
		</statBases>
		<socialPropernessMatters>true</socialPropernessMatters>
		<stuffCategories>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>45</costStuffCount>
		<pathCost>30</pathCost>
		<designationHotKey>Misc3</designationHotKey>
		<fillPercent>0.35</fillPercent>
		<defaultPlacingRot>South</defaultPlacingRot>
		<uiIconOffset>(0,0.1)</uiIconOffset>
		<uiIconScale>0.90</uiIconScale>
		<building>
			<isSittable>true</isSittable>
		</building>
		<designationCategory>VaultItems</designationCategory>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
	</ThingDef>
	
