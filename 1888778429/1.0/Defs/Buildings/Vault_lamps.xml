	<ThingDef ParentName="BuildingBase">
		<defName>vttablelamp</defName>
		<label>small lamp</label>
		<description>This is a tiny lamp to put on endtables or other furniture.</description>
		<category>Building</category>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Furniture/Vault_mini_Lamp</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawRotated>false</drawRotated>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0.35,0.35,0.3,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconScale>0.5</uiIconScale>
		<rotatable>true</rotatable>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>8</pathCost>
		<fillPercent>0.15</fillPercent>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Steel>5</Steel>
		</costList>	
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0</Flammability>
			<Mass>1</Mass>
			<Beauty>15</Beauty>
		</statBases>
		<selectable>true</selectable>
		<building>
			<isEdifice>false</isEdifice>
		</building>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>5</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>5</glowRadius>
				<glowColor>(252,187,113,0)</glowColor>
			</li>
		</comps>
		<soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>VaultItems</designationCategory>
		<destroyable>true</destroyable>
		<tradeability>None</tradeability>
		<clearBuildingArea>false</clearBuildingArea>
		<minifiedDef>MinifiedThing</minifiedDef>		
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
	</ThingDef>
	
	<ThingDef Abstract="True" Name="WallLightBase" ParentName="BuildingBase">
		<thingClass>Building</thingClass>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>Standable</passability>
		<statBases>
			<MaxHitPoints>45</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0.0</Flammability>
			<Mass>2</Mass>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>20</Steel>
		</costList>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<building>
			<isEdifice>false</isEdifice>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<tickerType>Normal</tickerType>
		<rotatable>true</rotatable>
		<selectable>true</selectable>
		<destroyable>true</destroyable>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>VaultItems</designationCategory>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>


	<ThingDef ParentName="WallLightBase">
		<defName>VT_Wall_LampE</defName>
		<label>Vault lamp</label>
		<graphicData>
			<texPath>Things/Building/Furniture/VT_WallLit</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
		</graphicData>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>10</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>10</glowRadius>
				<glowColor>(252,187,113,0)</glowColor>
			</li>
		</comps>
		<uiIconPath>Things/Building/Furniture/VT_WallLit_south</uiIconPath>
		<uiIconScale>3</uiIconScale>
		<uiIconOffset>(0,-0.2)</uiIconOffset>
		<description>A light fixture designed to be mounted on a wall.</description>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designationCategory>VaultItems</designationCategory>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
	
	<ThingDef ParentName="WallLightBase">
		<defName>VT_Heater</defName>
		<label>Vault Heater</label>
		<description>Creates heat up to 25 degree celsius.</description>
		<graphicData>
			<texPath>Things/Building/Furniture/VT_heater_WallLit</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
		</graphicData>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>30</basePowerConsumption>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>35</heatPerSecond>
				<heatPushMaxTemperature>25</heatPushMaxTemperature>
			</li>
			
		</comps>
		<uiIconPath>Things/Building/Furniture/VT_heater_WallLit_south</uiIconPath>
		<uiIconScale>3</uiIconScale>
		<uiIconOffset>(0,-0.2)</uiIconOffset>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designationCategory>VaultItems</designationCategory>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
	
	<ThingDef ParentName="WallLightBase">
		<defName>VT_Cooler</defName>
		<label>Vault Cooling</label>
		<description>Reduces heat down to 21 degree celsius.</description>
		<graphicData>
			<texPath>Things/Building/Furniture/VT_cooler_WallLit</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
		</graphicData>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>30</basePowerConsumption>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>-35</heatPerSecond>
				<heatPushMinTemperature>21</heatPushMinTemperature>
			</li>		
		</comps>
		<uiIconPath>Things/Building/Furniture/VT_cooler_WallLit_south</uiIconPath>
		<uiIconScale>3</uiIconScale>
		<uiIconOffset>(0,-0.2)</uiIconOffset>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designationCategory>VaultItems</designationCategory>
		<designatorDropdown>VaultCool</designatorDropdown>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
	
	<ThingDef ParentName="WallLightBase">
		<defName>VT_freezer</defName>
		<label>Vault Freezer</label>
		<description>Reduces heat down to -1 degree celsius.</description>
		<graphicData>
			<texPath>Things/Building/Furniture/VT_cooler_WallLit</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
		</graphicData>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>100</basePowerConsumption>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>-40</heatPerSecond>
				<heatPushMinTemperature>-5</heatPushMinTemperature>
			</li>		
		</comps>
		<uiIconPath>Things/Building/Furniture/VT_cooler_WallLit_south</uiIconPath>
		<uiIconScale>3</uiIconScale>
		<uiIconOffset>(0,-0.2)</uiIconOffset>
		<costList>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>		
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designationCategory>VaultItems</designationCategory>
		<designatorDropdown>VaultCool</designatorDropdown>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
	
	<DesignatorDropdownGroupDef>
		<defName>VaultCool</defName>
	</DesignatorDropdownGroupDef>	


	
</Defs>
