<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="VaultArtableFurnitureBase">
		<defName>vault_lamp</defName>
		<label>Interior Lamp</label>
		<description>An interior lamp.</description>
		<category>Building</category>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Furniture/Vault_Interior_Lamp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawRotated>false</drawRotated>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(4,4)</drawSize>
			<damageData>
				<rect>(0.35,0.35,0.3,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Furniture/Vault_Interior_Lamp</uiIconPath>
		<uiIconScale>1.0</uiIconScale>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>8</pathCost>
		<tickerType>Normal</tickerType>
		<fillPercent>0.2</fillPercent>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToBuild>250</WorkToBuild>
			<Flammability>0</Flammability>
			<Mass>0.03</Mass>
			<Beauty>8</Beauty>
		</statBases>
		<selectable>true</selectable>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Steel>25</Steel>
		</costList>
		<size>(1,1)</size>
		<soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>25</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>15</glowRadius>
				<glowColor>(252,187,113,0)</glowColor>
			</li>
		</comps>
		<designationCategory>VaultItems</designationCategory>
		<destroyable>true</destroyable>
		<tradeability>None</tradeability>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
	</ThingDef>
	
