<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- Start of WorkGiverDefs --> 
  <!-- ============= Art ============= -->
<!-- Personal Desk -->  
  <WorkGiverDef>
    <defName>VT_workbench</defName>
    <label>Synthehize material at the Vault-Tec Assembly Bench</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Crafting</workType>
    <priorityInType>100</priorityInType>
    <fixedBillGiverDefs>
      <li>vt_workbench</li>
    </fixedBillGiverDefs>
    <verb>Synthesize</verb>
    <gerund>Synthesize at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
<!-- Crafting Station -->
  <WorkGiverDef>
    <defName>VT_bore</defName>
    <label>Grind for Materials</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Mining</workType>
    <priorityInType>100</priorityInType>
    <fixedBillGiverDefs>
      <li>vt_megabore</li>
    </fixedBillGiverDefs>
    <verb>Mine</verb>
    <gerund>Mine at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
  

</Defs>