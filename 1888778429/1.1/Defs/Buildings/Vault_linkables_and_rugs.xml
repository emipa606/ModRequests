	<ThingDef ParentName="BuildingBase">
		<defName>Vt_server</defName>
		<label>Vault Server</label>
		<description>This vault server offers is caluclation-capabilities to speed up nearby production up to 10%</description>
		<graphicData>
			<texPath>Things/Building/Furniture/vtserver</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,3)</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<uiIconPath>Things/Building/Furniture/vtserver_south</uiIconPath>
		<uiIconScale>0.8</uiIconScale>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Mass>10</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>-5</Beauty>
		</statBases>
		<size>(1,1)</size>
		<comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerPlant</compClass>
        <basePowerConsumption>50</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
	  <li Class="CompProperties_Glower">
        <glowRadius>4</glowRadius>
        <!-- <glowColor>(46,228,161,0)</glowColor> -->
		<glowColor>(117,15,15,1)</glowColor>
      </li>
		<li Class="CompProperties_Facility">
			<statOffsets>
				<WorkTableWorkSpeedFactor>0.10</WorkTableWorkSpeedFactor>
			</statOffsets>
			<maxDistance>20</maxDistance>
			<maxSimultaneous>10</maxSimultaneous>
		</li>
		</comps>
		<costList>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>	
		<passability>Impassable</passability>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>300</costStuffCount>
		<rotatable>true</rotatable>
		<designationHotKey>Misc4</designationHotKey>
		<designationCategory>VaultItems</designationCategory>
		<destroyable>true</destroyable>
		<tradeability>None</tradeability>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
	</ThingDef>
	
	
	<ThingDef ParentName="BuildingBase">
	<defName>VTVaultVent</defName>
	<label>vault vent</label>
	<thingClass>Building_Vent</thingClass>
    <graphicData>
      <texPath>Things/Building/Furniture/vaultvent</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
    <blockWind>true</blockWind>
		<fillPercent>1</fillPercent>
    <coversFloor>true</coversFloor>
    <blockLight>true</blockLight>
    <castEdgeShadows>true</castEdgeShadows>
    <canOverlapZones>false</canOverlapZones>
    <staticSunShadowHeight>1.0</staticSunShadowHeight>
    <statBases>
			<WorkToMake>400</WorkToMake>
			<MaxHitPoints>4000</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<tickerType>Rare</tickerType>
		<description>A strong, heat-resistant vent designed for use in a vault.</description>
		<costList>
			<Steel>75</Steel>
		</costList>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<placeWorkers>
			<li>PlaceWorker_Vent</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<designationCategory>Temperature</designationCategory>
    <comps>
      <li Class="CompProperties_Flickable">
        <commandTexture>UI/Commands/Vent</commandTexture>
        <commandLabelKey>CommandDesignateOpenCloseVentLabel</commandLabelKey>
        <commandDescKey>CommandDesignateOpenCloseVentDesc</commandDescKey>
      </li>
    </comps>
	<researchPrerequisites>
		<li>VaultFurniture</li>
	</researchPrerequisites>
  </ThingDef>
	

	
	<ThingDef ParentName="BuildingBase">
		<defName>vtcompter</defName>
		<label>Vault Terminal</label>
		<description>This is a small vault terminal. It can be placed almost anywhere. Increases production by 10%.</description>
		<graphicData>
			<texPath>Things/Building/Furniture/terminal</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawRotated>false</drawRotated>
			<drawSize>(2.5,2.5)</drawSize>
			<damageData>
				<rect>(0.35,0.35,0.3,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconScale>0.5</uiIconScale>
		<rotatable>true</rotatable>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>8</pathCost>
		<fillPercent>0.15</fillPercent>
		<costList>
			<Steel>20</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>	
		<minifiedDef>MinifiedThing</minifiedDef>
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0</Flammability>
			<Mass>1</Mass>
			<Beauty>0.5</Beauty>
		</statBases>
		<selectable>true</selectable>
		<building>
			<isEdifice>false</isEdifice>
		</building>	
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>5</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(252,187,113,0)</glowColor>
			</li>
			<li Class="CompProperties_Facility">
			<statOffsets>
				<WorkTableWorkSpeedFactor>0.10</WorkTableWorkSpeedFactor>
			</statOffsets>
			<maxDistance>5</maxDistance>
			<maxSimultaneous>4</maxSimultaneous>
		</li>
		</comps>
		<soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>VaultItems</designationCategory>
		<destroyable>true</destroyable>
		<tradeability>None</tradeability>
		<clearBuildingArea>false</clearBuildingArea>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
	</ThingDef>
	
	 
	

	
</Defs>
