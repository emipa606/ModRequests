<Defs>

	<ThingDef ParentName="VaultRugBase">
		<defName>groundplate_one</defName>
		<label>Vault Groundplate</label>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Rug/groundplate</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<building>    
			<isEdifice>false</isEdifice>
		</building>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<passability>Standable</passability>
		<statBases>
			<MaxHitPoints>140</MaxHitPoints>
			<WorkToBuild>20</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>0.1</Beauty>
			<Mass>10</Mass>
		</statBases>
		<size>(3,3)</size>
		<costList>
		<Steel>1</Steel>
		</costList>
		<description>A vault floor tile. 3x3 big. has pure decorative purpose</description>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<rotatable>false</rotatable>
		<clearBuildingArea>false</clearBuildingArea>
		<saveCompressible>false</saveCompressible>
		<designationHotKey>Misc7</designationHotKey>
		<designationCategory>VaultItems</designationCategory>
		<researchPrerequisites>
			<li>VaultFurniture</li>
		</researchPrerequisites>
		<designatorDropdown>Vaultruggs</designatorDropdown>
	</ThingDef>
	
	<DesignatorDropdownGroupDef>
		<defName>Vaultruggs</defName>
	</DesignatorDropdownGroupDef>	

