<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <TerrainDef ParentName="FloorBase">
    <defName>Toxistrand_Carpet</defName>
    <label>toxistrand carpet</label>
    <description>A "carpet-like" layering of the toxistrand fabric creating a glue-like surface. Due to its sticky nature, it is extremely hard to clean.</description>
    <texturePath>Terrain/Surfaces/Carpet</texturePath>
      <color>(241,200,129)</color>
    <edgeType>Hard</edgeType>
    <renderPrecedence>70</renderPrecedence>
    <researchPrerequisites>
      <li>CarpetMaking</li>
    </researchPrerequisites>
    <pathCost>14</pathCost>
    <costList>
      <ToxiStrandTextile>15</ToxiStrandTextile>
    </costList>
    <designationHotKey>Misc6</designationHotKey>
    <statBases>
      <Beauty>2</Beauty>
      <Flammability>0.0</Flammability>
      <WorkToBuild>800</WorkToBuild>
      <CleaningTimeFactor>5</CleaningTimeFactor>
    </statBases>
    <uiOrder>2150</uiOrder>
  </TerrainDef>

</Defs>