<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantBase">
    <defName>Plant_ToxiStrand</defName>
    <label>toxistrand</label>
    <description>A toxic variant of devilstrand, forms strand-like goo as it coagulates. can be used in standard apparel and be used to make glue like flooring.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/ToxiStrand</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>14</pathCost>
    <ingestible />
    <plant>
      <growDays>22.5</growDays>
      <sowWork>400</sowWork>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>ToxiStrandTextile</harvestedThingDef>
      <harvestYield>6</harvestYield>
      <neverBlightable>true</neverBlightable>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <sowMinSkill>12</sowMinSkill>
      <sowResearchPrerequisites>
        <li>Devilstrand</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.1</topWindExposure>
      <visualSizeRange>0.3~1.0</visualSizeRange>
      <wildOrder>2</wildOrder>
      <pollution>PollutedOnly</pollution>
      <fertilityMin>0.50</fertilityMin>
      <fertilitySensitivity>0</fertilitySensitivity>
    </plant>
  </ThingDef>
</Defs>