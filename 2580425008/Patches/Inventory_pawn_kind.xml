<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Brewing Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
        <operations>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_melee"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_Whiskey</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_trader"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_DoubleWhiskey</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_molotov"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_Vodka</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>2</max>
        		  </countRange>
        		</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_pompe"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_Gin</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_fusil"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_Gin</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/PawnKindDef[defName = "Mafia_leader"]/inventoryOptions/subOptionsChooseOne</xpath>
                <value>
        		<li>
        		  <thingDef>VBE_AmbrandyNormal</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
        		<li>
        		  <thingDef>VBE_AmbrandyGood</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
        		<li>
        		  <thingDef>VBE_AmbrandyExcellent</thingDef>
        		  <countRange>
        		    <min>1</min>
        		    <max>1</max>
        		  </countRange>
        		</li>
                </value>
            </li>
        </operations>
    </match>
</Operation>



</Patch>