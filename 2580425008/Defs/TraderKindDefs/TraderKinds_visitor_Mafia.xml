<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Visitor_Mafia</defName>
    <label>visiteur</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>30~60</countRange>
      </li>
      <li MayRequire="VanillaExpanded.VBrewE" Class="StockGenerator_Category">
        <categoryDef>VBE_Drinks</categoryDef>
        <thingDefCountRange>-2~2</thingDefCountRange>
        <totalPriceRange>20~40</totalPriceRange>
      </li>
      <li MayRequire="VanillaExpanded.VBrewE" Class="StockGenerator_Category">
        <categoryDef>VBE_DrinksLuxurySpirits</categoryDef>
        <thingDefCountRange>-10~2</thingDefCountRange>
        <totalPriceRange>50~100</totalPriceRange>
      </li>
      <li MayRequire="VanillaExpanded.VBrewE" Class="StockGenerator_Category">
        <categoryDef>VBE_DrinksSimpleSpirits</categoryDef>
        <thingDefCountRange>-1~2</thingDefCountRange>
        <totalPriceRange>20~100</totalPriceRange>
      </li>
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <totalPriceRange>50~100</totalPriceRange>
      </li>
      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>1</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>
      
      
      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

</Defs>

