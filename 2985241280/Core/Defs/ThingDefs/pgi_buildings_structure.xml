<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="PGI_PStructureBase" Name="PGI_PerfectColumn_MK">
        <defName>PGI_PerfectColumnMKI</defName>
        <label>perfect column</label>
        <description>A perfect column designed for interior decoration.</description>
        <graphicData>
            <texPath>Building/Atlas/pgicolumn</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(1,1)</drawSize>
            <drawOffset>(0,0,0.2)</drawOffset>
        </graphicData>
        <canOverlapZones>true</canOverlapZones>
        <specialDisplayRadius>6.9</specialDisplayRadius>
        <passability>PassThroughOnly</passability>
        <costList>
            <PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
        </costList>
        <comps Inherit="false">
            <li Class="CompProperties_MeditationFocus">
                <statDef>MeditationFocusStrength</statDef>
                <focusTypes>
                    <li>Flame</li>
                    <li>Dignified</li>
                    <li>Artistic</li>
                    <li>Morbid</li>
                    <li>Minimal</li>
                </focusTypes>
            </li>
        </comps>
    </ThingDef>
    <ThingDef ParentName="PGI_PerfectColumn_MK">
		<defName>PGI_PerfectColumnMKII</defName>
		<label>perfect column MK2</label>
		<graphicData>
			<texPath>Building/Atlas/pgicolumnstuff</texPath>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
        <stuffCategories>
            <li>Metallic</li>
            <li>Woody</li>
            <li>Stony</li>
            <li>Fabric</li>
            <li>Leathery</li>
            <li MayRequire="Ludeon.Rimworld.Anomaly">Bioferrite</li>
        </stuffCategories>
        <costStuffCount>1</costStuffCount>
        <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<costList Inherit="false"/>
    </ThingDef>

    <ThingDef ParentName="PGI_PStructureBase" Name="PGI_PerfectWall_MK">
		<defName>PGI_PerfectWallMKI</defName>
		<label>perfect wall</label>
		<description>Perfect wall, can't be harmed by any attack.</description>
		<graphicData>
			<texPath>Building/Atlas/pgiwall_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>CornerFiller</linkType>
			<linkFlags>
				<li>Wall</li>
			</linkFlags>
		</graphicData>
		<uiIconPath>Building/Atlas/pgiwallicon</uiIconPath>
		<costList>
			<PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
		</costList>
		<building>
            <isPlaceOverableWall>true</isPlaceOverableWall>
            <supportsWallAttachments>true</supportsWallAttachments>
            <relatedBuildCommands>
                <li>PGI_PerfectDoorMKI</li>
                <li>PGI_PerfectDoorMKII</li>
                <li>PGI_PerfectOrnateDoorMKI</li>
                <li>PGI_PerfectOrnateDoorMKII</li>
            </relatedBuildCommands>
		</building>
	</ThingDef>
    <ThingDef ParentName="PGI_PerfectWall_MK">
		<defName>PGI_PerfectWallMKII</defName>
		<label>perfect wall MK2</label>
		<graphicData>
			<texPath>Building/Atlas/pgiwallstuff_Atlas</texPath>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<uiIconPath>Building/Atlas/pgiwallstufficon</uiIconPath>
        <stuffCategories>
            <li>Metallic</li>
            <li>Woody</li>
            <li>Stony</li>
            <li>Fabric</li>
            <li>Leathery</li>
            <li MayRequire="Ludeon.Rimworld.Anomaly">Bioferrite</li>
        </stuffCategories>
        <costStuffCount>1</costStuffCount>
        <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<costList Inherit="false"/>
    </ThingDef>

    <ThingDef ParentName="PGI_PStructureBase" Name="PGI_PerfectDoor_MK">
		<defName>PGI_PerfectDoorMKI</defName>
		<label>perfect door</label>
		<description>Perfect door, can't be harmed by any attack.</description>
		<thingClass>Building_Door</thingClass>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<graphicData>
			<texPath>Building/Atlas/pgidoor_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<uiIconPath>Building/Atlas/pgidooricon</uiIconPath>
		<costList>
			<PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
		</costList>
        <statBases>
            <DoorOpenSpeed>10</DoorOpenSpeed>
        </statBases>
        <drawerType>RealtimeOnly</drawerType>
        <passability>Standable</passability>
		<neverMultiSelect>false</neverMultiSelect>
        <coversFloor>false</coversFloor>
		<building>
            <roamerCanOpen>false</roamerCanOpen>
			<canPlaceOverWall>true</canPlaceOverWall>
			<soundDoorOpenPowered>Door_OpenPowered</soundDoorOpenPowered>
			<soundDoorClosePowered>Door_ClosePowered</soundDoorClosePowered>
			<soundDoorOpenManual>Door_OpenManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_CloseManual</soundDoorCloseManual>
			<blueprintClass>Blueprint_Door</blueprintClass>
            <relatedBuildCommands>
                <li>PGI_PerfectWallMKI</li>
                <li>PGI_PerfectWallMKII</li>
            </relatedBuildCommands>
		</building>
		<comps>
			<li Class="CompProperties_Forbiddable">
				<allowNonPlayer>false</allowNonPlayer>
			</li>
		</comps>
        <placeWorkers>
            <li>PlaceWorker_DoorLearnOpeningSpeed</li>
        </placeWorkers>
	</ThingDef>
    <ThingDef ParentName="PGI_PerfectDoor_MK">
		<defName>PGI_PerfectDoorMKII</defName>
		<label>perfect door MK2</label>
		<graphicData>
