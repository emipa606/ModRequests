            <li>Stony</li>
            <li>Fabric</li>
        </stuffCategories>
        <costStuffCount>1</costStuffCount>
        <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<costList Inherit="false"/>
    </ThingDef>

    <ThingDef ParentName="PGI_PStructureBase" Name="PGI_PerfectOrnateDoor_MK">
		<defName>PGI_PerfectOrnateDoorMKI</defName>
		<label>perfect ornate door</label>
		<description>Perfect ornate door, larger than normal door.</description>
		<thingClass>Building_MultiTileDoor</thingClass>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<graphicData>
			<texPath>Building/Atlas/pgiodoor2x1_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<uiIconPath>Building/Atlas/pgiordooricon</uiIconPath>
        <useBlueprintGraphicAsGhost>true</useBlueprintGraphicAsGhost>
        <size>(2,1)</size>
        <rotatable>true</rotatable>
		<costList>
			<PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
		</costList>
        <statBases>
            <DoorOpenSpeed>10</DoorOpenSpeed>
        </statBases>
        <drawerType>RealtimeOnly</drawerType>
        <passability>Standable</passability>
		<neverMultiSelect>false</neverMultiSelect>
        <coversFloor>false</coversFloor>
		<building>
            <roamerCanOpen>false</roamerCanOpen>
			<canPlaceOverWall>true</canPlaceOverWall>
			<soundDoorOpenPowered>Door_OpenPowered</soundDoorOpenPowered>
			<soundDoorClosePowered>Door_ClosePowered</soundDoorClosePowered>
			<soundDoorOpenManual>Door_OpenManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_CloseManual</soundDoorCloseManual>
			<blueprintClass>Blueprint_Build</blueprintClass>
            <relatedBuildCommands>
                <li>PGI_PerfectWallMKI</li>
                <li>PGI_PerfectWallMKII</li>
            </relatedBuildCommands>
            <blueprintGraphicData>
                <texPath>Building/Atlas/pgiordooricon</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <shaderType>EdgeDetect</shaderType>
                <drawSize>(2.6, 3.1)</drawSize>
            </blueprintGraphicData>
            <doorTopGraphic>
                <texPath>Building/Atlas/pgiodoor_Top</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <drawSize>(3.1, 2.3)</drawSize>
            </doorTopGraphic>
            <doorSupportGraphic>
                <texPath>Building/Atlas/pgiodoor_Support</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <drawSize>(3.1, 2.3)</drawSize>
            </doorSupportGraphic>
		</building>
		<comps>
			<li Class="CompProperties_Forbiddable">
				<allowNonPlayer>false</allowNonPlayer>
			</li>
		</comps>
        <placeWorkers>
            <li>PlaceWorker_MultiCellDoor</li>
        </placeWorkers>
	</ThingDef>
    <ThingDef ParentName="PGI_PerfectOrnateDoor_MK">
		<defName>PGI_PerfectOrnateDoorMKII</defName>
		<label>perfect ornate door MK2</label>
