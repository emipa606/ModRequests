	</ThingDef>

    <ThingDef ParentName="PGI_PStructureBase" Name="PGI_PerfectEmbrasure_MK">
		<defName>PGI_PerfectEmbrasureMKI</defName>
		<label>perfect embrasure</label>
		<description>Perfect wall with holes. Allow pawns shooting from relative safety.</description>
		<altitudeLayer>Building</altitudeLayer>
		<graphicData>
			<texPath>Building/Atlas/pgiembrasure_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
            <linkType>CornerFiller</linkType>
            <linkFlags>
                <li>Wall</li>
            </linkFlags>
		</graphicData>
		<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
		<uiIconPath>Building/Atlas/pgiembrasureicon</uiIconPath>
		<costList>
			<PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
		</costList>
        <fillPercent>0.7</fillPercent>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
            <isPlaceOverableWall>true</isPlaceOverableWall>
            <supportsWallAttachments>true</supportsWallAttachments>
            <relatedBuildCommands>
                <li>PGI_PerfectWallMKI</li>
                <li>PGI_PerfectWallMKII</li>
                <li>PGI_PerfectDoorMKI</li>
                <li>PGI_PerfectDoorMKII</li>
            </relatedBuildCommands>
		</building>
	</ThingDef>
    <ThingDef ParentName="PGI_PerfectEmbrasure_MK">
		<defName>PGI_PerfectEmbrasureMKII</defName>
		<label>perfect embrasure MK2</label>
		<graphicData>
			<texPath>Building/Atlas/pgiembrasurestuff_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<uiIconPath>Building/Atlas/pgiembrasurestufficon</uiIconPath>
        <stuffCategories>
            <li>Metallic</li>
            <li>Woody</li>
            <li>Stony</li>
            <li>Fabric</li>
            <li>Leathery</li>
            <li MayRequire="Ludeon.Rimworld.Anomaly">Bioferrite</li>
        </stuffCategories>
        <costStuffCount>1</costStuffCount>
        <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<costList Inherit="false"/>
	</ThingDef>

    <ThingDef ParentName="PGI_PBuildingsBase" Name="PGI_PerfectFence_MK">
        <defName>PGI_PerfectFenceMKI</defName>
        <label>perfect fence</label>
        <description>Just perfect for your crops or animals.</description>
        <thingClass>Building</thingClass>
        <category>Building</category>
        <graphicData>
            <texPath>Building/Atlas/pgifence_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <linkType>Asymmetric</linkType>
            <linkFlags>
				<li>Fences</li>
            </linkFlags>
            <asymmetricLink>
                <linkFlags>
                    <li>Wall</li>
                </linkFlags>
                <linkToDoors>true</linkToDoors>
                <drawDoorBorderEast>
                    <color>(0,0,0)</color>
                    <size>(0.04, 0.77)</size>
                    <offset>(0.5, 0, -0.08)</offset>
                </drawDoorBorderEast>
                <drawDoorBorderWest>
                    <color>(0,0,0)</color>
                    <size>(0.04, 0.77)</size>
                    <offset>(-0.5, 0, -0.08)</offset>
                </drawDoorBorderWest>
            </asymmetricLink>
        </graphicData>
        <uiIconPath>Building/Atlas/pgifenceicon</uiIconPath>
		<useHitPoints>false</useHitPoints>
		<costList>
			<PGI_PerfectlyGenericItem>1</PGI_PerfectlyGenericItem>
		</costList>
        <passability>PassThroughOnly</passability>
        <blockWind>false</blockWind>
        <fillPercent>0.7</fillPercent>
        <building>
            <isFence>true</isFence>
            <isPlaceOverableWall>true</isPlaceOverableWall>
            <supportsWallAttachments>true</supportsWallAttachments>
            <ai_neverTrashThis>true</ai_neverTrashThis>
            <relatedBuildCommands>
                <li>PenMarker</li>
            </relatedBuildCommands>
        </building>
    </ThingDef>
	<ThingDef ParentName="PGI_PerfectFence_MK">
        <defName>PGI_PerfectFenceMKII</defName>
        <label>perfect fence MK2</label>
        <graphicData>
            <texPath>Building/Atlas/pgifencestuff_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
        </graphicData>
        <uiIconPath>Building/Atlas/pgifencestufficon</uiIconPath>
        <stuffCategories>
            <li>Metallic</li>
