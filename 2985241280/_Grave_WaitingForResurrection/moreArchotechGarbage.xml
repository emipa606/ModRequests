<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
	    <mods>
            <li>More Archotech Garbage Reupload</li>
	    </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <!-- Recipes -->
				<li Class="PatchOperationAdd">
                    <xpath>Defs</xpath>
                    <value>
                        <RecipeDef ParentName="PGIRecipe_ConverterWorksiteBase">
                            <defName>PGI_MAG_ArchotechScrap</defName>
                            <label>make archotech fragment</label>
                            <products>
                                <ArchotechScrap>75</ArchotechScrap>
                            </products>
                        </RecipeDef>
                        <RecipeDef ParentName="PGIRecipe_ConverterWorksiteBase">
                            <defName>PGI_MAG_ArchotechMass</defName>
                            <label>make archotech mass</label>
                            <products>
                                <archotechmatteraddingsomecraptoavoidproblems>75</archotechmatteraddingsomecraptoavoidproblems>
                            </products>
                        </RecipeDef>
                        <RecipeDef ParentName="PGIRecipe_ConverterWorksiteBase">
                            <defName>PGI_MAG_ArchotechishMedicine</defName>
                            <label>make archotech medicine</label>
                            <products>
                                <ArchotechishMedicine>75</ArchotechishMedicine>
                            </products>
                        </RecipeDef>
                    </value>
				</li>
            </operations>
	    </match>
    </Operation>
    
</Patch>