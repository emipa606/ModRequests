<Defs>
    <RecipeDef Abstract="True" ParentName="PGI_PRecipeCostBase" Name="PGIRecipe_PerfectMechGestatorBase">
        <recipeUsers>
            <li>PGI_PerfectMechGestator</li>
        </recipeUsers>
        <description>Gestate mech.</description>
        <jobString>Gestating mech.</jobString>
        <gestationCycles>1</gestationCycles>
        <formingTicks>1</formingTicks>
        <mechanitorOnlyRecipe>true</mechanitorOnlyRecipe>
    </RecipeDef>
    
    <RecipeDef ParentName="PGIRecipe_PerfectMechGestatorBase">
        <defName>PGI_PMechResurrect</defName>
        <label>resurrect mechanoid</label>
        <description>Bring a friendly mechanoid back to life.</description>
        <jobString>Send out a necromancer</jobString>
        <mechResurrection>true</mechResurrection>
        <fixedIngredientFilter>
            <specialFiltersToAllow>
                <li>AllowCorpsesMechFriendly</li>
            </specialFiltersToAllow>
        </fixedIngredientFilter>
        <forceHiddenSpecialFilters>
            <li>AllowCorpsesMechFriendly</li>
        </forceHiddenSpecialFilters>
        <ingredients Inherit="false">
            <li>
                <filter>
                    <categories>
                        <li>CorpsesMechanoid</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
    </RecipeDef>
    
