		<defaultLabelColor>(0.65, 0.3, 0.9)</defaultLabelColor>
		<isBad>false</isBad>
	</HediffDef>

	<HediffDef Name="addedPartGlittery" Abstract="True">
		<hediffClass>Hediff_Implant</hediffClass>
		<defaultLabelColor>(244,210,131)</defaultLabelColor>
		<isBad>false</isBad>
