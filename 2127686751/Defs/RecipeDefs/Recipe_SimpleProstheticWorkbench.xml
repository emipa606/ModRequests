	<RecipeDef ParentName="RecipeSimpleProsthesis">
		<defName>CreateArtificialSpine</defName>
		<label>craft artificial spine</label>
		<description>Crafts an artificial spine from steel.</description>
		<products>
			<ArtificialSpine>1</ArtificialSpine>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>35</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

