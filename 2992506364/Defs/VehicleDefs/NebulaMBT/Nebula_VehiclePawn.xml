<?xml version="1.0" encoding="utf-8" ?>

<Defs>	
	<Vehicles.VehicleDef ParentName="VehiclePawn_Armored">
		<defName>TLRS_Nebula_tank</defName>
		<label>Nebula</label>
		<description>A main battle tank. Used on the frontlines of ground assaults and in urban combat. This kind of tank was developed by the development collaboration of the research and engineering departments after the failure of the 6th generation tanks. These 7th generation tanks are made with armor stronger than catapract and armament more advanced than the previous models. The liberation has planned to make the tank invulnerable to any kinds of attacks with an energy shield and robust weapons such as lancer turrets that are capable of shooting a piercing laser to counter the strongest armor, but the program to make this tank as advanced as possible was cut due to the army'sÂ  demand for a reliable, cheap to produce, and effective combat ability.\n\n&lt;color=#bb8f04&gt;Crew:&lt;/color&gt; Driver x1, Gunner x1\n&lt;color=#bb8f04&gt;Fuel type:&lt;/color&gt; Chemfuel</description>
		
		<graphicData>
			<texPath>Things/Vehicle/TLRS_taki</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(4, 8)</drawSize>
			<color>(72, 77, 72)</color>
			<colorTwo>(56, 55, 50)</colorTwo>
			<colorThree>(100, 69, 59)</colorThree>
		</graphicData>
		
		<fillPercent>1</fillPercent>
		
		<buildDef>TLRS_MBT_Blueprint</buildDef>
		
		<size>(3, 6)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Combat</vehicleCategory>
		<techLevel>Spacer</techLevel>
		<vehicleType>Land</vehicleType>
		
		<statBases>
			<ArmorRating_Blunt>0.9</ArmorRating_Blunt>
            <ArmorRating_Sharp>1.4</ArmorRating_Sharp>
			<Flammability>0.1</Flammability>
			<MarketValue>2240</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>450</Mass>
			<MoveSpeed>7.2</MoveSpeed>
			<CargoCapacity>450</CargoCapacity>
			<RepairRate>0.3</RepairRate>
			<AccelerationRate>0.06</AccelerationRate>
		</vehicleStats>
		
		<soundSustainersOnEvent>
			<li>
				<start>IgnitionOn</start>
				<stop>IgnitionOff</stop>
				<value>VVE_VehicleEngineHeavy</value>
			</li>
		</soundSustainersOnEvent>
		
		<race>
			<baseBodySize>6.5</baseBodySize>
		</race>
		
		<properties>
			<customRoadCosts AssignDefaults = "0.4" />
			
			<vehicleJobLimitations>
				<li>(RepairVehicle, 3)</li>
			</vehicleJobLimitations>
			
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
				</li>
				<li>
					<key>gunner</key>
					<label>Gunner</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>Turret</handlingTypes>
					
					<turretIds>
						<li>mainTurret</li>
						<li>coaxialTurret</li>
					</turretIds>
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>250</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>External</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<radius>3</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, -1)</li>
						<li>(0, -2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftTrack</key>
				<label>Left Track</label>
				<health>275</health>
				<depth>External</depth>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Left</side>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>RightTrack</key>
