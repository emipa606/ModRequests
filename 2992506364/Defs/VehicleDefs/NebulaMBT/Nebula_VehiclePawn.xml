			<li>
				<key>BackLeftDriveSprocket</key>
				<label>Back Left Drive Sprocket</label>
				<health>75</health>
				<depth>External</depth>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(-1, -2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightDriveSprocket</key>
				<label>Back Right Drive Sprocket</label>
