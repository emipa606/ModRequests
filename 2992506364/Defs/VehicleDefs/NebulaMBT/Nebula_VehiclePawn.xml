						<drawLayer>2</drawLayer>
						
						<angleRestricted/>
						<aimPieOffset>(0, 2.55)</aimPieOffset>
						
						<key>coaxialTurret</key>
						<parentKey>mainTurret</parentKey>
					</li>
				</turrets>
			</li>
		</comps>
	</Vehicles.VehicleDef>
	
