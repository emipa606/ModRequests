<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!---->
	<ThingDef ParentName="BuildingBase">
		<defName>NecrogeneExtractor</defName>
		<label>Necrogene extractor</label>
		<description>An incomprehensibly complex archotech mechanism capable of extracting some of the genes of the dead. It is not known what it was created for, but we can find a use for it.</description>
		<thingClass>Necrogene.Building_NecrogeneExtractor</thingClass>
		<containedPawnsSelectable>true</containedPawnsSelectable>
		<passability>Impassable</passability>
		<fillPercent>0.4</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<techLevel>Archotech</techLevel>
		<graphicData>
			<texPath>Buildings/NecrogeneExtractor/NecrogeneExtractor</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2, 2)</drawSize>
			<shadowData>
				<volume>(1.7, 0.8, 1.7)</volume>
			</shadowData>
		</graphicData>
		<size>(2, 2)</size>
		<canOverlapZones>false</canOverlapZones>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(1, 0, 2)</interactionCellOffset>
		<defaultPlacingRot>South</defaultPlacingRot>
		<building>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
		</building>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>9000</WorkToBuild>
			<Mass>50</Mass>
			<Flammability>0.5</Flammability>
			<MarketValue>2500</MarketValue>
		</statBases>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>Artifacts</li>
		</thingCategories>
		<tradeTags>
			<li>Artifact</li>
		</tradeTags>
		<thingSetMakerTags>
			<li>RewardStandardHighFreq</li>
		</thingSetMakerTags>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>400</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
	</ThingDef>
	<!---->
</Defs>