	<!-- Samurai Yoroi -->
	<ThingDef ParentName="ArmorSmithableBase">
		<defName>Ancient_SamuraiYoroi</defName>
		<label>Samurai Yoroi</label>
		<description>Sets of Heavy armor worn by Samurai class of feudal japan. It has slightly lower defense than plate armor, but has higher flexibility.</description>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<researchPrerequisite>PlateArmor</researchPrerequisite>
			<recipeUsers>
				<li>ElectricSmithy</li>
				<li>FueledSmithy</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>7</Crafting>
			</skillRequirements>
		</recipeMaker>
		<graphicData>
			<texPath>Armor/SamuraiYoroi</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>170</costStuffCount>
		<statBases>
			<WorkToMake>46000</WorkToMake>
			<MaxHitPoints>290</MaxHitPoints>
			<Mass>12</Mass>
			<StuffEffectMultiplierArmor>0.71</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>1.0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<Insulation_Heat>3</Insulation_Heat>
			<EquipDelay>12</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.35</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Legs</li>
