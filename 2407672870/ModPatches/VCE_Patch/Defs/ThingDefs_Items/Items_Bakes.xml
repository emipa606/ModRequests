<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="MealCooked">
		<defName>Insect_SimpleBake</defName>
		<label>insect simple bake</label>
		<description>A simple bake quickly cooked from insect meat and flour.</description>
		<graphicData>
			<texPath>Things/Meals/Insect_BakeSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>15</MarketValue>
			<WorkToMake>450</WorkToMake>
			<Nutrition>0.7</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
