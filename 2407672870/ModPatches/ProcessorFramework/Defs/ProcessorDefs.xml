<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ProcessorFramework.ProcessDef>
		<defName>AttractInsects</defName>
		<thingDef>Meat_Megaspider</thingDef>
		<ingredientFilter>
			<categories>
				<li>PlantFoodRaw</li>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
			</disallowedThingDefs>
		</ingredientFilter>
		<processDays>3</processDays>
		<efficiency>.8</efficiency>
		<capacityFactor>1</capacityFactor>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>0~45</temperatureSafe>
		<temperatureIdeal>10~36</temperatureIdeal>
		<ruinedPerDegreePerHour>0</ruinedPerDegreePerHour>
		<speedBelowSafe>0</speedBelowSafe>
		<speedAboveSafe>0</speedAboveSafe>	
		<bonusOutputs>
			<Megascarab>(.1,1)</Megascarab>
		</bonusOutputs>
	</ProcessorFramework.ProcessDef>
  
	<ProcessorFramework.ProcessDef>
		<defName>BreedInsects</defName>
		<thingDef>Meat_Megaspider</thingDef>
		<ingredientFilter>
			<categories>
				<li>PlantFoodRaw</li>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
			</disallowedThingDefs>
		</ingredientFilter>
		<processDays>1.2</processDays>
		<efficiency>1.0</efficiency>
		<capacityFactor>1</capacityFactor>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>0~45</temperatureSafe>
		<temperatureIdeal>10~36</temperatureIdeal>
		<ruinedPerDegreePerHour>0</ruinedPerDegreePerHour>
		<speedBelowSafe>0</speedBelowSafe>
		<speedAboveSafe>0</speedAboveSafe>						
  </ProcessorFramework.ProcessDef>
</Defs>