  <RecipeDef Name="Insect_CookMealBase" Abstract="True">
    <fixedIngredientFilter>
      <categories>
        <li>FoodRaw</li>
      </categories>
      <specialFiltersToDisallow>
        <li>AllowPlantFood</li>
      </specialFiltersToDisallow>
