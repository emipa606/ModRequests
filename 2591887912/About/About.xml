<?xml version="1.0" encoding="utf-8"?>

<ModMetaData>
    <name>Colonists Never Die</name>
    <author>busted_bunny</author>
    <supportedVersions>
        <li>1.3</li>
        <li>1.4</li>
    </supportedVersions>
    <description>
        This mod makes it so player controlled colonists can't die. Their vital parts like head or torso and vital
        organs are protected from getting destroyed as well, offering you death-free rimworld experience.
    </description>
    <packageId>bustedbunny.colonistsneverdie</packageId>
    <modDependenciesByVersion>
        <v1.3>
            <li>
                <packageId>brrainz.harmony</packageId>
                <displayName>Harmony</displayName>
                <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
                <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
            </li>
        </v1.3>
        <v1.4>
            <li>
                <packageId>brrainz.harmony</packageId>
                <displayName>Harmony</displayName>
                <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
                <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
            </li>
        </v1.4>
    </modDependenciesByVersion>
    <loadAfter>
        <li>brrainz.harmony</li>
    </loadAfter>
</ModMetaData>