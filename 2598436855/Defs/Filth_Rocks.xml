<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BaseFilth">
		<thingClass>ScatteredStones.Rocks</thingClass>
		<defName>Owl_Filth_Rocks</defName>
		<label>scattered stones</label>
		<graphicData>
			<texPath>Things/Filth/OwlRocks</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<color>(105,95,97)</color>
			<drawSize>(4,4)</drawSize>
		</graphicData>
		<filth>
			<placementMask>
				<li>Any</li>
			</placementMask>
			<disappearsInDays>60~90</disappearsInDays>
			<rainWashes>false</rainWashes>
			<cleaningWorkToReduceThickness>240</cleaningWorkToReduceThickness>
			<canFilthAttach>false</canFilthAttach>
			<cleaningSound>Interact_CleanFilth_Scattered</cleaningSound>
			<maxThickness>1</maxThickness>
		</filth>
		<statBases>
			<Beauty>-10</Beauty>
			<BeautyOutdoors>0</BeautyOutdoors>
			<Cleanliness>-2</Cleanliness>
		</statBases>
	</ThingDef>
</Defs>