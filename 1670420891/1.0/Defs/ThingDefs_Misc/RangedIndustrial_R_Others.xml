<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- ================================= Non-AR, AK or Bullpup stuff ==================================== -->
     <!-- ================================= ACR ==================================== -->

   <ThingDef ParentName="RNBaseAssaultRifle">
    <defName>RNGun_ACRContractorAR</defName>
    <label>ACR Contractor</label>
    <description>The Remington ACR (Adaptive Combat Rifle) is a modular assault rifle designed by Magpul Industries of Austin, Texas and was known as the Masada.

In late January 2008, Bushmaster Firearms International entered into a licensing agreement with Magpul whereby Bushmaster would take over production, future development, and sales of the Masada.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Rifles/Others/ACR_Contractor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
      <researchPrerequisite>PrecisionRifling</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <statBases>
      <WorkToMake>40000</WorkToMake>
      <Mass>3.5</Mass>
      <AccuracyTouch>0.65</AccuracyTouch>
      <AccuracyShort>0.73</AccuracyShort>
      <AccuracyMedium>0.67</AccuracyMedium>
      <AccuracyLong>0.56</AccuracyLong>
      <RangedWeapon_Cooldown>1.5</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>7</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>RNBullet_556x45_Rifle</defaultProjectile>
        <warmupTime>0.85</warmupTime>
        <range>35.9</range>
        <burstShotCount>3</burstShotCount>
        <ticksBetweenBurstShots>7</ticksBetweenBurstShots>
        <soundCast>RNShot_GenericAR</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
		<weaponTags>
			<!--<li>IndustrialGunAdvanced</li>-->
			<li>RN_ACR_Contractor</li>
			<li>RH_UC_ACR</li>
		</weaponTags>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

