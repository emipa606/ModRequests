      <Mass>3.5</Mass>
      <AccuracyTouch>0.61</AccuracyTouch>
      <AccuracyShort>0.70</AccuracyShort>
      <AccuracyMedium>0.66</AccuracyMedium>
      <AccuracyLong>0.59</AccuracyLong>
      <RangedWeapon_Cooldown>1.5</RangedWeapon_Cooldown>
