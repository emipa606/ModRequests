			<texPath>Things/Item/Equipment/SMG/P90_Contractor</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Interact_SMG</soundInteract>
    <recipeMaker>
      <researchPrerequisite>GasOperation</researchPrerequisite>
