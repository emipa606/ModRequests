<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef Name="Kenshi_Skeleton_PawnKindBase" Abstract="True">
    <race>Kenshi_Skeleton</race>
    
    <defaultFactionType>TribeCivil</defaultFactionType>

    <backstoryCategories>
      <li>SkeletonBackstoryAncient</li>
    </backstoryCategories>

    <chemicalAddictionChance>0.0</chemicalAddictionChance>

    <apparelTags>
      <li>Neolithic</li>
      <li>MedievalMilitary</li>
      <li>IndustrialBasic</li>
    </apparelTags>

    <initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>10~30</initialResistanceRange>

  </PawnKindDef>

  <!--- Low tier Soldier Melee -->

  <PawnKindDef ParentName="Kenshi_Skeleton_PawnKindBase">
    <defName>Kenshi_Skeleton_Soldier_Melee</defName>
    <label>soldier</label>

    <combatPower>60</combatPower>
    <itemQuality>Poor</itemQuality>

    <gearHealthRange>
      <min>0.2</min>
      <max>1.1</max>
    </gearHealthRange>

    <apparelMoney>
      <min>0</min>
      <max>150</max>
    </apparelMoney>

    <weaponMoney>
      <min>100</min>
      <max>500</max>
    </weaponMoney>

    <weaponTags>
      <li>MedievalMeleeDecent</li>
      <li>MedievalMeleeAdvanced</li>
    </weaponTags>

    <techHediffsMoney>
      <min>80</min>
      <max>1000</max>
    </techHediffsMoney>

    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
    </techHediffsTags>

    <techHediffsChance>0.15</techHediffsChance>
  </PawnKindDef>

