<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <BodyDef>
    <defName>DV_GiantOminousArchotechCube</defName>
    <label>cube-shaped archobeast</label>
    <corePart>
      <def>DV_OuterCubeLayer</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>DV_ArchotechGiantEye</def>
          <coverage>0.30</coverage>
          <height>Top</height>
          <groups>
            <li>HeadAttackTool</li>
          </groups>
          <parts>
            <li>
              <def>DV_ArchotechQuantumProcessor</def>
              <coverage>0.07</coverage>
              <depth>Inside</depth>
            </li>
            <li>
              <def>DV_ArchotechPupil</def>
              <customLabel>giant archopupil</customLabel>
              <coverage>0.08</coverage>
            </li>
            <li>
              <def>DV_ArchotechOmniSensor</def>
              <customLabel>vanometric omni sensor</customLabel>
              <coverage>0.08</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>DV_SemiInnerCubeLayer</def>
          <coverage>0.71</coverage>
          <parts>
            <li>
              <def>DV_VanometricPowerCell</def>
              <coverage>0.05</coverage>
              <depth>Inside</depth>
            </li>
            <li>
              <def>DV_InnerCubeLayer</def>
              <coverage>0.8</coverage>
              <parts>
                <li>
                  <def>DV_VanometricSiphon</def>
                  <coverage>0.05</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                   <def>DV_IchorStorageTank</def>
                   <coverage>0.09</coverage>
                   <depth>Inside</depth>
                  </li>
                <li>
                  <def>DV_InnerCubeCore</def>
                  <coverage>0.75</coverage>
                  <parts>
                    <li>
                      <def>DV_ArchoGyroscope</def>
                      <coverage>0.06</coverage>
                      <depth>Inside</depth>
                    </li>
                    <li>
                       <def>DV_ArchoDimensionalStabilizer</def>
                       <coverage>0.06</coverage>
                       <depth>Inside</depth>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>

