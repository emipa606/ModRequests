<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <!-- Outlander -->  
  
  <PawnKindDef ParentName="RSSkeleton_Base">
    <defName>Town_Guard_Skele</defName>
    <label>town guard</label>
    <labelPlural>town guards</labelPlural>		
    <combatPower>60</combatPower>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>BackstoriesSkeleton</li>
        </categories>
      </li>
    </backstoryFiltersOverride>	
    <isFighter>true</isFighter>
    <gearHealthRange>0.6~2</gearHealthRange>
    <apparelTags>
      <li>IndustrialMilitaryBasic</li>
      <li>BeltDefensePop</li>
    </apparelTags>
    <apparelMoney>400~600</apparelMoney>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <weaponTags>
      <li>Gun</li>
    </weaponTags>
    <weaponMoney>250~400</weaponMoney>
    <techHediffsChance>0.04</techHediffsChance>
    <techHediffsMoney>200~700</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <initialWillRange>6~8</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>	
  </PawnKindDef>
  
  <PawnKindDef ParentName="RSSkeleton_Base">
    <defName>Skeleton_Drifter</defName>
    <label>skeleton</label>
    <labelPlural>skeletons</labelPlural>		
    <defaultFactionType>OutlanderCivil</defaultFactionType>
    <backstoryFiltersOverride>
      <li>
        <categories>	
            <li>BackstoriesSkeleton</li>
        </categories>
      </li>			
    </backstoryFiltersOverride>	
    <chemicalAddictionChance>0.2</chemicalAddictionChance>
    <apparelTags>
      <li>IndustrialBasic</li>
      <li>IndustrialAdvanced</li>	  
    </apparelTags>
    <apparelRequired>
      <li>Apparel_Pants</li>
      <li>Apparel_Duster</li>
    </apparelRequired>
    <apparelColor>(58,58,58)</apparelColor>	
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <weaponTags>
      <li>Gun</li>
    </weaponTags>
    <weaponMoney>250~300</weaponMoney>	
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>200~800</techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <initialWillRange>3~5</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>	
  </PawnKindDef>  
  
   <!-- Tribal Skeletons (rare) --> 
 
  <PawnKindDef ParentName="TribalBase">
    <defName>Tribal_Skeleton</defName>
    <label>skeleton</label>
    <combatPower>75</combatPower>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>BackstoriesSkeleton</li>
        </categories>
      </li>
    </backstoryFiltersOverride>		
    <canBeSapper>true</canBeSapper>
    <apparelMoney>200~550</apparelMoney>
    <weaponMoney>300~300</weaponMoney>
    <weaponTags>
      <li>NeolithicMeleeAdvanced</li>
      <li>NeolithicRangedHeavy</li>	  
    </weaponTags>
    <initialWillRange>6~8</initialWillRange>
    <initialResistanceRange>13~20</initialResistanceRange>
  </PawnKindDef>
  

   <!-- Hostile Skeletons -->  

  <PawnKindDef ParentName="PirateBase">
    <defName>Pirate_Skele</defName>
    <race>Skeleton_</race>		
    <label>skeleton gunner</label>
    <combatPower>65</combatPower>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>BackstoriesSkeleton</li>
        </categories>
      </li>
    </backstoryFiltersOverride>	
    <gearHealthRange>0.7~2.3</gearHealthRange>
    <apparelMoney>400~1000</apparelMoney>
    <apparelTags>
      <li>IndustrialBasic</li>
      <li>IndustrialAdvanced</li>
      <li>IndustrialMilitaryBasic</li>
      <li>IndustrialMilitaryAdvanced</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <weaponMoney>250~345</weaponMoney>
    <weaponTags>
      <li>Gun</li>
    </weaponTags>
    <techHediffsChance>0.04</techHediffsChance>
    <techHediffsMoney>50~800</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <inventoryOptions>
      <skipChance>0.9</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>1</countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <biocodeWeaponChance>0.2</biocodeWeaponChance>
    <initialWillRange>8~10</initialWillRange>
    <initialResistanceRange>20~24</initialResistanceRange>		
  </PawnKindDef>  
  
  
   <!-- Spacer Skeletons = Meant for Empire faction, create new PawnKindDef if you do not want imperialistic skeletons. -->  
  
  <PawnKindDef MayRequire="Ludeon.RimWorld.Royalty">
    <defName>Skele_Spacer_Reaper</defName>
    <race>Skeleton_</race>	
    <label>imperial 'reaper'</label>
	<labelPlural>imperial 'reapers'</labelPlural>	
    <royalTitleChance>0.1</royalTitleChance>
    <titleSelectOne>
      <li>Freeholder</li>
      <li>Yeoman</li>
    </titleSelectOne>		
    <combatPower>130</combatPower>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>BackstoriesSkeleton</li>
        </categories>
      </li>
    </backstoryFiltersOverride>	
    <isFighter>true</isFighter>
    <gearHealthRange>1.6~2.8</gearHealthRange>
    <apparelTags>
      <li>IndustrialAdvanced</li>
      <li>IndustrialMilitaryAdvanced</li>
      <li>BeltDefensePop</li>
    </apparelTags>
    <apparelMoney>2500~5000</apparelMoney>
    <apparelAllowHeadgearChance>0.6</apparelAllowHeadgearChance>
    <weaponTags>
      <li>IndustrialGunAdvanced</li>
      <li>SpacerGun</li>
      <li>SniperRifle</li>  
    </weaponTags>
    <weaponMoney>2500~4000</weaponMoney>
    <techHediffsChance>0.08</techHediffsChance>
    <techHediffsMoney>200~1000</techHediffsMoney>
    <techHediffsTags>
       <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <skills>
      <li>
        <skill>Shooting</skill>
        <range>6~9</range>
      </li>
	  <li>
        <skill>Melee</skill>
        <range>4~6</range>
      </li>
    </skills>	
    <initialWillRange>15~18</initialWillRange>
    <initialResistanceRange>28~37</initialResistanceRange>	
  </PawnKindDef>  
  
</Defs>