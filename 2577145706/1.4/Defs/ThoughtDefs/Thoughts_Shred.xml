<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- We use this as a very fast expiring thought so pawns don't mind taking apart mechanents. -->
    <ThoughtDef>
        <defName>SkeletonButcher</defName>
        <showBubble>false</showBubble>
        <durationDays>0.0001</durationDays>
        <stackLimit>1</stackLimit>
        <stackedEffectMultiplier>1</stackedEffectMultiplier>
        <stages>
            <li>
                <label>_</label>
                <description>Don't look at me, I'm naked!!! 'O'</description>
                <baseMoodEffect>0</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef> 
</Defs>
