<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonSkeleton</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[NameSkeleton]</li>
        <li>name->[NameSkeleton] the [Adjectives]</li>	
        <li>name->[NameSkeleton] [Adjectives]</li>		
        <li>name->[Adjectives]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>NameSkeleton</keyword>
          <path>Skele_Names/NameSkeleton</path>
        </li>
        <li Class="Rule_File">
            <keyword>Adjectives</keyword>
            <path>Skele_Names/Adjectives</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>
 
</Defs>

