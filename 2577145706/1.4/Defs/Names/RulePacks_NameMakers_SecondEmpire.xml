<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>NamerFactionSkeletalEmpire</defName>
    <rulePack>
      <rulesStrings>	
		<li>r_name(p=3)->The [Numerical] [SkeletonCity]</li>	
		<li>r_name(p=3)->The [Numerical] [SkeletonDesc] [SkeletonCity]</li>				
	    <li>r_name(p=3)->The [Numerical] [SkeletonAdjectives] [SkeletonCity]</li>			
		
	    <li>r_name(p=2)->[SkeletonDesc] [SkeletonCity]</li>	
        <li>r_name(p=2)->[SkeletonAdjectives]-[SkeletonDesc]</li>			
	    <li>r_name(p=2)->[SkeletonAdjectives] [SkeletonCity]</li>	
	    <li>r_name(p=2)->[skeletal_wordgen]</li>	
	    <li>r_name(p=2)->The [skeletal_wordgen] [SkeletonCity]</li>	
	    <li>r_name(p=2)->The [Numerical] [skeletal_wordgen] [SkeletonCity]</li>			

	    <li>r_name->[NameSkeleton]'s [SkeletonCity]</li>	
	    <li>r_name->[NameSkeleton]'s [SkeletonAdjectives] [SkeletonDesc]</li>					

	    <li>skeletal_wordgen->[SSyll][SSyllMid]</li>	
	    <li>skeletal_wordgen->[SSyll][SSyllMid][SSyllEnd]</li>			
      </rulesStrings> 
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>SkeletonDesc</keyword>
					<path>Faction_Names/FactionNameSkeletonDesc</path>
				</li>
	            <li Class="Rule_File">
                    <keyword>SkeletonCity</keyword>
                    <path>Faction_Names/FactionsNameSkeleCity</path>
                </li>				
	            <li Class="Rule_File">
                    <keyword>SkeletonAdjectives</keyword>
                    <path>Faction_Names/FactionNameSkeleAdjectives</path>
                </li>	
                <li Class="Rule_File">
                    <keyword>Numerical</keyword>
                    <path>Titles/Numerical</path>
                </li>
                <li Class="Rule_File">
                    <keyword>NameSkeleton</keyword>
                    <path>Skele_Names/NameSkeleton</path>
                </li>				

				
	            <li Class="Rule_File">
                    <keyword>SSyll</keyword>
                    <path>Skele_Syllables/SkeleName_Syll</path>
                </li>				
	            <li Class="Rule_File">
                    <keyword>SSyllMid</keyword>
                    <path>Skele_Syllables/SkeleName_SyllMid</path>
                </li>	
                <li Class="Rule_File">
                    <keyword>SSyllEnd</keyword>
                    <path>Skele_Syllables/SkeleName_SyllEnd</path>
                </li>				
			</rulesRaw>	  
    </rulePack>
  </RulePackDef>
</Defs>

