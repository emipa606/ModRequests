<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ultratech: Altered Carbon Remastered</li>
		</mods>
		<success>Always</success>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName='Skeleton_']/modExtensions</xpath>
					<order>Append</order>
					<value>
						<li Class="AlteredCarbon.ExcludeRacesModExtension">
							<canBeGrown>false</canBeGrown>
							<acceptsStacks>true</acceptsStacks>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>