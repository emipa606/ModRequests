<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="PlantFoodRawBase">
        <defName>COMIGO_WheatGrain</defName>
        <label>wheat grains</label>
        <description>Raw wheat grain, ready to be milled or ground into flour. Can be left as is for a long time if stored in a dry barn, and is a good animal feed.</description>
        <soundInteract>Grain_Drop</soundInteract>
        <soundDrop>Grain_Drop</soundDrop>
        <graphicData>
            <texPath>Things/Items/Raw/COMIGO_WheatGrain</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.5</MarketValue>
            <Nutrition>0.05</Nutrition>
            <Flammability>1.3</Flammability>
        </statBases>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>45</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <ingestible>
            <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
            <optimalityOffsetFeedingAnimals>5</optimalityOffsetFeedingAnimals>
            <foodType>VegetableOrFruit</foodType>
        </ingestible>
    </ThingDef>
</Defs>