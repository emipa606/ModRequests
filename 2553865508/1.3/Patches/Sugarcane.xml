<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VCE_Sugarcane"]/description</xpath>
        <value>
            <description>A thick, tall, tropical grass with jointed stems. While not exactly the most nutritious thing to eat on its own, this genetically modified sugarcane is easily milled into sugar and then used for a variety of culinary creations - Particularly desserts.</description>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VCE_Sugarcane"]/plant/harvestYield</xpath>
        <value>
            <harvestYield>10</harvestYield>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VCE_Sugarcane"]/plant/harvestedThingDef</xpath>
        <value>
            <harvestedThingDef>COMIGO_Sugarcane</harvestedThingDef>
        </value>
    </Operation>
</Patch>