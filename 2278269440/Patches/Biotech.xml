<?xml version="1.0" encoding="utf-8" ?>

<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
          <li>Biotech</li>
        </mods>
        <match Class="PatchOperationSequence">			
            <operations>  
  <!--   Boreal Forest  -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "BorealForest"]/wildPlants/Plant_TreeGrayPine</xpath>
						<value>
							<Plant_TreeGrayPine>8.0</Plant_TreeGrayPine>
						</value>    
			  </li>
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "BorealForest"]/wildPlants/Plant_GrayGrass</xpath>
						<value>
							<Plant_GrayGrass>4.0</Plant_GrayGrass>
						</value>    
			  </li>  
  <!--   Tundra  -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "Tundra"]/wildPlants/Plant_TreeGrayPine</xpath>
						<value>
							<Plant_TreeGrayPine>0.9</Plant_TreeGrayPine>
						</value>    
			  </li>
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "Tundra"]/wildPlants/Plant_GrayGrass</xpath>
						<value>
							<Plant_GrayGrass>2</Plant_GrayGrass>
						</value>    
			  </li>
  <!--     Temperate Forest     -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TemperateForest"]/wildPlants/Plant_Witchwood</xpath>
						<value>
							<Plant_Witchwood>6.2</Plant_Witchwood>
						</value>    
			  </li>
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TemperateForest"]/wildPlants/Plant_GrayGrass</xpath>
						<value>
							<Plant_GrayGrass>0.2</Plant_GrayGrass>
						</value>    
			  </li>
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TemperateForest"]/wildPlants/Plant_Ripthorn</xpath>
						<value>
							<Plant_Ripthorn>0.65</Plant_Ripthorn>
						</value>    
			  </li>
  <!--     Temperate Swamp    -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TemperateSwamp"]/wildPlants/Plant_Witchwood</xpath>
						<value>
							<Plant_Witchwood>5.0</Plant_Witchwood>
						</value>    
			  </li>
  <!--    Tropical Rainforest   -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TropicalRainforest"]/wildPlants/Plant_RatPalm</xpath>
						<value>
							<Plant_RatPalm>3.8</Plant_RatPalm>
						</value>    
			  </li>
  <!--    Tropical Swamp   -->
			  <li Class="PatchOperationReplace">
					<xpath>Defs/BiomeDef[defName = "TropicalSwamp"]/wildPlants/Plant_RatPalm</xpath>
						<value>
							<Plant_RatPalm>3</Plant_RatPalm>
						</value>    
			  </li>			
				
			</operations>
        </match>
	</Operation>	
	
</Patch>	