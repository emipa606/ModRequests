<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<PreceptDef>
		<defName>InsectMeatEating_Neutral</defName>
		<issue>InsectMeat</issue>
		<label>don't care</label>
		<description>Meat is meat, regardless of the animal it comes from.</description>
		<impact>Low</impact>
		<displayOrderInIssue>0</displayOrderInIssue>
		<displayOrderInImpact>100</displayOrderInImpact>
		<associatedMemes>
			<li>Tunneler</li>
		</associatedMemes>
		<exclusionTags>
			<li>InsectMeatEatingLoved</li>
		</exclusionTags>
		<comps>
			<li Class="PreceptComp_SelfTookMemoryThought">
				<eventDef>AteInsectMeatDirect</eventDef>
				<thought>AteInsectMeat_Neutral</thought>
			</li>
			<li Class="PreceptComp_SelfTookMemoryThought">
				<eventDef>AteInsectMeatAsIngredient</eventDef>
				<thought>AteInsectMeatAsIngredient_Neutral</thought>
			</li>
			</comps>
	</PreceptDef>

	<ThoughtDef>
		<defName>AteInsectMeat_Neutral</defName>
		<showBubble>false</showBubble>
		<durationDays>1</durationDays>
		<stages>
			<li>
				<label>ate insect meat</label>
				<description>I ate some insect meat. Not sure what all the fuss is about.</description>
			</li>
		</stages>
	</ThoughtDef>

	<ThoughtDef>
		<defName>AteInsectMeatAsIngredient_Neutral</defName>
		<showBubble>false</showBubble>
		<durationDays>1</durationDays>
		<stages>
			<li>
				<label>ate cooked insect meat</label>
				<description>I ate a meal that contained insect meat. Apparently. It didn't taste that unusual to me.</description>
			</li>
		</stages>
	</ThoughtDef>
	
</Defs>