<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Gerrymon's Nautian Style</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName = "GM_Ocean"]/thingDefStyles</xpath>
						<value>
							<li>
								<thingDef>RitualArmchair</thingDef>
								<styleDef>Ocean_Armchair</styleDef>
							</li>
						</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName = "GM_Ocean"]/thingDefStyles</xpath>
						<value>
							<li>
								<thingDef>RitualChair</thingDef>
								<styleDef>Ocean_DiningChair</styleDef>
							</li>
						</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

