<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Stoneborn - Dwarven Style Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName = "DV_Dwarven"]/thingDefStyles</xpath>
						<value>
							<li>
								<thingDef>RitualChair</thingDef>
								<styleDef>DV_Dwarven_DiningChair</styleDef>
							</li>
						</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

