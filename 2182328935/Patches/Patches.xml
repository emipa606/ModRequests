<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationAdd">
		<xpath>
		Defs/ThingDef[defName="VFES_Artillery_Weapon"]/building/fixedStorageSettings/filter/categories</xpath>
		<value>
			<li>WDHeavyShells</li>				
		</value>
</Operation>
	
		
<Operation Class="PatchOperationSequence" MayRequire="Aoba.Fortress.Industrial">
  <operations>
  
    <li Class="PatchOperationAdd"> 
      <xpath>Defs/ThingDef[defName="FT_Gun_TurretHexMortar"]/building/fixedStorageSettings/filter/categories</xpath>
	  <value>
				<li>WDHeavyShells</li>
		</value>
    </li>
    
	<li Class="PatchOperationAdd"> 
      <xpath>Defs/ThingDef[defName="FT_Gun_TurretPrince"]/building/fixedStorageSettings/filter/categories</xpath>
	  <value>
				<li>WDHeavyShells</li>
		</value>
    </li>
	
	<li Class="PatchOperationAdd"> 
      <xpath>Defs/ThingDef[defName="FT_Gun_TurretEmpero"]/building/fixedStorageSettings/filter/categories</xpath>
	  <value>
				<li>WDHeavyShells</li>
				<li>WDCannonShells</li>
		</value>
    </li>
	
  </operations>
</Operation>
	
	
<Operation Class="PatchOperationFindMod">
	<mods>
        <li>Vanilla Factions Expanded - Pirates</li>
    </mods>
	<match Class="PatchOperationAdd">
		<xpath MayRequire="OskarPotocki.VFE.Pirates">Defs/ThingDef[defName="VFEP_Artillery_FieldGun"]/building/fixedStorageSettings/filter/categories</xpath>
			<value>
				<li>WDCannonShells</li>
			</value>
	</match>
</Operation>
</Patch>

