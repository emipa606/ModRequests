<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef Name="LanenDarn_ShieldBase" ParentName="BaseMeleeWeapon_Blunt_Quality" Abstract="True">
        <techLevel>Neolithic</techLevel>
        <thingCategories Inherit="False">
            <li>Apparel</li>
        </thingCategories>
        <equippedStatOffsets>
            <MeleeDodgeChance>1</MeleeDodgeChance>
        </equippedStatOffsets>
		<thingClass>VFECore.Apparel_Shield</thingClass>
		<apparel>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<bodyPartGroups>
                <li>Neck</li>
                <li>Torso</li>
                <li>Shoulders</li>
                <li>Arms</li>
                <li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>VFEC_OuterShell</li>
			</layers>
		<countsAsClothingForNudity>false</countsAsClothingForNudity>
		<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
		</apparel>
        <comps>
            <li>
                <compClass>CompColorable</compClass>
            </li>
        </comps>
        <equippedAngleOffset>-30</equippedAngleOffset>
        <tools>
            <li>
                <label>base</label>
                <capacities>
                    <li>Poke</li>
                </capacities>
                <power>9</power>
                <cooldownTime>2</cooldownTime>
            </li>
            <li>
                <label>centre</label>
                <labelUsedInLogging>false</labelUsedInLogging>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>9</power>
                <cooldownTime>2</cooldownTime>
            </li>
        </tools>
        <modExtensions>
            <li Class="VFECore.ThingDefExtension">
                <useFactionColourForPawnKinds>

                </useFactionColourForPawnKinds>
            </li>
        </modExtensions>
    </ThingDef>
  ---Basic Hoplon---	
	<ThingDef ParentName="LanenDarn_ShieldBase" Name="LanenDarn_BaseShield" Abstract="True">
		<label>Greek Shield</label>
        <description>A great round shield covered with sheet metal.</description>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <costStuffCount>150</costStuffCount>
    <recipeMaker>
      <recipeUsers>
        <li>LanenDarn_AncientAnvil</li>
        <li>FueledSmithy</li>
		<li>ElectricSmithy</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
      <researchPrerequisite>LanenDarn_BaseHoplons</researchPrerequisite>
    </recipeMaker>
        <statBases>
            <MaxHitPoints>200</MaxHitPoints>
            <Flammability>1</Flammability>
            <StuffEffectMultiplierArmor>0.80</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
            <WorkToMake>4000</WorkToMake>
            <Mass>4</Mass>
        </statBases>
    </ThingDef>

	<ThingDef ParentName="LanenDarn_BaseShield">
        <defName>LanenDarn_HoplonShield</defName>
        <label>Basic Hoplon</label>
        <description>A great round shield made from wood, serving as the base to the metal covered Hoplon.</description>
        <graphicData>
            <texPath>Apparel/OnTop/Shields/HoplonShield/IconHoplonShield</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1</drawSize>
        </graphicData>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <costStuffCount>150</costStuffCount>
        <statBases>
            <MaxHitPoints>200</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <StuffEffectMultiplierArmor>0.80</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>2.5</EquipDelay>
            <WorkToMake>2000</WorkToMake>
            <Mass>2.5</Mass>
        </statBases>
        <comps>
            <!--li Class="CompOversizedWeapon.CompProperties_OversizedWeapon"/-->
            <li Class="VFECore.CompProperties_Shield">
                <shieldTags>
                    <li>LanenDarn_MacedonShield</li>
                </shieldTags>
                <useDeflectMetalEffect>false</useDeflectMetalEffect>
                <coveredBodyPartGroups>
                    <li>Neck</li>
                    <li>Torso</li>
                    <li>Arms</li>
                    <li>Hands</li>
                </coveredBodyPartGroups>
                <offHandGraphicData>
                    <texPath>Apparel/OnTop/Shields/HoplonShield/HoplonShield</texPath>
                    <graphicClass>Graphic_Multi</graphicClass>
                    <shaderType>CutoutComplex</shaderType>
                    <drawSize>1.4</drawSize>
                </offHandGraphicData>
                <offHandHoldOffset>
                    <northDefault>
                        <offset>(0.05, 0, 0)</offset>
                        <behind>true</behind>
                    </northDefault>
                    <east>
                        <offset>(-0.15, 0, 0)</offset>
                        <behind>true</behind>
                    </east>
                    <south>
                        <offset>(0.2, 0, 0)</offset>
                    </south>
                    <west>
                        <offset>(0.15, 0, 0)</offset>
                    </west>
                </offHandHoldOffset>
            </li>
        </comps>
    </ThingDef>
