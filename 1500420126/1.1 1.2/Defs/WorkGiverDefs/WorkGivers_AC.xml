<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
	<WorkGiverDef>
		<defName>FillCompostBin</defName>
		<label>fill compost bin with raw compost</label>
		<giverClass>AdvancedCultivation.WorkGiver_FillCompostBin</giverClass>
		<workType>Hauling</workType>
		<verb>fill</verb>
		<gerund>filling</gerund>
		<priorityInType>120</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

	<WorkGiverDef>
		<defName>EmptyCompostBin</defName>
		<label>take fermented compost out of compost bin</label>
		<giverClass>AdvancedCultivation.WorkGiver_EmptyCompostBin</giverClass>
		<workType>Hauling</workType>
		<verb>take fermented compost</verb>
		<gerund>taking fermented compost from</gerund>
		<priorityInType>130</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

  <WorkGiverDef>
    <defName>DoBillsMakeCompost</defName>
    <label>make compost</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Crafting</workType>
    <priorityInType>95</priorityInType>
    <fixedBillGiverDefs>
      <li>TableButcher</li>
			<li>ButcherSpot</li>
    </fixedBillGiverDefs>
    <verb>craft</verb>
    <gerund>making compost at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>AC_TillCell</defName>
    <label>till field</label>
    <giverClass>AdvancedCultivation.WorkGiver_Till</giverClass>
    <workType>Growing</workType>
    <verb>till</verb>
    <gerund>tilling</gerund>
    <scanThings>false</scanThings>
    <scanCells>true</scanCells>
    <priorityInType>75</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>
  
</Defs>