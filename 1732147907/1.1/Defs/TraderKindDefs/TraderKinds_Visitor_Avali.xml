<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Visitor_Avali_Standard</defName>
    <label>trader</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>50~250</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>-2~5</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentSpacer</thingDef>
        <countRange>-2~2</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>1~6</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineUltratech</thingDef>
        <countRange>1~3</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSmallSurvivalPack</thingDef>
        <countRange>3~6</countRange>
      </li>
      
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <totalPriceRange>100~300</totalPriceRange>
      </li>

      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>1</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>
      
      <!-- Weapons -->
      <li Class="StockGenerator_Tag">
        <tradeTag>AvaliMeleeWeapon</tradeTag>
        <countRange>0~1</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>AvaliRangedWeapon</tradeTag>
        <countRange>0~1</countRange>
      </li>
      
      <!-- Apparel -->
      <li Class="StockGenerator_Tag">
        <tradeTag>AvaliApparel</tradeTag>
        <countRange>0~1</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Apparel_LifeSupportBeltAvali</thingDef>
        <countRange>1~1</countRange>
      </li>
      
      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->
      
      <!-- Exotic -->
      <!-- none -->
      
      <!-- Slaves -->
      <!-- none -->
      
      <!-- Animals -->
      <!-- none -->
      
      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

</Defs>