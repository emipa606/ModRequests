      <texPath>Canon-Flags/Banners/BOS_Banner_Mojave</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(5,5)</drawSize>
    </graphicData>
    <uiIconPath>Canon-Flags/Banners/BOS_Banner_Mojave_Icon</uiIconPath>
  </ThingDef>
  
