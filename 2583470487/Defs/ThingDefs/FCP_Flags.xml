<Defs>
  <!-- ============================================================================= -->
  <!-- BASE ALL BANNERS -->
  <!-- ============================================================================= -->
  <ThingDef Abstract="True" Name="CanonFalloutFlagBase" ParentName="BuildingBase">
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <constructEffect>ConstructMetal</constructEffect>
    <category>Building</category>
    <altitudeLayer>Blueprint</altitudeLayer>
    <blockWind>false</blockWind>
    <castEdgeShadows>false</castEdgeShadows>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <designationCategory>Furniture</designationCategory>
    <!--<placeWorkers>
		<li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>-->
    <passability>PassThroughOnly</passability>
    <pathCost>60</pathCost>
    <!--<building>
		<isEdifice>true</isEdifice>
    </building>
	<clearBuildingArea>true</clearBuildingArea>-->
    <researchPrerequisites>
      <li>CarpetMaking</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>
  <!-- ============================================================================= -->
  <!-- BASE Banner B (High Flags) -->
  <!-- ============================================================================= -->
  <ThingDef Abstract="True" Name="CanonFalloutFlagBaseB" ParentName="CanonFalloutFlagBase">
    <description>A High Flag, marking your territory.</description>
    <designatorDropdown>CanonFalloutBannerBaseB</designatorDropdown>
    <thingClass>Building</thingClass>
    <costList>
      <Steel>55</Steel>
      <Cloth>5</Cloth>
    </costList>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>0.2</Flammability>
      <!--<MarketValue>23</MarketValue>-->
      <Mass>7.5</Mass>
      <Beauty>25</Beauty>
    </statBases>
    <size>(1,1)</size>
    <rotatable>false</rotatable>
  </ThingDef>
  
