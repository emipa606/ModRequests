<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Children, school and learning</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<!-- Changes the minimum generation age to 20 -->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/PawnKindDef[defName="DIYHumans_CraftedHuman"]/minGenerationAge</xpath>
					<value>
						<minGenerationAge>20</minGenerationAge>
					</value>
				</li>

				<!-- Changes the maximum generation age to 40 -->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/PawnKindDef[defName="DIYHumans_CraftedHuman"]/maxGenerationAge</xpath>
					<value>
						<maxGenerationAge>20</maxGenerationAge>
					</value>
				</li>

			</operations>
		</match>

	</Operation>

</Patch>