<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Recipe for creating human -->
	<RecipeDef ParentName="SurgeryFlesh">
		<defName>DIYHumans_CreateHuman</defName>
		<label>Create human</label>
		<description>Create a fully-formed human from scratch by stitching together various body parts and organs.</description>
		<jobString>Creating a human.</jobString>
		<workAmount>80000</workAmount>
		<workSkillLearnFactor>2</workSkillLearnFactor>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Meat_Human</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Leather_Human</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Leg</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Arm</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Spine</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Stomach</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Brain</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Heart</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Lung</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Kidney</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Liver</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Eye</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Nose</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Ear</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Jaw</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Meat_Human</li>
				<li>Leather_Human</li>
				<li>Leg</li>
				<li>Arm</li>
				<li>Spine</li>
				<li>Stomach</li>
				<li>Brain</li>
				<li>Heart</li>
				<li>Lung</li>
				<li>Kidney</li>
				<li>Liver</li>
				<li>Eye</li>
				<li>Nose</li>
				<li>Ear</li>
				<li>Jaw</li>
			</thingDefs>
			<categories>
				<li>Medicine</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<DIYHumans_CraftedHuman>1</DIYHumans_CraftedHuman>
		</products>
		<unfinishedThingDef>DIYHumans_UnfinishedPawn</unfinishedThingDef>
		<skillRequirements>
			<Medicine>8</Medicine>
		</skillRequirements>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
	</RecipeDef>

</Defs>