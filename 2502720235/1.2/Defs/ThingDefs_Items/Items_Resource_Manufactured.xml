<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Crafted human -->
	<ThingDef ParentName="ResourceBase">
		<defName>DIYHumans_CraftedHuman</defName>
		<label>human</label>
		<description>A baseline human, mostly unmodified by gene engineering and mostly unchanged by evolutionary pressures on non-Earth planets.</description>
		<graphicData>
			<texPath>Things/Pawn/Humanoid/DIYHumans_CraftedHuman</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>1</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<useHitPoints>true</useHitPoints>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<MarketValue>1750</MarketValue>
			<Flammability>1.00</Flammability>
			<Mass>80</Mass>
			<DeteriorationRate>0.00</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="DIYHumans.CompProperties_CraftedPawn">
				<pawnKindDef>DIYHumans_CraftedHuman</pawnKindDef>
				<mentalStateChance>0.3</mentalStateChance>
				<possibleMentalStates>
					<li>WanderConfused</li>
					<li>PanicFlee</li>
					<li>Berserk</li>
					<li>Jailbreaker</li>
					<li>Slaughterer</li>
					<li>MurderousRage</li>
					<li>Wander_Psychotic</li>
					<li>SadisticRage</li>
					<li>Wander_Sad</li>
				</possibleMentalStates>
			</li>
		</comps>
	</ThingDef>

</Defs>