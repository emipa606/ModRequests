  <ThingDef Name="ROM_StarVampireSpawner">
    <defName>ROM_StarVampireSpawner</defName>
    <label>Star Vampire Spawner</label>
    <thingClass>ThingWithComps</thingClass>
    <graphicData>
      <texPath>BlankTex</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1</drawSize>
    </graphicData>
    <useHitPoints>false</useHitPoints>
    <selectable>false</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <tickerType>Normal</tickerType>
    <alwaysHaulable>false</alwaysHaulable>
    <comps>
      <li Class="CompDelayedSpawner.CompProperties_DelayedSpawner">
        <spawnList>
          <li>
            <pawnKind>ROM_StarVampire</pawnKind>
            <faction>ROM_StarVampire</faction>
          </li>
        </spawnList>
        <tickRate>60</tickRate>
        <ticksUntilSpawning>35</ticksUntilSpawning>
        <spawnsOnce>true</spawnsOnce>
        <destroyAfterSpawn>true</destroyAfterSpawn>
      </li>
    </comps>
  </ThingDef>
