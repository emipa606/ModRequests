  <!-- RACE DEFS -->
  <ThingDef ParentName="BaseCosmicHorrorRaceInit">
    <defName>ROM_DeepOne</defName>
    <label>Deep One</label>
    <description>A fish headed but frog bodied humanoid with large bulging eyes and webbed hands. They have powerful scaly bodies, designed to withstand the pressures of the ocean.</description>
    <techLevel>Animal</techLevel>
    <statBases>
      <Mass>200</Mass>
      <MoveSpeed>4.6</MoveSpeed>
      <ArmorRating_Blunt>0.16</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.16</ArmorRating_Sharp>
      <ComfyTemperatureMin>-80</ComfyTemperatureMin>
      <ComfyTemperatureMax>80</ComfyTemperatureMax>
    </statBases>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>ROM_ScratchMonstrous</li>
        </capacities>
        <power>11</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_LeftClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>ROM_ScratchMonstrous</li>
        </capacities>
        <power>11</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_RightClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>15</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>1</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <!--
    <verbs>
      <li>
        <verbClass>Verb_MeleeAttackDamage</verbClass>
        <defaultCooldownTime>0.9</defaultCooldownTime>
        <meleeDamageBaseAmount>12</meleeDamageBaseAmount>
        <meleeDamageDef>Cut</meleeDamageDef>
        <linkedBodyPartsGroup>ROM_Claws</linkedBodyPartsGroup>
      </li>
      <li>
        <verbClass>Verb_MeleeAttackDamage</verbClass>
        <defaultCooldownTime>1.5</defaultCooldownTime>
        <meleeDamageBaseAmount>20</meleeDamageBaseAmount>
        <meleeDamageDef>Bite</meleeDamageDef>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      </li>
    </verbs>
	-->
    <race>
      <bloodDef>ROM_Filth_BloodDeepOne</bloodDef>
      <intelligence>ToolUser</intelligence>
      <thinkTreeMain>ROM_CosmicHorror</thinkTreeMain>
      <thinkTreeConstant>ROM_CosmicHorrorConstant</thinkTreeConstant>
      <body>ROM_DeepOneBody</body>
      <baseBodySize>1.0</baseBodySize>
      <baseHealthScale>1.05</baseHealthScale>
      <!--
      <leatherColor>(57,59,74)</leatherColor>      
	  <leatherCommonalityFactor>0.01</leatherCommonalityFactor>
      <leatherLabel>deephide</leatherLabel>
      <leatherStatFactors>
        <ArmorRating_Sharp>1.5</ArmorRating_Sharp>
        <ArmorRating_Blunt>1.5</ArmorRating_Blunt>
        <ArmorRating_Heat>4.0</ArmorRating_Heat>
        <Insulation_Cold>1.5</Insulation_Cold>
        <Insulation_Heat>0.5</Insulation_Heat>
      </leatherStatFactors>
      <leatherMarketValueFactor>3</leatherMarketValueFactor>
	  -->
      <leatherDef>CH_Leather_DeepOne</leatherDef>
      <lifeExpectancy>1200</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>ROM_Spawn</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>ROM_FullyFormed</def>
          <minAge>100</minAge>
          <soundWounded>Pawn_ROM_DeepOne_Wounded</soundWounded>
          <soundDeath>Pawn_ROM_DeepOne_Death</soundDeath>
          <soundCall>Pawn_ROM_DeepOne_Call</soundCall>
        </li>
        <li>
          <def>ROM_Ancient</def>
          <minAge>500</minAge>
        </li>
        <li>
          <def>ROM_Eldritch</def>
          <minAge>1000</minAge>
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>1000</min>
        <max>2000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <butcherProducts>
      <ROM_RawCHFood_DeepOneMeat>50</ROM_RawCHFood_DeepOneMeat>
    </butcherProducts>
    <recipes>
      <li>Euthanize</li>
    </recipes>
    <modExtensions>
      <li Class="CosmicHorror.PawnExtension">
        <invisible>false</invisible>
        <regenRate>0.0</regenRate>
        <regenInterval>0</regenInterval>
        <sanityLossRate>0.008</sanityLossRate>
        <painFactor>1</painFactor>
      </li>
    </modExtensions>
  </ThingDef>
