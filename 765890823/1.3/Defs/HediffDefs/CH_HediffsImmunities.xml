<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef>
    <defName>ROM_CosmicHorrorImmunities</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>eldritch immunities</label>
    <description>eldritch immunities</description>
    <defaultLabelColor>(1,0,0.5)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <!--comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.18</severityPerDay>
      </li>
    </comps-->
    <stages>
      <li>
        <becomeVisible>false</becomeVisible>
        <makeImmuneTo>
          <li>ROM_SanityLoss</li>
          <li>WoundInfection</li>
          <li>Flu</li>
          <li>GutWorms</li>
          <li>MuscleParasites</li>
          <li>FibrousMechanites</li>
          <li>SensoryMechanites</li>
          <li>SleepingSickness</li>
          <li>Malaria</li>
          <li>Plague</li>
        </makeImmuneTo>
      </li>
    </stages>
  </HediffDef>
  <HediffDef>
    <defName>ROM_StarVampireDescent</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>descending</label>
    <description>descending</description>
    <defaultLabelColor>(1,0,0.5)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>
          <min>1000</min>
          <max>1000</max>
        </disappearsAfterTicks>
      </li>
    </comps>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <postFactor>0.5</postFactor>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>