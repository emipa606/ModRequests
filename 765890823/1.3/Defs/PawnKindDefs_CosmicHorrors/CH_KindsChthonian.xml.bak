    <combatPower>1000</combatPower>
    <defaultFactionType>ROM_Chthonian</defaultFactionType>
    <aiAvoidCover>true</aiAvoidCover>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Pawn/CosmicHorrors/CallOfCthulhu_Chthonian</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>7.0</drawSize>
          <shadowData>
            <volume>(0.7, 0.6, 0.4)</volume>
            <offset>(0,0,-0.25)</offset>
          </shadowData>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Pawn/CosmicHorrors/CallOfCthulhu_Chthonian</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>7.5</drawSize>
          <shadowData>
            <volume>(1, 0.8, 0.5)</volume>
            <offset>(0,0,-0.5)</offset>
          </shadowData>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Pawn/CosmicHorrors/CallOfCthulhu_Chthonian</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>8.1</drawSize>
          <shadowData>
            <volume>(1, 0.8, 0.5)</volume>
