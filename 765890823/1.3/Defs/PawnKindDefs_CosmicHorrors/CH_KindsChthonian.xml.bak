  <!-- RACE DEFS -->
  <ThingDef ParentName="BaseCosmicHorrorRaceInit">
    <defName>ROM_Chthonian</defName>
    <label>Chthonian</label>
    <description>Chthonians are powerful burrowers which can live for more than a thousand years, and are protective of their young. It is said that a chanting sound accompanies every chthonian, and that by such they can be detected while underground and unseen.</description>
    <statBases>
      <MoveSpeed>5.7</MoveSpeed>
      <ComfyTemperatureMin>-100</ComfyTemperatureMin>
      <ComfyTemperatureMax>100</ComfyTemperatureMax>
    </statBases>
    <techLevel>Animal</techLevel>
    <tools>
      <li>
        <label>tentacles</label>
        <capacities>
          <li>ROM_BluntMonstrous</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_Tentacles</linkedBodyPartsGroup>
      </li>
      <li>
        <label>coiling tentacles</label>
        <capacities>
          <li>ROM_ConstrictMonstrous</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_Tentacles</linkedBodyPartsGroup>
        <chanceFactor>0.25</chanceFactor>
      </li>
      <li>
        <label>overwhelming mind</label>
        <capacities>
          <li>ROM_Psychic</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_Mind</linkedBodyPartsGroup>
      </li>
      <li>
        <label>body</label>
        <capacities>
          <li>ROM_BluntMonstrous</li>
        </capacities>
        <power>25</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_BodyAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.33</chanceFactor>
      </li>
    </tools>
    <race>
      <body>ROM_ChthonianBody</body>
      <herdAnimal>false</herdAnimal>
      <baseBodySize>3.5</baseBodySize>
      <baseHungerRate>4.5</baseHungerRate>
      <baseHealthScale>13.0</baseHealthScale>
      <foodType>OmnivoreAnimal, AnimalProduct</foodType>
      <bloodDef>ROM_Filth_BloodDeepOne</bloodDef>
      <intelligence>ToolUser</intelligence>
      <thinkTreeMain>ROM_CosmicHorror</thinkTreeMain>
      <leatherDef>CH_Leather_Chthonian</leatherDef>
      <!-- Removed from 1.0
      <leatherColor>(78,60,61)</leatherColor>
      <leatherInsulation>1.35</leatherInsulation>
      <leatherLabel>chthonian chitin</leatherLabel>
      <leatherCommonalityFactor>0.01</leatherCommonalityFactor>
      <leatherMarketValueFactor>8</leatherMarketValueFactor>
      <leatherStatFactors>
        <ArmorRating_Sharp>3.4</ArmorRating_Sharp>
        <ArmorRating_Blunt>3.9</ArmorRating_Blunt>
        <ArmorRating_Heat>4.0</ArmorRating_Heat>
      </leatherStatFactors>
	  -->
      <trainability>Advanced</trainability>
      <gestationPeriodDays>100</gestationPeriodDays>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.035</manhunterOnTameFailChance>
      <wildness>0.989</wildness>
      <lifeExpectancy>2500</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>ROM_FullyFormed</def>
          <minAge>100</minAge>
          <soundWounded>Pawn_ROM_Chthonian_Wounded</soundWounded>
          <soundDeath>Pawn_ROM_Chthonian_Death</soundDeath>
          <soundCall>Pawn_ROM_Chthonian_Call</soundCall>
        </li>
        <li>
          <def>ROM_Ancient</def>
          <minAge>500</minAge>
          <soundWounded>Pawn_ROM_Chthonian_Wounded</soundWounded>
          <soundDeath>Pawn_ROM_Chthonian_Death</soundDeath>
          <soundCall>Pawn_ROM_Chthonian_Call</soundCall>
        </li>
        <li>
          <def>ROM_Eldritch</def>
          <minAge>2000</minAge>
          <soundWounded>Pawn_ROM_Chthonian_Wounded</soundWounded>
          <soundDeath>Pawn_ROM_Chthonian_Death</soundDeath>
          <soundCall>Pawn_ROM_Chthonian_Call</soundCall>
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>5000</min>
        <max>8000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <butcherProducts>
      <ROM_RawCHFood_ChthonianMeat>60</ROM_RawCHFood_ChthonianMeat>
    </butcherProducts>
    <recipes>
      <li>Euthanize</li>
    </recipes>
    <modExtensions>
      <li Class="CosmicHorror.PawnExtension">
        <invisible>false</invisible>
        <regenRate>0.0</regenRate>
        <regenInterval>0</regenInterval>
        <sanityLossRate>0.04</sanityLossRate>
        <painFactor>0.3</painFactor>
      </li>
    </modExtensions>
  </ThingDef>
