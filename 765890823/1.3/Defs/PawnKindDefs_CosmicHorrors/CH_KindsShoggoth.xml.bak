  <!-- RACE DEFS-->
  <ThingDef ParentName="BaseCosmicHorrorRaceInit" Name="ROM_Shoggoth">
    <defName>ROM_Shoggoth</defName>
    <label>shoggoth</label>
    <description>Large and oily masses that shift and change shape according to their needs. Eyes, mouth and tentacles appear, then just as swifty, are reabsorbed into the luminescent black protoplasm. They are reluctant servants to those that are willing to risk their rebellious nature.</description>
    <statBases>
      <Mass>3000</Mass>
      <MoveSpeed>1.5</MoveSpeed>
      <ArmorRating_Blunt>0.5</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.5</ArmorRating_Sharp>
      <LeatherAmount>0</LeatherAmount>
    </statBases>
    <tools>
      <li>
        <label>maws</label>
        <capacities>
          <li>ToxicBite</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>tentacles</label>
        <capacities>
          <li>ROM_BluntMonstrous</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>ROM_Tentacles</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claws</label>
        <capacities>
          <li>ROM_BluntMonstrous</li>
        </capacities>
        <power>13</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <chanceFactor>0.25</chanceFactor>
      </li>
      <li>
        <label>left claws</label>
        <capacities>
          <li>ROM_BluntMonstrous</li>
        </capacities>
        <power>13</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <chanceFactor>0.25</chanceFactor>
      </li>
    </tools>
    <!-- <comps> -->
    <!-- <li Class="CompBalloon.CompProperties_Balloon"> -->
    <!-- </li> -->
    <!-- </comps> -->
    <race>
      <bloodDef>ROM_Filth_BloodShoggoth</bloodDef>
      <hasGenders>false</hasGenders>
      <intelligence>ToolUser</intelligence>
      <thinkTreeMain>Animal</thinkTreeMain>
      <thinkTreeConstant>AnimalConstant</thinkTreeConstant>
      <body>ROM_ShoggothBody</body>
      <useLeatherFrom>Bear_Grizzly</useLeatherFrom>
      <baseBodySize>3.0</baseBodySize>
      <baseHealthScale>2.0</baseHealthScale>
      <lifeExpectancy>1200</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>ROM_Spawn</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>ROM_FullyFormed</def>
          <minAge>100</minAge>
          <soundWounded>Pawn_ROM_Shoggoth_Wounded</soundWounded>
          <soundDeath>Pawn_ROM_Shoggoth_Death</soundDeath>
          <soundCall>Pawn_ROM_Shoggoth_Call</soundCall>
        </li>
        <li>
          <def>ROM_Ancient</def>
          <minAge>500</minAge>
        </li>
        <li>
          <def>ROM_Eldritch</def>
          <minAge>1000</minAge>
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>1000</min>
        <max>2000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <butcherProducts>
      <ROM_RawCHFood_ShoggothMeat>30</ROM_RawCHFood_ShoggothMeat>
    </butcherProducts>
    <recipes>
      <li>Euthanize</li>
    </recipes>
    <modExtensions>
      <li Class="CosmicHorror.PawnExtension">
        <invisible>false</invisible>
        <regenRate>0.05</regenRate>
        <regenInterval>1000</regenInterval>
        <sanityLossRate>0.012</sanityLossRate>
        <painFactor>0.25</painFactor>
      </li>
    </modExtensions>
  </ThingDef>
