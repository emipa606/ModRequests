<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerIdeoRelic"]/rulePack/rulesStrings</xpath>
				<value>
					<li>relic->monument</li>
					<li>relic->oblisk</li>
					<li>relic->reminder</li>
					<li>relic->trophy</li>
					<li>relic->souvenier</li>
					<li>relic->heirloom</li>
					<li>relic->remembrance</li>
					
					<li>first->wide</li>
					<li>first->clouds</li>
					<li>first->planet</li>
					<li>first->distant</li>
					<li>first->far-out</li>
					<li>first->stretched</li>
					<li>first->undead</li>
					<li>first->wravenged</li>
				</value>
			</li>
		<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerAltar"]/rulePack/rulesStrings</xpath>
				<value>
					<li>buildingName->tabernacle</li>
					<li>buildingName->sanctum</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>