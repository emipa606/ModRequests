<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerRitualFestival"]/rulePack/rulesStrings</xpath>
				<value>
					<li>r_ritualName->[festivalName]</li>
					<li>r_ritualName->[joyword] [festivalName]</li>
					<li>joyword->happy</li>
					<li>joyword->incredible</li>
					<li>joyword->uplifiting</li>
					<li>joyword->gay</li> <!-- Fun fact: this word isn't just for gender preferrances- look it up...-->
					<li>joyword->merry</li>
					<li>joyword->fun</li>
					<li>joyword->crazy</li>
					<li>joyword->wild</li>
					<li>joyword->goofy</li>
				</value>
			</li>
		<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerFestivalGlobal"]/rulePack/rulesStrings</xpath>
				<value>
					<li>festival->gathering</li>
					<li>festival->merrymaking</li>
					<li>festival->frolic</li>
					<li>festival->ball</li>
					<li>festival->fair</li>
					<li>festival->communion</li>
					<li>festival->rejoicing</li>
					<li>festival->revelry</li>
					<li>festival->ceremony</li>
					<li>festival->gala</li>
					<li>festival->advent</li>
					<li>festival->fair</li>
					<li>festival->jubilee</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>