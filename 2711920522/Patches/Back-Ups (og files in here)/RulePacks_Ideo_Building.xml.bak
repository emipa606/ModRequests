<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Root -->

  <RulePackDef>
    <defName>NamerIdeoBuilding</defName>
    <rulePack>
      <rulesStrings>
        <li>r_ideoBuildingName(p=2)->[memeAdjective] [buildingName]</li>
        <li>r_ideoBuildingName->[buildingName] of [chosenTheme]</li>
        <li>r_ideoBuildingName->[chosenAdjective] [buildingName]</li>
        <li>r_ideoBuildingName->[keyDeity]'s [buildingName]</li>
        <li>r_ideoBuildingName->[buildingName] of [keyDeity]</li>
        <li>r_ideoBuildingName->[buildingName] of [memeConcept]</li>
        <li>r_ideoBuildingName->[buildingName] of [chosenIdeoName]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <!-- Building-specific base labels -->

  <RulePackDef>
    <defName>NamerAltar</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->altar</li>
        <li>buildingName->shrine</li>
        <li>buildingName->chancel</li>
        <li>buildingName->tomb</li>
		<li>buildingName->receptical</li>
		<li>buildingName->temple</li>
		<li>buildingName->sanctuary</li>
		<li>buildingName->pagoda</li>
		<li>buildingName->memorial</li>
		<li>buildingName->monument</li>
		<li>buildingName->beacon</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerIdeoSculpture</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->statue</li>
        <li>buildingName->sculpture</li>
        <li>buildingName->carving</li>
        <li>buildingName->image</li>
        <li>buildingName->form</li>
        <li>buildingName->shape</li>
		<li>buildingName->figure</li>
        <li>buildingName->structure</li>
        <li>buildingName->illistration</li>
		<li>buildingName->molding</li>
        <li>buildingName->design</li>
        <li>buildingName->figure</li>
		<li>buildingName->icon</li>
        <li>buildingName->statue</li>
        <li>buildingName->depiction</li>
		<li>buildingName->model</li>
        <li>buildingName->symbol</li>
        <li>buildingName->imitation</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerIdeogram</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->ideogram</li>
        <li>buildingName->symbol</li>
        <li>buildingName->mark</li>
        <li>buildingName->shape</li>
        <li>buildingName->emblem</li>
        <li>buildingName->figure</li>
        <li>buildingName->image</li>
        <li>buildingName->logo</li>
		<li>buildingName->emulation</li>
        <li>buildingName->crest</li>
        <li>buildingName->footprint</li>
        <li>buildingName->depiction</li>
        <li>buildingName->definition</li>
        <li>buildingName->presence</li>
        <li>buildingName->trademark</li>
        <li>buildingName->token</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerChristmasTree</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->tree</li>
        <li>buildingName->pine</li>
		<li>buildingName(p=0.5)->plant</li>
        <li>buildingName(p=0.3)->symbol</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBurnTarget</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->totem</li>
		<li>buildingName->tower</li>
		<li>buildingName->heap</li>
		<li>buildingName->flame stack</li>
        <li>buildingName->effigy</li>
        <li>buildingName->pyre</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerSmoker</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->smoker</li>
        <li>buildingName->gasser</li>
        <li>buildingName->fumer</li>
        <li>buildingName->smolderer</li>
	    <li>buildingName->furnace</li>
        <li>buildingName->burner</li>
        <li>buildingName->incinerator</li>
        <li>buildingName->inferno</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerCannibalPlatter</defName>
    <rulePack>
      <rulesStrings>
        <li>buildingName->platter</li>
        <li>buildingName->dish</li>
        <li>buildingName->feast</li>
        <li>buildingName->manpork</li>
		<li>buildingName->meal</li>
        <li>buildingName->flesh</li>
        <li>buildingName->human meat</li>
        <li>buildingName->man meal</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>