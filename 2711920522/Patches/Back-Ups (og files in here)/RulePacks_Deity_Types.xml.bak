<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Shared content -->

  <RulePackDef>
    <defName>DeityTypeMakerGlobal</defName>
    <include>
      <li>GlobalUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_deityType->[god] of [memeConcept]</li>
		<li>r_deityType->[god] of [memeConcept]</li>
		<li>r_deityType->[holy] [god] of [memeConcept]</li>

        <li>god(p=7)->God</li>
		<li>holy(p=7)->Holy</li>
		<li>holy->All Knowing, All Seeing and</li>
		<li>holy->Mighty</li>
		<li>holy->Majestic</li>
		<li>holy->Feared</li>
		<li>holy->Powerful</li>
		<li>holy->Temporary</li>
		<li>god(p=7)->All Powerful Being</li>
		<li>god(p=7)->Deity</li>
		<li>god(p=7)->Avatar</li>
		<li>god(p=7)->God</li>
        <li>god(p=4)->[memeGod]</li>
        <li>god(p=3)->Spirit</li>
		<li>god(p=3)->Etheral</li>
		<li>god(p=3)->Spirit</li>
        <li>god->Creator</li>
        <li>god->Master</li>
		<li>god->Parent</li>
		<li>god->Architect</li>
		<li>god->Almighty</li>
		<li>god->Lord</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <!-- Culture-specific content -->

  <RulePackDef>
    <defName>DeityTypeMakerCorunan</defName>
    <include>
      <li>DeityTypeMakerGlobal</li>
    </include>
  </RulePackDef>
  
