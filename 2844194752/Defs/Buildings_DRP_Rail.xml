<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="SleeperBase" Name="CAT_DRP_Sleeper">
        <defName>CAT_DRP_Sleeper</defName>
        <label>Wooden Sleeper 1x1</label>

        <placingDraggableDimensions>1</placingDraggableDimensions>
        <size>(1,1)</size>
        <graphicData>
            <texPath>Things/Building/DecoRailProp/sleeper_1_1</texPath>
            <drawSize>(1,1)</drawSize>
        </graphicData>
        <modExtensions>
            <li MayRequire="owlchemist.perspectivebuildings" Class="Perspective.Offsetter">
                <offsets>
                    <li>(0, 0, 0.5)</li>
                    <li>(0, 0, -0.5)</li>
                    <li>(0.5, 0, 0)</li>
                    <li>(-0.5, 0, 0)</li>
                </offsets>
            </li>
        </modExtensions>

        <statBases>
            <WorkToBuild>100</WorkToBuild>
            <Flammability>70</Flammability>
        </statBases>
        <costList>
            <WoodLog>1</WoodLog>
        </costList>
    </ThingDef>

