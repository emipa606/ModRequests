<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="DoorBase">
    <defName>AutoAnimalFlap</defName>
    <label>auto animal flap</label>
    <description>A doorway hung with thick overlapping strips of fabric or leather. It allows animals to pass while insulating the temperature. Animal flaps are commonly used as barn doors to give penned animals a sheltered place to sleep. Powered operation allows people and animal to move through the door without slowing down.</description>
    <uiOrder>2051</uiOrder>
    <statBases>
      <WorkToBuild>950</WorkToBuild>
      <MaxHitPoints>20</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Building/Door/AutoAnimalFlap_Mover</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <enabled>false</enabled>
      </damageData>
    </graphicData>
    <uiIconPath>Things/Building/Door/AutoAnimalFlap_MenuIcon</uiIconPath>
    <costList>
      <Steel>25</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <costStuffCount>25</costStuffCount>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>25</basePowerConsumption>
      </li>
      <li Class="CompProperties_Breakdownable"/>
      <li Class="CompProperties_Styleable" />
    </comps>
    <stuffCategories Inherit="False">
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <building>
      <roamerCanOpen>true</roamerCanOpen>
      <ai_chillDestination>false</ai_chillDestination>
      <blueprintGraphicData>
        <texPath>Things/Building/Door/AnimalFlap_Blueprint</texPath>
        <graphicClass>Graphic_Multi</graphicClass>
      </blueprintGraphicData>
      <paintable>false</paintable>
    </building>
    <researchPrerequisites>
      <li>Autodoors</li>
    </researchPrerequisites>
  </ThingDef>
</Defs>
