<?xml version="1.0" encoding="utf-8"?>
<Defs>

<!--Arrow-->
	<ThingDef ParentName="AmmunitionBase">
        <defName>B_ArrowAmmo</defName>
        <label>arrows</label>
        <description>Crudely made ammunition from a bygone era. Very pointy, do not touch the tips.</description>
        <graphicData>
            <texPath>Ammo/Arrows</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <soundInteract>Bow_Recurve</soundInteract>
        <techLevel>Neolithic</techLevel>
        <statBases>
            <MarketValue>0.5</MarketValue>
            <Mass>0.1</Mass>
        </statBases>
        <tradeTags>
            <li>AmmoPrimitive</li>
        </tradeTags>
        <modExtensions>
            <li MayRequire="LimeTreeSnake.Ammunition" Class="Ammunition.DefModExtensions.AmmunitionExtension" />
        </modExtensions>
    </ThingDef>
	
<!--Musket-->	
	<ThingDef ParentName="AmmunitionBase">
        <defName>B_MusketAmmo</defName>
        <label>musket balls</label>
        <description>Early firearms and cannons use black powder to propel projectiles. I don't suggest inhaling the powder.</description>
        <graphicData>
            <texPath>Ammo/Musket</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <soundInteract>Interact_Rifle</soundInteract>
        <techLevel>Medieval</techLevel>
        <statBases>
            <MarketValue>0.5</MarketValue>
            <Mass>0.025</Mass>
        </statBases>
        <tradeTags>
            <li>AmmoMedieval</li>
        </tradeTags>
        <comps>
            <li Class="CompProperties_Explosive">
                <explosiveRadius>2.0</explosiveRadius>
                <explosiveDamageType>Burn</explosiveDamageType>
                <requiredDamageTypeToExplode>Flame</requiredDamageTypeToExplode>
                <startWickHitPointsPercent>0.7</startWickHitPointsPercent>
                <wickTicks>
                    <min>30</min>
                    <max>60</max>
                </wickTicks>
            </li>
        </comps>
        <modExtensions>
            <li MayRequire="LimeTreeSnake.Ammunition" Class="Ammunition.DefModExtensions.AmmunitionExtension" />
        </modExtensions>
    </ThingDef>

