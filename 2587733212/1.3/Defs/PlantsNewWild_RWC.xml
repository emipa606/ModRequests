	<!-- Corn -->
	<ThingDef ParentName="RWC_PlantBase">
		<defName>PlantWildCorn</defName>
		<label>wild corn plant</label>
		<description>A tall grain plant which produces ears of soft, edible seeds. While corn takes a long time to grow, it yields lots of food compared to other crops. Harvested corn takes a long time to spoil.</description>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<Nutrition>0.30</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/CornPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>12</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.70</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawCorn</harvestedThingDef>
			<harvestYield>22</harvestYield>
			<topWindExposure>0.35</topWindExposure>
			<growDays>11.3</growDays>
			<immatureGraphicPath>Things/Plant/CornPlant_Immature</immatureGraphicPath>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.4</max>
			</visualSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<wildBiomes>
				<AridShrubland>0.05</AridShrubland>
				<BorealForest>0.01</BorealForest>
				<Desert>0.02</Desert>
				<ExtremeDesert>0.001</ExtremeDesert>
				<TemperateForest>0.03</TemperateForest>
				<TropicalRainforest>0.01</TropicalRainforest>
				<Tundra>0.001</Tundra>
				<ColdBog>0.0</ColdBog>
				<TemperateSwamp>0.0</TemperateSwamp>
				<TropicalSwamp>0.0</TropicalSwamp>
			</wildBiomes>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	<!-- Cotton -->
