<Defs>

	<!-- Bases -->
  <ThingDef Name="RS13_SlimeCoreBase" ParentName="RS13_ResourceBaseComponent" Abstract="True">
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1</drawSize>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>1</stackLimit>
    <useHitPoints>true</useHitPoints>
    <drawGUIOverlay>false</drawGUIOverlay>
	<thingCategories>
      <li>RS13_Category_SlimeCores</li>
    </thingCategories>
  </ThingDef>
  
  <!-- Slime Cores -->

