<Defs>

  <ThingDef Abstract="True" Name="RS13_SpawnEggBase" ParentName="RS13_EggBase" >
    <tickerType>Normal</tickerType>
    <ingestible>
      <preferability>DesperateOnly</preferability>
    </ingestible>
    <graphicData>
      <color>(255,255,255)</color>
