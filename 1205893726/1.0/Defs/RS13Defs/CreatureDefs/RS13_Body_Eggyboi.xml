<?xml version="1.0" encoding="utf-8" ?>

<!--
	Each coverage value is relative to its parent.
	Remaining body part coverage (100% minus coverage of all child nodes) is a chance to hit exactly this part.
	Damage "starts" at corePart, and then "goes deeper" until it hits remaining body part coverage or leaf node
  - this body part is then hit.
-->

<Defs>

	<BodyDef>
		<defName>RS13_Body_Eggyboi</defName>
		<label>Eggyboi</label>
		<corePart>
			<def>Body</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<groups>
				<li>HeadAttackTool</li>
			</groups>
			<parts>
				<li>
					<def>RS13_Organ_Eggyboi_DonkPocket</def>
					<coverage>0.10</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>front left leg</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Foot</def>
							<customLabel>front left foot</customLabel>
							<coverage>0.15</coverage>
							<groups>
								<li>FrontLeftPaw</li>
							</groups>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>front right leg</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Foot</def>
							<customLabel>front right foot</customLabel>
							<coverage>0.15</coverage>
							<groups>
								<li>FrontRightPaw</li>
							</groups>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>rear left leg</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Foot</def>
							<customLabel>rear left foot</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
				<li>
					<def>Leg</def>
					<customLabel>rear right leg</customLabel>
					<coverage>0.07</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Foot</def>
							<customLabel>rear right foot</customLabel>
							<coverage>0.15</coverage>
						</li>
					</parts>
				</li>
			</parts>
		</corePart>
	</BodyDef>

</Defs>