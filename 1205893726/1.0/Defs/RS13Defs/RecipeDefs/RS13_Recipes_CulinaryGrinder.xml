<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef ParentName="RS13_Recipe_Cook_Ingredient_Base">
		<defName>RS13_Recipe_Grind_Meatwheat</defName>
		<label>Grind Meatwheat into meat</label>
		<description>Throw in Meatwheat, out comes meat.</description>
		<jobString>Grinding Meatwheat</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>RS13_Plant_Harvest_MeatWheat</li>
			  </thingDefs>
			</filter>
			<count>1</count>
		  </li>
		</ingredients>
		<products>
			<RS13_Food_Ingredient_RawMeat>1</RS13_Food_Ingredient_RawMeat>
		</products>
	</RecipeDef>

	<RecipeDef ParentName="RS13_Recipe_Cook_Ingredient_Base">
		<defName>RS13_Recipe_Grind_Wheat</defName>
		<label>Grind Wheat into Flour</label>
		<description>Throw in Wheat, out comes flour.</description>
		<jobString>Grinding Wheat</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>RS13_Plant_Harvest_Wheat</li>
			  </thingDefs>
			</filter>
			<count>1</count>
		  </li>
		</ingredients>
		<products>
			<RS13_Food_Ingredient_Flour>1</RS13_Food_Ingredient_Flour>
		</products>
	</RecipeDef>
  
</Defs>