<?xml version="1.0" encoding="utf-8" ?>
<Defs>
   
	<RecipeDef>
    <defName>RS13_Recipe_Protolathe_MMI</defName>
    <label>Print MMI</label>
    <description>Print a Man-machine Interface for merging man and machine in some twisted mockery of life.</description>
    <jobString>Making an affront to god.</jobString>
    <workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
    <requiredGiverWorkType>Research</requiredGiverWorkType>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RS13_SteelSheet</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
	<products>
		<RS13_Resource_Component_MMI>1</RS13_Resource_Component_MMI>
	</products>
    <workSkill>Intellectual</workSkill>
   </RecipeDef>
   
	<RecipeDef>
    <defName>RS13_Recipe_Protolathe_PositronicBrain</defName>
    <label>Print Positronic Brain</label>
    <description>Print an entirely artificial brain as a substitute for a lack of the real thing amongst the crew.</description>
    <jobString>Making a sparky brain.</jobString>
    <workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
    <requiredGiverWorkType>Research</requiredGiverWorkType>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RS13_SteelSheet</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
	<products>
		<RS13_Resource_Component_PositronicBrain>1</RS13_Resource_Component_PositronicBrain>
	</products>
    <workSkill>Intellectual</workSkill>
   </RecipeDef>
  
</Defs>