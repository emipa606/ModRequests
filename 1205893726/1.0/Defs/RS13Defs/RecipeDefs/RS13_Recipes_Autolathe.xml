<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
    <defName>RS13_Recipe_Autolathe_Cable</defName>
    <label>Make Cable</label>
    <description>Cut steel sheets into cables.</description>
    <jobString>Making Cables.</jobString>
    <workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
    <requiredGiverWorkType>Smithing</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RS13_SteelSheet</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
	<products>
		<RS13_Resource_Processed_Cable>50</RS13_Resource_Processed_Cable>
	</products>
    <workSkill>Crafting</workSkill>
   </RecipeDef>
   
