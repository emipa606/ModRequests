<Defs>
  
  <ThingDef ParentName="RS13_BedBase">
    <defName>RS13_Furniture_Bed_Plain</defName>
    <label>Bare Bed</label>
    <description>A cozy mattress with no sheets on a frame for resting.</description>
    <graphicData>
      <texPath>Buildings/Furniture/Beds/Bare/bare</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <rect>(0,0,0,0)</rect>
      </damageData>
    </graphicData>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>EndTable</li>
          <li>Dresser</li>
        </linkableFacilities>
      </li>
    </comps>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
      <WorkToBuild>10</WorkToBuild>
      <Mass>50</Mass>
      <BedRestEffectiveness>1</BedRestEffectiveness>
      <Comfort>0.75</Comfort>
      <ImmunityGainSpeedFactor>1.1</ImmunityGainSpeedFactor>
	  <SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
    </statBases>
    <size>(1,1)</size>
    <building>
      <bed_showSleeperBody>False</bed_showSleeperBody>
      <bed_healPerDay>3</bed_healPerDay>
    </building>
    <fillPercent>0.4</fillPercent>
    <pathCost>40</pathCost>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <castEdgeShadows>True</castEdgeShadows>
    <uiIconScale>0.90</uiIconScale>
  </ThingDef>
 
