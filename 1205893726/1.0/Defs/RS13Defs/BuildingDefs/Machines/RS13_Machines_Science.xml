<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RS13_BenchBase">
    <defName>RS13_Machine_CircuitImprinter</defName>
    <label>Circuit Imprinter</label>
    <description>Prints circuits for a variety of machines using glass, sulphuric acid, and occasionaly rarer minerals.</description>
    <thingClass>Building_WorkTable_HeatPush</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/CircuitImprinter/circuitimprinter</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <costList>
      <RS13_Resource_Processed_Cable>3</RS13_Resource_Processed_Cable>
	  <RS13_SteelSheet>5</RS13_SteelSheet>
	  <RS13_Resource_Circuit_CircuitImprinter>1</RS13_Resource_Circuit_CircuitImprinter>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>10</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <recipes>
      <li>RS13_Recipe_Circuit_AirlockElectronics</li>
	  <li>RS13_Recipe_Circuit_PowerControlModule</li>
      <li>RS13_Recipe_Circuit_ArcadeMachine</li>
      <li>RS13_Recipe_Circuit_Autolathe</li>
      <li>RS13_Recipe_Circuit_BioGenerator</li>
      <li>RS13_Recipe_Circuit_ChemDispenser</li>
	  <li>RS13_Recipe_Circuit_ChemGrinder</li>
      <li>RS13_Recipe_Circuit_CircuitImprinter</li>
      <li>RS13_Recipe_Circuit_ChemMixer</li>
      <li>RS13_Recipe_Circuit_CommsConsole</li>
      <li>RS13_Recipe_Circuit_CulinaryGrinder</li>
      <li>RS13_Recipe_Circuit_DestructiveAnalyser</li>
	  <li>RS13_Recipe_Circuit_ExosuitFabricator</li>
      <li>RS13_Recipe_Circuit_HeavyLathe</li>
	  <li>RS13_Recipe_Circuit_Microwave</li>
      <li>RS13_Recipe_Circuit_OreRedemption</li>
      <li>RS13_Recipe_Circuit_PortableGenerator</li>
      <li>RS13_Recipe_Circuit_Protolathe</li>
      <li>RS13_Recipe_Circuit_SlotMachine</li>
      <li>RS13_Recipe_Circuit_SMESUnit</li>
      <li>RS13_Recipe_Circuit_SnackVendor</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>30</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
    </comps>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
  </ThingDef>
  
