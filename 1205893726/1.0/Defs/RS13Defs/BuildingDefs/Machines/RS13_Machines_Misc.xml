  <ThingDef ParentName="RS13_BuildingBase" Name="RS13_Machine_CommunicationsConsole">
    <defName>RS13_Machine_CommunicationsConsole</defName>
    <label>Communications Console</label>
    <description>Communicate with papa NanoTrasen or receive warnings to ignore.</description>
    <thingClass>Building_CommsConsole</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/CommsConsole/commsconsole</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <rect>(0,0,3,1.8)</rect>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
      <shadowData>
        <volume>(3,0.5,2)</volume>
      </shadowData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Flammability>0.6</Flammability>
    </statBases>
    <size>(1,1)</size>
    <costList>
      <RS13_SteelSheet>10</RS13_SteelSheet>
      <RS13_GlassSheet>6</RS13_GlassSheet>
      <RS13_Resource_Circuit_CommsConsole>1</RS13_Resource_Circuit_CommsConsole>
    </costList>
    <defaultPlacingRot>South</defaultPlacingRot>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <comps>
	<li Class="Rimstation13.RS13_CompProperties_AlertLevels" />
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>15</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      
    </comps>
  </ThingDef>
  
  <!-- I left this here to avoid causing errors for people who built the console before I changed the name -->
  <ThingDef ParentName="RS13_Machine_CommunicationsConsole">
    <defName>RS13_CommunicationsConsole</defName>
  </ThingDef>
  
  <ThingDef ParentName="BuildingBase">
    <defName>RS13_Machine_Beacon</defName>
    <label>Beacon</label>
    <thingClass>Building_OrbitalTradeBeacon</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/TrackingBeacon/trackingbeacon</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.3, 0.2, 0.3)</volume>
        <offset>(0,0,-0.1)</offset>
      </shadowData>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <minifiedDef>RS13_MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Flammability>0.5</Flammability>
      <Mass>1</Mass>
    </statBases>
    <description>Provides your location to NanoTrasen, enabling shuttle deliveries.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <fillPercent>0.15</fillPercent>
    <costList>
      <RS13_SteelSheet>5</RS13_SteelSheet>
      <RS13_Resource_Processed_Cable>2</RS13_Resource_Processed_Cable>
    </costList>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <pathCost>8</pathCost>
    <designationCategory>Misc</designationCategory>
    <rotatable>false</rotatable>
    <placeWorkers>
      <li>PlaceWorker_ShowTradeBeaconRadius</li>
    </placeWorkers>
  </ThingDef>
  
    <ThingDef ParentName="RS13_BuildingBase">
    <defName>RS13_Crematorium</defName>
    <label>Crematorium</label>
    <description>Perfect for impromptu BBQ or disposing of Greytiders and changelings.</description>
    <thingClass>Building_WorkTable_HeatPush</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/Crematorium/crematorium</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1,1)</drawSize>
      <shadowData>
        <volume>(1.95,0.5,1.3)</volume>
      </shadowData>
      <damageData>
        <rect>(0.3,0.1,2.4,1.8)</rect>
      </damageData>
    </graphicData>
    <costList>
      <RS13_SteelSheet>20</RS13_SteelSheet>
      <RS13_Resource_Processed_Cable>10</RS13_Resource_Processed_Cable>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>100</WorkToBuild>
      <MaxHitPoints>300</MaxHitPoints>
      <Cleanliness>-20</Cleanliness>
      <Beauty>-10</Beauty>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <fillPercent>1</fillPercent>
    <passability>Impassable</passability>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <interactionCellIcon>DiningChair</interactionCellIcon>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <recipes>
      <li>CremateCorpse</li>
      <li>BurnApparel</li>
      <li>BurnDrugs</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>30</basePowerConsumption>
        <shortCircuitInRain>true</shortCircuitInRain>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Glower">
        <glowRadius>6</glowRadius>
        <glowColor>(217,112,33,0)</glowColor>
      </li>
      <li Class="CompProperties_HeatPusher">
        <compClass>CompHeatPusherPowered</compClass>
        <heatPerSecond>12</heatPerSecond>
      </li>
    </comps>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <heatPerTickWhileWorking>0.32</heatPerTickWhileWorking>
    </building>
  </ThingDef>


</Defs>
