<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RS13_BenchBase">
    <defName>RS13_Machine_ChemDispenser</defName>
    <label>Chem Dispenser</label>
    <description>Dispenses chemicals for use in medicines, compounds, cooking, explosives, poison and the clown.</description>
    <thingClass>Building_WorkTable_HeatPush</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/ChemDispenser/chemdispenser</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <costList>
      <RS13_Resource_Processed_Cable>3</RS13_Resource_Processed_Cable>
	  <RS13_SteelSheet>5</RS13_SteelSheet>
	  <RS13_Resource_Circuit_ChemDispenser>1</RS13_Resource_Circuit_ChemDispenser>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>10</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <recipes>
      <li>RS13_Recipe_Chemdispenser_Aluminium</li>
      <li>RS13_Recipe_Chemdispenser_Bromine</li>
      <li>RS13_Recipe_Chemdispenser_Carbon</li>
      <li>RS13_Recipe_Chemdispenser_Chlorine</li>
      <li>RS13_Recipe_Chemdispenser_Copper</li>
      <li>RS13_Recipe_Chemdispenser_Ethanol</li>
      <li>RS13_Recipe_Chemdispenser_Fluorine</li>
      <li>RS13_Recipe_Chemdispenser_Hydrogen</li>
      <li>RS13_Recipe_Chemdispenser_Iodine</li>
      <li>RS13_Recipe_Chemdispenser_Iron</li>
      <li>RS13_Recipe_Chemdispenser_Lithium</li>
      <li>RS13_Recipe_Chemdispenser_Mercury</li>
      <li>RS13_Recipe_Chemdispenser_Nitrogen</li>
      <li>RS13_Recipe_Chemdispenser_Oxygen</li>
      <li>RS13_Recipe_Chemdispenser_Phosphorus</li>
      <li>RS13_Recipe_Chemdispenser_Potassium</li>
      <li>RS13_Recipe_Chemdispenser_Radium</li>
      <li>RS13_Recipe_Chemdispenser_Silicon</li>
      <li>RS13_Recipe_Chemdispenser_Silver</li>
      <li>RS13_Recipe_Chemdispenser_Sodium</li>
      <li>RS13_Recipe_Chemdispenser_Sugar</li>
      <li>RS13_Recipe_Chemdispenser_Sulfur</li>
      <li>RS13_Recipe_Chemdispenser_Sulphuric_Acid</li>
      <li>RS13_Recipe_Chemdispenser_Water</li>
      <li>RS13_Recipe_Chemdispenser_Welding_Fuel</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>30</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      
    </comps>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
  </ThingDef>

