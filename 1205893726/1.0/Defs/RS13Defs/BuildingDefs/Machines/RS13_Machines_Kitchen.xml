<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RS13_BenchBase">
    <defName>RS13_Machine_KitchenTable</defName>
    <label>Kitchen Table</label>
    <description>The perfect surface for cooking things on.</description>
    <thingClass>Building_WorkTable_HeatPush</thingClass>
    <graphicData>
      <texPath>Buildings/Machines/KitchenTable/kitchentable</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <costList>
      <RS13_SteelSheet>3</RS13_SteelSheet>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>10</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <recipes>
	  <li>RS13_Recipe_Cook_Ingredient_Dough</li>
	  <li>RS13_Recipe_Cook_Ingredient_RawCutlet</li>
	  <li>RS13_Recipe_Cook_Ingredient_RawBacon</li>
	  <li>RS13_Recipe_Cook_Ingredient_CheeseWedge</li>
	  <li>RS13_Recipe_Cook_Ingredient_CheeseWheel</li>
	  <li>RS13_Recipe_Cook_Ingredient_CakeBatter</li>
	  <li>RS13_Recipe_Cook_Ingredient_PotatoWedges</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <comps>
      
    </comps>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
  </ThingDef>

