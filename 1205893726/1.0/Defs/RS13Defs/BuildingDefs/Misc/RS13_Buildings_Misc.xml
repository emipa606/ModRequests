<Defs>

<ThingDef ParentName="RS13_BuildingBase">
	<defName>RS13_Building_Crate_NTStarter</defName>
	<label>NanoTrasen Base Pod Crate</label>
	<description>Unpacks into a small starter station. Some assembly required.</description>
	<thingClass>Building</thingClass>
	<minifiedDef>RS13_MinifiedThing</minifiedDef>
	<graphicData>
		<texPath>Buildings/Furniture/Crates/6</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<drawSize>(1,1)</drawSize>
		<shadowData>
		<volume>(1.83, 0.5, 1.73)</volume>
		<offset>(0,0,-0.05)</offset>
		</shadowData>
	</graphicData>
	<rotatable>false</rotatable>
	<resourcesFractionWhenDeconstructed>2</resourcesFractionWhenDeconstructed>
	<altitudeLayer>Building</altitudeLayer>
	<passability>Impassable</passability>
	<blockWind>true</blockWind>
	<fillPercent>1</fillPercent>
	<blockLight>true</blockLight>
	<canOverlapZones>true</canOverlapZones>
	<statBases>
		<WorkToBuild>100</WorkToBuild>
		<MaxHitPoints>1000</MaxHitPoints>
		<Flammability>0</Flammability>
		<Mass>20</Mass>
	</statBases>
	<tickerType>Normal</tickerType>
	<size>(1,1)</size>
	<building>
		<alwaysDeconstructible>true</alwaysDeconstructible>
	</building>
	<costList>
		<RS13_SteelSheet>1</RS13_SteelSheet>
	</costList>
	<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
	<thingCategories>
		<li>BuildingsFurniture</li>
	</thingCategories>
	<comps>
		<li Class="Rimstation13.RS13_CompProperties_CompBaseCrate">
			<contents>
				<li>RS13_Machine_Power_PortableGenerator</li>
				<li>RS13_Machine_Power_SMESUnit</li>
				<li>RS13_Machine_OreRedemptionMachine</li>
				<li>RS13_Machine_ChemDispenser</li>
				<li>RS13_Machine_CircuitImprinter</li>
			</contents>
			<wallDef>RS13_Shuttle_Wall</wallDef>
			<terrainDef>RS13_Floor_Shuttle</terrainDef>
			<doorDef>RS13_ShuttleDoor</doorDef>
			<width> 6</width>
			<height>6</height>
		</li>
	</comps>
	<placeWorkers>
		<li>Rimstation13.RS13_PlaceWorker_CompBaseCrate</li>
	</placeWorkers>
	<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
</ThingDef>

