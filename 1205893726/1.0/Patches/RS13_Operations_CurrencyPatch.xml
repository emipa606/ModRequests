<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Silver"]/label</xpath>
		<value>
			<label>space cash</label>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Silver"]/graphicData</xpath>
		<value>
			<graphicData>
			  <texPath>Resources/Cash</texPath>
			  <graphicClass>Graphic_StackCount</graphicClass>
			</graphicData>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Silver"]/stackLimit</xpath>
		<value>
			<stackLimit>5000</stackLimit>
		</value>
	</Operation>
	
</Patch>
