<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RS13_ResourceBaseProcessed">
    <defName>RS13_Resource_Processed_SteelRod</defName>
    <label>Steel Rod</label>
    <description>Rods of STEEL. An essential building component and vicious throwing weapon.</description>
    <graphicData>
      <texPath>Resources/Crafted/Steelrod</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>1</drawSize>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>200</stackLimit>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>4</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>0.025</Mass>
      <Flammability>0.2</Flammability>
    </statBases>
    <drawGUIOverlay>true</drawGUIOverlay>
  </ThingDef>
  
