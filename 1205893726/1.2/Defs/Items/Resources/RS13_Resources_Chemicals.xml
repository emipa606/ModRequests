<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="RS13_Chemical_Base" ParentName="RS13_Resource_Base" Abstract="True">
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>500</stackLimit>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>10</MarketValue>
      <Mass>0.025</Mass>
      <Flammability>0.5</Flammability>
	  <DeteriorationRate>0</DeteriorationRate>
	</statBases>
    <intricate>false</intricate>
    <thingCategories>
      <li>RS13_Category_Chemicals</li>
    </thingCategories>
  </ThingDef>
  
  <ThingDef ParentName="RS13_Chemical_Base">
	<defName>RS13_Chemical_Biomass</defName>
    <label>Biomass</label>
    <description>Pure Biomass distilled from an assortment of biological matter haphazardly tossed into a BioGenerator.</description>
    <graphicData>
      <texPath>Resources/Chemicals/biomass</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
  </ThingDef>
  
