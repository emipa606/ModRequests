		<MaxHitPoints>180</MaxHitPoints>
		<Flammability>1.0</Flammability>
	</statBases>
	<size>(1,1)</size>
	<designationCategory>Production</designationCategory>
	<passability>PassThroughOnly</passability>
	<pathCost>70</pathCost>
	<hasInteractionCell>True</hasInteractionCell>
	<interactionCellOffset>(0,0,-1)</interactionCellOffset>
	<surfaceType>Item</surfaceType>
	<inspectorTabs>
		<li>ITab_Bills</li>
	</inspectorTabs>
	<comps>
	<li Class="CompProperties_Power">
		<compClass>CompPowerTrader</compClass>
		<shortCircuitInRain>true</shortCircuitInRain>
		<basePowerConsumption>30</basePowerConsumption>
	</li>
	<li Class="CompProperties_Refuelable">
	<fuelLabel>Inventory</fuelLabel>
	<fuelGizmoLabel>Inventory</fuelGizmoLabel>
	<fuelConsumptionRate>15</fuelConsumptionRate>
	<fuelCapacity>1</fuelCapacity>
	<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
	<drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
	<outOfFuelMessage>Inventory empty.</outOfFuelMessage>
	<fuelIconPath>Resources/VendorRefills/vendorrefill_clothes</fuelIconPath>
		<fuelFilter>
			<thingDefs>
				<li>RS13_Resource_VendorRefill_Clothes</li>
			</thingDefs>
		</fuelFilter>
	</li>
	<li Class="CompProperties_Flickable"/>
	</comps>
	<building>
		<isMealSource>false</isMealSource>
		<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
	</building>
</ThingDef>
  
