<Defs>

  <RecipeDef>
    <defName>MakeBorgEyeImplant</defName>
    <label>make Borg Eye Implant</label>
    <description>make Borg Eye Implant.</description>
    <jobString>making Borg Eye Implant.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>8000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>BorgThings</li>
          </categories>
        </filter>
        <count>80</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>BorgThings</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <BorgEyeImplant>1</BorgEyeImplant>
    </products>
    <skillRequirements>
      <Crafting>1</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
		<recipeUsers>
      <li>BorgAlcoveShip</li>
	  <li>BorgMachiningStationImplants</li>
    </recipeUsers>
  </RecipeDef>
 


