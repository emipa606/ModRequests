<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerBorgUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>BorgNames->NameBanks/BorgNames</li>
        <li>BorgFactionNames->NameBanks/BorgFactionNames</li>

