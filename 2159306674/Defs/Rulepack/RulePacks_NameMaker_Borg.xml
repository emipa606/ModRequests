      <li>NamerBorgUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=12)-># [BorgNames] of-[BorgNames]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  
