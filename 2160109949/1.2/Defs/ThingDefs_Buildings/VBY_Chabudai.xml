<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
   <ThingDef Abstract="True" ParentName="FurnitureWithQualityBase" Name="VBY_ChabudaiBase">
    <description>Low-to-the-floor tables for socializing and eating.</description>
    <thingClass>Building</thingClass>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <pathCost>40</pathCost>
    <fillPercent>0.2</fillPercent>
    <socialPropernessMatters>true</socialPropernessMatters>
    <canOverlapZones>false</canOverlapZones>
    <stuffCategories>      
      <li>Woody</li>      
    </stuffCategories>	
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <staticSunShadowHeight>0.10</staticSunShadowHeight>
    <surfaceType>Eat</surfaceType>  
    <comps>
      <li>
        <compClass>CompGatherSpot</compClass>
	  </li>	
	  <li Class="CompProperties_Art">
        <nameMaker>NamerArtFurniture</nameMaker>
        <descriptionMaker>ArtDescription_Furniture</descriptionMaker>
        <minQualityForArtistic>Excellent</minQualityForArtistic>
      </li>      
    </comps>
	<inspectorTabs>
      <li>ITab_Art</li>
    </inspectorTabs>
  </ThingDef>

  <ThingDef ParentName="VBY_ChabudaiBase">
    <defName>VBY_ChabudaiRound</defName>
    <label>round chabudai</label>
    <graphicData>
      <texPath>Things/Building/Furniture/ChabudaiRound/VBY_ChabudaiRound</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawRotated>false</drawRotated>
      <drawSize>(2,2)</drawSize>
      
	  <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
	<costStuffCount>18</costStuffCount>
    <uiIconScale>0.65</uiIconScale>
    <statBases>
      <MaxHitPoints>55</MaxHitPoints>
      <WorkToBuild>650</WorkToBuild>
      <Mass>3.7</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
    </statBases>
    <size>(2,1)</size>    
  </ThingDef> 

  <ThingDef ParentName="VBY_ChabudaiBase">
    <defName>VBY_ChabudaiRect</defName>
    <label>rectangle chabudai</label>
    <graphicData>
      <texPath>Things/Building/Furniture/VBY_ChabudaiRect/VBY_ChabudaiRect5</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  
      <drawSize>(1.25, 2.5)</drawSize>
      
	  <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
	<costStuffCount>25</costStuffCount>
    <uiIconScale>0.85</uiIconScale>
    <statBases>
      <MaxHitPoints>65</MaxHitPoints>
      <WorkToBuild>750</WorkToBuild>
      <Mass>5.7</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
    </statBases>
    <size>(1,2)</size>    
  </ThingDef>    
  
  
  
</Defs>  