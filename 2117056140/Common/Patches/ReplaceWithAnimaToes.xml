<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Anima Animals</li>
			<li>Anima Animals - Community Pack (temp)[HH]</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Vanilla Expanded Framework</li>
			</mods>
			  <match Class="PatchOperationSequence">
				 <operations>
					<li Class="PatchOperationReplace">
						<xpath>Defs/TerrainDef[defName="VBY_HideCarpet_WoolAnimaThrumbo" or defName="VBY_HideCarpet_WoolAnimaYak"]/traversedThought</xpath>
						<value>
							<traversedThought>VBY_ExtraWarmAnimaToes</traversedThought>
						</value>
					</li>
				 </operations>
			  </match>
		</match>	
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Anima Animals - Community Pack</li>
			<li>Anima Animals - Community Pack (temp)[HH]</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Vanilla Expanded Framework</li>
			</mods>
			  <match Class="PatchOperationSequence">
				 <operations>
					<li Class="PatchOperationReplace">
						<xpath>Defs/TerrainDef[defName="VBY_HideCarpet_WoolAnimaMuffalo"]/traversedThought</xpath>
						<value>
							<traversedThought>VBY_ExtraWarmAnimaToes</traversedThought>
						</value>
					</li>
				 </operations>
			  </match>
		</match>	
	</Operation>

</Patch>