	<RecipeDef>
		<defName>MakeBerryPie</defName>
		<label>Make Berry Pie</label>
		<description>Make a berry pie from corn or rice, any milk, egg and berries.</description>
		<jobString>Making Berry Pie.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>1000</workAmount>
		<soundWorking>Recipe_CookMeal</soundWorking>          
		<allowMixingIngredients>true</allowMixingIngredients>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>EggsFertilized</li>
						<li>EggsUnfertilized</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Milk</li>
						<li>SCSmokeleafmilk</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RawCorn</li>
						<li>RawRice</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RawBerries</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawCorn</li>
				<li>RawRice</li>
				<li>RawBerries</li>
				<li>Milk</li>
				<li>SCSmokeleafmilk</li>
			</thingDefs>
			<categories>
				<li>EggsFertilized</li>
				<li>EggsUnfertilized</li>
			</categories>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FoodRaw</li>
			</categories>
			<disallowedCategories>
				<li>EggsFertilized</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				<li>InsectJelly</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<products>
			<BerryPie>1</BerryPie>
		</products>
		<workSkill>Cooking</workSkill>
		<skillRequirements>
			<Cooking>12</Cooking>
		</skillRequirements>
		<researchPrerequisite>GrandmaAnnasCookbook</researchPrerequisite>
	</RecipeDef>


