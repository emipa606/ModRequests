<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rimatomics</li>
		</mods>
		<match Class="PatchOperationReplace">
			<xpath>/Defs/Androids.AndroidUpgradeDef[defName="Upgrade_VanometricCell"]</xpath>
			<value>
				<Androids.AndroidUpgradeDef>
					<defName>Upgrade_VanometricCell</defName>
					<label>mini R.T.G</label>
					<description>Miniature version of the R.T.G. When installed it frees the Android of the need for both energy and food. \n (will be displayed as Vanometric cell)</description>
					<commandType>Androids.UpgradeCommand_Hediff</commandType>
					<iconTexturePath>Icons/Upgrades/energise</iconTexturePath>
					<orderID>0</orderID>
					<upgradeGroup>Utility</upgradeGroup>
					<costList>
						  <Steel>30</Steel>
						  <Silver>100</Silver>
						  <Plutonium>20</Plutonium>
						  <ComponentIndustrial>1</ComponentIndustrial>
						  <ComponentSpacer>1</ComponentSpacer>
					</costList>
					<hediffToApply>ChjAndroidUpgrade_VanometricCell</hediffToApply>
					<extraPrintingTime>120000</extraPrintingTime>
					<requiredResearch>ChJAndroidUpgrade_Utility</requiredResearch>
				</Androids.AndroidUpgradeDef>
			</value>
		</match>
	</Operation>
</Patch>