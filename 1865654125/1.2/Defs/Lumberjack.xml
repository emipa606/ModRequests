<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef ParentName="BuildingBase">
    <defName>GloomyDeco_Lumberjack</defName>
    <label>stump</label>
    <description>The usual tree stump. It could be used to chop fireowod.</description>
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <graphicData>
      <texPath>GloomyDeco/Lumberjack</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1.0,1.0)</drawSize>
      <damageData>
        <rect>(0.1,0.1,0.8,0.8)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>20</pathCost>
    <canOverlapZones>true</canOverlapZones>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToBuild>400</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Mass>2</Mass>
	  <Beauty>5</Beauty>
    </statBases>
    <size>(1,1)</size>
    <stuffCategories>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>15</costStuffCount>
    <costList>
      <Steel>5</Steel>
    </costList>
    <defaultPlacingRot>South</defaultPlacingRot>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>OldStyleDeco</designationCategory>
	<!-- custom -->
    <resourcesFractionWhenDeconstructed>0.2</resourcesFractionWhenDeconstructed>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
</ThingDef>
</Defs>