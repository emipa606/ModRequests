<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[@Name="VG_WildFruitTree"]</xpath>
        <value>
        <minifiedDef>MinifiedTree</minifiedDef>
        </value>
    </Operation>

<Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[@Name="VG_FruitTree"]/statBases</xpath>
        <value>
        <Mass>150</Mass>
        </value>
    </Operation>
	
	<Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[@Name="VG_FruitTree"]/plant</xpath>
        <value>
        <forceIsTree>true</forceIsTree>
        </value>
    </Operation>

</Patch>

