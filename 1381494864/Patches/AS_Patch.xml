<?xml version="1.0" encoding="utf-8" ?>
<Patch>    

	<!--========== Ancient Concrete Barrier Patch ==========-->
		
	<Operation Class="PatchOperationSequence">
	    <success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName = "AncientConcreteBarrier"]</xpath>
				<value>
					<designationCategory>Structure</designationCategory>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName = "AncientConcreteBarrier"]/statBases/Beauty</xpath>
				<value>
					<Beauty>1</Beauty>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>