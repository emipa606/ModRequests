<!-- With this, archotech floors will have the same infestation-blocking properties as armored floors (if Armored Floors is active) -->
<Patch>
	

	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Armored Floors</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "FloorArchotechUwU"]/tags</xpath>
					<value>
						<li>InfestationBlocker</li>
					</value>
				</li>					
			</operations>
		</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Armored Floors</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "FloorArchotechOwO"]/tags</xpath>
					<value>
						<li>InfestationBlocker</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>