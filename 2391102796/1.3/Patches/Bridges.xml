<!-- With this, archotech fragmens should drop from...stuff -->
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply More Bridges (Continued)</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<TerrainDef>
							<defName>FloorArchotechUwU_BridgeDeep</defName>
							<label>archotech deep bridge</label>
							<description>An archotech floor tile. Very clean and sturdy. Despite its weight it seems to effortlessly float on water.</description>
							<texturePath>Terrain/Floors/SexyArchotechFloorPanels</texturePath>
							<designatorDropdown>MAG_ArchotechFloors</designatorDropdown>
							<layerable>true</layerable>
							<affordances>
								<li>Light</li>
								<li>Medium</li>
								<li>Heavy</li>
							</affordances>
							<tags>
								<li>Floor</li>
								<li>FineFloor</li>
							</tags>
							<statBases>
								<Cleanliness>1.2</Cleanliness>
								<WorkToBuild>12000</WorkToBuild>
								<MarketValue>20</MarketValue>
								<Beauty>20</Beauty>
							</statBases>
							<costList>
								<Plasteel>5</Plasteel>
								<ArchotechScrap>1</ArchotechScrap>
							</costList>
							<designationCategory>Floors</designationCategory>
							<fertility>0</fertility>
							<constructEffect>ConstructMetal</constructEffect>
							<filthAcceptanceMask></filthAcceptanceMask>
							<terrainAffordanceNeeded>BridgeableDeep</terrainAffordanceNeeded>
							<researchPrerequisites>
								<li>ArchoTechStart</li>
							</researchPrerequisites>
						</TerrainDef>


						<TerrainDef>
							<defName>FloorArchotechOwO_BridgeDeep</defName>
							<label>simple archotech deep bridge</label>
							<description>An archotech floor tile. Very clean and sturdy. Despite its weight it seems to effortlessly float on water. Now with 50% less eye-melting patterns.</description>
							<texturePath>Terrain/Floors/SexyArchotechFloorPanelsOffline</texturePath>
							<designatorDropdown>MAG_ArchotechFloors</designatorDropdown>
							<layerable>true</layerable>
							<affordances>
								<li>Light</li>
								<li>Medium</li>
								<li>Heavy</li>
							</affordances>
							<tags>
								<li>Floor</li>
								<li>FineFloor</li>
							</tags>
							<statBases>
								<Cleanliness>1.2</Cleanliness>
								<WorkToBuild>12000</WorkToBuild>
								<MarketValue>20</MarketValue>
								<Beauty>20</Beauty>
							</statBases>
							<costList>
								<Plasteel>5</Plasteel>
								<ArchotechScrap>1</ArchotechScrap>
							</costList>
							<designationCategory>Floors</designationCategory>
							<fertility>0</fertility>
							<constructEffect>ConstructMetal</constructEffect>
							<filthAcceptanceMask></filthAcceptanceMask>
							<terrainAffordanceNeeded>BridgeableDeep</terrainAffordanceNeeded>
							<researchPrerequisites>
								<li>ArchoTechStart</li>
							</researchPrerequisites>
						</TerrainDef>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
	
	
	


	
</Patch>