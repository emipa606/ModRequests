<ThingDef ParentName="FurnitureBase">
    <defName>TB_TankBedMedical</defName>
    <label>hospital tank bed</label>
    <description>Hospital tank bed, or "Auto-Doc" is a chamber modified by glitterworld specialists for medical purposes. This modification is ultra-expensive to construct, however it regenerates wounds automatically. It is also good for performing surgeries and immunity recovery.</description>
    <thingClass>Building_Bed</thingClass>
    <graphicData>
      <texPath>Things/Building/TB_TankBedMedical</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(3,4)</drawSize>
      <damageData>
        <rect>(0,0.05,1,1.95)</rect>
      </damageData>
    </graphicData>
    <castEdgeShadows>True</castEdgeShadows>
    <statBases>
      <MaxHitPoints>320</MaxHitPoints>
      <WorkToBuild>65000</WorkToBuild>
      <Mass>90</Mass>
      <Flammability>0</Flammability>
      <Beauty>0</Beauty>
      <BedRestEffectiveness>3</BedRestEffectiveness>
      <Comfort>0.45</Comfort>
      <ImmunityGainSpeedFactor>1.45</ImmunityGainSpeedFactor>
      <MedicalTendQualityOffset>0.40</MedicalTendQualityOffset>
			<SurgerySuccessChanceFactor>1.35</SurgerySuccessChanceFactor>
		</statBases>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTransmitter</compClass>
        <transmitsPower>True</transmitsPower>
      </li>
	<li Class="CompProperties_AssignableToPawn">
        <drawAssignmentOverlay>false</drawAssignmentOverlay>
        <compClass>CompAssignableToPawn_Bed</compClass>
      </li>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>VitalsMonitor</li>
        </linkableFacilities>
      </li>
    </comps>
    <size>(1,2)</size>
	<designationCategory>Furniture</designationCategory>
    <costList>
      <Steel>220</Steel>
	  <ComponentIndustrial>8</ComponentIndustrial>
	  <AIPersonaCore>1</AIPersonaCore>
      <ComponentSpacer>10</ComponentSpacer>
	  <MedicineUltratech>10</MedicineUltratech>
    </costList>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <bed_healPerDay>330</bed_healPerDay>
      <bed_defaultMedical>True</bed_defaultMedical>
    </building>
    <fillPercent>0.6</fillPercent>
    <pathCost>77</pathCost>
    <leaveResourcesWhenKilled>False</leaveResourcesWhenKilled>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <researchPrerequisites>
      <li>HospitalBed</li>
      <li>MultiAnalyzer</li>
      <li>Bionics</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>14</constructionSkillPrerequisite>
    <techLevel>Spacer</techLevel>
    <uiIconScale>0.90</uiIconScale>
  </ThingDef>
