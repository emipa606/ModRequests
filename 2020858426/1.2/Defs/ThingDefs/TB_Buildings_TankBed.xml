<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  
  
  
  <ThingDef ParentName="FurnitureBase">
    <defName>TB_TankBed</defName>
    <label>tank bed</label>
    <description>Tank Beds are chambers designed to decrease fatigue. Three hours inside a tank bed is said to be just as refreshing as a full 8 hours of sleep.</description>
    <thingClass>Building_Bed</thingClass>
    <altitudeLayer>Building</altitudeLayer>
	<tickerType>Normal</tickerType>
    <drawGUIOverlay>True</drawGUIOverlay>
    <canOverlapZones>false</canOverlapZones>
    <passability>PassThroughOnly</passability>
    <graphicData>
      <texPath>TB_TankBed</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(3,4)</drawSize>
      <damageData>
        <rect>(0,0.05,1,1.95)</rect>
      </damageData>
    </graphicData>
    <statBases>
      <MaxHitPoints>320</MaxHitPoints>
      <Flammability>0</Flammability>
      <Beauty>0</Beauty>
      <Comfort>-70</Comfort>
      <WorkToBuild>3100</WorkToBuild>
      <Mass>90</Mass>
      <BedRestEffectiveness>3</BedRestEffectiveness>
      <ImmunityGainSpeedFactor>0</ImmunityGainSpeedFactor>
	  <SurgerySuccessChanceFactor>0.65</SurgerySuccessChanceFactor>
    </statBases>
	<designationCategory>Furniture</designationCategory>
    <size>(1,2)</size>
    <costList>
      <Steel>220</Steel>
	  <ComponentIndustrial>5</ComponentIndustrial>
      <ComponentSpacer>1</ComponentSpacer>
	  <MedicineUltratech>1</MedicineUltratech>
    </costList>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <bed_healPerDay>0</bed_healPerDay>
    </building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTransmitter</compClass>
        <transmitsPower>True</transmitsPower>
      </li>
	<li Class="CompProperties_AssignableToPawn">
        <drawAssignmentOverlay>false</drawAssignmentOverlay>
        <compClass>CompAssignableToPawn_Bed</compClass>
      </li>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>EndTable</li>
          <li>Dresser</li>
        </linkableFacilities>
      </li>
    </comps>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0.2</resourcesFractionWhenDeconstructed>
    <fillPercent>0.6</fillPercent>
    <defaultPlacingRot>North</defaultPlacingRot>
    <pathCost>77</pathCost>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <castEdgeShadows>True</castEdgeShadows>
    <techLevel>Industrial</techLevel>
    <constructionSkillPrerequisite>9</constructionSkillPrerequisite>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <researchPrerequisites><li>MultiAnalyzer</li></researchPrerequisites>
  </ThingDef>
  
