<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="TreeBase">
		<defName>Fallout_MutfruitTree</defName>
		<label>mutfruit tree</label>
		<description>Short trees with yellow leaves capable of growing a wide variety of harvestable fruit. Variants include regular plants, those that produce wild mutfruit, those recently harvested, growing in planters, or mutated within a blast zone. The regular variant can be planted in one's C.A.M.P. or at a public workshop.</description>
		<graphicData>
			<texPath>Things/Plant/Trees/TreeMutfruit</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.3, 0.15)</volume>
				<offset>(0,0,-0.38)</offset>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<statBases>
			<Beauty>2</Beauty>
			<Nutrition>1.25</Nutrition>
		</statBases>
		<ingestible />
		<plant>
			<dieIfLeafless>false</dieIfLeafless>
			<growDays>38.01</growDays>
			<visualSizeRange>0.8~2.0</visualSizeRange>
			<sowMinSkill>6</sowMinSkill>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.71</fertilityMin>
			<topWindExposure>0.15</topWindExposure>
			<mustBeWildToSow>true</mustBeWildToSow>
			<immatureGraphicPath>Things/Plant/Trees/TreeMutfruit_Immature</immatureGraphicPath>
			<leaflessGraphicPath>Things/Plant/Trees/TreeMutfruit_Leafless</leaflessGraphicPath>
			<harvestWork>800</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestYield>60</harvestYield>
			<harvestedThingDef>Fallout_Mutfruit</harvestedThingDef>
			<harvestFailable>true</harvestFailable>
			<harvestAfterGrowth>0.20</harvestAfterGrowth>
		</plant>
	</ThingDef>

</Defs>