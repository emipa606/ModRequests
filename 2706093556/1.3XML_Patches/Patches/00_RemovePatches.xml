<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="XmlExtensions.PatchOperationSafeRemove">
	  <success>Always</success>
	  <xpath>/Defs/ResearchProjectDef[defName="VFES_TrenchWarfare" or defName="VWE_TrenchWarfare" or defName="PRF_Crates" or defName="PRF_SteelCrates"]/prerequisites</xpath>
	</Operation>

	<Operation Class="XmlExtensions.PatchOperationAddOrReplace">
	  <success>Always</success>
	  <xpath>/Defs/ResearchProjectDef[defName="PRF_Crates" or defName="PRF_SteelCrates"]</xpath>
	  <value>
		<techLevel>Medieval</techLevel>
	  </value>
	</Operation>


</Patch>