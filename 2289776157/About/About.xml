<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
  <name>LetterReorder</name>
  <author>legodude17</author>
  <supportedVersions>
    <li>1.2</li>
  </supportedVersions>
  <description>A simple mod that flips the order of letters in the sidebar, so that the oldest is at the bottom. No more accidentally dismissing new notifications!</description>
  <packageId>legodude17.letterreorder</packageId>
  <modDependencies>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
  </modDependencies>
  <loadAfter>
    <li>Ludeon.RimWorld</li>
    <li>Ludeon.RimWorld.Royalty</li>
    <li>brrainz.harmony</li>
  </loadAfter>
</ModMetaData>
