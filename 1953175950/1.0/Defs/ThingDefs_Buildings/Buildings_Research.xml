<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BenchBase">
    <defName>FimirMearghResearchBench</defName>
    <label>fimir rune</label>
    <description>A rune to the unknown. A place where fimirs dive into the deep unknown for knowledge.</description>
    <thingClass>Building_ResearchBench</thingClass>
    <size>(3,3)</size>
    <costList>
      <Heart>1</Heart>
    </costList>
    <graphicData>
      <drawRotated>false</drawRotated>
      <texPath>Things/FimirBuildings/FimirResearch/FimirResearchCircle</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3.5)</drawSize>
    </graphicData>
    <castEdgeShadows>false</castEdgeShadows>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>0</pathCost>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>5000</WorkToBuild>
      <Flammability>0</Flammability>
      <ResearchSpeedFactor>0.8</ResearchSpeedFactor>
    </statBases>
    <fillPercent>0.5</fillPercent>
    <interactionCellOffset>(0,0,-2)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>
    <designationCategory>Production</designationCategory>
    <surfaceType>Item</surfaceType>
  </ThingDef>

</Defs>
