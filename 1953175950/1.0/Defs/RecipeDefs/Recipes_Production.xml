<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Make_FimirSalve</defName>
    <label>make fimir salve out of liver</label>
    <description>Make a batch of salve out of a liver.</description>
    <jobString>Mashing liver.</jobString>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <workAmount>6000</workAmount>
    <skillRequirements>
      <Intellectual>4</Intellectual>
    </skillRequirements>
    <researchPrerequisite>FimirSalveMed</researchPrerequisite>
    <workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Liver</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Liver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <FimirMedicineSalve>5</FimirMedicineSalve>
    </products>
  </RecipeDef>

