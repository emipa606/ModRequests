<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationSequence">
        <operations>
            <li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Baseliner"]/Groups
                </xpath> 
                <value>
                    <li>
                        <Endogenes> 
                            <!-- 0.05 in each = ~100% of spawn in total -->
                            <!-- 0.01 in each = ~20% of spawn in total -->		
							<Gene_Archon>0.005</Gene_Archon>
                            <Gene_Archotechist>0.005</Gene_Archotechist>
							<Gene_Chronopath>0.005</Gene_Chronopath>							
							<Gene_Conflagrator>0.005</Gene_Conflagrator>
							<Gene_Empath>0.005</Gene_Empath>
							<Gene_Frostshaper>0.005</Gene_Frostshaper>
							<Gene_Harmonist>0.005</Gene_Harmonist>
							<Gene_Necropath>0.005</Gene_Necropath>
							<Gene_Nightstalker>0.005</Gene_Nightstalker>
							<Gene_Protector>0.005</Gene_Protector>
							<Gene_Skipmaster>0.005</Gene_Skipmaster>
							<Gene_Staticlord>0.005</Gene_Staticlord>
							<Gene_Technomancer>0.005</Gene_Technomancer>
							<Gene_Warlord>0.005</Gene_Warlord>
							<Gene_Wildspeaker>0.005</Gene_Wildspeaker>
							<Gene_Puppeteer>0.005</Gene_Puppeteer>

                            <Gene_Ravager MayRequire="eclair155.vpebiotechpatches">0.005</Gene_Ravager>
                            <Gene_Xenoseer MayRequire="eclair155.vpebiotechpatches">0.005</Gene_Xenoseer>
                            <Gene_Oculist MayRequire="eclair155.vpebiotechpatches">0.005</Gene_Oculist>

                            <Gene_Stunskip MayRequire="rabbit.stuncastervpe2">0.005</Gene_Stunskip>
                            <Gene_CP_Combat MayRequire="edern.combatpsycasts">0.005</Gene_CP_Combat>
                            <Gene_LightSeeker MayRequire="myf.lightseeker">0.0005</Gene_LightSeeker>
                            <Gene_Ranger MayRequire="aranmaho.rangerclass">0.005</Gene_Ranger>

                            <Gene_Bond_Intertwined MayRequire="aranmaho.makai.psycast">0.005</Gene_Bond_Intertwined>
                            <Gene_String_of_Fate MayRequire="aranmaho.makai.psycast">0.005</Gene_String_of_Fate>
                            <Gene_Corrupted_Prophet MayRequire="aranmaho.makai.psycast">0.005</Gene_Corrupted_Prophet>
                            <Gene_Distorted_Reality MayRequire="aranmaho.makai.psycast">0.005</Gene_Distorted_Reality>
                            <Gene_Passioned_Flame MayRequire="aranmaho.makai.psycast">0.005</Gene_Passioned_Flame>
                            <Gene_Perfect_Knowledge MayRequire="aranmaho.makai.psycast">0.005</Gene_Perfect_Knowledge>
                            <Gene_Golden_Order MayRequire="aranmaho.makai.psycast">0.005</Gene_Golden_Order>
                            <Gene_True_Destruction MayRequire="aranmaho.makai.psycast">0.005</Gene_True_Destruction>
                            <Gene_Destined_Death MayRequire="aranmaho.makai.psycast">0.005</Gene_Destined_Death>
                        </Endogenes>
                    </li>
                    <!-- For adding chances for a second gene to spawn -->
                    <!--
                    <li>
                        <Endogenes>
                            <Gene_Conflagrator>1</Gene_Conflagrator>
                        </Endogenes>
                    </li>
                    -->
                </value>
            </li>
            
			<li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Impid"]/Groups
                </xpath>  
                <value>
                    <li>
                        <Endogenes>
							<Gene_Conflagrator>0.15</Gene_Conflagrator>
                            <Gene_Passioned_Flame MayRequire="aranmaho.makai.psycast">0.15</Gene_Passioned_Flame>
                        </Endogenes>
                    </li>
                </value>
            </li>

			<li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Yttakin"]/Groups
                </xpath> 
                <value>
                    <li>
                        <Endogenes>
							<Gene_Wildspeaker>0.1</Gene_Wildspeaker>
                            <Gene_Ravager MayRequire="eclair155.vpebiotechpatches">0.1</Gene_Ravager>
                        </Endogenes>
                    </li>
                </value>
            </li>

			<li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Neanderthal"]/Groups
                </xpath>
                <value>
                    <li>
                        <Endogenes>
							<Gene_Frostshaper>0.2</Gene_Frostshaper>
                            <Gene_Ravager MayRequire="eclair155.vpebiotechpatches">0.05</Gene_Ravager>
                        </Endogenes>
                    </li>
                </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Pigskin"]/Groups
                </xpath>
                <value>
                    <li>
                        <Endogenes>
							<Gene_Wildspeaker>0.1</Gene_Wildspeaker>
                        </Endogenes>
                    </li>
                </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Dirtmole"]/Groups
                </xpath>
                <value>
                    <li>
                        <Endogenes>
							<Gene_Nightstalker>0.1</Gene_Nightstalker>
							<Gene_Technomancer>0.1</Gene_Technomancer>
                        </Endogenes>
                    </li>
                </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>Defs/SpawnThoseGenes.SpawnThoseGenesXenotypes[defName = "Waster"]/Groups
                </xpath>
                <value>
                    <li>
                        <Endogenes>
							<Gene_Necropath>0.2</Gene_Necropath>
                        </Endogenes>
                    </li>
                </value>
            </li>

        </operations>
    </Operation>
</Patch>