<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- Undo most of the changes made to xenotypes by the "Vanilla Psycast Expanded - Biotech Integration" mod-->

    <Operation Class="PatchOperationRemove">
        <xpath>/Defs/XenotypeDef[defName="Hussar"]/genes/li[text()="Gene_Skipmaster"]</xpath>
    </Operation>
    
    <Operation Class="PatchOperationSequence">
        <operations>
            <li Class="PatchOperationRemove">
                <xpath>/Defs/XenotypeDef[defName="Impid"]/genes/li[text()="Gene_Conflagrator"]</xpath>
            </li>

            <li Class="PatchOperationRemove">
                <xpath>/Defs/XenotypeDef[defName="Impid"]/genes/li[text()="Gene_Staticlord"]</xpath>
            </li>
        </operations>
    </Operation>

