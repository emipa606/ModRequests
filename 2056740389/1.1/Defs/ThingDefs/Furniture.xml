<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--Parents-->
 
	<ThingDef Name="ClutterFurnitureBuildingBase" Abstract="True">
		<category>Building</category>
		<thingClass>Building</thingClass>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<selectable>true</selectable>
		<drawerType>MapMeshAndRealTime</drawerType>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<repairEffect>Repair</repairEffect>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
  	</ThingDef>
  
	<ThingDef Name="ClutterFurnitureBase" ParentName="ClutterFurnitureBuildingBase" Abstract="True">
		<minifiedDef>ClutterMinifiedFurniture</minifiedDef>
		<tradeTags>
			<li>Furniture</li>
		</tradeTags>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
	</ThingDef>
  
	<ThingDef Name="ClutterFurnitureWithQualityBase" ParentName="ClutterFurnitureBase" Abstract="True">
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
		<recipeMaker>
			<workSpeedStat>SmeltingSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<recipeUsers>
				<li>FurnitureAssembler</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedSculpture</unfinishedThingDef>
		</recipeMaker>
		<statBases>
			<Mass>2</Mass>
		</statBases>
	</ThingDef>

  	<ThingDef Name="ClutterArtableFurnitureBase" ParentName="ClutterFurnitureWithQualityBase" Abstract="True">
		<comps>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtFurniture</nameMaker>
				<descriptionMaker>ArtDescription_Furniture</descriptionMaker>
				<minQualityForArtistic>Excellent</minQualityForArtistic>
			</li>
		</comps>
		<inspectorTabs>
			<li>ITab_Art</li>
		</inspectorTabs>
  	</ThingDef>

