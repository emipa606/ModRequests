	<ThingDef ParentName="ClutterFurnitureWithQualityBase">
		<defName>ClutterOldDesk</defName>
		<label>Desk</label>
		<category>Building</category>
		<thingClass>Building</thingClass>
		<graphicData>
			<shaderType>Transparent</shaderType>
			<texPath>Clutter/Furniture/Desk/OldDesk</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4,2)</drawSize>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<surfaceType>Eat</surfaceType>
		<socialPropernessMatters>true</socialPropernessMatters>
		<building>
			<joyKind>Gaming_Dexterity</joyKind>
      		<forceShowRoomStats>true</forceShowRoomStats>
		</building>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<Mass>21</Mass>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>10</Beauty>
		</statBases>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,1)</interactionCellOffset>
		<pathCost>60</pathCost>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.5</fillPercent>
		<size>2,1</size>
		<description>Comes with a plastic plant and full coffee cup filled with almost drinkable steel coffee</description>
		<costList>
			<ClutterSteelParts>10</ClutterSteelParts>
			<Steel>25</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<rotatable>true</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<!-- li>ClutterBookShelf</li-->
					<li>ClutterStool</li>
					<li>ClutterComfySeat</li>
				</linkableFacilities>
			</li>
		</comps>
	    <recipeMaker>
			<researchPrerequisite>ComfyTechTierII</researchPrerequisite>
			<workAmount>8000</workAmount>
		</recipeMaker>
		<researchPrerequisites>
			<li>ComfyTechTierII</li>
		</researchPrerequisites>
	</ThingDef>
	
