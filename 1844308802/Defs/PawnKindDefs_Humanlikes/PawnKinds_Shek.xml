      <min>0.2</min>
      <max>1.1</max>
    </gearHealthRange>
    <apparelMoney>
      <min>150</min>
      <max>300</max>
    </apparelMoney>
    <weaponMoney>
      <min>50</min>
      <max>150</max>
    </weaponMoney>
