					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/Mechlink</li>
					</texPaths>
					<baseLayer>60</baseLayer>
					<parentTagDef>Head</parentTagDef>
					<visibleFacing>
						<li>North</li>
					</visibleFacing>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="RemoteShielderImplant"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_RemoteShielder</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/RemoteShielder</li>
					</texPaths>
					<baseLayer>60</baseLayer>
					<parentTagDef>Head</parentTagDef>
					<visibleFacing>
						<li>North</li>
					</visibleFacing>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="RemoteRepairerImplant"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_RemoteRepairer</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/RemoteRepairer</li>
					</texPaths>
					<baseLayer>60</baseLayer>
					<parentTagDef>Head</parentTagDef>
					<visibleFacing>
						<li>North</li>
					</visibleFacing>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="ControlSublinkImplant"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_Sublink</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/Sublink</li>
					</texPaths>
					<baseLayer>90</baseLayer>
					<parentTagDef>Head</parentTagDef>
					<visibleFacing>
						<li>North</li>
						<li>East</li>
						<li>West</li>
					</visibleFacing>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="MechFormfeederImplant"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_Processor</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/Processor</li>
					</texPaths>
					<baseLayer>15</baseLayer>
					<visibleFacing>
						<li>South</li>
						<li>East</li>
					</visibleFacing>
					<drawData>
						<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
						<childScale>0.7</childScale>
						<bodyTypeScales>
							<Hulk>1.4</Hulk>
							<Fat>1.2</Fat>
							<Male>1.1</Male>
							<Thin>0.7</Thin>
						</bodyTypeScales>
						<dataEast>
							<offset>(0.2, 0, 0)</offset>
						</dataEast>
					</drawData>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="RepairProbeImplant"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_RepairProbe</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/RepairProbe</li>
					</texPaths>
					<baseLayer>0</baseLayer>
					<drawSize>1.25</drawSize>
					<visibleFacing>
						<li>East</li>
						<li>South</li>
						<li>West</li>
						<li>North</li>
					</visibleFacing>
					<drawData>
						<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
						<childScale>0.7</childScale>
						<bodyTypeScales>
							<Hulk>1.4</Hulk>
							<Fat>1.2</Fat>
							<Thin>0.8</Thin>
						</bodyTypeScales>
						<dataNorth>
							<flip>true</flip>
							<layer>60</layer>
						</dataNorth>
						<dataEast>
							<layer>60</layer>
							<flip>true</flip>
						</dataEast>
						<dataWest>
							<flip>true</flip>
						</dataWest>
					</drawData>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
</Patch>