<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="PsychicBond"]</xpath>
		<value>
			<renderNodeProperties>
				<li>
					<debugLabel>BodyPart_PsyBond</debugLabel>
					<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
					<texPaths>
						<li>Things/Pawn/BodyPart/Biotech/PsyBond</li>
					</texPaths>
					<baseLayer>90</baseLayer>
					<parentTagDef>Head</parentTagDef>
					<visibleFacing>
						<li>East</li>
						<li>South</li>
						<li>West</li>
						<li>North</li>
					</visibleFacing>
				</li>
			</renderNodeProperties>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="MechlinkImplant"]</xpath>
