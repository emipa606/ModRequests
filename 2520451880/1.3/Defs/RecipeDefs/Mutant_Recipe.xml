    <defName>InstallMutationStomach_WVC</defName>
    <label>install mutant stomach</label>
    <description>Install an mutant stomach.</description>
    <jobString>Installing mutant stomach.</jobString>
    <descriptionHyperlinks>
      <ThingDef>MutationStomach_WVC</ThingDef>
      <HediffDef>MutationStomach_WVC</HediffDef>
    </descriptionHyperlinks>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MutationStomach_WVC</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MutationStomach_WVC</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Stomach</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MutationStomach_WVC</addsHediff>
  </RecipeDef>
