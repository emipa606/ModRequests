<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>RadWorld</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
			   <li Class="PatchOperationAdd">
				  <xpath>/Defs/HediffDef[defName="ArchonitesNetwork_WVC"]/stages/li/statOffsets</xpath>
				  <value>
					<RW_RadiationResistance>1.0</RW_RadiationResistance>
				  </value>
			   </li>
			</operations>
		</match>
  </Operation>
</Patch>
