  </HediffDef>
  <!-- Vanilla Factions Expanded - Mechanoids -->
  <HediffDef ParentName="ParentDeathSwarm_WVC">
	<defName>DeathSwarmVFEM_WVC</defName>
	<spawnThingOnRemoved MayRequire="oskarpotocki.vfe.mechanoid">VFE_ComponentMechanoid</spawnThingOnRemoved>
  </HediffDef>
  <!-- Solid -->
  <HediffDef>
	<defName>DeathSwarmSolid_WVC</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>recycling</label>
	<description>Archonites recycling this organ, it only partially fulfills its old functions.</description>
    <defaultLabelColor>(0.5, 0.5, 0.5)</defaultLabelColor>
	<descriptionHyperlinks>
	  <HediffDef>DeathArchonites_WVC</HediffDef>
	</descriptionHyperlinks>
	<spawnThingOnRemoved>ComponentSpacer</spawnThingOnRemoved>
	<everCurableByItem>false</everCurableByItem>
	<scenarioCanAdd>false</scenarioCanAdd>
	<minSeverity>0.001</minSeverity>
	<maxSeverity>1</maxSeverity>
    <lethalSeverity>1.00</lethalSeverity>
    <initialSeverity>0.001</initialSeverity>
	<stages>
	  <li>
		<minSeverity>0</minSeverity>
        <partEfficiencyOffset>-0.5</partEfficiencyOffset>
      </li>
	  <li>
		<minSeverity>0.7</minSeverity>
        <partEfficiencyOffset>-0.9</partEfficiencyOffset>
      </li>
	</stages>
	<comps>
		<li Class="HediffCompProperties_SeverityPerDay">
		  <severityPerDay>10.0</severityPerDay>
		</li>
	</comps>
  </HediffDef>
</Defs>
