<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="SkyfallerBase">
        <defName>SRTS_Trains_Steam_Engine_A_Coal_Incoming</defName>
        <label>Steam engine incoming</label>
        <thingClass>SRTS.SRTSIncoming</thingClass>
        <graphicData>
            <texPath>Things/Building/SRTS_Trains/Cars/steam_engine_a_incoming</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutFlying</shaderType>
            <drawSize>(4,16)</drawSize>
            <drawOffset>(0,0,1)</drawOffset>
        </graphicData>
        <rotatable>false</rotatable>
        <skyfaller>
            <movementType>Decelerate</movementType>
            <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
            <shadowSize>(16,4)</shadowSize>
            <anticipationSound>SRTS_Trains_Sound_Steam_Train_Incoming</anticipationSound>
            <anticipationSoundTicks>100</anticipationSoundTicks>
            <motesPerCell>0</motesPerCell>
        </skyfaller>
    </ThingDef>

    <ThingDef ParentName="SkyfallerBase">
        <defName>SRTS_Trains_Steam_Engine_A_Coal_Leaving</defName>
        <label>Steam engine leaving</label>
        <thingClass>SRTS.SRTSLeaving</thingClass>
        <graphicData>
            <texPath>Things/Building/SRTS_Trains/Cars/steam_engine_a_leaving</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutFlying</shaderType>
            <drawSize>(4,16)</drawSize>
            <drawOffset>(0,0,1)</drawOffset>
        </graphicData>
        <rotatable>false</rotatable>
        <skyfaller>
            <movementType>Accelerate</movementType>
            <reversed>true</reversed>
            <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
            <shadowSize>(16,4)</shadowSize>
            <anticipationSound>SRTS_Trains_Sound_Steam_Train_Leaving</anticipationSound>
            <anticipationSoundTicks>-10</anticipationSoundTicks>
            <ticksToImpactRange>
                <min>-40</min>
                <max>-15</max>
            </ticksToImpactRange>
        </skyfaller>
    </ThingDef>

    <ThingDef>
        <defName>SRTS_Trains_Steam_Engine_A_Coal_Active</defName>
        <label>Steam engine 1 active</label>
        <category>Item</category>
        <thingClass>ActiveDropPod</thingClass>
        <scatterableOnMapGen>false</scatterableOnMapGen>
        <tickerType>Normal</tickerType>
        <graphicData>
            <texPath>Things/Building/SRTS_Trains/Cars/steam_engine_a_leaving</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(4,16)</drawSize>
            <drawOffset>(0,0,1)</drawOffset>
        </graphicData>
        <rotatable>false</rotatable>
        <altitudeLayer>Building</altitudeLayer>
        <blockWind>true</blockWind>
        <useHitPoints>true</useHitPoints>
        <statBases>
            <MaxHitPoints>150</MaxHitPoints>
            <Flammability>0</Flammability>
        </statBases>
        <selectable>true</selectable>
    </ThingDef>

