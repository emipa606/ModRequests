<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Guest</defName>
    <label>guest</label>
    <stockGenerators>
	
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>50~250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>40~80</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSurvivalPack</thingDef>
        <countRange>-5~5</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>-2~5</countRange>
     </li>
      <li Class="StockGenerator_MultiDef">
        <countRange>1~4</countRange>
        <thingDefs>
          <li>MedicineHerbal</li>
          <li>MedicineIndustrial</li>
        </thingDefs>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>

      <li Class="StockGenerator_WeaponsRanged">
        <countRange>0~1</countRange>
      </li>

      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <totalPriceRange>10~100</totalPriceRange>
      </li>

      <!-- Weapons -->
      <li Class="StockGenerator_WeaponsRanged">
        <countRange>0~1</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>WeaponsMelee</categoryDef>
        <countRange>0~1</countRange>
      </li>
      
      <!-- Apparel -->
      <li Class="StockGenerator_Clothes">
        <countRange>0~1</countRange>
      </li>
      
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
	  <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>
