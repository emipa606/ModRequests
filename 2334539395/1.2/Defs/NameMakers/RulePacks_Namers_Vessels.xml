<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonVessel</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[VesselFirst] [VesselLast]</li>
        <li>name->[VesselFirst] '[Nickname]' [VesselLast]</li>
      </rulesStrings>
	<rulesRaw>
		<li Class="Rule_File">
      <keyword>VesselFirst</keyword>
      <path>Vessel/VesselNames/VesselFirst</path>
    </li>
		<li Class="Rule_File">
      <keyword>VesselLast</keyword>
      <path>Vessel/VesselNames/VesselLast</path>
    </li>
		<li Class="Rule_File">
      <keyword>Nickname</keyword>
      <path>Nicknames/Nickname</path>
    </li>
  </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
