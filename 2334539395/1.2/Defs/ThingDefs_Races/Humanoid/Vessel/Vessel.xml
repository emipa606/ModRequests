<?xml version="1.0" encoding="utf-8"?>
<Defs>
<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Vessel</defName>
    <label>Vessel</label>
    <description>The Vessels are beings of the Void who were created by the Pale King. No mind to think. No will to break. No voice to cry suffering. Born of God and Void.</description>
    <statBases>
      <MarketValue>2000</MarketValue>
      <MentalBreakThreshold>-1.00</MentalBreakThreshold>
      <MoveSpeed>5.2</MoveSpeed>
      <ComfyTemperatureMin>-29</ComfyTemperatureMin>
      <ComfyTemperatureMax>48</ComfyTemperatureMax>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>1.00</ArmorRating_Blunt>
      <ArmorRating_Heat>0.30</ArmorRating_Heat>
    </statBases>

    <alienRace>
        <generalSettings>
            <maleGenderProbability>0.5</maleGenderProbability>
            <humanRecipeImport>true</humanRecipeImport>
            <allowHumanBios>true</allowHumanBios>
            <immuneToAge>true</immuneToAge>
            <alienPartGenerator>
                <colorChannels>
                  <li>
                    <name>skin</name>
                    <first Class="ColorGenerator_Options">
                      <options>
                        <li>
                          <weight>10</weight>
                          <only>RGBA(1.000, 1.000, 1.000, 1.000)</only>
                        </li>
                      </options>
                    </first>
                  </li>
                </colorChannels>
                <aliencrowntypes>
                    <li>Average_Normal</li>
                    <li>Average_Pointy</li>
                    <li>Average_Wide</li>
                    <li>Narrow_Normal</li>
                    <li>Narrow_Pointy</li>
                    <li>Narrow_Wide</li>
                </aliencrowntypes>
                <alienbodytypes>
                    <li>Thin</li>
                </alienbodytypes>
                <useGenderedHeads>false</useGenderedHeads>
                <useGenderedBodies>false</useGenderedBodies>
            </alienPartGenerator>
        </generalSettings>
        <graphicPaths>
            <li>
                <body>Things/TRAHS/Pawn/Vessel/Bodies/</body>
                <head>Things/TRAHS/Pawn/Vessel/Heads/</head>
            </li>
        </graphicPaths>
        <hairSettings>
            <hasHair>true</hasHair>
            <hairTags>
              <li>Vessel</li>
            </hairTags>
        </hairSettings>
        <thoughtSettings>
            <cannotReceiveThoughtsAtAll>true</cannotReceiveThoughtsAtAll>
        </thoughtSettings>
        <raceRestriction>
            <apparelList>
                <li>TRAHS_RedDress</li>
                <li>TRAHS_DarkCloak</li>
                <li>TRAHS_TatteredCloak</li>
            </apparelList>
            <researchList>
                <li>
                    <projects>
                        <li>TRAHS_NailCrafting</li>
                        <li>TRAHS_NailSharpening</li>
                        <li>TRAHS_NailPolishing</li>
                    </projects>
                </li>
            </researchList>
            <onlyUseRaceRestrictedApparel>true</onlyUseRaceRestrictedApparel>
        </raceRestriction>
        <compatibility>
            <isFlesh>false</isFlesh>
            <isSentient>false</isSentient> 
            <hasBlood>true</hasBlood>
        </compatibility>
    </alienRace>

    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4.1</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>7</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4.1</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>7</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>4.1</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
        <soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
        <soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>2.5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <hasGenders>false</hasGenders>
      <makesFootprints>false</makesFootprints>
      <lifeExpectancy>1000</lifeExpectancy>
      <leatherDef>Leather_Human</leatherDef>
      <bloodDef>Filth_BloodVessel</bloodDef>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
      <baseHungerRate>0</baseHungerRate>
      <baseBodySize>0.5</baseBodySize>
      <baseHealthScale>2</baseHealthScale>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
      <meatMarketValue>0</meatMarketValue>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <manhunterOnTameFailChance>1</manhunterOnTameFailChance>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.01, 0.02)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1.2</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>4</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>13</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>18</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <soundMeleeDodge>Pawn_MeleeDodge</soundMeleeDodge>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(50,100)</li>
          <li>(60,30)</li>
          <li>(70,18)</li>
          <li>(80,10)</li>
          <li>(90,3)</li>
          <li>(100,0)</li>
        </points>
      </ageGenerationCurve>
    </race>
    <recipes>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
      <li>CureScaria</li>
      <li MayRequire="Ludeon.RimWorld.Royalty">CureBloodRot</li>
      <li MayRequire="Ludeon.RimWorld.Royalty">CureAbasia</li>
    </recipes>

</AlienRace.ThingDef_AlienRace>
</Defs>