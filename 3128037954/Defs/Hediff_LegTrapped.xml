<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<HediffDef ParentName="DiseaseBase">
	<defName>LegTrapped</defName>
	<label>trapped leg</label>
	<description>Leg stuck. Can't get it out. Ugh.</description>
	<isBad>false</isBad>
	<initialSeverity>1</initialSeverity>
	<maxSeverity>1</maxSeverity>
	<tendable>true</tendable>
    <comps>
		<li Class="BluesTraps.HediffCompDisappearSlider"> </li>
    </comps>
    <stages>
      <li>
        <label>immobile</label>
        <capMods>
			<li> <capacity>Moving</capacity> <offset>-1</offset> </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>

</Defs>