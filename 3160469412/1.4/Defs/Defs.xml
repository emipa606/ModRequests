<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
  <defName>ConvertPersonaSubcoreHigh</defName>
  <label>convert AI Persona Core to High Subcore</label>
  <description>Uses an AI persona core to create a high mech subcore.</description>
  <jobString>Converting persona core to high subcore.</jobString>
  <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
  <effectWorking>Cook</effectWorking>
  <soundWorking>Recipe_Machining</soundWorking>
  <workAmount>10000</workAmount>
  <ingredients>
    <li>
      <filter>
        <thingDefs>
          <li>AIPersonaCore</li>
        </thingDefs>
      </filter>
      <count>1</count>
    </li>
  </ingredients>
  <fixedIngredientFilter>
    <thingDefs>
      <li>AIPersonaCore</li>
    </thingDefs>
  </fixedIngredientFilter>
  <products>
    <SubcoreHigh>1</SubcoreHigh>
  </products>
  <skillRequirements>
    <Crafting>10</Crafting>
    <Intellectual>12</Intellectual>
  </skillRequirements>
  <workSkill>Crafting</workSkill>
  <displayPriority>0</displayPriority>
</RecipeDef>


</Defs>