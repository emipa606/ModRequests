<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--Beetle-->

  <BodyDef>
    <defName>ClockworkBeetle</defName>
    <label>clockwork beetle</label>
    <corePart>
      <def>WindupTorso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>WindupNeck</def>
		      <customLabel>head joint</customLabel>
          <coverage>0.05</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>WindupHead</def>
              <coverage>0.10</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>WindupBrain</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>WindupEye</def>
                  <customLabel>left compound eye</customLabel>
                  <coverage>0.05</coverage>
                </li>
                <li>
                  <def>WindupEye</def>
                  <customLabel>right compound eye</customLabel>
                  <coverage>0.05</coverage>
                </li>
				<li>
                  <def>WindupEye</def>
                  <customLabel>left ocellus</customLabel>
                  <coverage>0.03</coverage>
                </li>
                <li>
                  <def>WindupEye</def>
                  <customLabel>right ocellus</customLabel>
                  <coverage>0.03</coverage>
                </li>			
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupShoulder</def>
          <customLabel>left manipulator joint</customLabel>
          <coverage>0.1</coverage>
		  <height>Middle</height>
          <parts>
            <li>
              <def>WindupArm</def>
              <customLabel>left arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>WindupHand</def>
                  <customLabel>left grasper</customLabel>
                  <coverage>0.2</coverage>                 
                  <parts>
                    <li>
                      <def>WindupFinger</def>
                      <customLabel>left upper claw</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>WindupFinger</def>
                      <customLabel>left lower claw</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupShoulder</def>
          <customLabel>right manipulator joint</customLabel>
          <coverage>0.1</coverage>
		  <height>Middle</height>
          <parts>
            <li>
              <def>WindupArm</def>
              <customLabel>right arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>WindupFinger</def>
                      <customLabel>right upper claw</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>WindupFinger</def>
                      <customLabel>right lower claw</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>RightHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
           </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>left front leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>left front foot</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>right front leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>right front foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
		     <li>
          <def>WindupLeg</def>
          <customLabel>left middle leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>left middle foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>right middle leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>right middle foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
		     <li>
          <def>WindupLeg</def>
          <customLabel>left back leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>left back foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>right back leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>right back foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupHeart</def>
          <coverage>0.06</coverage>
          <depth>Inside</depth>
        </li>
		<li>
            <def>WindupEar</def>
            <customLabel>tympanic sheet</customLabel>
            <coverage>0.05</coverage>
			<depth>Inside</depth>
        </li>
        <li>
          <def>WindupKidney</def>
		      <customLabel>filtration matrix</customLabel>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!--Quardroped with Claws-->

  <BodyDef>
    <defName>QuadrupedWindupWithPaws</defName>
    <label>quadruped windup</label>
    <corePart>
      <def>WindupTorso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>WindupHeart</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>WindupKidney</def>
          <customLabel>left filtration matrix</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>WindupKidney</def>
          <customLabel>right filtration matrix</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>WindupNeck</def>
		      <customLabel>head joint</customLabel>
          <coverage>0.22</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>WindupHead</def>
              <coverage>0.75</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>WindupBrain</def>
                  <coverage>0.25</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>WindupEye</def>
                  <customLabel>left optic</customLabel>
                  <coverage>0.12</coverage>
                </li>
                <li>
                  <def>WindupEye</def>
                  <customLabel>right optic</customLabel>
                  <coverage>0.12</coverage>
                </li>
                <li>
                  <def>WindupEar</def>
                  <customLabel>left tympanum</customLabel>
                  <coverage>0.08</coverage>
                </li>
                <li>
                  <def>WindupEar</def>
                  <customLabel>right tympanum</customLabel>
                  <coverage>0.08</coverage>
                </li>
                <li>
                  <def>WindupNose</def>
                  <coverage>0.1</coverage>
                </li>
                <li>
                  <def>WindupMouth</def>
                  <coverage>0.10</coverage>
                  <groups>
                    <li>Teeth</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>front left leg</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>front left paw</customLabel>
              <coverage>0.15</coverage>
              <groups>
                <li>FrontLeftPaw</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>front right leg</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>front right paw</customLabel>
              <coverage>0.15</coverage>
              <groups>
                <li>FrontRightPaw</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>rear left leg</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>rear left paw</customLabel>
              <coverage>0.15</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>WindupLeg</def>
          <customLabel>rear right leg</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>WindupFoot</def>
              <customLabel>rear right paw</customLabel>
              <coverage>0.15</coverage>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>

</Defs>
