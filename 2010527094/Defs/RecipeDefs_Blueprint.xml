<Defs>
	<RecipeDef>
		<defName>CraftBlueprintNeolithic</defName>
		<label>craft neolithic blueprint</label>
		<description>Craft a blueprint.</description>
		<jobString>Crafting blueprint.</jobString>
		<workSpeedStat>ResearchSpeed</workSpeedStat>
		<effectWorking>MakingBlueprint</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workAmount>10000</workAmount>
		<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Cloth</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<TechnologyBlueprintNeolithic>1</TechnologyBlueprintNeolithic>
		</products>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
		<workSkill>Intellectual</workSkill>
		<recipeUsers>
			<li>BlueprintBench</li>
		</recipeUsers>
		<researchPrerequisite>NeolithicBlueprint</researchPrerequisite>
	</RecipeDef>

