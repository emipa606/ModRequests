<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef ParentName="FloorBase">
    <defName>ChemfuelDitch</defName>
    <label>chemfuel ditch</label>
    <description>Slick chempowder puddles used to slow down intruders. While useless on it's own; if able to be lit; will cause the other puddles around to combust.</description>
    <texturePath>Terrain/Surfaces/MW_ChemfuelPuddle</texturePath>
    <edgeType>Water</edgeType>
    <renderPrecedence>240</renderPrecedence>
    <pathCost>5</pathCost>
    <statBases>
      <Beauty>-25</Beauty>
      <Cleanliness>-5</Cleanliness>
      <WorkToBuild>50</WorkToBuild>
      <FilthMultiplier>0.1</FilthMultiplier>
      <Flammability>4</Flammability>
    </statBases>
  <costList>
      <Chemfuel>1</Chemfuel>
  </costList>
  <designationHotKey>Misc6</designationHotKey>
  <researchPrerequisites>
    <li>MW_Chemdraw</li>
  </researchPrerequisites>
  </TerrainDef>
  
</Defs>