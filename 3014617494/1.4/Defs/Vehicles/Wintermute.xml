<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>SCWintermuteBlueprint</defName>
		<label>Wintermute</label>
		<description>A Wintermute snow crawler. While generally slower compared to most vehicles, the Wintermute more than makes up for this with it's near indifference to most terrain, granting it strong off-road capabilities.\n\nDespite being called the winter-"mute" this vehicle has quite the noisy reputation.\n\nDue to it's impressive traversal capabilities, the Wintermute is the go to for many research teams and exploration vessels, it will get you to where you need to go as long as the planet is vaguely habitable and you've got enough fuel.\n\n&lt;color=#4ec9e4&gt;Roles:&lt;/color&gt; Driver x1, Passenger x3\n&lt;color=#4ec9e4&gt;Fuel type:&lt;/color&gt; Chemfuel</description>
		<graphicData>
			<texPath>Things/Vehicles/Wintermute/SnowCrawler</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,6)</drawSize>
		</graphicData>
		<size>(2,5)</size>
		<statBases>
			<MaxHitPoints>800</MaxHitPoints>
			<WorkToBuild>11000</WorkToBuild>
			<Flammability>0.1</Flammability>
		</statBases>
		<researchPrerequisites><li>Fabrication</li><li>BiofuelRefining</li></researchPrerequisites>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
		<costList>
			<SnowCrawlerEnginePart>1</SnowCrawlerEnginePart>
			<Steel>400</Steel>
			<ComponentIndustrial>15</ComponentIndustrial>
		</costList>
		<designationCategory>VF_Vehicles</designationCategory>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<soundBuilt>SnowCrawlerBuilt</soundBuilt>
		<thingToSpawn>SCWintermute</thingToSpawn>
	</Vehicles.VehicleBuildDef>





	<Vehicles.VehicleDef ParentName="BaseVehiclePawn">
		<defName>SCWintermute</defName>
		<label>Wintermute</label>
		<description>A Wintermute snow crawler. While generally slower compared to most vehicles, the Wintermute more than makes up for this with it's near indifference to most terrain, granting it strong off-road capabilities.\n\nDespite being called the winter-"mute" this vehicle has quite the noisy reputation.\n\nDue to it's impressive traversal capabilities, the Wintermute is the go to for many research teams and exploration vessels, it will get you to where you need to go as long as the planet is vaguely habitable and you've got enough fuel.\n\n&lt;color=#4ec9e4&gt;Roles:&lt;/color&gt; Driver x1, Passenger x3\n&lt;color=#4ec9e4&gt;Fuel type:&lt;/color&gt; Chemfuel</description>
		<graphicData>
			<texPath>Things/Vehicles/Wintermute/SnowCrawler</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(3,6)</drawSize>
			<color>(200, 108, 36)</color>
			<colorTwo>(120, 120, 120)</colorTwo>
			<colorThree>(240, 240, 240)</colorThree>
		</graphicData>
		<race>
			<baseBodySize>4</baseBodySize>
		</race>
		<statBases>
			<ArmorRating_Blunt>0.25</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.50</ArmorRating_Sharp>
			<Flammability>0.1</Flammability>
			<MarketValue>2000</MarketValue>
		</statBases>
		<vehicleStats>
			<Mass>350</Mass>
			<MoveSpeed>7.5</MoveSpeed>
			<CargoCapacity>325</CargoCapacity>
			<RepairRate>1</RepairRate>
			<AccelerationRate MayRequire="oskarpotocki.vanillavehiclesexpanded">0.06</AccelerationRate>
		</vehicleStats>
		<fillPercent>0.80</fillPercent>
		<buildDef>SCWintermuteBlueprint</buildDef>
		<size>(2,5)</size>
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Transport</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		<nameable>true</nameable>
		<soundSustainersOnEvent>
			<li>
				<key>(IgnitionOn, IgnitionOff)</key>
				<value>SnowCrawlerLoop</value>
			</li>
		</soundSustainersOnEvent>
		<soundOneShotsOnEvent Inherit = "False">
			<li>
				<key>PawnEntered</key>
				<value>SnowCrawlerHatch</value>
			</li>
			<li>
				<key>PawnExited</key>
				<value>SnowCrawlerHatch</value>
			</li>
			<li>
				<key>IgnitionOn</key>
				<value>SnowCrawlerStartup</value>
			</li>
			<li>
				<key>IgnitionOff</key>
				<value>SnowCrawlerShutdown</value>
			</li>
		</soundOneShotsOnEvent>
		<properties>
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
				</li>
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>3</slots>
				</li>
			</roles>
			<customBiomeCosts>
				<li>
					<key>Tundra</key>
					<value>1</value>
				</li>
				<li>
					<key>IceSheet</key>
					<value>1</value>
				</li>
				<li>
					<key>BorealForest</key>
					<value>1</value>
				</li>
				<li>
					<key>ColdBog</key>
					<value>1</value>
				</li>
				<li>
					<key>SeaIce</key>
					<value>1</value>
				</li>
			</customBiomeCosts>
			<customTerrainCosts AllowTerrainWithTag="Soil" PathCost="1">
				<li>
					<key>Ice</key>
					<value>1</value>
				</li>
			</customTerrainCosts>
			<customHillinessCosts>
				<li>
					<key>SmallHills</key>
					<value>0</value>
				</li>
				<li>
					<key>LargeHills</key>
					<value>0.5</value>
				</li>
				<li>
					<key>Mountainous</key>
					<value>1</value>
				</li>
			</customHillinessCosts>
			<customSnowCategoryTicks AssignDefaults = "0"/>
			<customRoadCosts AssignDefaults = "0.4"/>
			<winterSpeedMultiplier>0</winterSpeedMultiplier>
			<worldSpeedMultiplier>1</worldSpeedMultiplier>
			<offRoadMultiplier>1</offRoadMultiplier>
		</properties>
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>80</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>Internal</depth>
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<radius>4</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				<hitbox>
					<side>Front</side>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftTreads</key>
				<label>Left Treads</label>
				<health>200</health>
				<depth>External</depth>	
				<tags>
					<li>Wheel</li>
				</tags>
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Left</side>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>RightTreads</key>
				<label>Right Treads</label>
				<health>200</health>
				<depth>External</depth>	
				<tags>
					<li>Wheel</li>
				</tags>
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Right</side>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>Gearbox</key>
				<label>Gearbox</label>
				<health>85</health>
				<depth>Internal</depth>
				<hitbox>
					<cells>
						<li>(0, 1)</li>
						<li>(1, 1)</li>
					</cells>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftSidePlating</key>
				<label>Left Hull Plating</label>
				<health>150</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Left</side>
				</hitbox>
			</li>
			<li>
				<key>RightSidePlating</key>
				<label>Right Hull Plating</label>
				<health>150</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>FuelTank</key>
				<label>Fuel Tank</label>
				<health>60</health>
				<depth>Internal</depth>
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
				</reactors>
				<hitbox>
					<cells>
						<li>(0, -2)</li>
						<li>(1, -2)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>ForwardBumper</key>
				<label>Forward Bumper</label>
				<health>75</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>RearBumper</key>
				<label>Rear Bumper</label>
				<health>50</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Back</side>
				</hitbox>
			</li>
			<li>
				<key>StorageRacks</key>
				<label>Storage Racks</label>
				<health>200</health>
				<depth>Internal</depth>
				<hitbox>
					<side>Back</side>
				</hitbox>
				<categories>
					<li>CargoCapacity</li>
				</categories>
			</li>
			<li>
				<key>ControlPanel</key>
				<label>Control Panel</label>
				<health>35</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Roof</key>
				<label>Roof</label>
				<health>100</health>
				<depth>External</depth>
				<hitWeight>10</hitWeight>
			</li>
		</components>
		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<leakDef>Filth_Fuel</leakDef>
				<fuelConsumptionRate>24</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.5</fuelConsumptionWorldMultiplier>
				<fuelCapacity>200</fuelCapacity>
			</li>
		</comps>
	</Vehicles.VehicleDef>
</Defs>