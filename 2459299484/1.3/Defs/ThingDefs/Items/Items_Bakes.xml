<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MealCooked">
    <defName>SB_SaucedBakeSimple</defName>
    <label>sauced simple bake</label>
    <description>A simple sauced bake quickly cooked from a few ingredients and flour.</description>
    <graphicData>
      <texPath>Things/Bakes/SB_SaucedSimpleBake</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>20</MarketValue>
      <WorkToMake>550</WorkToMake>
      <Nutrition>0.7</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
	  <tasteThought>SB_AteSaucedSimpleBake</tasteThought>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
			<li Class="VanillaCookingExpanded.CompProperties_StackByCondiments">
				<condimentTagToCheck>Condiments</condimentTagToCheck>
			</li>
    </comps>
  </ThingDef>

