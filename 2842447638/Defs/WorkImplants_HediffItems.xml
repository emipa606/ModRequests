<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="GR_MK_WorkImplant_Base_Item"
			  Abstract="True"
			  ParentName="BodyPartBase">
		<tradeability>Sellable</tradeability>
		<thingCategories>
			<li>GR_ImplantCategory</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Item/GR_AnimalGeneticHybridOrgan</texPath>
			<graphicClass>Graphic_Single</graphicClass>

		</graphicData>
		<statBases>
			<DeteriorationRate>2.0</DeteriorationRate>
		</statBases>

		<thingSetMakerTags>
			<li>NotForGuests</li>
		</thingSetMakerTags>
	</ThingDef>
	
	<HediffDef Name="GR_MK_WorkImplant_Base" Abstract="True">
	<hediffClass>HediffWithComps</hediffClass>
		<defaultLabelColor>(232, 157, 27)</defaultLabelColor>
		<isBad>false</isBad>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>		
	</HediffDef>

	<RecipeDef Name="GR_MK_WorkImplant_Base_Recipe"
			   Abstract="True"
			     ParentName="SurgeryInstallImplantBase">
		<appliedOnFixedBodyParts>
			<li>Brain</li>
		</appliedOnFixedBodyParts>
		<workAmount>25000</workAmount>
	</RecipeDef>
	
	<!--Clean-->
	
	<HediffDef ParentName="GR_MK_WorkImplant_Base">
		<defName>GR_AnimalWorkImplant_Clean</defName>
		<label>animal task motivator (clean)</label>
		<labelNoun>an animal task motivator (clean)</labelNoun>
		<description>An installed animal task motivator implant. This implant will allow this creature to perform simple tasks.</description>
		<descriptionHyperlinks>
			<ThingDef>GR_AnimalWorkImplant_Clean</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>GR_AnimalWorkImplant_Clean</spawnThingOnRemoved>
		<stages>
			<li>
				<statOffsets>
					<RestRateMultiplier>-0.2</RestRateMultiplier>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>

	<ThingDef ParentName="GR_MK_WorkImplant_Base_Item">
		<defName>GR_AnimalWorkImplant_Clean</defName>
		<label>animal Implant: animal task motivator (clean)</label>
		<description>A biomechanical animal implant composed of a miniature computer and human brain tissue. It regularly “motivates” its host to perform simple tasks whenever they’re idle. In spite of possibly being a crime against nature, it doesn’t cause any negative side effects other than the need to take slightly longer naps.\n\nThis particular model allows an animal to perform clean tasks.</description>
		<statBases>
			<MarketValue>225</MarketValue>
			<Mass>3</Mass>
		</statBases>
		<techHediffsTags>
			<li>Advanced</li>
		</techHediffsTags>
	</ThingDef>
	<RecipeDef ParentName="GR_HybridProsthesisBase">
		<defName>GR_MakeAnimalWorkImplant_Clean</defName>
		<label>Make animal task motivator (clean)</label>
		<description>Craft an animal task motivator implant. This implant will allow this creature to perform simple tasks.</description>
		<jobString>Making animal task motivator implant.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_HumanoidGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ComponentSpacer</li>
				<li>Plasteel</li>
				<li>GR_HumanoidGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_AnimalWorkImplant_Clean>1</GR_AnimalWorkImplant_Clean>
		</products>

	</RecipeDef>
	<RecipeDef ParentName="GR_MK_WorkImplant_Base_Recipe">
		<defName>GR_InstallAnimalWorkImplant_Clean</defName>
		<label>&lt;color=#E89D1B&gt;install animal Implant: animal task motivator (clean) &lt;/color&gt;</label>
		<description>Installs animal task motivator implant. This implant will allow this creature to perform simple tasks.</description>
		<descriptionHyperlinks>
			<ThingDef>GR_AnimalWorkImplant_Clean</ThingDef>
			<HediffDef>GR_AnimalWorkImplant_Clean</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing animal task motivator (clean).</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_AnimalWorkImplant_Clean</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>GR_AnimalWorkImplant_Clean</li>
			</thingDefs>
		</fixedIngredientFilter>
		<addsHediff>GR_AnimalWorkImplant_Clean</addsHediff>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>GR_RemoveAnimalWorkImplant_Clean</defName>
		<label>remove animal task motivator (clean)</label>
