	  <useWornGraphicMask>true</useWornGraphicMask>
      <wornGraphicPath>Things/Pawn/Warcasketlike/AncientWC_Cloak/WarcasketHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
