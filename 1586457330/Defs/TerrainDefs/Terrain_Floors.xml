<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<TerrainDef ParentName="FloorBase">
		<defName>MAC_TampedDirt</defName>
		<label>tamped dirt</label>
		<description>Dirt tamped to form an stable floor.</description>
		<texturePath>TampedDirt</texturePath>
		<edgeType>Hard</edgeType>
		<renderPrecedence>70</renderPrecedence>
		<statBases>
			<Beauty>-1</Beauty>
			<Cleanliness>-0.5</Cleanliness>
			<WorkToBuild>50</WorkToBuild>
		</statBases>
		<affordances>
			<li>Light</li>
			<li>Medium</li>
			<li>Heavy</li>
			<li>Diggable</li>
		</affordances>
		<terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
	</TerrainDef>

</Defs>