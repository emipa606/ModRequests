<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Crude -->
	<ThingDef ParentName="BodyPartBionicBase">
		<defName>QE_CrudeBionicEye</defName>
		<label>crude bionic eye</label>
		<description>This artificial eye is itchy and uncomfortable, but it functions better than a biological eye in every way. A lattice-dust healing system allows it to recover from damage.</description>
		<techLevel>Industrial</techLevel>
		<costList>
			<Steel>20</Steel>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<statBases>
			<MarketValue>400</MarketValue>
			<WorkToMake>19000</WorkToMake>
			<Mass>0.3</Mass>
		</statBases>
		<recipeMaker>
			<recipeUsers Inherit="False">
				<li>TableMachining</li>
			</recipeUsers>
			<researchPrerequisite>QE_CrudeBionics</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

