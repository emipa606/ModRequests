<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Medieval</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- ============================ Melee ========================== -->
				
				<!-- ==================== Medieval ==================== -->

				<!-- ===== Medieval Good Melee ===== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "VFEM_MeleeWeapon_Claymore" or defName = "VFEM_MeleeWeapon_HeavyMace"]/weaponTags</xpath>
					<value>
						<li>WoW_Medieval_Melee_Good</li>
					</value>
				</li>


				<!-- ============================ Ranged ========================== -->
            
				<!-- ==================== Medieval ==================== -->

				<!-- ===== Medieval Good Ranged ===== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "VFEM_Bow_HeavyCrossbow"]/weaponTags</xpath>
					<value>
						<li>WoW_Medieval_Ranged_Good</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
</Patch>