<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<FactionDef Parent="WoW_Base_Faction">
		<defName>HighElf</defName>
		<label>silver covenant enclave</label>
		<description>The Silver Covenant is a militant faction and the largest bastion of high elves on azeroth. They reject the admission of blood elves into the Kirin Tor and have taken it upon themselves to serve as a military deterrent for any potential Horde uprising.</description>
		<pawnsPlural>High Elves</pawnsPlural>
		<fixedName>The Silver Covenant</fixedName>
		<settlementGenerationWeight>.1</settlementGenerationWeight>
		<requiredCountAtGameStart>1</requiredCountAtGameStart>
		<leaderTitle>Ranger General</leaderTitle>
		<factionIconPath>World/WorldObjects/SilverCovenant Icon</factionIconPath>
		<settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
		<colorSpectrum>
			<li>(141,157,215)</li>
		</colorSpectrum>
		<settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
		<techLevel>Medieval</techLevel>
		<backstoryFilters>
			<li>
				<categories>
					<li>WoW_HighElf_Backstory</li>
				</categories>
				<commonality>1</commonality>
			</li>
		</backstoryFilters>
		<hairTags>
			<li>WoW_BloodElf_Hair</li>
		</hairTags>
		<visitorTraderKinds>
			<li>Traderkind_HighElf_Visitor</li>
		</visitorTraderKinds>
		<caravanTraderKinds>
			<li>Traderkind_HighElf_Caravan_General</li>
			<li>Traderkind_HighElf_Caravan_Armament</li>
		</caravanTraderKinds>
		<baseTraderKinds>
			<li>Traderkind_HighElf_Base</li>
		</baseTraderKinds>
		<maxPawnCostPerTotalPointsCurve>
			<points>
				<li>(0,35)</li>
				<li>(70, 50)</li>
				<li>(700, 100)</li>
				<li>(1300, 150)</li>
				<li>(100000, 10000)</li>
			</points>
		</maxPawnCostPerTotalPointsCurve>
		<pawnGroupMakers>
			<li>
				<kindDef>Peaceful</kindDef>
				<options>
					<WoW_HighElf_Villager>50</WoW_HighElf_Villager>
					<WoW_HighElf_Scout>100</WoW_HighElf_Scout>
					<WoW_HighElf_Ranger>100</WoW_HighElf_Ranger>
					<WoW_HighElf_Swordsman>50</WoW_HighElf_Swordsman>
					<WoW_HighElf_SpellBreaker>40</WoW_HighElf_SpellBreaker>
					<WoW_HighElf_RangerCaptain>20</WoW_HighElf_RangerCaptain>
				</options>
			</li>
			<li>
				<kindDef>Trader</kindDef>
				<traders>
					<WoW_HighElf_Trader>10</WoW_HighElf_Trader>
				</traders>
				<carriers>
					<Muffalo>3</Muffalo>
				</carriers>
				<guards>
				   <WoW_HighElf_Scout>100</WoW_HighElf_Scout>
					<WoW_HighElf_Ranger>100</WoW_HighElf_Ranger>
					<WoW_HighElf_Swordsman>50</WoW_HighElf_Swordsman>
					<WoW_HighElf_SpellBreaker>20</WoW_HighElf_SpellBreaker>
				</guards>
			</li>
			<li>
				<kindDef>Combat</kindDef>
				<options>
				   <WoW_HighElf_Scout>100</WoW_HighElf_Scout>
					<WoW_HighElf_Ranger>100</WoW_HighElf_Ranger>
					<WoW_HighElf_Swordsman>50</WoW_HighElf_Swordsman>
					<WoW_HighElf_SpellBreaker>50</WoW_HighElf_SpellBreaker>
					<WoW_HighElf_RangerCaptain>20</WoW_HighElf_RangerCaptain>
				</options>
			</li>
			<li>
				<kindDef>Settlement</kindDef>
				<options>
					<WoW_HighElf_Ranger>100</WoW_HighElf_Ranger>
					<WoW_HighElf_Swordsman>50</WoW_HighElf_Swordsman>
					<WoW_HighElf_SpellBreaker>50</WoW_HighElf_SpellBreaker>
					<WoW_HighElf_RangerCaptain>20</WoW_HighElf_RangerCaptain>
				</options>
			</li>
		</pawnGroupMakers>
