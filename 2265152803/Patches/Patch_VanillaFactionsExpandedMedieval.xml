<?xml version="1.0" encoding="utf-8"?>
<Patch>
<!--Check if VFEM is loaded - then grant VFEM_MedievalStart the research AYTallow and AYCharcoal -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Vanilla Factions Expanded - Medieval</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> <!-- Buff Medieval Start a bit -->
				<xpath>Defs/ResearchProjectDef[defName="AYCharcoal"]</xpath>
				<value><tags><li>VFEM_MedievalStart</li></tags></value>
			</li>
			</operations>
		</match>
	</Operation>
</Patch>