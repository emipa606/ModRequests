<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>Porter_Blueprint</defName>
		<label>Porter</label>
		<description>A sturdy, durable, and inexpensive pickup truck with 2 seats and a huge cargo box. Its off-road capabilities are not inferior, making it a useful truck</description>
		<designationCategory>TC_Vehicles</designationCategory>
		
		<graphicData>
			<texPath>Vehicles/Porter/Porter</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,6)</drawSize>
		</graphicData>
		
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>9500</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		
		<size>(2,5)</size>
		
		<researchPrerequisites>
			<li>Fabrication</li>
		</researchPrerequisites>
		
		<costList>
			<EngineMedium>1</EngineMedium>
			<ComponentHeavyCasting>1</ComponentHeavyCasting>
			<ComponentWheelTire>4</ComponentWheelTire>
			<ComponentIndustrial>6</ComponentIndustrial>
			<Steel>300</Steel>
		</costList>
		
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		
		<thingToSpawn>TC_Porter</thingToSpawn>
		
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>
</Defs>
