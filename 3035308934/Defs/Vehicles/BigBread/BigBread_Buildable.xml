<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable"><!-- 建筑形态的载具，基本上与BuildingDef相同-->
		<defName>BigBread_Blueprint</defName>
		<label>BigBread</label>
		<description>A van, with an additional row of seats, is capable of serving as a personnel carrier for impoverished colonies</description>
		<designationCategory>TC_Vehicles</designationCategory>
		
		<graphicData>
			<texPath>Vehicles/BigBread/BigBread</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,6)</drawSize>
		</graphicData>
		
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>9500</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		
		<size>(2,5)</size>
		
		<researchPrerequisites>
			<li>Fabrication</li>
		</researchPrerequisites>
		
		<costList>
			<EngineMedium>1</EngineMedium>
			<ComponentHeavyCasting>1</ComponentHeavyCasting>
			<ComponentWheelTire>4</ComponentWheelTire>
			<ComponentIndustrial>8</ComponentIndustrial>
			<Steel>280</Steel>
		</costList>
		
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		
		<thingToSpawn>TC_BigBread</thingToSpawn><!-- 建造后生成的载具Kind,这是与建筑唯一不一样的-->
		
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>
</Defs>
