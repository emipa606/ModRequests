<?xml version="1.0" encoding="utf-8" ?>

<Defs>	
	<Vehicles.VehicleDef ParentName="TC_BaseVehicle">
		<defName>TC_BigBread</defName>
		<label>BigBread</label>
		<description>A van, with an additional row of seats, is capable of serving as a personnel carrier for impoverished colonies.It is suitable for driving on the road, but its off-road capability is slightly poor</description>
		
		<graphicData>
			<texPath>Vehicles/BigBread/BigBread</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(3,6)</drawSize>
			<color>(130, 130, 130)</color>
			<colorTwo>(130, 30, 30)</colorTwo>
			<colorThree>(30, 15, 20)</colorThree>
		</graphicData>
		
		<fillPercent>1</fillPercent>
		
		<buildDef>BigBread_Blueprint</buildDef>
		
		<size>(2,5)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Transport</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		
		<statBases>
			<ArmorRating_Blunt>0.24</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.48</ArmorRating_Sharp>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<Flammability>0.5</Flammability>
			<MarketValue>2550</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>200</Mass>
			<MoveSpeed>12</MoveSpeed>
			<CargoCapacity>250</CargoCapacity>
			<RepairRate>0.8</RepairRate>
		</vehicleStats>
		
		
		<race>
			<baseBodySize>3.25</baseBodySize>
		</race>
		
		<properties>
				    <worldSpeedMultiplier>1</worldSpeedMultiplier>
			<customRoadCosts AssignDefaults = "0.25"/>
			
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
				</li>
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>6</slots>
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>60</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftWheel</key>
				<label>Front Left Wheel</label>
				<health>40</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightWheel</key>
				<label>Front Right Wheel</label>
				<health>40</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackLeftWheel</key>
				<label>Back Left Wheel</label>
				<health>40</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, -2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightWheel</key>
				<label>Back Right Wheel</label>
				<health>40</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, -2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftSuspension</key>
				<label>Front Left Suspension</label>
				<health>40</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightSuspension</key>
				<label>Front Right Suspension</label>
				<health>40</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>Gearbox</key>
				<label>Gearbox</label>
				<health>60</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 1)</li>
						<li>(1, 1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftSidePanel</key>
				<label>Left Side Panel</label>
				<health>60</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Left</side>
				</hitbox>
			</li>
			<li>
				<key>RightSidePanel</key>
				<label>Right Side Panel</label>
				<health>60</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>Chemtank</key>
				<label>Chemtank</label>
				<health>50</health>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, -2)</li>
						<li>(1, -2)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>FrontBumper</key>
				<label>Front Bumper</label>
				<health>50</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>RearBumper</key>
				<label>Rear Bumper</label>
				<health>20</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Back</side>
				</hitbox>
			</li>
			<li>
				<key>Upholstery</key>
				<label>Upholstery</label>
				<health>30</health>
				<depth>Internal</depth>
				<hitWeight>5</hitWeight>
			</li>
			<li>
				<key>Dashboard</key>
				<label>Dashboard</label>
				<health>30</health>
				<depth>Internal</depth>
				<hitWeight>2</hitWeight>
			</li>
			<li>
				<key>SteeringWheel</key>
				<label>Steering Wheel</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Alternator</key>
				<label>Alternator</label>
				<health>40</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Hood</key>
				<label>Hood</label>
				<health>50</health>
				<depth>External</depth>
				<hitWeight>5</hitWeight>
			</li>
			<li>
				<key>Roof</key>
				<label>Roof</label>
				<health>30</health>
				<depth>External</depth>
				<hitWeight>5</hitWeight>
			</li>
		</components>
		
		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<leakDef>Filth_Fuel</leakDef>
				<fuelConsumptionRate>16.5</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.5</fuelConsumptionWorldMultiplier>
				<fuelCapacity>180</fuelCapacity>
			</li>
		</comps>
	</Vehicles.VehicleDef>
</Defs>