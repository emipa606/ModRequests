<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>Fellowtown_Blueprint</defName>
		<label>Fellowtown</label>
		<description>There is a dual mounted anti-aircraft gun in the cargo box of the pickup truck, with a huge recoil force that may overturn the vehicle body. Armed elements in war zones are quite fond of such modifications</description>
		<designationCategory>TC_Vehicles</designationCategory>
		
		<graphicData>
			<texPath>Vehicles/Fellowtown/Fellowtown</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,6)</drawSize>
		</graphicData>
		
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>9500</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		
		<size>(2,5)</size>
		
		<researchPrerequisites>
			<li>Fabrication</li>
		</researchPrerequisites>
		
		<costList>
			<EngineMedium>1</EngineMedium>
			<ComponentHeavyCasting>2</ComponentHeavyCasting>
			<ComponentWheelTire>4</ComponentWheelTire>
			<ComponentIndustrial>10</ComponentIndustrial>
			<Steel>300</Steel>
		</costList>
		
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		
		<thingToSpawn>TC_Fellowtown</thingToSpawn>
		
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>
</Defs>
