					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>Chemtank</key>
				<label>Chemtank</label>
				<health>50</health>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
