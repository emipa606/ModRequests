<?xml version="1.0" encoding="utf-8" ?>

<Defs>	
	<Vehicles.VehicleDef ParentName="TC_BaseVehicle">
		<defName>TC_Fellowtown</defName>
		<label>Fellowtown</label>
		<description>There is a dual mounted anti-aircraft gun in the cargo box of the pickup truck, with a huge recoil force that may overturn the vehicle body. Armed elements in war zones are quite fond of such modifications</description>
		
		<graphicData>
			<texPath>Vehicles/Fellowtown/Fellowtown</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(3,6)</drawSize>
			<color>(130, 130, 130)</color>
			<colorTwo>(130, 30, 30)</colorTwo>
			<colorThree>(30, 15, 20)</colorThree>
		</graphicData>
		
		<fillPercent>1</fillPercent>
		
		<buildDef>Fellowtown_Blueprint</buildDef>
		
		<size>(2,5)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Transport</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		
		<statBases>
			<ArmorRating_Blunt>0.24</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.48</ArmorRating_Sharp>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<Flammability>0.5</Flammability>
			<MarketValue>2730</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>260</Mass>
			<MoveSpeed>14</MoveSpeed>
			<CargoCapacity>350</CargoCapacity>
			<RepairRate>0.8</RepairRate>
		</vehicleStats>
		

		
		<race>
			<baseBodySize>3.25</baseBodySize>
		</race>
		
		<properties>
				    <worldSpeedMultiplier>1</worldSpeedMultiplier>
			<customRoadCosts AssignDefaults = "0.25"/>
		
			<customHillinessCosts>
				<SmallHills>0.25</SmallHills>
				<LargeHills>0.75</LargeHills>
				<Mountainous>1.0</Mountainous>
			</customHillinessCosts>
			
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
				</li>
				<li>
					<key>gunner</key>
					<label>Gunner</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>Turret</handlingTypes>
					<turretIds>
						<li>mainTurret</li>
					</turretIds>
					<pawnRenderer>
						<layer>1</layer>
						<layerNorth>1</layerNorth>
						<layerSouth>1</layerSouth>
						<angleNorth>0</angleNorth>
						<angleEast>0</angleEast>
						<angleNorthEast>0</angleNorthEast>
						<angleSouthEast>0</angleSouthEast>
						<drawOffsetNorth>(0.5,0,-0.25)</drawOffsetNorth>
						<drawOffsetSouth>(-0.5,0,0.95)</drawOffsetSouth>
						<drawOffsetEast>(-0.65,0,0.35)</drawOffsetEast>

						<east>West</east>
						<west>East</west>
						<north>South</north>
						<northEast>West</northEast>
						<northWest>East</northWest>
						<south>North</south>
						<southEast>West</southEast>
						<southWest>East</southWest>
					</pawnRenderer>
				</li>
				
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>1</slots>
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>60</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftWheel</key>
				<label>Front Left Wheel</label>
				<health>40</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
