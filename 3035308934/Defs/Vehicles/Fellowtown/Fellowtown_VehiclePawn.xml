		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<leakDef>Filth_Fuel</leakDef>
				<fuelConsumptionRate>20.5</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.5</fuelConsumptionWorldMultiplier>
				<fuelCapacity>120</fuelCapacity>
			</li>
			<li Class="Vehicles.CompProperties_VehicleTurrets">
				<turrets>
					<li>
						<turretDef>TC_Fellowtown_Turret</turretDef>
						<renderProperties>
							<north>(0, -1.2)</north>
							<south>(0, 1.4)</south>
							<east>(-1.3, 0.39)</east>
						</renderProperties>
						<gizmoLabel>main turret</gizmoLabel>
						<angleRestricted>(45,315)</angleRestricted>
						<aimPieOffset>(0, 2.55)</aimPieOffset>
						<key>mainTurret</key>
					</li>
				</turrets>
			</li>
		</comps>
	</Vehicles.VehicleDef>
</Defs>