				<hitbox>
					<cells>
						<li>(0, 1)</li>
						<li>(1, 1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftSidePanel</key>
