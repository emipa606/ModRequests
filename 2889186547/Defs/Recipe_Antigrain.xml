      <thingDefs>
        <li>Steel</li>
        <li>Chemfuel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Shell_HighExplosive>4</Shell_HighExplosive>
    </products>
    <skillRequirements>
      <Crafting>8</Crafting>
      <Intellectual>5</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>Mortars</researchPrerequisite>
  </RecipeDef>

