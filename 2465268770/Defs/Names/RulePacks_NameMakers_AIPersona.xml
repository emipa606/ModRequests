<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>AIPersonaStandard</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[AIPersonaSerials] '[AIPersonaMonikers]' [AIPersonaNumbers]</li>

      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>AIPersonaSerials</keyword>
          <path>AIPersona/Serial</path>
        </li>
        <li Class="Rule_File">
          <keyword>AIPersonaNumbers</keyword>
          <path>AIPersona/Number</path>
        </li>
        <li Class="Rule_File">
          <keyword>AIPersonaMonikers</keyword>
          <path>AIPersona/Moniker</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>