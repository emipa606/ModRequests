	<xpath>*/AlienRace.ThingDef_AlienRace[defName="AIPersonaRace"]/alienRace/generalSettings/disallowedTraits</xpath>
	<value>
				<li><defName>VTE_Squeamish</defName></li>
				<li><defName>VTE_IronStomach</defName></li>
				<li><defName>VTE_DrunkenMaster</defName></li>
