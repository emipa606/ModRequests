<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<!-- Robots Don't Do Drugs -->
	<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Factions Expanded - Insectoids</li>
    </mods>
    <match Class="PatchOperationAdd">
	<xpath>*/AlienRace.ThingDef_AlienRace[defName="AIPersonaRace"]/alienRace/generalSettings/chemicalSettings</xpath> 
	<value>
				<li><chemical>VFEI_RoyalJellyChemical</chemical><ingestible>false</ingestible></li>
	</value></match>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Brewing Expanded - Coffees and Teas</li>
    </mods>
    <match Class="PatchOperationAdd">
	<xpath>*/AlienRace.ThingDef_AlienRace[defName="AIPersonaRace"]/alienRace/generalSettings/chemicalSettings</xpath> 
	<value>
				<li><chemical>VBE_Caffeine</chemical><ingestible>false</ingestible></li>
				<li><chemical>VBE_Tobacco</chemical><ingestible>false</ingestible></li>
	</value></match>
	</Operation>
	
</Patch>