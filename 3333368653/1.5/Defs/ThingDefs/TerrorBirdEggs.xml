<Defs>

<!-- Fertilized eggs -->

    <ThingDef ParentName="EggFertBase">
        <defName>EggPenglenkenFertilized</defName>
        <label>penglenken egg (fert.)</label>
        <description>Large Egg of Pengleken, will need proper tending and time to to hatch into a proper large Kelenken.</description>
        <possessionCount>1</possessionCount>
        <graphicData>
          <color>(125,138,97)</color>
          <drawSize>1.2</drawSize>
          <texPath>Resources/Eggs/LargeEggs</texPath>
          <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
          <Nutrition>0.5</Nutrition>
          <MarketValue>19.0</MarketValue>
        </statBases>
        <comps>
          <li Class="CompProperties_Hatcher">
            <hatcherDaystoHatch>7</hatcherDaystoHatch>
            <hatcherPawn>pphhyy_Penglenken</hatcherPawn>
          </li>
        </comps>
      </ThingDef>

