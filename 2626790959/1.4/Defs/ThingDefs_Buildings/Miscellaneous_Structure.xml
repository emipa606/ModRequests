	<ThingDef ParentName="ReinforcedBuildingBase">
		<defName>ReinforcedWall</defName>
		<label>Reinforced Wall</label>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<description>Very heavily reinforced wall.</description>
		<graphicData>
			<texPath>Structures/ReinforcedWalls/Reinforced_Wall</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<linkType>CornerFiller</linkType>
			<linkFlags>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<uiIconPath>Structures/ReinforcedWalls/ReinforcedWallIco</uiIconPath>
		<statBases>
			<MaxHitPoints>1400</MaxHitPoints>
			<WorkToBuild>550</WorkToBuild>
		</statBases>
		<costList>
			<DDA_ReinforcedPlate>5</DDA_ReinforcedPlate>
		</costList>
		<costStuffCount>7</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>1</fillPercent>
		<coversFloor>true</coversFloor>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<holdsRoof>true</holdsRoof>
		<designationCategory>Structure</designationCategory>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<canOverlapZones>false</canOverlapZones>
		<stuffCategories>
			<li>Woody</li>
			<li>Metallic</li>
			<li>Stony</li>
		</stuffCategories>
		<building>
			<isInert>true</isInert>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
			<!--=
			<ignoreNeedsPower>true</ignoreNeedsPower>
			-->
		</building>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>.7</multiplier>
			</li>
		</damageMultipliers>
		<researchPrerequisites>
			<li>ReinforcedWalls</li>
		</researchPrerequisites>
	</ThingDef>
	
	<ThingDef ParentName="ReinforcedBuildingBase" Name="Embrasure">
		<defName>ReinforcedEmbrasure</defName>
		<label>Reinforced Embrasure</label>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<description>Very heavily reinforced wall with embrasures for firing while behind cover.</description>
		<graphicData>
			<texPath>Structures/ReinforcedEmbrasures2/Reinforced_Embrasure</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<linkType>CornerFiller</linkType>
			<linkFlags>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<uiIconPath>Structures/ReinforcedEmbrasures2/ReinforcedEmbrasureIco</uiIconPath>
		<statBases>
			<MaxHitPoints>1400</MaxHitPoints>
			<WorkToBuild>700</WorkToBuild>
		</statBases>
		<costList>
			<DDA_ReinforcedPlate>5</DDA_ReinforcedPlate>
		</costList>
		<costStuffCount>7</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<coversFloor>true</coversFloor>
		<fillPercent>0.85</fillPercent>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<holdsRoof>true</holdsRoof>
		<designationCategory>Structure</designationCategory>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<canOverlapZones>false</canOverlapZones>
		<stuffCategories>
			<li>Woody</li>
			<li>Metallic</li>
			<li>Stony</li>
		</stuffCategories>
		<building>
			<isInert>true</isInert>
			<ai_chillDestination>false</ai_chillDestination>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
			<!--=
			<ignoreNeedsPower>true</ignoreNeedsPower>
			-->
		</building>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>.7</multiplier>
			</li>
		</damageMultipliers>
		<researchPrerequisites>
			<li>ReinforcedWalls</li>
		</researchPrerequisites>
	</ThingDef>
	
	
</Defs>