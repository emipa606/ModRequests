<ThingDef ParentName="SkyfallerBase">
	<defName>I3ShuttleVega_Leaving</defName>
	<label>Shuttle (leaving)</label>
	<thingClass>SRTS.SRTSLeaving</thingClass>
	<graphicData>
		<texPath>Vega/Texture</texPath>
		<graphicClass>Graphic_Multi</graphicClass>
		<shaderType>CutoutFlying</shaderType>
		<drawSize>(17,17)</drawSize>
	</graphicData>
	<altitudeLayer>MoteOverhead</altitudeLayer>
	<skyfaller>
		<movementType>Accelerate</movementType>
		<reversed>true</reversed>
		<shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
		<shadowSize>(5,5)</shadowSize>
		<anticipationSound>I3Shuttle_Launch</anticipationSound>
		<anticipationSoundTicks>-120</anticipationSoundTicks>
		<ticksToImpactRange>
			<min>-120</min>
			<max>-120</max>
		</ticksToImpactRange>
	</skyfaller>
</ThingDef>

