		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
		<costList>
			<Cloth>30</Cloth>
			<Plasteel>60</Plasteel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToMake>10000</WorkToMake>
			<Mass>3</Mass>
