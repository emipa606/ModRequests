<Defs>


<!-- ========================= Airborne =========================-->

	<!-- Airborne Uniform -->

<ThingDef ParentName="ApparelMakeableBase">
		<defName>ReunionAirborneUniform</defName>
		<label>reunion airborne uniform</label>
		<description>A light, protective coverall used by the reunion faction airborne units.</description>
		<graphicData>
			<texPath>Things/Apparel/Airborne/Clothes/Airborne</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.55</drawSize>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
		<costList>
			<Steel>10</Steel>
			<Cloth>60</Cloth>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>3000</WorkToMake>
			<Mass>0.8</Mass>
			<Flammability>0.4</Flammability>
			<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
			<ArmorRating_Heat>0.15</ArmorRating_Heat>
			<Insulation_Cold>15</Insulation_Cold>
			<Insulation_Heat>20</Insulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.10</MoveSpeed>
		</equippedStatOffsets>
    <thingCategories Inherit='False'>
      <li>Reunion_Apparel_Airborne</li>
    </thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Airborne/Clothes/Airborne</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>ReunionAirborne</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
            </defaultOutfitTags>
		</apparel>
    </ThingDef>
	
	<!-- Airborne Mask -->
	
	<ThingDef ParentName="ArmorMachineableBase">
    <defName>ReunionAirborneMask</defName>
    <label>reunion airborne mask</label>
    <description>A ballistic mask used by the reunion faction airborne units.</description>
    <graphicData>
      <texPath>Things/Apparel/Airborne/Headgear/AirborneHood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1.55</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <MaxHitPoints>60</MaxHitPoints>
      <Mass>2</Mass>
      <Flammability>0.7</Flammability>
      <ArmorRating_Sharp>0.70</ArmorRating_Sharp>
	  <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Heat>0.15</ArmorRating_Heat>
      <Insulation_Cold>5</Insulation_Cold>
      <Insulation_Heat>8</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <thingCategories>
      <li>Reunion_Apparel_Airborne</li>
    </thingCategories>
    <costList>
	  <Cloth>40</Cloth>
      <Steel>30</Steel>
	  <Plasteel>10</Plasteel>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
        <li>UpperHead</li>
      </bodyPartGroups>
       <wornGraphicPath>Things/Apparel/Airborne/Headgear/AirborneHood</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>ReunionSoldier</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

