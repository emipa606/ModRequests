    </equippedStatOffsets>
    <thingCategories>
      <li>EncApparels</li>
    </thingCategories>
    <costList>
      <Cloth>30</Cloth>
<Plasteel>30</Plasteel>
      <Steel>60</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/FlakVest/FlakVest</wornGraphicPath>
