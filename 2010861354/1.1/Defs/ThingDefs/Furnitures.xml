  <ThingDef ParentName="BoSBuildingBase">
    <defName>BoS_panel</defName>
    <label>control panel</label>
    <description>A serie of switches and display devices, intended to serve as an auxilliary interface for pre-war mainframes. Boost research speed by 10%.</description>
    <graphicData>	
      <texPath>Furnitures/BoSpanel</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
<shaderType>CutoutComplex</shaderType>
	  <drawSize>(2.5,2.5)</drawSize>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>
    <researchPrerequisites><li>BoS_standard</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
<castEdgeShadows>false</castEdgeShadows>
   <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <fillPercent>0.6</fillPercent>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>0.7</Flammability>
    </statBases>
    <size>(1,1)</size>
    <costList>
      <Steel>80</Steel>
<ComponentIndustrial>12</ComponentIndustrial>
    </costList>
<comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>90</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
   <li Class="CompProperties_Breakdownable"/>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <ResearchSpeedFactor>0.1</ResearchSpeedFactor>
        </statOffsets>
        <maxSimultaneous>2</maxSimultaneous>
 </li>
<li Class="CompProperties_Glower">
        <glowRadius>2</glowRadius>
        <glowColor>(80,217,80,0)</glowColor>
          </li>
    </comps>
    <designationCategory>Misc</designationCategory>
    <uiIconScale>1</uiIconScale>
   <uiIconPath>Furnitures/BoSpanel</uiIconPath>
	  </ThingDef>

<ThingDef ParentName="BuildingBase">
    <defName>Enc_SatCom</defName>
    <label>communication array</label>
    <description>A compact dual emitter/receiver device that allows radio contact with other factions and orbital traders if placed outside. Will not short-circuit in rain.</description>
    <thingClass>Building_CommsConsole</thingClass>
    <graphicData>
      <texPath>Furnitures/EncSatcom</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
           <shadowData>
        <volume>(1,0.5,1)</volume>
      </shadowData>
    </graphicData>
<minifiedDef>MinifiedThing</minifiedDef>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
       <size>(1,2)</size>
