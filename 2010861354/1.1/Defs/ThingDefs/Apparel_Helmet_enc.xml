<?xml version="1.0" encoding="UTF-8"?>


<Defs>



<!-- ==================================== Enclave officer cap =====================================-->


<ThingDef ParentName="ApparelMakeableBase">
<defName>DRKApparelEncCap</defName>
<label>Enclave cap</label>
<description>A black service cap, matching the uniform.</description>

<techLevel>Spacer</techLevel>


<recipeMaker>
<researchPrerequisite>enc_dota</researchPrerequisite>
<skillRequirements>
<Crafting>5</Crafting>
</skillRequirements>
</recipeMaker>


<graphicData>
<texPath>Things/enclave/enccap</texPath>
<graphicClass>Graphic_Single</graphicClass>
</graphicData>
<generateCommonality>0.1</generateCommonality>


<statBases>
<WorkToMake>600</WorkToMake>
<MaxHitPoints>100</MaxHitPoints>
<Mass>1</Mass>
<Flammability>1</Flammability>
<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
<ArmorRating_Blunt>0.15</ArmorRating_Blunt>
<ArmorRating_Heat>0.1</ArmorRating_Heat>
<Insulation_Cold>10</Insulation_Cold>
<Insulation_Heat>15</Insulation_Heat>
<EquipDelay>1</EquipDelay>
</statBases>

<equippedStatOffsets>
<MentalBreakThreshold>-0.10</MentalBreakThreshold>
<SocialImpact>0.1</SocialImpact>
</equippedStatOffsets>



<thingCategories>
<li>EncHeadgears</li>
</thingCategories>

<costList>
<Cloth>20</Cloth>
</costList>


<apparel>
<bodyPartGroups>
<li>FullHead</li>
</bodyPartGroups>

<wornGraphicPath>Things/enclave/enccap</wornGraphicPath>


<layers>
<li>Overhead</li>
</layers>


<tags>
<li>Enclave</li>
<li>Royal</li>
</tags>


<defaultOutfitTags>
<li>Worker</li>
<li>Soldier</li>
</defaultOutfitTags>
</apparel>



<thingSetMakerTags>
<li>RewardSpecial</li>
</thingSetMakerTags>

</ThingDef>
