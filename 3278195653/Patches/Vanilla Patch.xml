<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!---->
	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationAddModExtension">
				<xpath>*/WorldGenStepDef[defName="Terrain"]</xpath>
				<value>
					<li Class="WorldGenStepCustomizer.DefModExtension_WorldGenStepCustomizer">
						<locked>true</locked>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>*/WorldGenStepDef[defName="Components"]</xpath>
				<value>
					<li Class="WorldGenStepCustomizer.DefModExtension_WorldGenStepCustomizer">
						<locked>true</locked>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>*/WorldGenStepDef[defName="Factions"]</xpath>
				<value>
					<li Class="WorldGenStepCustomizer.DefModExtension_WorldGenStepCustomizer">
						<locked>true</locked>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>*/WorldGenStepDef[defName="Features"]</xpath>
				<value>
					<li Class="WorldGenStepCustomizer.DefModExtension_WorldGenStepCustomizer">
						<locked>true</locked>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
	<!---->
</Patch>