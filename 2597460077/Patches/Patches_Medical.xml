<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/StatDef[defName = "MedicalTendQuality" or defName = "MedicalSurgerySuccessChance"]</xpath>
        <value>
            <parts>
                <li Class="ResearchableStatUpgrades.StatPart_ResearchDependent">
                    <researchFactors>
                        <li>
                            <def>MedicalI</def>
                            <factor>1.025</factor>
                        </li>
                        <li>
                            <def>MedicalII</def>
                            <factor>1.05</factor>
                        </li>
                    </researchFactors>
                </li>
            </parts>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/StatDef[defName = "MedicalTendSpeed" or defName = "MedicalOperationSpeed"]</xpath>
        <value>
            <parts>
                <li Class="ResearchableStatUpgrades.StatPart_ResearchDependent">
                    <researchFactors>
                        <li>
                            <def>MedicalI</def>
                            <factor>1.5</factor>
                        </li>
                        <li>
                            <def>MedicalII</def>
                            <factor>1.075</factor>
                        </li>
                    </researchFactors>
                </li>
            </parts>
        </value>
    </Operation>
</Patch>