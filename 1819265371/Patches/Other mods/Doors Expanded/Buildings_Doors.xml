<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Doors Expanded</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		  <operations>
			  <li Class="PatchOperationAdd">
				  <xpath>/Defs/DoorsExpanded.DoorExpandedDef[defName = "PH_DoorDouble" or defName = "PH_DoorTriple"]</xpath>
				  <value>
					  <researchPrerequisites>
						  <li>TS_Doors</li>
					  </researchPrerequisites>
				  </value>
			  </li>
		  </operations>
	  </match>
  </Operation>

    <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Doors Expanded (Dev)</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		  <operations>
			  <li Class="PatchOperationAdd">
				  <xpath>/Defs/ThingDef[defName = "PH_DoorDouble" or defName = "PH_DoorTriple"]</xpath>
				  <value>
					  <researchPrerequisites>
						  <li>TS_Doors</li>
					  </researchPrerequisites>
				  </value>
			  </li>
		  </operations>
	  </match>
  </Operation>

</Patch>