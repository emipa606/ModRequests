<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[KV] Infinite Storage - 1.1</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "IS_BodyPartStorage" or defName = "IS_MeatStorage" or defName = "IS_SilverStorage" or defName = "IS_TextileStorage"]</xpath>
					<value>
						<researchPrerequisites>
							<li>InfiniteStorage</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>

