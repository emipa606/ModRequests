<?xml version="1.0" encoding="utf-8" ?>

<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>LWM's Deep Storage</li>
        </mods>
        <match Class="PatchOperationInsert">
            <xpath>/Defs/ResearchProjectDef[defName = "TS_Walls"]</xpath>
            <value>
                <ResearchProjectDef>
                    <defName>TS_LWM_DS_Storage</defName>
                    <label>Storage</label>
                    <description>A new way to store all your stuff! At least, in concept. Your researcher doesn't quite know how to store every thing "exactly", but he's confident in getting some ideas by researching other things.</description>
                    <techLevel>Neolithic</techLevel>
                    <baseCost>500</baseCost>
                    <prerequisites>
                        <li>TS_BasicFurniture</li>
                    </prerequisites>
                </ResearchProjectDef>
            </value>
        </match>
    </Operation>

</Patch>