<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[WD] Expanded Floors</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "WDPlywoodFloor"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_IndoorFlooring</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "WDWidePlanksFloor"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_IndoorFlooring</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "WDCheckWoodenFloor"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_IndoorFlooring</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "WDParquet"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_IndoorFlooring</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TerrainDef[defName = "WDMosaicWoodenFloor"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_IndoorFlooring</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>