<?xml version="1.0" encoding="utf-8" ?>

<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Architect Expanded - Fences</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName = "Closeboard" or defName = "CloseboardGate" or defName = "Farm" or defName = "FarmGate"]</xpath>
            <value>
                <researchPrerequisites>
                    <li>TS_Walls</li>
                </researchPrerequisites>
            </value>
        </match>
    </Operation>

</Patch>