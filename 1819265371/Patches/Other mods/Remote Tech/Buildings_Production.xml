<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Remote Tech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName = "rxBench"]/researchPrerequisites</xpath>
					<value>
						<researchPrerequisites>
							<li>IEDs</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>