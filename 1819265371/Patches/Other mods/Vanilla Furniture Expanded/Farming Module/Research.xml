						<prerequisites>
							<li>TS_Agriculture</li>
						</prerequisites>
                        <requiredResearchBuilding>SimpleResearchBench</requiredResearchBuilding>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>