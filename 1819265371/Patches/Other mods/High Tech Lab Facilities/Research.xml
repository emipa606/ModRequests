<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>High Tech Laboratory Facilities (Continued)</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationInsert">
					<xpath>/Defs/ResearchProjectDef[defName = "TS_Walls"]</xpath>
					<value>
						<ResearchProjectDef>
							<defName>TS_HTLF_LabFacilities</defName>
							<label>lab facilities</label>
							<description>One day while researching, your researcher was inspired by his hi-tech research bench and invented some additional lab facilities such as the research terminal, lab station and the lab counter.</description>
							<baseCost>3000</baseCost>
							<techLevel>Industrial</techLevel>
							<prerequisites>
								<li>MicroelectronicsBasics</li>
							</prerequisites>
						</ResearchProjectDef>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>