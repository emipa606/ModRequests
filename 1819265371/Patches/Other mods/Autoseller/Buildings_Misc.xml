<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Auto Seller</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		  <operations>
			  <li Class="PatchOperationAdd">
				  <xpath>/Defs/ThingDef[defName = "ASReportsLog"]</xpath>
				  <value>
					  <researchPrerequisites>
						  <li>TS_AS_ReportsLog</li>
					  </researchPrerequisites>
				  </value>
			  </li>
			  <li Class="PatchOperationAdd">
				  <xpath>/Defs/ThingDef[defName = "ASReportsPrinter"]</xpath>
				  <value>
					  <researchPrerequisites>
						  <li>Electricity</li>
					  </researchPrerequisites>
				  </value>
			  </li>
		  </operations>
	  </match>
  </Operation>

</Patch>