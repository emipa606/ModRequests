<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Auto Seller</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		  <operations>
			  <li Class="PatchOperationInsert">
				  <xpath>/Defs/ResearchProjectDef[defName = "TS_Walls"]</xpath>
				  <value>
					  <ResearchProjectDef>
						  <defName>TS_AS_ReportsLog</defName>
						  <label>reports log</label>
						  <tab>TS_PrimitiveTechs</tab>
						  <description>Shopping lists are very useful, as you have experienced with the multiple traders who have already passed by. Now you can write those lists for yourself as well, and put them up on a tree for all to see!</description>
						  <techLevel>Neolithic</techLevel>
						  <baseCost>250</baseCost>
						  <prerequisites>
							  <li>TS_Crafting</li>
						  </prerequisites>
					  </ResearchProjectDef>
				  </value>
			  </li>
		  </operations>
	  </match>
  </Operation>

</Patch>