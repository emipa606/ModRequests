<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Feed The Colonists</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName = "Make4Pemmican"]/researchPrerequisite</xpath>
					<value>
						<researchPrerequisite>TS_PrimitiveCooking</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "Make4Kibble"]</xpath>
					<value>
						<researchPrerequisite>TS_PrimitiveCooking</researchPrerequisite>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>