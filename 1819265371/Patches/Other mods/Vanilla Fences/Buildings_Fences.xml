<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Fences</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name = "WoodenFenceBase"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_VF_WoodenFences</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name = "StoneFenceBase"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_VF_StoneFences</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name = "MetalFenceBase"]</xpath>
					<value>
						<researchPrerequisites>
							<li>TS_VF_MetalFences</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>