<Defs>


	<ThingCategoryDef>
		<defName>WTH_ArchotechModules</defName>
		<label>archotech modules</label>
		<parent>Manufactured</parent>
		<resourceReadoutRoot>true</resourceReadoutRoot>
		<iconPath>UI/Icons/ThingCategories/Manufactured</iconPath>
	</ThingCategoryDef>
<!-- Speed -->
	<ThingDef ParentName="BodyPartBase">
		<defName>WTH_ArchotechSpeedModule</defName>
		<label>archotech speed module</label>
		<description>Can be installed in a mechanoid. Increases the movement speed of the mechanoid. This complex module can be installed with ease by almost anyone, it automatically implants itself into the machine if pushed into the right spot.</description>
		<graphicData>
			<texPath>Things/MechModuleTierThree</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>1</stackLimit>
		<statBases>
			<MaxHitPoints>70</MaxHitPoints>
			<Mass>0.6</Mass>
			<MarketValue>9500</MarketValue>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2.0</DeteriorationRate>
			<SellPriceFactor>0.70</SellPriceFactor>
		</statBases>
		<intricate>true</intricate>
		<thingSetMakerTags>
			<li>RewardStandardHighFreq</li>
		</thingSetMakerTags>
		<thingCategories>
			<li>WTH_ArchotechModules</li>
		</thingCategories>
		<tradeTags>
			<li>Artifact</li>
		</tradeTags>
	</ThingDef>

	<HediffDef>
		<defName>WTH_ArchotechSpeedModule</defName>
		<label>archotech speed module</label>
		<description>archotech speed module</description>
		<defaultLabelColor>(80,220,100)</defaultLabelColor>
		<isBad>false</isBad>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>0.75</offset>
					</li>
				</capMods>
			<statOffsets>
					<MoveSpeed>2.5</MoveSpeed>
			</statOffsets>
			</li>
		</stages>
		<modExtensions>
			<li Class="WhatTheHack.DefModExtension_Hediff">
				<extraButcherProduct>WTH_ArchotechSpeedModule</extraButcherProduct>
				<canUninstall>true</canUninstall>
				<spawnChance>0</spawnChance>
				<destroyOnDeathChance>0.8</destroyOnDeathChance>
			</li>
		</modExtensions>
	</HediffDef>

	<RecipeDef>
		<defName>WTH_InstallArchotechSpeedModule</defName>
		<label>install archotech speed module</label>
		<description>Install an archotech speed module.</description>
		<workerClass>WhatTheHack.Recipes.Recipe_ModifyMechanoid</workerClass>
		<jobString>Installing archotech speed module.</jobString>
		<workAmount>1000</workAmount>
		<surgerySuccessChanceFactor>99999</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0.0</deathOnFailedSurgeryChance>
		<skillRequirements>
			<Crafting>1</Crafting>
			<Intellectual>1</Intellectual>
		</skillRequirements>
		<workSkill>Intellectual</workSkill>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WTH_ArchotechSpeedModule</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<appliedOnFixedBodyParts>
			<li>Reactor</li>
		</appliedOnFixedBodyParts>
		<addsHediff>WTH_ArchotechSpeedModule</addsHediff>
		<modExtensions>
			<li Class="WhatTheHack.DefModExtension_Recipe">
				<requireBed>true</requireBed>
			</li>
		</modExtensions>
	</RecipeDef>

