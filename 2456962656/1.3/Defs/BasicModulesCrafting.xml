<Defs>
	<!--Mining-->
	
	<RecipeDef ParentName="WTH_Craft_Module_Base">
		<defName>WTH_Craft_MiningModule</defName>
		<label>craft mining module</label>
		<description>Craft a mining module</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>WTH_MechanoidParts</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>WTH_MechanoidChip</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
				<li>WTH_MechanoidChip</li>
				<li>WTH_MechanoidParts</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<WTH_MiningModule>1</WTH_MiningModule>
		</products>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<researchPrerequisite>WTH_WorkModules</researchPrerequisite>
	</RecipeDef>

