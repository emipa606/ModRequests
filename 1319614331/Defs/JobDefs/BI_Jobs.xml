<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <JobDef>
    <defName>BI_Butcher</defName>
    <driverClass>BetterInfestations.JobDriver_Butcher</driverClass>
    <reportString>butchering TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

  <JobDef>
    <defName>BI_Maintain</defName>
    <driverClass>BetterInfestations.JobDriver_Maintain</driverClass>
    <reportString>maintaining TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>BI_GotoSpawnHive</defName>
    <driverClass>BetterInfestations.JobDriver_GotoSpawnHive</driverClass>
    <reportString>moving.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>BI_GotoPatrol</defName>
    <driverClass>BetterInfestations.JobDriver_GotoPatrol</driverClass>
    <reportString>moving.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>BI_ChewConduits</defName>
    <driverClass>BetterInfestations.JobDriver_ChewConduits</driverClass>
    <reportString>chewing TargetA.</reportString>
    <alwaysShowWeapon>true</alwaysShowWeapon>
    <casualInterruptible>false</casualInterruptible>
    <collideWithPawns>true</collideWithPawns>
  </JobDef>

</Defs>
