<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <RulePackDef>
    <defName>NamerDeityScav</defName>
    <rulePack>
      <rulesStrings>
        <li>r_deityName->[ScavGodNoun]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>ScavGodNoun</keyword>
          <path>NameBanks/ScavGodNoun</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerDeityLeader</defName>
    <rulePack>
      <rulesStrings>
        <li>r_leaderTitle->[ScavGodNoun]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>ScavGodNoun</keyword>
          <path>NameBanks/ScavGodNoun</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerScavPawn</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[ScavFirstname] [ScavSecondname]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>ScavFirstname</keyword>
          <path>NameBanks/ScavFirstname</path>
        </li>
        <li Class="Rule_File">
          <keyword>ScavSecondname</keyword>
          <path>NameBanks/ScavSecondname</path>
        </li>
        <li Class="Rule_File">
          <keyword>ScavOnename</keyword>
          <path>NameBanks/ScavOnename</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
 
  <RulePackDef>
    <defName>NamerScavFaction</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[ScavPrefix] [ScavNoun]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>ScavNoun</keyword>
          <path>NameBanks/ScavFactionNoun</path>
        </li>
        <li Class="Rule_File">
          <keyword>ScavPrefix</keyword>
          <path>NameBanks/ScavFactionPrefix</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerScavBase</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[ScavBase]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>ScavBase</keyword>
          <path>NameBanks/ScavTown</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
 
</Defs>