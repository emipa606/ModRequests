<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>EggRakkFertilized</defName>
    <label>rakk egg (fert.)</label>
    <description>A fertilized rakk egg. If all goes well, it should hatch into a baby rakk. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(80,50,50)</color>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>20.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>7</hatcherDaystoHatch>
        <hatcherPawn>Rakk</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
</Defs>