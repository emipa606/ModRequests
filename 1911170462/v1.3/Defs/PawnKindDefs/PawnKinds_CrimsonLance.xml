<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef>
    <defName>CrimsonLance_CEO</defName>
    <label>general</label>
    <race>Human</race>
    <combatPower>55</combatPower>
    <factionLeader>true</factionLeader>
    <defaultFactionType>CrimsonLance</defaultFactionType>
    <baseRecruitDifficulty>0.80</baseRecruitDifficulty>
    <invNutrition>3</invNutrition>
    <itemQuality>Normal</itemQuality>
    <backstoryCryptosleepCommonality>0.25</backstoryCryptosleepCommonality>
    <maxGenerationAge>58</maxGenerationAge>
    <gearHealthRange>
      <min>0.7</min>
      <max>2.3</max>
    </gearHealthRange>
    <apparelRequired>
      <li>Apparel_CrimsonLanceArmor</li>
      <li>Apparel_CrimsonLanceHelmet</li>
    </apparelRequired>
    <apparelAllowHeadgearChance>0.1</apparelAllowHeadgearChance>
    <apparelMoney>
      <min>0</min>
      <max>0</max>
    </apparelMoney>
    <weaponMoney>
      <min>10000</min>
      <max>10000</max>
    </weaponMoney>
    <weaponTags>
      <li>AtlasRifle</li>
    </weaponTags>
    <techHediffsMoney>
      <min>1000</min>
      <max>1500</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
    <techHediffsChance>0.01</techHediffsChance>
    <inventoryOptions>
      <skipChance>0.8</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>50</min>
            <max>200</max>
          </countRange>
        </li>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>
            <min>1</min>
            <max>1</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <initialWillRange>1~2</initialWillRange>
    <initialResistanceRange>10~19</initialResistanceRange>
  </PawnKindDef>

