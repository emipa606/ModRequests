      <rulesStrings>
        <li>name(p=4)->[SylStartM][SylEndM]</li>
        <li>name(p=3)->[SylStartM][SylEndM] the [SirenAdjectives]</li>			
        <li>name->[Name][SylEndM]</li>
        <li>name->[Name]</li>		
        <li>name->[SirenAdjectives]</li>			
        <li>name->[SylStartM][SylEndM] of the [Place]</li>			
      </rulesStrings>
      <rulesRaw>	
		<li Class="Rule_File">
			<keyword>Name</keyword>
			<path>Names/FullNames</path>
		</li>	  
		<li Class="Rule_File">
			<keyword>Place</keyword>
			<path>Names/Regions</path>
		</li>				
        <li Class="Rule_File">
            <keyword>SylStartM</keyword>
            <path>Siren_Syllables/SirenName_Syll</path>
        </li>
        <li Class="Rule_File">
            <keyword>SylEndM</keyword>
            <path>Siren_Syllables/SirenName_SyllEnd</path>
        </li>	
	    <li Class="Rule_File">
            <keyword>SirenAdjectives</keyword>
            <path>Faction_Names/SirenFactEvil/FactionNameAdjectivesEvil</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>
</Defs>

