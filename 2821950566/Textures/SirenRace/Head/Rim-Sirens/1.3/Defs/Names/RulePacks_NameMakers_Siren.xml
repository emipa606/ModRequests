<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>NamerPersonSirenFemale</defName>
    <rulePack>
      <rulesStrings>
        <li>name(p=4)->[SylStartF][SylEndF]</li>	
        <li>name(p=3)->[SylStartF][SylEndF] the [SirenAdjectives]</li>				
        <li>name->[Name][SylEndF]</li>		
        <li>name->[Name]</li>	
        <li>name->[SirenAdjectives]</li>			
        <li>name->[SylStartF][SylEndF] of the [Place]</li>			
      </rulesStrings>
      <rulesRaw>
		<li Class="Rule_File">
			<keyword>Name</keyword>
			<path>Names/FullNames</path>
		</li>		
		<li Class="Rule_File">
			<keyword>Place</keyword>
			<path>Names/Regions</path>
		</li>		
        <li Class="Rule_File">
            <keyword>SylStartF</keyword>
            <path>Siren_Syllables/SirenName_Syll</path>
        </li>
        <li Class="Rule_File">
            <keyword>SylEndF</keyword>
            <path>Siren_Syllables/SirenName_SyllEnd</path>
        </li>	
	    <li Class="Rule_File">
            <keyword>SirenAdjectives</keyword>
            <path>Faction_Names/SirenFactEvil/FactionNameAdjectivesEvil</path>
        </li>			
      </rulesRaw>
    </rulePack>
  </RulePackDef>
 
  <RulePackDef>
    <defName>NamerPersonSirenMale</defName>
