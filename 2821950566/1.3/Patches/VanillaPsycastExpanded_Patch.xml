<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Psycasts Expanded</li>
    </mods>	
	<match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
            <xpath>Defs/FactionDef[defName = "Empire"]/pawnGroupMakers</xpath>
            <value>
               <li Class="VanillaPsycastsExpanded.PawnGroupMaker_PsycasterRaid">
                  <kindDef>Combat</kindDef>
                  <commonality>10</commonality>
                  <options>
                     <Siren_Gunner>5</Siren_Gunner>
                     <Siren_Charger>3.5</Siren_Charger>
                     <Siren_Heavy>2</Siren_Heavy>
                     <Siren_Elite>4</Siren_Elite>
                     <Siren_Caster_Nightstalker>0.2</Siren_Caster_Nightstalker>
                     <Siren_Caster_Necropath>0.2</Siren_Caster_Necropath>
                     <Siren_Caster_Frostshaper>0.2</Siren_Caster_Frostshaper>
					 <Siren_Caster_Staticlord>0.2</Siren_Caster_Staticlord>
                  </options>
               </li>
               <li Class="VanillaPsycastsExpanded.PawnGroupMaker_PsycasterRaid">
                  <kindDef>Combat</kindDef>
                  <commonality>100</commonality>
                  <options>
                     <Siren_Gunner>5</Siren_Gunner>
                     <Siren_Charger>3.5</Siren_Charger>
                     <Siren_Heavy>2</Siren_Heavy>
                     <Siren_Elite>4</Siren_Elite>
                     <Siren_Caster_Nightstalker>0.2</Siren_Caster_Nightstalker>
					 <Siren_Caster_Frostshaper>0.2</Siren_Caster_Frostshaper>
                     <Siren_Caster_Necropath>0.4</Siren_Caster_Necropath>
                  </options>
               </li>
               <li Class="VanillaPsycastsExpanded.PawnGroupMaker_PsycasterRaid">
                  <kindDef>Combat</kindDef>
                  <commonality>50</commonality>
                  <options>
                     <Siren_Gunner>5</Siren_Gunner>
                     <Siren_Charger>3.5</Siren_Charger>
                     <Siren_Heavy>2</Siren_Heavy>
                     <Siren_Elite>4</Siren_Elite>
                     <Siren_Caster_Nightstalker>0.1</Siren_Caster_Nightstalker>
                  </options>
               </li>
            </value>
        </li>  		
	  </operations>
    </match>		  
  </Operation>
</Patch>