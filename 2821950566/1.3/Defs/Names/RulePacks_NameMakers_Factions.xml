<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>NamerFactionSirenEvil</defName>
    <rulePack>
      <rulesStrings>
	    <li>r_name(p=2)->[SirenAdjectivesEvil] [SirenEvilCITY]</li>		
	    <li>r_name(p=2)->The [SirenEvilCITY] of [GodName]</li>		
	    <li>r_name(p=2)->The [GodName]-[SirenEvilCITY]</li>		 
        <li>r_name(p=2)->The [SirenEvilCITY] by the [Place]</li>	 		
        <li>r_name->[GodName]s [Place]</li>
        <li>r_name->[GodName]s [SirenAdjectivesEvil] [Place]</li>	 
        <li>r_name->[GodName]s [SirenEvilCITY]</li>
        <li>r_name->[GodName]s [SirenAdjectivesEvil] [SirenEvilCITY]</li>			
        <li>r_name->[SirenAdjectivesEvil] [GodName]</li>		
      </rulesStrings> 
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>GodName</keyword>
					<path>Names/FullNames</path>
				</li>	
				<li Class="Rule_File">
					<keyword>Place</keyword>
					<path>Names/Regions</path>
				</li>				
	            <li Class="Rule_File">
                    <keyword>SirenEvilCITY</keyword>
                    <path>Faction_Names/SirenFactEvil/FactionsNameSirenCityEvil</path>
                </li>	
	            <li Class="Rule_File">
                    <keyword>SirenAdjectivesEvil</keyword>
                    <path>Faction_Names/SirenFactEvil/FactionNameAdjectivesEvil</path>
                </li>				
			</rulesRaw>	  
    </rulePack>
  </RulePackDef>  
</Defs>