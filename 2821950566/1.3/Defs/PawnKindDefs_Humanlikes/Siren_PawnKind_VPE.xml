<?xml version="1.0" encoding="utf-8" ?>
<Defs>  
   <PawnKindDef MayRequire="VanillaExpanded.VPsycastsE" ParentName="SirenPawnBase">
      <defName>Siren_Caster_Frostshaper</defName>
      <label>frostseeker</label>
      <combatPower>300</combatPower>
      <weaponMoney>99999</weaponMoney>	  
      <specificApparelRequirements>
         <li>
            <bodyPartGroup>UpperHead</bodyPartGroup>
            <requiredTag>BestowerHood</requiredTag>
         </li>  
         <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <requiredTag>RoyalRobe</requiredTag>
         </li>
      </specificApparelRequirements>  
      <techHediffsMoney>1000~4000</techHediffsMoney>
      <apparelColor>(64,2,60)</apparelColor>
      <weaponTags Inherit="False">
         <li>VPE_EltexMelee</li>
      </weaponTags>
      <requiredWorkTags>
         <li>Violent</li>
      </requiredWorkTags>
      <minGenerationAge>50</minGenerationAge>
      <techHediffsMaxAmount>3</techHediffsMaxAmount>
      <initialWillRange>6~8</initialWillRange>
      <initialResistanceRange>30~46</initialResistanceRange>
      <modExtensions>
         <li Class="VanillaPsycastsExpanded.PawnKindAbilityExtension_Psycasts">
            <implantDef>VPE_PsycastAbilityImplant</implantDef>
            <initialLevel>1</initialLevel>
            <statUpgradePoints>1~3</statUpgradePoints>
            <unlockedPaths>
               <VPE_Frostshaper>1~4|1~5</VPE_Frostshaper>
            </unlockedPaths>
         </li>
      </modExtensions>
   </PawnKindDef>
   
