<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>TGK</defName>
    <label>The Grim Kleaper</label>
    <devNote>pig-1</devNote>
    <description>This Animal Likes Exploiting Rimworld.</description>
    <statBases>
      <MoveSpeed>4.6</MoveSpeed>
      <MarketValue>200</MarketValue>
      <ComfyTemperatureMin>-23</ComfyTemperatureMin>
      <FilthRate>16</FilthRate>
    </statBases>
    <uiIconScale>2.25</uiIconScale>
    <tools>
      <li>
        <label>tusk</label>
        <capacities>
          <li>Scratch</li>
          <li>Stab</li>
        </capacities>
        <power>9.7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>TuskAttackTool</linkedBodyPartsGroup>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.6</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>QuadrupedAnimalWithHoovesAndTusks</body>
      <herdAnimal>true</herdAnimal>
      <baseBodySize>0.85</baseBodySize>
      <baseHealthScale>0.7</baseHealthScale>
      <baseHungerRate>0.30</baseHungerRate>
      <foodType>OmnivoreRoughAnimal</foodType>
      <wildness>0.50</wildness>
      <roamMtbDays>2</roamMtbDays>
      <trainability>None</trainability>
      <useMeatFrom>Pig</useMeatFrom>
      <leatherDef>Leather_Pig</leatherDef>
      <gestationPeriodDays>5.661</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.5, 1)</li>
          <li>(2.0, 0)</li>
