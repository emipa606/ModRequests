<ThingDef ParentName="HeronSWMetalDoorBase">
    <defName>PH_DoorThickBlastBDoor</defName>
    <!-- label and description are handled by CompProperties_PostProcessText -->
    <statBases>
      <Mass>200</Mass>
      <MaxHitPoints>2000</MaxHitPoints>
      <WorkToBuild>8000</WorkToBuild>
      <DoorOpenSpeed>0.25</DoorOpenSpeed> <!-- when powered, 0.25*4 = 1x door speed (1/4 autodoor speed) -->
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <Steel>160</Steel>
      <ComponentIndustrial>14</ComponentIndustrial>
    </costList>
    <costStuffCount>300</costStuffCount>
    <size>(3,2)</size>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <graphicData>
      <texPath>Things/Building/Door/Blast/SWDoorBlastBDoor_Mover</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,2)</drawSize>
      <damageData>
        <enabled>false</enabled>
      </damageData>
    </graphicData>
    <uiIconPath>Things/Building/Door/Blast/SWBlastDoorBUI</uiIconPath>
    <building>
      <soundDoorOpenManual>HeronSWBlastDoorAOpen</soundDoorOpenManual>
      <soundDoorCloseManual>HeronSWBlastDoorAClose</soundDoorCloseManual>
    </building>
    <comps>
      <li Class="DoorsExpanded.CompProperties_PostProcessText">
        <defaultLabelAndDescriptionFrom>PH_DoorBlastCDoor</defaultLabelAndDescriptionFrom>
        <appendSizeToLabel>true</appendSizeToLabel>
      </li>
      <li Class="DoorsExpanded.CompProperties_DoorExpanded">
        <remoteDoor>true</remoteDoor>
        <doorType>Standard</doorType>
        <fixedPerspective>true</fixedPerspective>
        <rotatesSouth>false</rotatesSouth>
        <doorAsync>
          <texPath>Things/Building/Door/Blast/SWDoorBlastBDoor_MoverAsync</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>(3,2)</drawSize>
          <damageData>
            <enabled>false</enabled>
          </damageData>
        </doorAsync>
        <doorFrame>
          <texPath>Things/Building/Door/Blast/SWDoorBlastBDoor_Frame</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <shaderType>CutoutComplex</shaderType>
          <drawSize>(3,2)</drawSize>
          <damageData>
            <enabled>false</enabled>
          </damageData>
        </doorFrame>
        <doorFrameSplit>
          <texPath>Things/Building/Door/Blast/SWDoorBlastBDoor_FrameAsync</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <shaderType>CutoutComplex</shaderType>
          <drawSize>(3,2)</drawSize>
          <damageData>
            <enabled>false</enabled>
          </damageData>
        </doorFrameSplit>
      </li>
      <li Class="DoorsExpanded.CompProperties_BreakdownableCustom">
        <breakdownMTBUnit>2.0</breakdownMTBUnit> <!-- half as likely to breakdown -->
      </li>
	  <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>100</basePowerConsumption>
      </li>
    </comps>
    <placeWorkers>
      <li>JecsTools.PlaceWorker_Outline</li>
    </placeWorkers>
	 <researchPrerequisites>
      <li>ProjectHeron_Swdoors</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>

