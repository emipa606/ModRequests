  	<ThingDef ParentName="PlantFoodRawBase">
		<defName>RawLiquoriceroot</defName>
		<label>liquoriceroot</label>
		<description>red and yellow fungal stalks that resemble candy coated liquorice.</description>
		<graphicData>
			<texPath>Things/Vegetables/Liquoriceroot/Liquoriceroot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>10</DeteriorationRate>
			<FoodPoisonChanceFixedHuman>0</FoodPoisonChanceFixedHuman>
			<MarketValue>4</MarketValue>
		</statBases>
		<ingestible>
			<preferability>RawTasty</preferability>
			<tasteThought>VBY_AteRawLiqouriceRoot</tasteThought>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>MeatRaw</li>
		</thingCategories>
	</ThingDef>
  
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>MooshMilk</defName>
    <label>mooshmilk</label>
    <description>Milk from a Mooshems Tree.Sounds disgusting, yet tastes great!</description>
    <graphicData>
      <texPath>Things/Item/Resource/MooshMilk</texPath>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>3.1</MarketValue>
      <Mass>0.045</Mass>
      <Flammability>0.2</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>15</daysToRotStart>
		<rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
	  <tasteThought>VBY_DrankMooshMilk</tasteThought>
      <foodType>AnimalProduct, Fluid</foodType>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
  </ThingDef>
  
