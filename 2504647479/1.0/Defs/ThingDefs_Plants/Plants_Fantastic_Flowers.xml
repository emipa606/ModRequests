  <ThingDef ParentName="FF_PlantBase">
    <defName>PlantBronzeleaf</defName>
    <label>Bronzeleaf</label>
    <description>A golden-bronze colored shrub.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>12</Beauty>
      <Nutrition>0.10</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Flowers/Bronzeleaf</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <sowWork>600</sowWork>
      <sowTags>
        <li>Decorative</li>
        <li>Ground</li>
      </sowTags>
      <growMinGlow>0.3</growMinGlow>
      <topWindExposure>0.1</topWindExposure>
      <growDays>2.1</growDays>
      <lifespanDaysPerGrowDays>14</lifespanDaysPerGrowDays>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
      <purpose>Beauty</purpose>
	   <wildBiomes>
	   <TemperateForest>0.02</TemperateForest>
	   <TropicalRainforest>0.02</TropicalRainforest>
	   </wildBiomes>
     <sowResearchPrerequisites>
        <li>FantasticFlora</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>
  
