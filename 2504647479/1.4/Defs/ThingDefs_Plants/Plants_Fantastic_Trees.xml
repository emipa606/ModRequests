<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--============================ New Trees ============================-->

  <ThingDef ParentName="FF_TreewoodBase">
    <defName>PlantTreeAzulcaps</defName>
    <label>azul caps</label>
    <description>An extraterrestrial tree native to the rim. It's orgins are rumored to come from a sundered planet.</description>
    <graphicData>
      <texPath>Things/Trees/Tree/TreeAzulcaps</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.15, 0.3, 0.1)</volume>
      </shadowData>
      <shaderParameters>
        <_FallColorSource>(0.576, 0.235, 0.160)</_FallColorSource>
        <_FallColorDestination>/Other/TreeFallGradient</_FallColorDestination>
        <_FallTransitionRange>(0.02, 0.10)</_FallTransitionRange>
        <_FallBehaviorEnabled>1</_FallBehaviorEnabled>
      </shaderParameters>
    </graphicData>
	<comps>
      <li Class="CompProperties_Glower">
        <glowRadius>2</glowRadius>
        <glowColor>(118,66,242,0)</glowColor>
      </li>
    </comps>
    <statBases>
      <Beauty>12</Beauty>
      <Nutrition>1</Nutrition>
    </statBases>
    <plant>
      <growDays>18</growDays>
      <harvestWork>700</harvestWork>
      <harvestYield>20</harvestYield>
	  <wildBiomes>
	    <TropicalRainforest>0.03</TropicalRainforest>
	    <TemperateSwamp>0.05</TemperateSwamp>
        <TropicalSwamp>0.05</TropicalSwamp>
        <BMT_ChromaticOasis MayRequire="BiomesTeam.Oasis">0.04</BMT_ChromaticOasis>
	  </wildBiomes>
      <sowResearchPrerequisites>
        <li>FantasticFloraII</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Ground</li>
      </sowTags>
    </plant>
  </ThingDef>

