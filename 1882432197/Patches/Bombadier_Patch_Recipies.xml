<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- Converts wooden buildings into stuffable types to utilise extra wood types added by this mod and also allows new wood types to be used as a fuel -->

 <!-- == Make Chemfuel From Wood == -->
 
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/ingredients/li/filter/thingDefs</xpath>
			<value>
				<li>DarkWoodLog</li>
				<li>NaturalWoodLog</li>
				<li>AntiqueWoodLog</li>
			</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/fixedIngredientFilter/thingDefs</xpath>
			<value>
				<li>DarkWoodLog</li>
				<li>NaturalWoodLog</li>
				<li>AntiqueWoodLog</li>
			</value>
	</Operation>
	
	
</Patch>

