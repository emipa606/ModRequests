<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Berryshroom-->
<ThingDef ParentName="PlantBase">
		<defName>Plant_BerryShroom</defName>
		<label>berryshroom plant</label>
		<description>This mushroom has been genetically engineered to grow berries. Faster to grow but less yield per harvest. Dies when exposed to light.</description>
		<statBases>
		<MaxHitPoints>150</MaxHitPoints>
			<Beauty>4</Beauty>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/BerryShroomPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
    <ingestible />
		<plant>
			<neverBlightable>true</neverBlightable>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0</growOptimalGlow>
			<cavePlant>true</cavePlant>
			<sowWork>800</sowWork>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.35</fertilitySensitivity>
			<dieIfLeafless>false</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestedThingDef>RawBerries</harvestedThingDef>
			<harvestYield>8</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<sowMinSkill>4</sowMinSkill>
			<topWindExposure>0.05</topWindExposure>
			<growDays>4</growDays>
			<leaflessGraphicPath>Things/Plant/BerryShroomPlant</leaflessGraphicPath>
      <immatureGraphicPath>Things/Plant/BerryShroomPlant</immatureGraphicPath>
			<visualSizeRange>0.3~1</visualSizeRange>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
	  <sowResearchPrerequisites>
        <li>grothingthings</li>
      </sowResearchPrerequisites>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>10</glowRadius>
				<glowColor>(18,53,72,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	
