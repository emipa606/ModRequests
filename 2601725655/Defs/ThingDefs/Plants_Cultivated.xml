<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--=========================== Steel ==============================-->
	<ThingDef ParentName="PlantBase">
		<defName>Plant_Steel</defName>
		<label>steel plant</label>
		<description>Very useful if you can't find any steel lying around.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
      <Nutrition>0.20</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/SteelPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
    <ingestible />
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>Steel</harvestedThingDef>
			<harvestYield>6</harvestYield>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<sowMinSkill>6</sowMinSkill>
			<topWindExposure>0.1</topWindExposure>
			<growDays>4</growDays>
			<visualSizeRange>0.3~1.05</visualSizeRange>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
	  <sowResearchPrerequisites>
        <li>grothingthings</li>
      </sowResearchPrerequisites>
		</plant>
	</ThingDef>
	
