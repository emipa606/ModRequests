<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="ApparelMakeableBase">
    <defName>N_OutfitA</defName>
    <description>A traditional breezy namekian outfit.</description>
    <label>Outfit 1</label>
    <graphicData>
      <texPath>Items/Outfit1/Outfit1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>100</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>2700</WorkToMake>
      <Mass>0.3</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.26</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.10</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
		<apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Outfit1/Outfit1</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>N_Apparel</li>
      </tags>
      <defaultOutfitTags>
        <li>N_Peaceful</li>
        <li>N_Fighter</li>
        <li>N_Trader</li>
        <li>Soldier</li>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
   <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>15</weight>
          <only>RGBA(1, 1, 1)</only>
        </li>
    </options>
    </colorGenerator>
      </ThingDef>


