<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <Operation Class="PatchOperationSequence" MayRequire="Ludeon.RimWorld.Ideology">
    <success>Always</success>
    <operations>
      <!--ThingStyleDef-->
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>Defs</xpath>
        <value>
          <ThingStyleDef ParentName="IdeoTableStyleBase">
            <defName>Morbid_Table1x1c</defName>
            <graphicData>
              <texPath>Things/Building/Furniture/MorbidTable1x1</texPath>
              <drawSize>(3,3)</drawSize>
            </graphicData>
          </ThingStyleDef>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>Defs</xpath>
        <value>
          <ThingStyleDef ParentName="IdeoTableStyleBase">
            <defName>Totemic_Table1x1c</defName>
            <graphicData>
              <texPath>Things/Building/Furniture/TotemicTable1x1</texPath>
              <drawSize>(3,3)</drawSize>
            </graphicData>
          </ThingStyleDef>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>Defs</xpath>
        <value>
          <ThingStyleDef ParentName="IdeoTableStyleBase">
            <defName>Spikecore_Table1x1c</defName>
            <graphicData>
              <texPath>Things/Building/Furniture/SpikecoreTable1x1</texPath>
              <drawSize>(3,3)</drawSize>
            </graphicData>
          </ThingStyleDef>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>Defs</xpath>
        <value>
          <ThingStyleDef ParentName="IdeoTableStyleBase">
            <defName>Rustic_Table1x1c</defName>
            <graphicData>
              <texPath>Things/Building/Furniture/RusticTable1x1</texPath>
              <drawSize>(3,3)</drawSize>
            </graphicData>
          </ThingStyleDef>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>Defs</xpath>
        <value>
          <ThingStyleDef ParentName="IdeoTableStyleBase">
            <defName>Techist_Table1x1c</defName>
            <graphicData>
              <texPath>Things/Building/Furniture/TechistTable1x1</texPath>
              <drawSize>(3,3)</drawSize>
            </graphicData>
          </ThingStyleDef>
        </value>
      </li>
      <!--StyleCategoryDef-->
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>/Defs/StyleCategoryDef[defName="Morbid"]/thingDefStyles</xpath>
        <value>
          <li>
            <thingDef>Table1x1c</thingDef>
            <styleDef>Morbid_Table1x1c</styleDef>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>/Defs/StyleCategoryDef[defName="Totemic"]/thingDefStyles</xpath>
        <value>
          <li>
            <thingDef>Table1x1c</thingDef>
            <styleDef>Totemic_Table1x1c</styleDef>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>/Defs/StyleCategoryDef[defName="Spikecore"]/thingDefStyles</xpath>
        <value>
          <li>
            <thingDef>Table1x1c</thingDef>
            <styleDef>Spikecore_Table1x1c</styleDef>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>/Defs/StyleCategoryDef[defName="Rustic"]/thingDefStyles</xpath>
        <value>
          <li>
            <thingDef>Table1x1c</thingDef>
            <styleDef>Rustic_Table1x1c</styleDef>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="Ludeon.RimWorld.Ideology">
        <xpath>/Defs/StyleCategoryDef[defName="Techist"]/thingDefStyles</xpath>
        <value>
          <li>
            <thingDef>Table1x1c</thingDef>
            <styleDef>Techist_Table1x1c</styleDef>
          </li>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>