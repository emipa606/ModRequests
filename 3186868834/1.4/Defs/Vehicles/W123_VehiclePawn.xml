<?xml version="1.0" encoding="utf-8" ?>

<Defs>		
	<Vehicles.VehicleDef ParentName="BaseVehiclePawn">
		<defName>BenzSedan</defName>
		<label>W123</label>
		<description>A executive car, known for its durability, reliability, and luxurious features.</description>
		<graphicData>
			<texPath>Vehicles/W123</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(5.5, 5.5)</drawSize>
			<color>(130, 130, 130)</color>
			<colorTwo>(130, 30, 30)</colorTwo>
			<colorThree>(30, 15, 20)</colorThree>
		</graphicData>
		
		<fillPercent>1</fillPercent>
		
		<buildDef>BenzSedan_Blueprint</buildDef>
		
		<size>(2,5)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Transport</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		
		<soundSustainersOnEvent>
			<li>
				<key>(IgnitionOn, IgnitionOff)</key>
				<value>CarIdle</value>
			</li>
		</soundSustainersOnEvent>
		<soundOneShotsOnEvent Inherit = "False">
			<li>
				<key>PawnEntered</key>
				<value>CarDoor</value>
			</li>
			<li>
				<key>PawnExited</key>
				<value>CarDoor</value>
			</li>
			<li>
				<key>IgnitionOn</key>
				<value>CarStartup</value>
			</li>
			<li>
				<key>IgnitionOff</key>
				<value>CarOff</value>
			</li>
		</soundOneShotsOnEvent>

		<statBases>
			<ArmorRating_Blunt>0.32</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.56</ArmorRating_Sharp>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<Flammability>0.5</Flammability>
			<MarketValue>3580</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>300</Mass>
			<MoveSpeed>10</MoveSpeed>
			<CargoCapacity>300</CargoCapacity>
			<RepairRate>0.3</RepairRate>
			<AccelerationRate MayRequire="oskarpotocki.vanillavehiclesexpanded">0.5</AccelerationRate>
		</vehicleStats>
		
		<race>
			<baseBodySize>2.5</baseBodySize>
		</race>
		
		<properties>
			<customRoadCosts AssignDefaults = "0.35"/>

			<vehicleJobLimitations>
				<li>(RepairVehicle, 4)</li>
			</vehicleJobLimitations>

			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
					
					<hitbox>
						<cells>
							<li>(0, 1)</li>
						</cells>
					</hitbox>
				</li>

				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>3</slots>
					
					<hitbox>
						<cells>
							<li>(1, 1)</li>
							<li>(0, 0)</li>
							<li>(1, 0)</li>
						</cells>
					</hitbox>					
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>85</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftWheel</key>
				<label>Front Left Wheel</label>
				<health>30</health>
				<depth>External</depth>
				
				<armor>	
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightWheel</key>
				<label>Front Right Wheel</label>
				<health>30</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
					
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackLeftWheel</key>
				<label>Back Left Wheel</label>
				<health>30</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightWheel</key>
				<label>Back Right Wheel</label>
				<health>30</health>
				<depth>External</depth>
				
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontLeftSuspension</key>
				<label>Front Left Suspension</label>
				<health>20</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontRightSuspension</key>
				<label>Front Right Suspension</label>
				<health>20</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackLeftSuspension</key>
				<label>Back Left Suspension</label>
				<health>20</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>BackRightSuspension</key>
				<label>Back Right Suspension</label>
				<health>20</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>Gearbox</key>
				<label>Gearbox</label>
				<health>45</health>
				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 1)</li>
						<li>(1, 1)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftSidePanel</key>
				<label>Left Side Panel</label>
				<health>45</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Left</side>
				</hitbox>
			</li>
			<li>
				<key>RightSidePanel</key>
				<label>Right Side Panel</label>
				<health>45</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>Chemtank</key>
				<label>Chemtank</label>
				<health>40</health>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.2</maxHealth>
						<radius>3</radius>
						<damageDef>Flame</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, -1)</li>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>FrontBumper</key>
				<label>Front Bumper</label>
				<health>30</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>RearBumper</key>
				<label>Rear Bumper</label>
				<health>30</health>
				<depth>External</depth>
				
				<hitbox>
					<side>Back</side>
				</hitbox>
			</li>
			<li>
				<key>Upholstery</key>
				<label>Upholstery</label>
				<health>30</health>
				<depth>Internal</depth>
				<hitWeight>5</hitWeight>
			</li>
			<li>
				<key>Dashboard</key>
				<label>Dashboard</label>
				<health>35</health>
				<depth>Internal</depth>
				<hitWeight>2</hitWeight>
			</li>
			<li>
				<key>SteeringWheel</key>
				<label>Steering Wheel</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Alternator</key>
				<label>Alternator</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
			<li>
				<key>Hood</key>
				<label>Hood</label>
				<health>35</health>
				<depth>External</depth>
				<hitWeight>5</hitWeight>
			</li>
			<li>
				<key>Roof</key>
				<label>Roof</label>
				<health>45</health>
				<depth>External</depth>
				<hitWeight>5</hitWeight>
			</li>
		</components>

		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<fuelConsumptionRate>20.5</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.8</fuelConsumptionWorldMultiplier>
				<fuelCapacity>150</fuelCapacity>
			</li>
			<li MayRequire="oskarpotocki.vanillavehiclesexpanded" Class="VanillaVehiclesExpanded.CompProperties_VehicleMovementController" />
		</comps>
	</Vehicles.VehicleDef>
</Defs>