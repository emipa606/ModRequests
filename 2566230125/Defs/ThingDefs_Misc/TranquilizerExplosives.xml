<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ShellBase">
		<defName>Shell_TranquilizerNuke</defName>
		<label>tranquilizer nuke</label>
		<description>An extremeley powerful explosive combined with tranquilizer gas. Explodes when damaged.</description>
		<graphicData>
			<texPath>TranquilizerMod/Things/Item/Projectile/Shell_Tranquilizer</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>60</explosiveRadius>
				<explosiveDamageType>TranquilizerNukeGasDamageDef</explosiveDamageType>
				<explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.7</startWickHitPointsPercent>
				<wickTicks>
					<min>30</min>
					<max>60</max>
				</wickTicks>
			</li>
		</comps>
		<projectileWhenLoaded>Bullet_Shell_Tranquilizer_Nuke</projectileWhenLoaded>
		<researchPrerequisites>
			<li>TranquilizerGasNukeResearch</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>