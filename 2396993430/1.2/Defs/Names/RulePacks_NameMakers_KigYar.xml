<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<RulePackDef>
		<defName>NamerPersonKigYar</defName>
		<rulePack>
			<rulesStrings>



				<li>name->[Names]</li>
				<li>name->[Names] [Clans]</li>
				<li>name->[Names][Odd][Clans]</li>

			</rulesStrings>

			<rulesRaw>
				<li Class="Rule_File">
					<keyword>Names</keyword>
					<path>KigYarNames/Names</path>
				</li>
				<li Class="Rule_File">
					<keyword>Clans</keyword>
					<path>KigYarNames/Clans</path>
				</li>
				<li Class="Rule_File">
					<keyword>Odd</keyword>
					<path>KigYarNames/Odd</path>
				</li>

			</rulesRaw>
		</rulePack>
	</RulePackDef>

</Defs>
