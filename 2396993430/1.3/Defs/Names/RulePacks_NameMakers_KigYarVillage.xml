<?xml version="1.0" encoding="utf-8"?>

<Defs>
	
  <!-- Faction -->
<RulePackDef>
	<defName>NamerFactionKigYarVillage</defName>
	<rulePack>
		<rulesStrings>
			<li>name->[Names]['sWords]</li>
			<li>name->[Names] [Clans]['sWords]</li>
			<li>name->[Names][Odd][Clans]['sWords]</li>
			<!--<li>[Worlds]['sWords]</li>-->

			<li>name->[FirstWord] [Names]</li>
			<li>name->[FirstWord] [Names] [Clans]</li>
			<li>name->[FirstWord] [Names][Odd][Clans]</li>
			<!--<li>[FirstWord] [Worlds]</li>-->

			<li>name->[Names][LastWord]</li>
			<li>name->[Names] [Clans][LastWord]</li>
			<li>name->[Names][Odd][Clans][LastWord]</li>
			<!--<li>[Worlds][LastWord]</li>-->
		</rulesStrings>

		<rulesRaw>

			<li Class="Rule_File">
				<keyword>Names</keyword>
				<path>KigYarNames/Names</path>
			</li>
			<li Class="Rule_File">
				<keyword>Clans</keyword>
				<path>KigYarNames/Clans</path>
			</li>
			<li Class="Rule_File">
				<keyword>Odd</keyword>
				<path>KigYarNames/Odd</path>
			</li>
			<li Class="Rule_File">
				<keyword>Worlds</keyword>
				<path>KigYarNames/Worlds</path>
			</li>
			<li Class="Rule_File">
				<keyword>'sWords</keyword>
				<path>KigYarNames/'sWords</path>
			</li>
			<li Class="Rule_File">
				<keyword>FirstWord</keyword>
				<path>KigYarNames/FirstWordFaction</path>
			</li>
			<li Class="Rule_File">
				<keyword>LastWord</keyword>
				<path>KigYarNames/LastWordFaction2</path>
			</li>
			<li Class="Rule_File">
				<keyword>BaseNames</keyword>
				<path>KigYarNames/BaseNames</path>
			</li>

		</rulesRaw>
	</rulePack>
</RulePackDef>

	<!--Settlements-->
