		<match Class="PatchOperationSequence">
		<operations>
		
		<li Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="GAB_Triticale"]/description</xpath>
		<value><description>A hybrid of wheat and rye. It has the high yield and quality of wheat, and the hardiness of rye. As a hybrid plant, it is sterile and does not produce seeds.</description></value>
		</li>

		
	
		<li Class="PatchOperationAdd">
<xpath>Defs</xpath>
<value>
<RecipeDef>
					<defName>ExtractSeed_Triticale</defName>
					<label>Create triticale seeds</label>
					<description>Combine wheat and rye into triticale.</description>
					<ingredients>
						<li>
							<filter>
								<thingDefs>
									<li MayRequire="VanillaExpanded.VCookE">GAB_RawWheat</li>
									<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawWheat</li>
								</thingDefs>
							</filter>
							<count>2</count>
						</li>
						
						<li>
							<filter>
								<thingDefs>
									<li>GAB_RawRye</li>
								</thingDefs>
							</filter>
							<count>2</count>
						</li>
					</ingredients>
					<fixedIngredientFilter>
						<thingDefs>
							<li MayRequire="VanillaExpanded.VCookE">GAB_RawWheat</li>
									<li>GAB_RawRye</li>
									<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawWheat</li>
						</thingDefs>
					</fixedIngredientFilter>
					<products>
						<Seed_GABTriticale>3</Seed_GABTriticale>
					</products>
							<recipeUsers>
			<li>SeedExtractionSpot</li>
		</recipeUsers>
			<researchPrerequisites>
		<li>GAB_HybridGrain</li>
	</researchPrerequisites>
				</RecipeDef>
</value>
