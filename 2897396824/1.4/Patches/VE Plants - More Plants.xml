<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods> 
		<match Class="PatchOperationFindMod">
		<mods>
		<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
	<li Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VCE_Rye"]/plant/harvestedThingDef</xpath>
		<value>
			<harvestedThingDef>GAB_RawRye</harvestedThingDef>
		</value>
	</li>
	
	<li Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VCE_Sorghum"]/plant/harvestedThingDef</xpath>
		<value>
			<harvestedThingDef>GAB_RawSorghum</harvestedThingDef>
		</value>
	</li>
	
		<li Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VCE_Barley"]/plant/harvestedThingDef</xpath>
		<value>
			<harvestedThingDef>GAB_RawBarley</harvestedThingDef>
		</value>
	</li>
	
	<li Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="VCE_Rye" or defName="VCE_Sorghum" or defName="VCE_Barley"]/plant/sowTags</xpath>
	</li>
			</operations>
		</match>
		</match>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>RimCuisine 2 Core (Continued)</li>
		</mods>
		<match Class="PatchOperationReplace">

		<xpath>/Defs/ThingDef[defName="RC2_PlantBarley"]/plant/harvestedThingDef</xpath>
		<value>
			<harvestedThingDef>GAB_RawBarley</harvestedThingDef>
		</value>
	
		</match>
	</Operation>
</Patch>

