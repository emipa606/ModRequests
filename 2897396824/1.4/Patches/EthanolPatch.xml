<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
    <Operation Class="VFECore.PatchOperationToggableSequence">
    <enabled>True</enabled>
    <label>Ethanol can refuel everything that uses chemfuel</label>
    <operations>
      <li Class="PatchOperationAdd">
        <order>Prepend</order>
        <xpath>/Defs/ThingDef/comps/li[contains(@Class,'CompProperties_Refuelable')]/fuelFilter[contains(thingDefs, "Chemfuel")]/thingDefs</xpath>
        <value>
          <li>GAB_Ethanol</li>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>