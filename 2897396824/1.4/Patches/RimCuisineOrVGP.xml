<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Change to Raw Wheat to spelt if necessary -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[1.0, 1.1] RimCuisine 2 Core</li>
			<li>RimCuisine 2 Core (Continued)</li>
			<li>VGP Vegetable Garden</li>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="GAB_RawWheat"]/label</xpath>
					<value>
						<label>spelt</label>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="GAB_RawWheat"]/description</xpath>
					<value>
						<description>Raw spelt kernels. Can be used in cooking or ground into flour.</description>
					</value>
				</li>
			
			</operations>
		</match>
	</Operation>
	
</Patch>

