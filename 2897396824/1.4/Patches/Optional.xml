<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="VFECore.PatchOperationToggableSequence">
    <enabled>False</enabled>
    <label>Stuffable metal for vinegar barrel</label>
    <operations>
      <li Class="PatchOperationSequence">
	  <operations>
	  <li Class="PatchOperationReplace">
	  <xpath>/Defs/ThingDef[defName="GAB_VinegarBarrel"]/costList</xpath>
        <value>
		<costList>
          			<WoodLog>100</WoodLog>
					</costList>
        </value>
	  </li>
	  
	  <li Class="PatchOperationAdd">
	  <xpath>/Defs/ThingDef[defName="GAB_VinegarBarrel"]</xpath>
        <value>
          			<stuffCategories>
					<li>Metallic</li>
					</stuffCategories>
					<costStuffCount>40</costStuffCount>
        </value>
	  </li>
		</operations>
      </li>
    </operations>
  </Operation>
</Patch>

