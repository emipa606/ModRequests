<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="MealBase">
		<defName>GAB_PickledMeat</defName>
		<label>pickled meat</label>
		<description>Meat pickled in vinegar and sealed in a jar, greatly increasing shelf life. While edible cold right out of the jar, it's far more nourishing mixed into a meal and heated.</description>
		<graphicData>
			<texPath>Things/Items/Pickles/GAB_PickledMeat</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<stackLimit>75</stackLimit>
        <rotatable>false</rotatable>
		<statBases>
			<MarketValue>2.3</MarketValue>
			<WorkToMake>700</WorkToMake>
			<Nutrition>0.05</Nutrition>
			<Mass>0.04</Mass>
		</statBases>
		<thingCategories>
			<li>MeatRaw</li>
		</thingCategories>
		<ingestible>
			<preferability>MealAwful</preferability>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<maxNumToIngestAtOnce>5</maxNumToIngestAtOnce>
			<tasteThought>GAB_ConsumedPickledFood</tasteThought>
			<specialThoughtAsIngredient>GAB_ConsumedPickledFood</specialThoughtAsIngredient>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
        		<daysToRotStart>90</daysToRotStart>
        		<rotDestroys>true</rotDestroys>
      		</li>
		</comps>
	</ThingDef>

