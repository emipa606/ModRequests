	<!--====================== Ability Sounds ======================-->
	<!-- Sounds related to the ability SeeRed-->
	
	<!-- SeeRed: Ability is warming up -->
	
	<SoundDef>
		<defName>RBM_SeeRedWarmup</defName>
		<context>MapOnly</context>
		<maxSimultaneous>2</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>RBM_SeeRed/Warmup</clipFolderPath>
					</li>
				</grains>
				<volumeRange>50</volumeRange>
				<muteWhenPaused>true</muteWhenPaused>
				<tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
			</li>
		</subSounds>
	</SoundDef>
	
