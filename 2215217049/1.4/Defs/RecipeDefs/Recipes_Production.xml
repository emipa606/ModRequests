  <RecipeDef>
    <defName>Make_AnimaTribal</defName>
    <label>make anima tribalwear</label>
    <description>Make an anima tribalwear.</description>
    <jobString>Making anima tribalwear.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>8000</workAmount>
    <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AnimaFiber</li>
          </thingDefs>
        </filter>
        <count>80</count>
      </li>
    </ingredients>
	<recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <li>CraftingSpot</li>
    </recipeUsers>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AnimaFiber</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Apparel_AnimaTribal>1</Apparel_AnimaTribal>
    </products>
	<researchPrerequisite>DomesticationAnimaGrass</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
  
