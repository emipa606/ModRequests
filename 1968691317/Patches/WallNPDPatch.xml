<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationConditional">
    <success>Always</success>
    <xpath>/Defs/ThingDef[defName="OverWallNutrientPaste"]</xpath>
    <match Class="PatchOperationReplace">
      <xpath>/Defs/ThingDef[defName="OverWallNutrientPaste"]/label</xpath>
      <value>
        <label>over-wall cheese weenie dispenser</label>
      </value>
    </match>
  </Operation>

  <Operation Class="PatchOperationConditional">
    <success>Always</success>
    <xpath>/Defs/ThingDef[defName="OverWallNutrientDispenser2W"]</xpath>
    <match Class="PatchOperationReplace">
      <xpath>/Defs/ThingDef[defName="OverWallNutrientDispenser2W"]/label</xpath>
      <value>
        <label>over-wall cheese weenie dispenser (wide)</label>
      </value>
    </match>
  </Operation>

</Patch>