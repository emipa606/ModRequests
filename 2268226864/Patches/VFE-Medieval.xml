<?xml version="1.0" encoding="utf-8"?>
<Patch>
<!-- ==================================================
	Increase chance of all pawns spawning with bionics
	============================================== -->
	<Operation Class="PatchOperationFindMod">
	<mods><li>Vanilla Factions Expanded - Medieval</li></mods>
	<match Class="PatchOperationSequence">
	<operations>
			<!-- ======== PawnKinds_Player ======== -->
			<!-- ============ Colonist ============ -->
			<li Class="PatchOperationReplace"> <!-- Default 0.02 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Subject"]/techHediffsChance</xpath>
				<value><techHediffsChance>0.12</techHediffsChance></value>
			</li>
			<li Class="PatchOperationReplace"> <!-- Default 50~50 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Subject"]/techHediffsMoney</xpath>
				<value>
					<techHediffsMoney>50~500</techHediffsMoney>
				</value>
			</li>
			<!-- ======== PawnKinds_Medieval ======== -->
			<!-- ============ VFEM_Medieval_Merchant ============ -->
			<li Class="PatchOperationAdd"> <!-- Default 0.00 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Merchant"]</xpath>
				<value>
					<techHediffsChance>0.06</techHediffsChance>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default 0~0 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Merchant"]</xpath>
				<value>
					<techHediffsMoney>50~250</techHediffsMoney>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default nothing -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Merchant"]</xpath>
				<value>
					<techHediffsTags>
						<li>Poor</li>
					</techHediffsTags>
				</value>
			</li>
			<!-- ============ VFEM_Medieval_Squire ============ -->
			<li Class="PatchOperationAdd"> <!-- Default 0.00 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Squire"]</xpath>
				<value>
					<techHediffsChance>0.06</techHediffsChance>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default 0~0 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Squire"]</xpath>
				<value>
					<techHediffsMoney>50~250</techHediffsMoney>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default nothing -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Squire"]</xpath>
				<value>
					<techHediffsTags>
						<li>Poor</li>
					</techHediffsTags>
				</value>
			</li>
			<!-- ============ VFEM_Medieval_Bowman ============ -->
			<li Class="PatchOperationAdd"> <!-- Default 0.00 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Bowman"]</xpath>
				<value>
					<techHediffsChance>0.06</techHediffsChance>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default 0~0 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Bowman"]</xpath>
				<value>
					<techHediffsMoney>50~250</techHediffsMoney>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default nothing -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Bowman"]</xpath>
				<value>
					<techHediffsTags>
						<li>Poor</li>
					</techHediffsTags>
				</value>
			</li>
			<!-- ============ VFEM_Medieval_Footsoldier ============ -->
			<li Class="PatchOperationAdd"> <!-- Default 0.00 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Footsoldier"]</xpath>
				<value>
					<techHediffsChance>0.06</techHediffsChance>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default 0~0 -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Footsoldier"]</xpath>
				<value>
					<techHediffsMoney>50~250</techHediffsMoney>
				</value>
			</li>
			<li Class="PatchOperationAdd"> <!-- Default nothing -->
				<xpath>Defs/PawnKindDef[defName="VFEM_Medieval_Footsoldier"]</xpath>
				<value>
					<techHediffsTags>
						<li>Poor</li>
					</techHediffsTags>
				</value>
			</li>
		</operations>
	</match>
	</Operation>
</Patch>