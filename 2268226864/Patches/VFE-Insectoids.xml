<?xml version="1.0" encoding="utf-8"?>
<Patch>
<!-- ==================================================
	Increase chance of all pawns spawning with bionics
	============================================== -->
	<Operation Class="PatchOperationFindMod">
	<mods><li>Vanilla Factions Expanded - Insectoids</li></mods>
	<match Class="PatchOperationSequence">
	<operations>
			<!-- ======== PawnKinds_Player ======== -->
			<!-- ============ VFE_Mercenary ============ -->
			<li Class="PatchOperationReplace"> <!-- Default 0.15 -->
				<xpath>Defs/PawnKindDef[defName="VFE_Mercenary"]/techHediffsChance</xpath>
				<value><techHediffsChance>0.22</techHediffsChance></value>
			</li>
		</operations>
	</match>
	</Operation>
</Patch>