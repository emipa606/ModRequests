<?xml version="1.0" encoding="utf-8"?>
<Patch>
<!-- ==================================================
	Increase chance of all pawns spawning with bionics
	============================================== -->
	<Operation Class="PatchOperationFindMod">
	<mods><li>[KIR]Playable Outlander Settlers [V1.2]</li></mods>
	<match Class="PatchOperationSequence">
	<operations>
			<!-- ======== PawnKinds_PlayerSettlers ======== -->
			<!-- ============ VFEV_Member ============ -->
			<li Class="PatchOperationReplace"> <!-- Default 0.03 -->
				<xpath>Defs/PawnKindDef[@Name="BaseSettlerPlayerPawn_POF"]/techHediffsChance</xpath>
				<value><techHediffsChance>0.15</techHediffsChance></value>
			</li>
		</operations>
	</match>
	</Operation>
</Patch>