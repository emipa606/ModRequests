<Defs>
  <RecipeDef Name="JDSUnpackCrateBase" Abstract="True">
    <description>...</description>
    <jobString>Unpacking Crate.</jobString>
    <workAmount>400</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <!-- Switched to standard products so we can do "do until you have X"
    <specialProducts>
      <li>Smelted</li>
    </specialProducts>-->
  </RecipeDef>
  
  <!-- ============= Recipe Woodlog ============= -->  
  
  <RecipeDef ParentName="JDSUnpackCrateBase">  
    <defName>JDSUnpackcrateWoodlog</defName>
    <label>Unpacking Crate WoodLog</label>
   <jobString>Unpacking WoodLog Crate.</jobString>	
    <products>
      <WoodLog>250</WoodLog>
    </products>	
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ResourcesCrateWoodLog</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>	
    <fixedIngredientFilter>
      <thingDefs>
        <li>ResourcesCrateWoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>	
  </RecipeDef>

