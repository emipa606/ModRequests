<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Compress_Steel -->
	<RecipeDef>
		<defName>Compress_Steel</defName>
		<label>Compressed Steel</label>
		<description>Compressed Steel</description>
		<jobString>Creating Compressed Steel from Steel.</jobString>
		<workAmount>20</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<CompressedSteel>1</CompressedSteel>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

