  <!-- Lootbug -->
    <ThingDef ParentName="UwUThingBase">
    <defName>UwULootbug</defName>
    <label>UwU Lootbug</label>
    <devNote></devNote>
    <description>UwU's are tiny, rare creatures on the Rim.  Easy to train... easy to keep... they are a delight to have around (providing passive mood bonuses to your colonists!) And it's becoming and increasing common hobby to collect them among the UrbWorlders.</description>
    <comps>
      <li Class="AnimalBehaviours.CompProperties_ThoughtEffecter">
        <radius>2</radius>
        <tickInterval>500</tickInterval>
        <thoughtDef>UwU_uwu_Lootbug</thoughtDef>
      </li>
    </comps>
    <race>
    <useMeatFrom>UwUCorgo</useMeatFrom>
      <wildBiomes>
        <TropicalRainforest>0.01</TropicalRainforest>
        <TemperateForest>0.01</TemperateForest>
        <BorealForest>0.01</BorealForest>
        <Tundra>0.01</Tundra>
        <AridShrubland>0.01</AridShrubland>
        <Desert>0.01</Desert>
        <TropicalSwamp>0.01</TropicalSwamp>
        <ColdBog>0.01</ColdBog>
        <TemperateSwamp>0.01</TemperateSwamp>
      </wildBiomes>
    </race>
  </ThingDef>

  <PawnKindDef ParentName="UwUPawnBase">
    <defName>UwULootbug</defName>
    <label>UwU Lootbug</label>
    <race>UwULootbug</race>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/uwu_lootbug</texPath>
          <drawSize>0.6</drawSize>
          <shaderType>CutoutComplex</shaderType>
