<Defs>
  <!-- Grasshopper Armor Torso -->
  <ThingDef ParentName="ApparelArmorPowerBase">
    <defName>JuniorPowerArmorGH</defName>
    <label>Grasshopper Armor</label>
    <description>An advanced suit of power armor, designed specifically for children. Akin to its older brother, Locust Armor, this suit has a built in jump pack, and provides similar protection.\n\nDue to its reduced weight and fuel capacity, the jump packs have a significantly increased range. Built-in advanced servos boost children's low carrying capacity.</description>
    <techLevel>Spacer</techLevel>
	<tickerType>Normal</tickerType>
    <soundInteract>Interact_ChargeRifle</soundInteract>	
    <graphicData>
      <texPath>Things/Pawn/Apparel/JuniorPA_Torso/JuniorPA</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.7</drawSize>
	  <!--shaderType>CutoutComplex</shaderType-->
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
	<costList>
      <ComponentSpacer>3</ComponentSpacer>
	  <Chemfuel>60</Chemfuel>
      <Plasteel>80</Plasteel>
      <Uranium>10</Uranium>
    </costList>
    <statBases>
      <WorkToMake>45000</WorkToMake>
      <MaxHitPoints>280</MaxHitPoints>
      <Mass>3</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.87</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.35</ArmorRating_Blunt>
      <ArmorRating_Heat>0.41</ArmorRating_Heat>
      <JumpRange>42.9</JumpRange>
      <Insulation_Cold>32</Insulation_Cold>
      <Insulation_Heat>9</Insulation_Heat>
      <EquipDelay>7</EquipDelay>
    </statBases>
	<equippedStatOffsets>
      <MoveSpeed>0.2</MoveSpeed>
	  <CarryingCapacity>40</CarryingCapacity>
	  <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.15</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <recipeMaker>
      <researchPrerequisites>
        <li>JumpPack</li>
        <li>ReconArmor</li>
      </researchPrerequisites>
      <recipeUsers>
        <li>FabricationBench</li>
      </recipeUsers>
	  <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
      <displayPriority>210</displayPriority>
    </recipeMaker>
    <apparel>
	  <useDeflectMetalEffect>true</useDeflectMetalEffect>
	  <useWornGraphicMask>true</useWornGraphicMask>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
	  <wearPerDay>0</wearPerDay>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
		<li>OnSkin</li>
        <li>Shell</li>
      </layers>
      <wornGraphicPath>Things/Pawn/Apparel/JuniorPA_Torso/JuniorPA</wornGraphicPath>
      <tags>
        <li>SpacerMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <developmentalStageFilter>Child</developmentalStageFilter>
    </apparel>
    <tradeTags>
      <li>HiTechArmor</li>
	  <li>Armor</li>
    </tradeTags>
	<comps>
      <li Class="CompProperties_Reloadable">
        <maxCharges>3</maxCharges>
        <ammoDef>Chemfuel</ammoDef>
        <ammoCountPerCharge>30</ammoCountPerCharge>
        <baseReloadTicks>60</baseReloadTicks>
        <soundReload>Standard_Reload</soundReload>
        <hotKey>Misc4</hotKey>
        <chargeNoun>jump</chargeNoun>
        <displayGizmoWhileUndrafted>false</displayGizmoWhileUndrafted>
      </li>
    </comps>
    <verbs>
      <li>
        <verbClass>Verb_Jump</verbClass>
        <label>jump</label>
        <commandIcon>Things/UI_Junior/JetpackJunior</commandIcon>
        <violent>false</violent>
        <hasStandardCommand>true</hasStandardCommand>
        <onlyManualCast>True</onlyManualCast>
        <forceNormalTimeSpeed>false</forceNormalTimeSpeed>
        <warmupTime>0.5</warmupTime>
        <warmupEffecter>JumpWarmupEffect</warmupEffecter>
        <flightEffecterDef>JumpFlightEffect</flightEffecterDef>
        <requireLineOfSight>true</requireLineOfSight>
        <targetParams>
          <canTargetLocations>true</canTargetLocations>
          <canTargetPawns>false</canTargetPawns>
          <canTargetBuildings>false</canTargetBuildings>
        </targetParams>
      </li>
    </verbs>
	<colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3)</min>
          <max>(0.5,0.5,0.5)</max>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  
