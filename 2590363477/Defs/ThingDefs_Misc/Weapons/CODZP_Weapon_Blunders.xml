<Defs>
<!--Blundergat-->
<ThingDef ParentName="CODZP_BaseWonderWeaponGun">
	<defName>CodZP_Gun_Blundergat</defName>
	<label>Blundergat</label>
	<description>A shotgun that fires 4 bullets at once</description>
	<descriptionHyperlinks>
			<ThingDef>CodZP_Gun_Acidgat</ThingDef>
			<ThingDef>CodZP_Gun_Magmagat</ThingDef>
	</descriptionHyperlinks>
	<graphicData>
		<texPath>Things/Item/Equipment/WeaponRanged/Blunders/CodZP_Blundergat</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<drawSize>1.5</drawSize>
	</graphicData>
	<soundInteract>CodZP_Interact_Blundergat</soundInteract>
	<statBases>
		<Mass>5</Mass>
		<AccuracyTouch>0.75</AccuracyTouch>
		<AccuracyShort>0.82</AccuracyShort>
		<AccuracyMedium>0.76</AccuracyMedium>
		<AccuracyLong>0.34</AccuracyLong>
		<RangedWeapon_Cooldown>0.96</RangedWeapon_Cooldown>
	</statBases>
	<weaponTags />
	<costList>
		<ComponentIndustrial>4</ComponentIndustrial>
		<Steel>100</Steel>
		<WoodLog>20</WoodLog>
	</costList>
	<verbs>
		<li>
			<verbClass>Verb_Shoot</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>CodZP_Bullet_Blundergat</defaultProjectile>
			<warmupTime>1.1</warmupTime>
			<range>14.9</range>
			<ticksBetweenBurstShots>0</ticksBetweenBurstShots>
			<burstShotCount>4</burstShotCount>
			<soundCast>CodZP_Shot_Blundergat</soundCast>
			<soundCastTail>GunTail_Medium</soundCastTail>
			<muzzleFlashScale>3</muzzleFlashScale>
		</li>
	</verbs>
	<weaponClasses>
      <li>ShortShots</li>
    </weaponClasses>
	<tools>
		<li>
			<label>stock</label>
			<capacities>
				<li>Blunt</li>
			</capacities>
			<power>9</power>
			<cooldownTime>2</cooldownTime>
		</li>
		<li>
			<label>barrel</label>
			<capacities>
				<li>Blunt</li>
				<li>Poke</li>
			</capacities>
			<power>9</power>
			<cooldownTime>2</cooldownTime>
		</li>
	</tools>
</ThingDef>
