<Defs>

<!--Raygun-->
  <SoundDef>
    <defName>CodZP_Interact_Raygun</defName>  
    <context>MapOnly</context>  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Weapons/CodZP_Interact_Raygun</clipPath>
          </li>
        </grains>
        <volumeRange>25~25</volumeRange>
      </li>
    </subSounds>
  </SoundDef>
  
  <SoundDef>
    <defName>CodZP_Shot_Raygun</defName>
    <context>MapOnly</context>
    <maxSimultaneous>3</maxSimultaneous>
    <eventNames />
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Weapons/CodZP_Shot_Raygun</clipPath>
          </li>
        </grains>
        <pitchRange>0.8~0.9</pitchRange>
        <volumeRange>25~25</volumeRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
  
