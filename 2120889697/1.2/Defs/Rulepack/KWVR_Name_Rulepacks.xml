<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
<!--■■■■■■■■■■■■■■■■■■PAWN NAMES■■■■■■■■■■■■■■■■■■■■■■■-->
  
  <RulePackDef>
    <defName>KWVR_NAMER_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>kwvrname->Names/KWVR_Names</li>
        <li>kwvrnamelast->Names/KWVR_NamesLast</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[kwvrname] [kwvrnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>KWVR_NAMER_MALE_DEF</defName>
    <include>
      <li>KWVR_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[kwvrname] [kwvrnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>KWVR_NAMER_FEMALE_DEF</defName>
    <include>
      <li>KWVR_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[kwvrname] [kwvrnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
<!--■■■■■■■■■■■■■■■■■■FACTION NAMES■■■■■■■■■■■■■■■■■■■■■■■-->
  
  <RulePackDef>
    <defName>KWVR_FACTION_NAMER_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>kwvrname->Names/KWVR_Factions</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[kwvrname]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>KWVR_SETTLEMENT_NAMER_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>ColonyStart->Names/KWVR_ColonyStart</li>
        <li>PrefixOne->Names/KWVR_ColonyPrefixOne</li>
        <li>PrefixTwo->Names/KWVR_ColonyPrefixTwo</li>
        <li>ColonyLast->Names/KWVR_ColonyLast</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[ColonyStart] [PrefixOne] [PrefixTwo] [ColonyLast]</li>
        <li>r_name->[PrefixOne] [PrefixTwo] [ColonyLast]</li>
        <li>r_name->[PrefixTwo] [ColonyLast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
</Defs>