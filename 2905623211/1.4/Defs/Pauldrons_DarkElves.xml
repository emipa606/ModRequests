<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- DevDesigner_DarkElves_BlackGuardPauldrons -->
  <ThingDef ParentName="DevDesigner_Elves_MetalPauldrons">
    <defName>DevDesigner_DarkElves_BlackGuardPauldrons</defName>
    <label>Black Guard Pauldrons (T,M,F)</label>
    <description>Black Guard Pauldrons</description>
    <graphicData>
      <texPath>Apparel/Pauldrons/DarkElves/BlackGuard/Pauldrons</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>

    <costList>
      <Gold>3</Gold>
    </costList>

    <statBases>
      <WorkToMake>10000</WorkToMake>
      <MaxHitPoints>1200</MaxHitPoints>
      <Mass>1.5</Mass>
      <StuffEffectMultiplierArmor>0.75</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.42</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.1</SocialImpact>
      <MentalBreakThreshold>-0.05</MentalBreakThreshold>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.2</SlaveSuppressionOffset>
    </equippedStatOffsets>

    <apparel>
      <wornGraphicPath>Apparel/Pauldrons/DarkElves/BlackGuard/Pauldrons</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <tags>
        <li>DevDesigner_DarkElves_BlackGuardPauldrons</li>
      </tags>
      <defaultOutfitTags></defaultOutfitTags>
    </apparel>

    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_DarkElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

