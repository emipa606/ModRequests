<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- DevDesigner_DarkElves_DreadspearArmor -->
  <ThingDef ParentName="DevDesigner_Elves_BreastplateBase">
    <defName>DevDesigner_DarkElves_DreadspearArmor</defName>
    <label>Dreadspear Armor (T,M,F)</label>
    <description>This breastplate is made of the finest steel and imbued with powerful magic. The breastplate is intricately etched with designs of twisted, thorny vines. The armor is tailored to fit the wearer like a glove, allowing for maximum mobility and flexibility. The breastplate is not only a practical piece of armor, but also serves as a symbol of fear and intimidation on the battlefield. Only the most skilled and formidable warriors are worthy of donning this breastplate</description>

    <graphicData>
      <texPath>Apparel/Breastplate/DarkElves/Dreadspear/Breastplate</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <apparel>
      <wornGraphicPath>Apparel/Breastplate/DarkElves/Dreadspear/Breastplate</wornGraphicPath>
      <tags>
        <li>DevDesigner_DarkElves_Apparel</li>
        <li>DevDesigner_DarkElves_Heavy</li>
        <li>DevDesigner_DarkElves_DreadspearArmor</li>
      </tags>
    </apparel>

    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_DarkElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

  <!-- DevDesigner_DarkElves_LeatherArmor -->
  <ThingDef ParentName="DevDesigner_Elves_Apparel">
    <defName>DevDesigner_DarkElves_LeatherArmor</defName>
    <label>elven leather armor (T,M,F)</label>
    <description>Leather armor a decent balance between protection and encumbrance.</description>

    <graphicData>
      <texPath>Apparel/LeatherArmor/DarkElves/DarkRider/LeatherArmor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToMake>5000</WorkToMake>
      <Mass>1</Mass>
      <StuffEffectMultiplierArmor>0.35</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.35</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.15</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>

    <costStuffCount>60</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>

    <thingCategories Inherit='False'>
      <li>Apparel</li>
    </thingCategories>

    <apparel>
      <wornGraphicPath>Apparel/LeatherArmor/DarkElves/DarkRider/LeatherArmor</wornGraphicPath>
      <tags>
        <li>DevDesigner_DarkElves_Apparel</li>
        <li>DevDesigner_DarkElves_Light</li>
        <li>DevDesigner_DarkElves_LeatherArmor</li>
      </tags>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags></defaultOutfitTags>
    </apparel>

    <colorGenerator Class="ColorGenerator_StandardApparel" />

    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_DarkElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

