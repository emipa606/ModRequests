<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- DevDesigner_WoodElves_MetalHeadgear -->
  <ThingDef Name="DevDesigner_WoodElves_MetalHeadgear" Abstract="True" ParentName="DevDesigner_Elves_MetalHeadgear">
    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_WoodElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <!-- DevDesigner_WoodElves_ClothHeadgear -->
  <ThingDef Name="DevDesigner_WoodElves_ClothHeadgear" Abstract="true" ParentName="DevDesigner_Elves_ClothHeadgear">
    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_WoodElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <ThingDef ParentName="DevDesigner_WoodElves_ClothHeadgear">
    <defName>DevDesigner_WoodElves_Hood</defName>
    <label>elven hood</label>
    <description>A draped hood which covers the head.</description>
    <graphicData>
      <texPath>Apparel/Hood/Hood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>4000</WorkToMake>
      <Mass>0.1</Mass>
      <EquipDelay>0.8</EquipDelay>
      <StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.30</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets Inherit="false">
      <HuntingStealth>0.50</HuntingStealth>
    </equippedStatOffsets>
    <apparel>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
      <wornGraphicPath>Apparel/Hood/Hood</wornGraphicPath>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>DevDesigner_Elves_Light</li>
        <li>DevDesigner_WoodElves_Apparel</li>
        <li>DevDesigner_WoodElves_Light</li>
        <li>DevDesigner_WoodElves_Hood</li>
      </tags>
    </apparel>
    <tradeTags>
      <li>DevDesigner_WoodElves_Clothing</li>
    </tradeTags>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>

  <ThingDef ParentName="DevDesigner_WoodElves_ClothHeadgear">
