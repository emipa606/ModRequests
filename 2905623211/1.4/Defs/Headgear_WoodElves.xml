      <EquipDelay>0.9</EquipDelay>
      <StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.35</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.30</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets Inherit="false">
      <HuntingStealth>0.50</HuntingStealth>
    </equippedStatOffsets>
    <apparel>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
      <wornGraphicPath>Apparel/Hood/MaskedHood</wornGraphicPath>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>DevDesigner_Elves_Light</li>
        <li>DevDesigner_WoodElves_Apparel</li>
        <li>DevDesigner_WoodElves_Light</li>
        <li>DevDesigner_WoodElves_UpgradedHood</li>
        <li>DevDesigner_WoodElves_MaskedHood</li>
      </tags>
    </apparel>
    <tradeTags>
      <li>DevDesigner_WoodElves_Clothing</li>
    </tradeTags>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>

  <ThingDef ParentName="DevDesigner_WoodElves_ClothHeadgear">
    <defName>DevDesigner_WoodElves_ArmoredHood</defName>
    <label>elven armored hood</label>
    <description>A draped hood which covers the head reinforced with wooden faceplate.</description>
    <graphicData>
      <texPath>Apparel/Hood/ArmoredHood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <WorkToMake>4000</WorkToMake>
      <Mass>0.15</Mass>
      <EquipDelay>0.9</EquipDelay>
      <StuffEffectMultiplierArmor>0.65</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.30</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets Inherit="false">
      <HuntingStealth>0.25</HuntingStealth>
    </equippedStatOffsets>
    <apparel>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
      <wornGraphicPath>Apparel/Hood/ArmoredHood</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>DevDesigner_WoodElves_Apparel</li>
        <li>DevDesigner_WoodElves_Light</li>
        <li>DevDesigner_WoodElves_UpgradedHood</li>
        <li>DevDesigner_WoodElves_ArmoredHood</li>
      </tags>
    </apparel>

    <costList>
      <WoodLog>30</WoodLog>
    </costList>
    <tradeTags>
      <li>DevDesigner_WoodElves_Clothing</li>
    </tradeTags>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>

  <!-- DevDesigner_WoodElves_StagKnightHelmet -->
  <ThingDef ParentName="DevDesigner_WoodElves_MetalHeadgear">
    <defName>DevDesigner_WoodElves_StagKnightHelmet</defName>
    <label>Stag Knight Helmet</label>
    <description>The Stag Knight Helmet is a beautifully crafted piece of armor, favored by the elite knights of the Elven courts. The helmet is made of a lightweight, silver-colored metal that shimmers in the sunlight. The helmet's most distinctive feature is the stag's head crest, which is made of polished antlers and is said to bring good luck to the wearer.</description>

    <graphicData>
      <texPath>Apparel/Headgear/WoodElves/StagKnight/Helmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>1.2</drawSize>
    </graphicData>

    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToMake>5000</WorkToMake>
      <Mass>0.3</Mass>
      <EquipDelay>0.8</EquipDelay>
      <StuffEffectMultiplierArmor>0.35</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.30</StuffEffectMultiplierInsulation_Heat>
    </statBases>

    <equippedStatOffsets>
      <SocialImpact>0.2</SocialImpact>
    </equippedStatOffsets>

    <costStuffCount>100</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>

    <apparel>
      <developmentalStageFilter>Adult</developmentalStageFilter>
      <wornGraphicPath>Apparel/Headgear/WoodElves/StagKnight/Helmet</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
      <bodyPartGroups>
        <li>UpperHead</li>
        <li>Eyes</li>
        <li>Mouth</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>DevDesigner_WoodElves_Apparel</li>
        <li>DevDesigner_WoodElves_Heavy</li>
        <li>DevDesigner_WoodElves_StagKnightHelmet</li>
      </tags>
    </apparel>

    <colorGenerator Class="ColorGenerator_StandardApparel" />

    <modExtensions>
      <li Class="VFECore.ApparelDrawPosExtension">
        <headgearDrawSettings>
          <drawPosNorthOffset>(0,-0.2,0)</drawPosNorthOffset>
          <drawPosSouthOffset>(0,0,0)</drawPosSouthOffset>
          <drawPosWestOffset>(0,0,0)</drawPosWestOffset>
          <drawPosEastOffset>(0,0,0)</drawPosEastOffset>
        </headgearDrawSettings>
      </li>
    </modExtensions>
  </ThingDef>

