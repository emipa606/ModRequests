<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- DevDesigner_Ageless -->
  <GeneDef>
    <defName>DevDesigner_Ageless</defName>
    <label>ageless</label>
    <description>Carriers of this gene continuously reverse the process of aging. Starting at the age of 13, carriers begin to biologically age slower. By 30, the aging process stops completely.</description>
    <iconPath>UI/Icons/Genes/Gene_Ageless</iconPath>
    <displayCategory>Healing</displayCategory>
    <biologicalAgeTickFactorFromAgeCurve>
      <points>
        <li>(13, 1)</li>
        <li>(18, 0.1)</li>
        <li>(30, 0)</li>
      </points>
    </biologicalAgeTickFactorFromAgeCurve>
    <biostatCpx>3</biostatCpx>
    <biostatMet>0</biostatMet>
    <symbolPack>
      <wholeNameSymbols>
        <li><symbol>immortal</symbol></li>
      </wholeNameSymbols>
      <prefixSymbols>
        <li><symbol>elven</symbol></li>
      </prefixSymbols>
    </symbolPack>
    <modExtensions>
      <li Class="VanillaGenesExpanded.GeneExtension">
        <hideGene>True</hideGene>
      </li>
    </modExtensions>
  </GeneDef>

  <!-- DevDesigner_PaleSkin -->
  <GeneDef ParentName="GeneSkinColorOverride" MayRequire="Ludeon.RimWorld.Biotech">
    <defName>DevDesigner_PaleSkin</defName>
    <label>pale skin</label>
    <description>Carriers of this gene produce a pigment that turns their skin pale.</description>
    <skinColorOverride>(255,230,221)</skinColorOverride>
    <randomBrightnessFactor>0.05</randomBrightnessFactor>
    <displayOrderInCategory>0</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>white</symbol></li>
        <li><symbol>pale</symbol></li>
      </prefixSymbols>
      <suffixSymbols></suffixSymbols>
    </symbolPack>
  </GeneDef>

</Defs>