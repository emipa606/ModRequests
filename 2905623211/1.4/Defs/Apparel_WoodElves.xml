<Defs>

  <!-- DevDesigner_WoodElves_Tunic -->
  <ThingDef ParentName="DevDesigner_Elves_Apparel">
    <defName>DevDesigner_WoodElves_Tunic</defName>
    <label>elven tunic (T,M,F)</label>
    <description>Slightly heavier than a normal shirt, a tunic is a great isolation against cold weather.</description>

    <graphicData>
      <texPath>Apparel/Tunic/Tunic</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <WorkToMake>2400</WorkToMake>
      <Mass>0.30</Mass>
      <EquipDelay>1.5</EquipDelay>
      <StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.65</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.35</StuffEffectMultiplierInsulation_Heat>
    </statBases>

    <costStuffCount>60</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>

    <thingCategories Inherit='False'>
      <li>Apparel</li>
    </thingCategories>

    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Tunic/Tunic</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>DevDesigner_Elves_Light</li>
        <li>DevDesigner_WoodElves_Apparel</li>
        <li>DevDesigner_WoodElves_Light</li>
        <li>DevDesigner_WoodElves_Tunic</li>
      </tags>
      <defaultOutfitTags></defaultOutfitTags>
    </apparel>

    <colorGenerator Class="ColorGenerator_StandardApparel" />

    <recipeMaker>
      <researchPrerequisite>DevDesigner_ElvenEquipment_WoodElves</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

