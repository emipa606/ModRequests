<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <FactionDef Name="DevDesigner_PlayerFactionElvesBase" Abstract="true" ParentName="PlayerFactionBase">
    <isPlayer>true</isPlayer>

    <techLevel>Medieval</techLevel>
    <categoryTag>Tribal</categoryTag>

    <pawnSingular>elf</pawnSingular>
    <pawnsPlural>elves</pawnsPlural>

    <factionNameMaker>DevDesigner_WoodElves_NamerFaction</factionNameMaker>
    <settlementNameMaker>DevDesigner_WoodElves_NamerSettlement</settlementNameMaker>

    <disallowedMemes></disallowedMemes>
    <disallowedPrecepts></disallowedPrecepts>
    <backstoryFilters>
      <li>
        <categories>
          <li>Tribal</li>
        </categories>
      </li>
    </backstoryFilters>

    <forageabilityFactor>1.7</forageabilityFactor>
    <startingResearchTags>
      <li>TribalStart</li>
      <li>WoodElvesStart</li>
    </startingResearchTags>
    <startingTechprintsResearchTags>
      <li>TribalStart</li>
      <li>TribalStartTechprints</li>
    </startingTechprintsResearchTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Cloth</li>
      </thingDefs>
    </apparelStuffFilter>
    <recipePrerequisiteTags>
      <li>Tribal</li>
    </recipePrerequisiteTags>
  </FactionDef>

  <!-- DevDesigner_WoodElvesFaction_Player -->
  <FactionDef ParentName="DevDesigner_PlayerFactionElvesBase">
    <defName>DevDesigner_WoodElvesFaction_Player</defName>
    <label>wood elf clan</label>
    <description>A small wood elf clan.</description>
    <allowedCultures>
      <li>DevDesinger_WoodElves_Culture</li>
    </allowedCultures>

    <basicMemberKind>DevDesigner_WoodElves_Penitent</basicMemberKind>

    <factionIconPath>UI/WoodElvesIcon</factionIconPath>
    <settlementTexturePath>World/WorldObjects/TribalSettlement</settlementTexturePath>

    <modExtensions>
      <li Class="VFECore.FactionDefExtension">
        <packAnimalTexNameSuffix>MedievalPack</packAnimalTexNameSuffix>
        <strangerInBlackReplacement>DevDesigner_WoodElves_StrangerInBlack</strangerInBlackReplacement>
      </li>
    </modExtensions>
  </FactionDef>

