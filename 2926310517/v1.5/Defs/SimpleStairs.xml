      <linkFlags>
        <li>Custom8</li>
      </linkFlags>
      <damageData>
        <rect>(0.35,0.1,0.3,0.7)</rect>
      </damageData>
    </graphicData>
    <uiIconPath>Things/Building/Linked/GO_HandrailSmooth_MenuIcon</uiIconPath>
    <uiIconPathsStuff>
      <li>
        <appearance>Planks</appearance>
        <iconPath>Things/Building/Linked/GO_HandrailPlanks_MenuIcon</iconPath>
      </li>
      <li>
        <appearance>Bricks</appearance>
        <iconPath>Things/Building/Linked/GO_HandrailBricks_MenuIcon</iconPath>
