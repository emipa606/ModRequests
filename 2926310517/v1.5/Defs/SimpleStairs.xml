<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--linkFlags:Custom8-Custom9-->

  <!--Stairs-->
  <ThingDef ParentName="BuildingBase">
    <defName>Gomi_SimpleStair</defName>
    <label>Stairs</label>
    <description>a simple stairs</description>
    <category>Building</category>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Things/Building/GO_stairs</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.03</drawSize>
    </graphicData>
    <uiIconScale>1</uiIconScale>
    <uiIconOffset>(0,0)</uiIconOffset>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <passability>Standable</passability>
    <fillPercent>0</fillPercent>
    <statBases>
      <MaxHitPoints>45</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Flammability>0</Flammability>
      <Mass>1</Mass>
      <Beauty>1</Beauty>
    </statBases>
    <selectable>true</selectable>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>1</costStuffCount>
    <building>
      <isEdifice>false</isEdifice>
      <paintable>true</paintable>
      <relatedBuildCommands>
        <li>Gomi_Handrail</li>
        <li>Gomi_HighWall</li>
        <li>Gomi_FloorFence</li>
      </relatedBuildCommands>
    </building>
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <rotatable>true</rotatable>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <useHitPoints>False</useHitPoints>
    <designationCategory>Structure</designationCategory>
    <destroyable>true</destroyable>
    <tradeability>None</tradeability>
    <clearBuildingArea>false</clearBuildingArea>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <placingDraggableDimensions>1</placingDraggableDimensions>
  </ThingDef>

  <!--Handrail-->
  <ThingDef ParentName="BuildingBase">
    <defName>Gomi_Handrail</defName>
    <label>Handrail</label>
    <description>handrail for stairs</description>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Linked/GO_Handrail</texPath>
      <graphicClass>Graphic_Appearances</graphicClass>
