<Defs>
<ThingDef ParentName="MealBase">
    <defName>MealPackagedPaste</defName>
    <label>packaged nutrient paste meal</label>
    <description>A synthetic mixture of protein, carbohydrates, and vitamins, amino acids and minerals. Everything the body needs, and absolutely disgusting. Packaged for travel</description>
    <graphicData>
      <texPath>Things/Item/Meal/Slop</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>10</MarketValue>
      <Nutrition>0.9</Nutrition>
      <WorkToMake>250</WorkToMake>
    </statBases>
    <ingestible>
      <preferability>RawTasty</preferability>
      <ateEvent>AteNutrientPaste</ateEvent>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <costList>
        <MealNutrientPaste>2</MealNutrientPaste>
    </costList>
    <tradeability>Buyable</tradeability>
    <recipeMaker>
        <researchPrerequisite>PackagedSurvivalMeal</researchPrerequisite>
        <recipeUsers>
            <li>ElectricStove</li>
            <li>FueledStove</li>
        </recipeUsers>
        <workSpeedStat>CookSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_CookMeal</soundWorking>
    </recipeMaker>
  </ThingDef>
</Defs>