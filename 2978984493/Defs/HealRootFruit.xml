<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="OrganicProductBase">
    <defName>HealRootFruit</defName>
    <label>healrootfruit</label>
    <description>"Fruit" extracted from a Healroot plant.</description>
    <thingClass>ThingWithComps</thingClass>
    <thingCategories>
      <li>Foods</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/HealRootFruit</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>0.5</MarketValue>
      <Mass>0.05</Mass>
      <Flammability>0.2</Flammability>
      <WorkToMake>80</WorkToMake>
      <DeteriorationRate>2</DeteriorationRate>
      <Nutrition>0.04</Nutrition>
    </statBases>
    <techLevel>Neolithic</techLevel>
    <ingestible>
      <foodType>Meal</foodType>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
      <optimalityOffsetHumanlikes>1</optimalityOffsetHumanlikes>
    </ingestible>
    <recipeMaker>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
      <workSkill>Cooking</workSkill>
    </recipeMaker>
    <costList>
      <MedicineHerbal>1</MedicineHerbal>
    </costList>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>70</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
      <li Class="CompProperties_Ingredients">
        <noIngredientsFoodKind>Meat</noIngredientsFoodKind>
      </li>
      <li Class="CompProperties_FoodPoisonable" />
    </comps>
    <allowedArchonexusCount>200</allowedArchonexusCount>
  </ThingDef>
  
</Defs>