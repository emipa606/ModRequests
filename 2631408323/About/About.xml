<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
	<name>Stop Quarrying When Full</name>
	<author>Ben Lubar</author>
    <packageId>me.lubar.StopQuarryingWhenFull</packageId>
	<supportedVersions>
		<li>1.3</li>
	</supportedVersions>
	<description><![CDATA[Stop pawns from quarrying when more than a certain percentage (75% by default) of the tiles have items on them.]]></description>
	<url>https://git.lubar.me/rimworld-mods/stop-quarrying-when-full</url>
	<modDependencies>
		<li>
			<packageId>UnlimitedHugs.HugsLib</packageId>
			<displayName>HugsLib</displayName>
			<downloadUrl>https://github.com/UnlimitedHugs/RimworldHugsLib/releases/latest</downloadUrl>
			<steamWorkshopUrl>steam://url/CommunityFilePage/818773962</steamWorkshopUrl>
		</li>
		<li>
			<packageId>Ogliss.TheWhiteCrayon.Quarry</packageId>
			<displayName>Quarry</displayName>
			<downloadUrl>https://github.com/Ogliss/Quarry/releases/latest</downloadUrl>
			<steamWorkshopUrl>steam://url/CommunityFilePage/2007576583</steamWorkshopUrl>
		</li>
	</modDependencies>
	<loadAfter>
		<li>UnlimitedHugs.HugsLib</li>
		<li>Ogliss.TheWhiteCrayon.Quarry</li>
	</loadAfter>
</ModMetaData>
