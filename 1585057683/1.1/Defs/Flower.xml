<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantBase">
    <defName>VeeFlowerA</defName>
    <label>Tiny Flower</label>
    <description>A tiny flower.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>4</Beauty>
      <Nutrition>0.18</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Flower/Tiny/flo</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
	<altitudeLayer>Item</altitudeLayer>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible />
    <plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<sowTags>
        <li>VeeFlower</li>
      </sowTags>
      <fertilityMin>0.05</fertilityMin>
      <visualSizeRange>0.9~1.0</visualSizeRange>
      <topWindExposure>0.1</topWindExposure>
      <wildOrder>1</wildOrder>
      <purpose>Beauty</purpose>
    </plant>
  </ThingDef>
	
	
</Defs>
