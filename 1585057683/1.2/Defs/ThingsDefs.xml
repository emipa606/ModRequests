				  <ThingDef ParentName="BuildingBase">
	<defName>VeeGlight</defName>
	<label>Gerden Lamp</label>
	<description>A pretty garden needs light at night, too!</description>
	<thingClass>Building</thingClass>
			<graphicData>
			<texPath>Building/GLight</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>	
				<uiIconPath>Building/GLightb</uiIconPath>
				<uiIconScale>1.5</uiIconScale>
				<constructEffect>ConstructMetal</constructEffect>
				<defaultPlacingRot>South</defaultPlacingRot>
				<altitudeLayer>BuildingOnTop</altitudeLayer>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>5</WorkToBuild>
			<Mass>10</Mass>
			<Beauty>2</Beauty>
		</statBases>
		<costList>
			<Steel>10</Steel>
		</costList>
					<building>
						<isEdifice>false</isEdifice>
					</building>
							<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>10</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>8</glowRadius>
				<glowColor>(253,242,225,0)</glowColor>
			</li>
		</comps>
				<minifiedDef>MinifiedThing</minifiedDef>	
			<thingCategories>
			<li>BuildingsFurniture</li>
			</thingCategories>
			<designationCategory>VeeDecoCategory</designationCategory>
			<clearBuildingArea>false</clearBuildingArea>
			<tradeability>None</tradeability>
	</ThingDef>
	
