    <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>Veeseedspot</defName>

    <label>Plant spot</label>
	<description>soil pits for planting plants.</description>
    <thingClass>Building_PlantGrower</thingClass>
    <graphicData>
      <texPath>Building/fss</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.01</fillPercent>
    <statBases>
      <MaxHitPoints>10</MaxHitPoints>
      <WorkToBuild>50</WorkToBuild>
      <Mass>2</Mass>
    </statBases>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
    </comps>
    <fertility>1.0</fertility>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <building>
      <defaultPlantToGrow>Plant_Rose</defaultPlantToGrow>
