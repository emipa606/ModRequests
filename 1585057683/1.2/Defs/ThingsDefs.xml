      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>15</costStuffCount>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>4</Mass>
	  <Beauty>3</Beauty>
	   <Flammability>1.0</Flammability>
      <WorkToBuild>500</WorkToBuild>
