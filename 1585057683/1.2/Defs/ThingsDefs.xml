<Defs>
	
	  <ThingDef ParentName="FurnitureWithQualityBase">  
	<defName>VFlowerarch</defName>
	<label>Flower arch</label>
	<description>Flower arch! Can change Shape With Q,E</description>
	<thingClass>Building</thingClass>
			<graphicData>
			<texPath>Building/arch/</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2)</drawSize>
	</graphicData>	
		<altitudeLayer>PawnState</altitudeLayer>
		<defaultPlacingRot>East</defaultPlacingRot>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Mass>10</Mass>
			<Beauty>2</Beauty>
			<Comfort>0.8</Comfort>
		</statBases>
		   <size>(2,2)</size>
		<costList>
			<Steel>30</Steel>
		</costList>
				<castEdgeShadows>true</castEdgeShadows>
				<fillPercent>0.3</fillPercent>
			<designationCategory>VeeDecoCategory</designationCategory>
				<staticSunShadowHeight>0.20</staticSunShadowHeight>
				<uiIconScale>0.80</uiIconScale>
						<building>
						<isEdifice>false</isEdifice>
					</building>
			<clearBuildingArea>false</clearBuildingArea>
			<tradeability>None</tradeability>
	</ThingDef>	
	
	
