<Defs>

  <!-- Tanegashima -->

  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>BA_Gun_Tanegashima_P</defName>
    <label>Tanegashima Short musket</label>
    <description>A traditional Firearm which is heavier compare to other short musket.</description>
    <graphicData>
      <texPath>Things/Weapon/Tanegashima_P</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.2</drawSize>
    </graphicData>
    <techLevel>Medieval</techLevel>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
      </li>
      <li Class="BA_Class.RainFireOffShoot_CompProperties">
        <FireOffWeather>
          <li>Rain</li>
          <li>FoggyRain</li>
          <li>RainyThunderstorm</li>
          <li>SnowHard</li>
        </FireOffWeather>
        <ThickCount>10</ThickCount>
        <Smoke>Musket_Smoke</Smoke>
      </li>
    </comps>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <WorkToMake>15000</WorkToMake>
      <Mass>3</Mass>
      <AccuracyTouch>0.8</AccuracyTouch>
      <AccuracyShort>0.6</AccuracyShort>
      <AccuracyMedium>0.5</AccuracyMedium>
      <AccuracyLong>0.2</AccuracyLong>
      <RangedWeapon_Cooldown>2.5</RangedWeapon_Cooldown>
    </statBases>
    <equippedAngleOffset>0</equippedAngleOffset>
    <weaponTags>
      <li>NeolithicRangedHeavy</li>
      <li>NeolithicRangedChief</li>
    </weaponTags>
    <costList>
      <WoodLog>40</WoodLog>
      <Steel>20</Steel>
    </costList>
    <recipeMaker>
      <recipeUsers>
        <li>BA_FueledGunSmithy</li>
      </recipeUsers>
      <researchPrerequisite>BA_MatchLock</researchPrerequisite>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
    </recipeMaker>
    <verbs>
      <li>
        <verbClass>BA_Class.Verb_RainFireOffShoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>BA_Bullet_BasicRound</defaultProjectile>
        <warmupTime>0.8</warmupTime>
        <range>25.9</range>
        <soundCast>BA_Musket</soundCast>
        <soundCastTail>GunTail_Heavy</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

