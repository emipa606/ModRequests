<Defs>
	
	<ThingDef ParentName="ResourceBase">
		<defName>ProtoBotAssemblyKit</defName>
		<label>ProtoBot assembly kit</label>
		<thingClass>ThingWithComps</thingClass>
		<description>A kit containing a disassembled ProtoBot with all the tools required to assemble it.</description>
		<graphicData>
			<texPath>Things/Items/ProtoBotAssemblyKit</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<stackLimit>1</stackLimit>
		<useHitPoints>true</useHitPoints>
		<statBases>
			<WorkToMake>42000</WorkToMake>
			<MarketValue>1065</MarketValue>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>60.0</Mass>
		</statBases>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Cook</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<recipeUsers>
				<li>CraftingSpot</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Steel>150</Steel>
			<ComponentIndustrial>10</ComponentIndustrial>
		</costList>
		<thingCategories>
			<li>Items</li>
		</thingCategories>
		<tradeability>All</tradeability>
		<techLevel>Spacer</techLevel>
		<tradeTags>
			<li>ExoticMisc</li>
		</tradeTags>
		<comps>
			<li Class="CompProperties_Usable">
				<useJob>UseItem</useJob>
				<useLabel>Assemble Robot</useLabel>
				<useDuration>600</useDuration>
			</li>
			<li Class="Androids.CompProperties_SpawnPawn">
				<compClass>Androids.CompUseEffect_SpawnDroid</compClass>
				<pawnKind>ProtoBot</pawnKind>
			</li>
			<li Class="CompProperties_UseEffect">
				<compClass>CompUseEffect_DestroySelf</compClass>
			</li>
		</comps>
	</ThingDef>
	
