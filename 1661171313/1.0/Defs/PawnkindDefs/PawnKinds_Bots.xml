	<PawnKindDef>
    <defName>MurderBotGunner</defName>
    <label>murderbot gunner</label>
    <race>MurderBot</race>
    <combatPower>70</combatPower>
    <defaultFactionType>Callistan</defaultFactionType>
    <baseRecruitDifficulty>0.70</baseRecruitDifficulty>
    <invNutrition>2.55</invNutrition>
    <itemQuality>Good</itemQuality>
    <backstoryCryptosleepCommonality>0.25</backstoryCryptosleepCommonality>
    <maxGenerationAge>35</maxGenerationAge>
    <gearHealthRange>
      <min>0.7</min>
      <max>2.3</max>
    </gearHealthRange>
    <apparelTags>
      <li>Spacer</li>
      <li>CallistanSoldier</li>
    </apparelTags>
    <apparelMoney>
      <min>5000</min>
      <max>10000</max>
    </apparelMoney>
    <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
	<backstoryCategories>
		<li>MurderBot</li>
	</backstoryCategories>
    <weaponMoney>
      <min>1200</min>
      <max>1400</max>
    </weaponMoney>
    <weaponTags>
      <li>Callistan</li>
    </weaponTags>
    <techHediffsMoney>
      <min>2000</min>
      <max>3500</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
    <techHediffsChance>0.075</techHediffsChance>
    <inventoryOptions>
      <skipChance>0</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>50</min>
            <max>200</max>
          </countRange>
        </li>
        <li>
          <thingDef>MedicineUltratech</thingDef>
          <countRange>
            <min>1</min>
            <max>1</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  
