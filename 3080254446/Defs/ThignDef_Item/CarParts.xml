			<Bulk MayRequire="CETeam.CombatExtended">8</Bulk>
			<Flammability>0.4</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<Beauty>-1</Beauty>
			<MarketValue>80</MarketValue>
		</statBases>
		<minRewardCount>4</minRewardCount>
		<pathCost>50</pathCost>
     </ThingDef>
	  
