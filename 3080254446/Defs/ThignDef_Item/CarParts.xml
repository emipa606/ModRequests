<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef Name="Vehicle_Part_Base" Abstract="True">
		<thingClass>ThingWithComps</thingClass>
		<label>unspecified resource</label>
		<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
		<stackLimit>8</stackLimit>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<statBases>
			<Mass>0.1</Mass>
			<Bulk MayRequire="CETeam.CombatExtended">0.1</Bulk>
			<Beauty>-6</Beauty>
		</statBases>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<techLevel>Industrial</techLevel>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
		<thingCategories>
			<li>BuildingsSpecial</li>
			<!--<li>Parts</li>-->
		</thingCategories>
		<tradeTags>
		  <li>ExoticMisc</li>
		</tradeTags>
     </ThingDef>
	  
    <ThingDef ParentName="Vehicle_Part_Base">
		<defName>UsableWheel</defName>
		<label>Usable Wheel</label>
		<description>Salvaged Wheel that is in surprisingly good condition.</description>
		<graphicData>
			<texPath>Things/ThingDef_Items/Wheel</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<stackLimit>4</stackLimit>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<statBases>
			<Mass>5</Mass>
			<Bulk MayRequire="CETeam.CombatExtended">12</Bulk>
			<Flammability>0.4</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<Beauty>-2</Beauty>
			<MarketValue>150</MarketValue>
		</statBases>
		<minRewardCount>4</minRewardCount>
		<pathCost>35</pathCost>
     </ThingDef>
	  
