<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>Lost_RustedSedan_Blueprint</defName>
		<label>Rusty Sedan</label>
        <description>A car from the old times. Has seen better days, but its functional. Has 5 seats but one is too small and no one wants to sit there. Also has little trunk space. \n\n&lt;color=#bb8f04&gt;Crew:&lt;/color&gt; Driver x1, Passenger x4\n&lt;color=#bb8f04&gt;Fuel type:&lt;/color&gt; Chemfuel</description>
		<designationCategory>VF_Vehicles</designationCategory>
		
		<graphicData>
			<texPath>Vehicles/RustedSedan/RustedCar</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>4</drawSize>
		</graphicData>
		<uiIconPath>Vehicles/RustedSedan/Icon_RustedCar</uiIconPath>
		
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>6000</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		
		<size>(2,4)</size>
		
		<researchPrerequisites>
			<li>LV_ResearchVehicle_RustedSedan</li>
		</researchPrerequisites>
		
		<costList>
			<Steel>400</Steel>
			<ComponentIndustrial>12</ComponentIndustrial>
			<UsableWheel>4</UsableWheel>
			<UsableSeat>4</UsableSeat>
			<UsableCarChassisPart>4</UsableCarChassisPart>
			<UsableCarFramePart>2</UsableCarFramePart>
			<UsableCarEngine>1</UsableCarEngine>
		</costList>
		
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		
		<thingToSpawn>Lost_RustedSedan</thingToSpawn>
		
		<constructionSkillPrerequisite>7</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>
</Defs>
