    <graphicData>
      <color>(112,82,65)</color>
    </graphicData>
    <statBases>
      <MarketValue>50.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>8</hatcherDaystoHatch>
        <hatcherPawn>BeastmenPawnHarpy</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

</Defs>
