<Defs>

  <!-- Bestigor -->
  
  <HediffDef ParentName="AddedBodyPartBase">
    <defName>BestigorHornLeft</defName>
    <label>left bestigor horn</label>
    <labelNoun>a large horn</labelNoun>
    <addedPartProps>
      <solid>true</solid>
      <betterThanNatural>false</betterThanNatural>
      <partEfficiency>1.0</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <MentalBreakThreshold>-0.02</MentalBreakThreshold>
          <TameAnimalChance>0.02</TameAnimalChance>
          <TrainAnimalChance>0.02</TrainAnimalChance>
        </statOffsets>
        <painFactor>0.9</painFactor>
      </li>
    </stages>
    <spawnThingOnRemoved>BestigorHorn</spawnThingOnRemoved>
  </HediffDef>
  
