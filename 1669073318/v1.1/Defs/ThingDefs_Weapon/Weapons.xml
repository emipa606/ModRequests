  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>MeleeWeapon_BigAxe</defName>
    <label>bestigor axe</label>
    <description>A large axe capable to deliver deadly blows.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Weapons/BigAxe</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1.25)</drawSize>
    </graphicData>
    <techLevel>Medieval</techLevel>
    <costStuffCount>140</costStuffCount>
    <statBases>
     <WorkToMake>18000</WorkToMake>
     <Mass>5</Mass>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.1</MoveSpeed>
    </equippedStatOffsets>
    <weaponTags>
      <li>Gor</li>
      <li>GorHigh</li>
    </weaponTags>
    <equippedAngleOffset>-65</equippedAngleOffset>
      <recipeMaker>
        <researchPrerequisite>LongBlades</researchPrerequisite>
        <skillRequirements>
          <Crafting>5</Crafting>
        </skillRequirements>
      </recipeMaker>
      <stuffCategories>
        <li>Metallic</li>
      </stuffCategories>
    <comps>
    </comps>
    <tools>
      <li>
        <label>blade</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>26</power>
        <cooldownTime>2.9</cooldownTime>
      </li>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

