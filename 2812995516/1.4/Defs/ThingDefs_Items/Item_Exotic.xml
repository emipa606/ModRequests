<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="VFEEA_Empowered_SuperNanite" ParentName="ResourceBase" Abstract= "True" >
		<description>A container with fully operational supernanites, enhancing the person injected with them with specific powers. Super-nanites were used by the long forgotten governments to create supersoldiers, now outlawed by the galactic law.</description>
		<graphicData>
			<texPath>Things/Item/Special/SuperNanites/Empowered_SuperNanites</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<stackLimit>1</stackLimit>
		<useHitPoints>true</useHitPoints>

		<statBases>
			<MarketValue>1050</MarketValue>
			<MaxHitPoints>60</MaxHitPoints>
			<Mass>1</Mass>
			<Flammability>0.2</Flammability>
			<DeteriorationRate>20</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>VFEEA_empowered_nanites</li>
		</thingCategories>
		<drawGUIOverlay>false</drawGUIOverlay>
		<tradeability>None</tradeability>
		<techLevel>Ultra</techLevel>
		<comps>
			<li Class="VFEAncients.CompProperties_NeedsContainment">
			<validContainers>
				<li>VFEA_SuperNaniteContainment</li>
			</validContainers>
			</li>
		</comps>
	</ThingDef>
		



