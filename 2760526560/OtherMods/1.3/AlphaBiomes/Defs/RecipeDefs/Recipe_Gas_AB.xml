<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <RecipeDef>
        <defName>HA_GasFromPropane</defName>
        <label>produce gas from propane</label>
        <description>Produce gas bottles by heating propane.</description>
        <jobString>Producing Gas.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>2500</workAmount>
        <ingredients>
          <li>
            <filter>
              <thingDefs>
                <li>AB_Propane</li>
              </thingDefs>
            </filter>
            <count>10</count>
          </li>
        </ingredients>
        <fixedIngredientFilter>
          <thingDefs>
            <li>AB_Propane</li>
          </thingDefs>
        </fixedIngredientFilter>
        <products>
            <HA_GasBottle>70</HA_GasBottle>
            <HA_Tar>3</HA_Tar>
        </products>
        <workSkill>Crafting</workSkill>
        <recipeUsers>
            <li>HA_Gasifier</li>
        </recipeUsers>
    </RecipeDef>

    <RecipeDef>
        <defName>HA_TarFromMushroomStalks</defName>
        <label>produce tar from Mushroom Stalks</label>
        <description>Produce tar by heating mushroom stalks. These mushroom logs are near worthless for producing gas, much of the mass is degraded into piles of tar.</description>
        <jobString>Producing Tar.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>2500</workAmount>
        <ingredients>
          <li>
            <filter>
              <thingDefs>
                <li>AB_MushroomWoodLog</li>
              </thingDefs>
            </filter>
            <count>50</count>
          </li>
        </ingredients>
        <fixedIngredientFilter>
          <thingDefs>
            <li>AB_MushroomWoodLog</li>
          </thingDefs>
        </fixedIngredientFilter>
        <products>
            <HA_GasBottle>2</HA_GasBottle>
            <HA_Tar>25</HA_Tar>
        </products>
        <workSkill>Crafting</workSkill>
        <recipeUsers>
            <li>HA_Gasifier</li>
        </recipeUsers>
    </RecipeDef>
</Defs>