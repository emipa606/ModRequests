<?xml version="1.0" encoding="UTF-8"?>
<Defs>
<Operation Class="PatchOperationFindMod">	
        <mods>
            <li>Alpha Animals</li>
        </mods>
    <RecipeDef>
        <defName>HA_GasFromRedWood</defName>
        <label>produce gas from Red Wood</label>
        <description>Produce gas bottles by heating red wood. This process takes much longer than other recipes as colonists must repeatedly empty the gasifier of strange red slime. It's almost as if the wood is bleeding.</description>
        <jobString>Producing Gas.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>3500</workAmount>
        <ingredients>
          <li>
            <filter>
              <thingDefs>
                <li>GU_RedWood</li>
              </thingDefs>
            </filter>
            <count>50</count>
          </li>
        </ingredients>
        <fixedIngredientFilter>
          <thingDefs>
            <li>GU_RedWood</li>
          </thingDefs>
        </fixedIngredientFilter>
        <products>
            <HA_GasBottle>30</HA_GasBottle>
            <AB_RawRedSlime>30</AB_RawRedSlime>
        </products>
        <workSkill>Crafting</workSkill>
        <recipeUsers>
            <li>HA_Gasifier</li>
        </recipeUsers>
    </RecipeDef>
</Operation>
</Defs>