<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Power</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName = "VFE_GasExtraction"]/techLevel</xpath>
                    <value>
                        <techLevel>Medieval</techLevel>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName = "VFE_GasExtraction"]/prerequisites</xpath>
                    <value>
                        <prerequisites>
                            <li>Smithing</li>
                        </prerequisites>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>