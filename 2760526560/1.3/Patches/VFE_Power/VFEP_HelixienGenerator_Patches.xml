<?xml version="1.0" encoding="utf-8" ?>
<Patch>
 <Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Furniture Expanded - Power</li>
    </mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "VPE_HelixienGenerator"]/comps/li[@Class = "GasNetwork.CompProperties_GasTrader"]/gasConsumption</xpath>
				<value>
					<gasConsumption>75</gasConsumption>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "VPE_IndustrialHelixienGenerator"]/comps/li[@Class = "CompProperties_Power"]/basePowerConsumption</xpath>
				<value>
					<basePowerConsumption>-2400</basePowerConsumption>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "VPE_IndustrialHelixienGenerator"]/comps/li[@Class = "GasNetwork.CompProperties_GasTrader"]/gasConsumption</xpath>
				<value>
					<gasConsumption>150</gasConsumption>
				</value>
			</li>
		</operations>
	</match>
 </Operation>	
</Patch>
