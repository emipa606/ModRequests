<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<QuestScriptDef>
		<defName>DP_GrandWalls</defName>
		<root Class="RimWorld.QuestGen.QuestNode_Sequence">
			<nodes>
				<li Class="QuestEditor_Library.QuestNode_RandomCustomMap">
					<datas>
						<li>
							<key>DP_MapGrandWalls</key>
							<value>1</value>
						</li>
					</datas>
					<siteIconPath>DP_Textures/DPQuestIcon</siteIconPath>
					<expandingIconPath>DP_Textures/DPQuestIcon</expandingIconPath>
					<replaceMapGeneration>True</replaceMapGeneration>
					<faction>DP_GenericHostile</faction>
					<storeAs>DP3</storeAs>
					<distance>
						<min>3</min>
						<max>20</max>
					</distance>
				</li>
								<li Class="QuestNode_WorldObjectTimeout">
				  <worldObject>$Dungeon</worldObject>
				  <isQuestTimeout>true</isQuestTimeout>
				  <delayTicks>$(30*60000)</delayTicks>
				  <inSignalDisable>Dungeon.MapGenerated</inSignalDisable>
				  <destroyOnCleanup>true</destroyOnCleanup>
				  <node Class="QuestNode_Sequence">
					<nodes>
					  <li Class="QuestNode_End">
						<outcome>Unknown</outcome>
					  </li>
					</nodes>
				  </node>
				</li>

			</nodes>
		</root>
		<questDescriptionRules>
			<rulesStrings>
				<li>questDescription-&gt;A massive military installation, heavily armed, fortified, and patrolled by a powerful offworld army.
\nThis is a challenge level.</li>
			</rulesStrings>
		</questDescriptionRules>
		<questNameRules>
			<rulesStrings>
				<li>questName -&gt; Grand Walls</li>
			</rulesStrings>
		</questNameRules>
		<autoAccept>True</autoAccept>
		<hideOnCleanup>False</hideOnCleanup>
		<expireDaysRange>
			<min>-1</min>
			<max>-1</max>
		</expireDaysRange>
		<defaultChallengeRating>5</defaultChallengeRating>
		<isRootSpecial>True</isRootSpecial>
		<sendAvailableLetter>True</sendAvailableLetter>
		<rootSelectionWeight>0</rootSelectionWeight>
	</QuestScriptDef>
</Defs>