<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Kit's Brazilian Crops (Continued)</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
	
					  <ProcessorFramework.ProcessDef>
						<defName>KIT_BeanStewFine</defName>
						<thingDef>KIT_BeanStewFine</thingDef>
						<ingredientFilter>
						  <thingDefs>
							<li>KIT_UncookedStewFine</li>
						  </thingDefs>
						</ingredientFilter>
						<capacityFactor>1</capacityFactor>
						<efficiency>10.0</efficiency>
						<processDays>.5</processDays>
					  </ProcessorFramework.ProcessDef>

					  <ProcessorFramework.ProcessDef>
						<defName>KIT_BeanStewLavish</defName>
						<thingDef>KIT_BeanStewLavish</thingDef>
						<ingredientFilter>
						  <thingDefs>
							<li>KIT_UncookedStewLavish</li>
						  </thingDefs>
						</ingredientFilter>
						<capacityFactor>1</capacityFactor>
						<efficiency>10.0</efficiency>
						<processDays>.7</processDays>
					  </ProcessorFramework.ProcessDef>

					  <ProcessorFramework.ProcessDef>
						<defName>KIT_BeanStewGourmet</defName>
						<thingDef>KIT_BeanStewGourmet</thingDef>
						<ingredientFilter>
						  <thingDefs>
							<li>KIT_UncookedStewGourmet</li>
						  </thingDefs>
						</ingredientFilter>
						<capacityFactor>1</capacityFactor>
						<efficiency>10.0</efficiency>
						<processDays>1</processDays>
					  </ProcessorFramework.ProcessDef>
					  
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>