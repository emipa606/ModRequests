<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>Joke_BoarBerries</defName>
    <label>boarberries</label>
    <description>Assorted berries plucked from a hedge hog. Tastes a little bit like pork, and thus confuses herbivore and carnivore alike. Nice to eat, even when raw. Counts as a vegetable/fruit.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/BoarBerries</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.2</MarketValue>
      <Mass>0.027</Mass>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>20</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tasteThought></tasteThought>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>
</Defs>