<?xml version="1.0" encoding="utf-8"?>
<Patch>
			<Operation Class="PatchOperationFindMod">
    <mods><li>[RH] Faction: Militaires Sans Frontieres (1.0)</li></mods>
    <match Class="PatchOperationAddModExtension">
        <xpath>Defs/FactionDef[defName="RH_MSF"]</xpath>
        <value>
            <li Class="MSFBuddies.ModExtension">
                <letter>NeutralEvent</letter>
                <delayDaysMin>0.5</delayDaysMin>
                <delayDaysMax>1</delayDaysMax>
                <repOffset>200</repOffset>
                <labelKey>RH_MSFFriendliesMSF_Label</labelKey>
                <textKey>RH_MSFFriendliesMSF_Text</textKey>
            </li>
        </value>
     </match>
	</Operation>
</Patch>