<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<IncidentDef>
		<defName>MSFRescueMIB</defName>
		<label>infiltrator</label>
		<category>Special</category>
    	<targetTags>
      		<li>Map_PlayerHome</li>
    	</targetTags>
		<workerClass>MSFRescue.IncidentWorker_MSFRescue</workerClass>
		<letterLabel>Infiltrator</letterLabel>
		<letterText>Sensing trouble, an [PAWN_kind] has arrived.\n\n[PAWN_pronoun] kept you waiting, huh?</letterText>
		<baseChance>0</baseChance>
		<minRefireDays>60</minRefireDays>
    	<minGreatestPopulation>6</minGreatestPopulation>
		<pawnKind>RH_MSF_Elite</pawnKind>
		<!--<pawnFixedGender>Male</pawnFixedGender>-->
		<pawnMustBeCapableOfViolence>true</pawnMustBeCapableOfViolence>
		<modExtensions>
			<li Class="MSFRescue.MSFRescueDefs">
				<NumMembers>1</NumMembers>
			</li>
		</modExtensions>
	</IncidentDef>
	
	<IncidentDef>
		<defName>MSFRescueTeam</defName>
		<label>operator</label>
		<category>Special</category>
    	<targetTags>
      		<li>Map_PlayerHome</li>
    	</targetTags>
		<workerClass>MSFRescue.IncidentWorker_MSFRescue</workerClass>
		<letterLabel>MSF soldier</letterLabel>
		<letterText>An MSF [PAWN_kind] has arrived. \n\n[PAWN_pronoun] is approaching your base.</letterText>
		<baseChance>0</baseChance>
		<minRefireDays>60</minRefireDays>
    	<minGreatestPopulation>2</minGreatestPopulation>
		<pawnKind>RH_MSF_Patrol</pawnKind>
		<!--<pawnFixedGender>Male</pawnFixedGender>-->
		<pawnMustBeCapableOfViolence>true</pawnMustBeCapableOfViolence>
		<modExtensions>
			<li Class="MSFRescue.MSFRescueDefs">
				<NumMembers>4</NumMembers>
			</li>
		</modExtensions>
	</IncidentDef>

</Defs>