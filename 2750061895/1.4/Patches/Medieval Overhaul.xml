<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
				
				<!-- Raw Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_RawWood"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_RawWood"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Raw Dark Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_RawDarkWood"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_RawDarkWood"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Dark Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_DarkWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_DarkWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Elven Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_ElvenWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_ElvenWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Dryad Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_DryadWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_DryadWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Ancient Wood -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_AncientWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_AncientWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Bone -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Bone"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="DankPyon_Bone"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- Alcohols -->
				
				<li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[@Name="DankPyon_DrugBase"]/tools</xpath>
				</li>
				
				<!-- DankPyon_Ale -->	
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Ale"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
				
				<!-- DankPyon_Cider -->	
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Cider"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Cider"]/equipmentType</xpath>
				</li>

				<!-- DankPyon_Wine -->		
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Wine"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Wine"]/equipmentType</xpath>
				</li>
	
				<!-- DankPyon_JuiceApple -->	

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceApple"]/tools</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceApple"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceApple"]/equipmentType</xpath>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceAppleUnfermented"]/tools</xpath>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceAppleUnfermented"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_JuiceAppleUnfermented"]/equipmentType</xpath>
				</li>
				
			</operations>
        </match>
    </Operation>
</Patch>