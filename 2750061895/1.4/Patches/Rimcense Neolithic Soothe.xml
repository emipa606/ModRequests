<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Rimcense: Neolithic Soothe</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
			
				<!-- VBY_SandalWoodLog -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBY_SandalWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="VBY_SandalWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- VBY_FrankincenseWoodLog -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBY_FrankincenseWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="VBY_FrankincenseWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				<!-- VBY_TragacanthWoodLog -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBY_TragacanthWoodLog"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="VBY_TragacanthWoodLog"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
			</operations>
        </match>
    </Operation>
</Patch>