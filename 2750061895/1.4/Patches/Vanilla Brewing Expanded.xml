<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Brewing Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
    
				<!-- Gin -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Gin"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Gin"]/equipmentType</xpath>
				</li>
				
				<!-- Vodka -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Vodka"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Vodka"]/equipmentType</xpath>
				</li>
				
				<!-- Tequila -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Tequila"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Tequila"]/equipmentType</xpath>
				</li>
				
				<!-- Whiskey -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Whiskey"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Whiskey"]/equipmentType</xpath>
				</li>
				
				<!-- Double Aged Whiskey -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_DoubleWhiskey"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_DoubleWhiskey"]/equipmentType</xpath>
				</li>
				
				<!-- Soda -->			
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Soda"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VBE_Soda"]/equipmentType</xpath>
				</li>

			</operations>
		</match>
	</Operation>
</Patch>