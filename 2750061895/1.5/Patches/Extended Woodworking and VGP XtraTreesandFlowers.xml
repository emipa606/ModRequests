<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Extended Woodworking</li>
        </mods>
		<match Class="PatchOperationFindMod">
		<mods>
			<li>VGP Xtra Trees and Flowers</li>
        </mods>
			<match Class="PatchOperationSequence">
				<operations>			
				
					<!-- Acacia Wood -->
					<li Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="WoodLog_Acacia"]/tools</xpath>
					</li>
		
					<li Class="PatchOperationAttributeSet">
						<xpath>Defs/ThingDef[defName="WoodLog_Acacia"]</xpath>
						<attribute>ParentName</attribute>
						<value>ResourceBase</value>
					</li>
					
					<!-- Camellia Wood -->
					<li Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="WoodLog_Camellia"]/tools</xpath>
					</li>
		
					<li Class="PatchOperationAttributeSet">
						<xpath>Defs/ThingDef[defName="WoodLog_Camellia"]</xpath>
						<attribute>ParentName</attribute>
						<value>ResourceBase</value>
					</li>
					
					<!-- Cherry Blossom Wood -->
					<li Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="WoodLog_CherryBlossom"]/tools</xpath>
					</li>
		
					<li Class="PatchOperationAttributeSet">
						<xpath>Defs/ThingDef[defName="WoodLog_CherryBlossom"]</xpath>
						<attribute>ParentName</attribute>
						<value>ResourceBase</value>
					</li>
					
					<!-- Japanese Maple Wood -->
					<li Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="WoodLog_JapaneseMaple"]/tools</xpath>
					</li>
		
					<li Class="PatchOperationAttributeSet">
						<xpath>Defs/ThingDef[defName="WoodLog_JapaneseMaple"]</xpath>
						<attribute>ParentName</attribute>
						<value>ResourceBase</value>
					</li>
					
					<!-- Red Maple Wood -->
					<li Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="WoodLog_RedMaple"]/tools</xpath>
					</li>
		
					<li Class="PatchOperationAttributeSet">
						<xpath>Defs/ThingDef[defName="WoodLog_RedMaple"]</xpath>
						<attribute>ParentName</attribute>
						<value>ResourceBase</value>
					</li>							
				
				</operations>
			</match>
		</match>
    </Operation>
</Patch>