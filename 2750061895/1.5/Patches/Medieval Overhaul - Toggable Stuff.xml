<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul: Toggable Stuff</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
				
				<!-- WoodTimber -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="WoodTimber"]/tools</xpath>
				</li>
	
				<li Class="PatchOperationAttributeSet">
					<xpath>Defs/ThingDef[defName="WoodTimber"]</xpath>
					<attribute>ParentName</attribute>
					<value>ResourceBase</value>
				</li>
				
				
			</operations>
        </match>
    </Operation>
</Patch>