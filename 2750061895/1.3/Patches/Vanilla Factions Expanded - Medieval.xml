<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Factions Expanded - Medieval</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
    
				<!-- Wine -->	

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VFEM_Wine"]/tools</xpath>
				</li>				
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VFEM_Wine"]/comps/li[compClass="CompEquippable"]</xpath>
				</li>
	
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="VFEM_Wine"]/equipmentType</xpath>
				</li>					
							
			</operations>
		</match>
	</Operation>
</Patch>