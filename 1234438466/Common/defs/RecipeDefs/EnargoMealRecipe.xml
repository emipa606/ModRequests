		<workAmount>300</workAmount>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<researchPrerequisite>EnargoPlanting</researchPrerequisite>
	</RecipeDef>
	
