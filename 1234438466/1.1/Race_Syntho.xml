<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef Name="BaseSyntho" Abstract="True">
    <thingClass>Pawn</thingClass>
    <category>Pawn</category>
    <selectable>true</selectable>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Pawn</altitudeLayer>
    <useHitPoints>false</useHitPoints>
    <hasTooltip>true</hasTooltip>
    <soundImpactDefault>BulletImpactFlesh</soundImpactDefault>
    <inspectorTabs>
      <li>ITab_Pawn_Health</li>
      <li>ITab_Pawn_Needs</li>
      <li>ITab_Pawn_Character</li>
      <li>ITab_Pawn_Training</li>
      <li>ITab_Pawn_Social</li>
      <li>ITab_Pawn_Guest</li>
      <li>ITab_Pawn_Prisoner</li>
      <li>ITab_Pawn_FormingCaravan</li>
      <li>ITab_Pawn_Gear</li>
      <li>ITab_Pawn_Log</li>
    </inspectorTabs>
    <comps>
        <li>
            <compClass>CompAttachBase</compClass>
        </li>
    </comps>
    <drawGUIOverlay>true</drawGUIOverlay>
</ThingDef>

<!--================Syntho==================-->

<alienRace.ThingDef_alienRace ParentName="BasePawn">
    <defName>Alien_Syntho</defName>
    <label>Syntho</label>
    <description>A Synthetically created person from the Glitterworlds.</description>
	<alienRace>
        <generalSettings>
			<humanRecipeImport>true</humanRecipeImport>
			<immuneToAge>false</immuneToAge>
			<canLayDown>true</canLayDown>
			<forcedRaceTraitEntries>
				<li>
					<defName>AnnoyingVoice</defName>
					<chance>10</chance>
				</li>
				<li>
					<defName>Beauty</defName>
					<degree>2</degree>
					<chance>10</chance>
				</li>
				<li>
					<defName>Beauty</defName>
					<degree>1</degree>
					<chance>15</chance>
				</li>
			</forcedRaceTraitEntries>
			<disallowedTraits>
				<!--li>DrugDesire</li-->
			</disallowedTraits>
			<maxDamageForSocialfight>3</maxDamageForSocialfight>
			<alienPartGenerator>
				<aliencrowntypes>
                    <li>Average_Pointy</li>
					<li>Average_Normal</li>
                </aliencrowntypes>
				<alienbodytypes>
					<li>Male</li>
					<li>Female</li>
					<li>Thin</li>
					<li>Hulk</li>
					<!--li>Fat</li-->
				</alienbodytypes>
				<useGenderedHeads>true</useGenderedHeads>
				<useGenderedBodies>false</useGenderedBodies>
				<alienskincolorgen Class="ColorGenerator_Options">
					<options>
                    <li><!--White-->
						<weight>3</weight>
						<only>RGBA(1.0,1.0,1.0,1)</only>
					</li>
					<li><!--Blue-->
						<weight>3</weight>
						<only>RGBA(0.6, 0.7, 1,1)</only>
					</li>
					<li><!--Pink-->
						<weight>3</weight>
						<only>RGBA(1.0, 0.6, 1,1)</only>
					</li>
					<li><!--Red-->
						<weight>3</weight>
						<only>RGBA(0.9, 0.4, 0.4,1)</only>
					</li>
					<li><!--Orange-->
						<weight>3</weight>
						<only>RGBA(1.0, 0.7, 0.1,1)</only>
					</li>
					<li><!--Green-->
						<weight>3</weight>
						<only>RGBA(0.6, 1, 0.6,1)</only>
					</li>
					<li><!--Teal-->
						<weight>3</weight>
						<only>RGBA(0.6, 1, 1,1)</only>
					</li>
					<li><!--Purple-->
						<weight>3</weight>
						<only>RGBA(0.8, 0.6, 1.0,1)</only>
					</li>
					</options>
				</alienskincolorgen>
				<alienhaircolorgen Class="ColorGenerator_Options">
                <options>
                    <li><!--White-->
						<weight>3</weight>
						<only>RGBA(1.0,1.0,1.0,1)</only>
					</li>
					<li><!--Blue-->
						<weight>3</weight>
						<only>RGBA(0.6, 0.7, 1,1)</only>
					</li>
					<li><!--Pink-->
						<weight>3</weight>
						<only>RGBA(1.0, 0.6, 1,1)</only>
					</li>
					<li><!--Red-->
						<weight>3</weight>
						<only>RGBA(0.9, 0.4, 0.4,1)</only>
					</li>
					<li><!--Orange-->
						<weight>3</weight>
						<only>RGBA(1.0, 0.7, 0.1,1)</only>
					</li>
					<li><!--Green-->
						<weight>3</weight>
						<only>RGBA(0.6, 1, 0.6,1)</only>
					</li>
					<li><!--Teal-->
						<weight>3</weight>
						<only>RGBA(0.6, 1, 1,1)</only>
					</li>
					<li><!--Purple-->
						<weight>3</weight>
						<only>RGBA(0.8, 0.6, 1.0,1)</only>
					</li>
                </options>
            </alienhaircolorgen>
				<useSkincolorForHair>false</useSkincolorForHair>
			</alienPartGenerator>
        </generalSettings>
        <graphicPaths>
            <li>
                <body>Syntho/Base/</body>
                <head>Syntho/Head/</head>
            </li>
        </graphicPaths>
        <hairSettings>
			<hasHair>true</hasHair>
			<getsGreyAt>250</getsGreyAt>
			<hairTags>
				<li>Rural</li>
				<li>Tribal</li>
				<li>Urban</li>
				<li>Punk</li>
            </hairTags>
        </hairSettings>
        <thoughtSettings>
            <cannotReceiveThoughts>
				<li>SleptOnGround</li>
				<li>AteEnargo</li>
            </cannotReceiveThoughts>
        </thoughtSettings>
        <relationSettings>
			<relationChanceModifierChild>0</relationChanceModifierChild>
            <relationChanceModifierExLover>2</relationChanceModifierExLover>
            <relationChanceModifierExSpouse>1</relationChanceModifierExSpouse>
            <relationChanceModifierFiance>1</relationChanceModifierFiance>
            <relationChanceModifierLover>2</relationChanceModifierLover>
            <relationChanceModifierParent>0</relationChanceModifierParent>
            <relationChanceModifierSibling>3</relationChanceModifierSibling>
            <relationChanceModifierSpouse>1</relationChanceModifierSpouse>
			<renamer>
               <li>
                  <relation>Sibling</relation>
                  <label>Vat Brother</label>
                  <femaleLabel>Vat Sister</femaleLabel>
               </li>
			   <li>
			    <relation>Parent</relation>
                  <label>Vat Brother</label>
                  <femaleLabel>Vat Sister</femaleLabel>
               </li>
			   <li>
			    <relation>Child</relation>
                  <label>Vat Brother</label>
                  <femaleLabel>Vat Sister</femaleLabel>
               </li>
            </renamer>
        </relationSettings>
        <raceRestriction>
			<!--recipeList>
                <li>CookMealEnargoStack</li>
				<li>CookMealEnargo</li>
            </recipeList-->
			<foodList>
                <li>EnargoMealS</li>
				<li>EnargoMealL</li>
				<li>MiniEnargo</li>
				<!--li>LiquidEnargo</li-->
				<li>RawEnargo</li>
				<li>EnargoPaste</li>
            </foodList>
			<onlyEatRaceRestrictedFood>true</onlyEatRaceRestrictedFood>			
			<!--plantList>
                <li>PlantEnargo</li>
            </plantList-->
			<!--onlyDoRaceRestrictedPlants>true</onlyDoRaceRestrictedPlants>
			<buildingList>
				<li>Syntho_workbench</li>
			</buildingList-->
        </raceRestriction>
    </alienRace>
	<statBases>
		<MarketValue>2500</MarketValue>
		<MoveSpeed>8</MoveSpeed>
		<Flammability>0.7</Flammability>
		<ComfyTemperatureMin>14</ComfyTemperatureMin>
		<ComfyTemperatureMax>24</ComfyTemperatureMax>
		<LeatherAmount>50</LeatherAmount>
		<ResearchSpeed>0.85</ResearchSpeed>
		<MiningSpeed>1</MiningSpeed>
		<MeleeDodgeChance>1.1</MeleeDodgeChance>
		<ImmunityGainSpeed>3</ImmunityGainSpeed>
		<CarryingCapacity>100</CarryingCapacity>
		<ShootingAccuracyPawn>1.1</ShootingAccuracyPawn>
		<AimingDelayFactor>.5</AimingDelayFactor>
		<WorkSpeedGlobal>1.2</WorkSpeedGlobal>
		<GlobalLearningFactor>0.90</GlobalLearningFactor>
	</statBases>
	<tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>25</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>25</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
	  <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>11.32</power>
        <cooldownTime>2.3</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.15</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.7</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
	<race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>260</lifeExpectancy>
      <leatherDef>Leather_Syntho</leatherDef>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
	  <bloodDef>SynthoBlood</bloodDef>
      <baseBodySize>1</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
      <foodType>OmnivoreHuman</foodType>
	  <lifeStageAges>
      <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1.2</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>4</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>13</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>18</minAge>
        </li>
      </lifeStageAges>
	        <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(1,0)</li>
          <li>(10,10)</li>
          <li>(25,50)</li>
          <li>(50,100)</li>
          <li>(100,100)</li>
          <li>(130,50)</li>
          <li>(175,25)</li>
		  <li>(200,10)</li>
          <li>(250,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>SynthoStandard</li>
		<li>Human</li>
      </hediffGiverSets>
		<fleshType>Synthetic</fleshType>
    </race>
