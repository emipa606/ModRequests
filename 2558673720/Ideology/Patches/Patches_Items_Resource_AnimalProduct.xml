<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Makes insect jelly trigger event when eaten -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="InsectJelly"]/ingestible</xpath>
		<value>
			<ateEvent>AteInsectJelly</ateEvent>
		</value>
	</Operation>

</Patch>