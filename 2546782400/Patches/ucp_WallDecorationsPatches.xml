<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>




				<li Class="PatchOperationReplace"> 
					<xpath>/Defs/ThingDef[defName="ucp_toiletpaper"]/designationCategory</xpath>
					<value>
						<designationCategory>Hygiene</designationCategory>

					</value>
				</li>
				
				<li Class="PatchOperationReplace"> 
					<xpath>/Defs/ThingDef[defName="ucp_mirror"]/designationCategory</xpath>
					<value>
						<designationCategory>Hygiene</designationCategory>

					</value>
				</li>
				
				<li Class="PatchOperationReplace"> 
					<xpath>/Defs/ThingDef[defName="ucp_mirrorlight"]/designationCategory</xpath>
					<value>
						<designationCategory>Hygiene</designationCategory>

					</value>
				</li>
					
			</operations>
		</match>
	</Operation>

</Patch>
