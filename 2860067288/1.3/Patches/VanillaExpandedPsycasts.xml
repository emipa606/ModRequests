<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Psycasts Expanded</li>
    </mods>
	<match Class="PatchOperationSequence">
		<operations>

			<li Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef [defName = "US_RoyalEye"]/costList</xpath>
			<value>
				<VPE_Eltex>1</VPE_Eltex>
			</value>
			</li>

			<li Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef [defName = "US_PsykerShades"]/costList</xpath>
			<value>
				<VPE_Eltex>2</VPE_Eltex>
			</value>
			</li>

		</operations>
	</match>
</Operation>

</Patch>