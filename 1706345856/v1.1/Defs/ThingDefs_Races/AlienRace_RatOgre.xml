<?xml version="1.0" encoding="utf-8" ?>

<Defs>

<ThingDef ParentName="AnimalThingBaseSkavenOgre">
    <defName>RatOgreBlade</defName>
    <label>rat ogre</label>
    <description>Rat Ogres are massive hulking monstrosities that represents the very pinnacle of Clan Moulder ingenuity, a lethal killing-machine beast that combines the inhumane reflexes of a Skaven with the brawn and size of an Ogre. Rat Ogres are creatures born from the fusion of crossbreeding different types of horrific creatures with Skaven blood, and surgically augmenting what is left. They have little room in their brain for anything other than the pursuit of fighting.</description>
    <statBases>
      <PainShockThreshold>2.5</PainShockThreshold>
      <MoveSpeed>5.1</MoveSpeed>
      <ComfyTemperatureMin>-20</ComfyTemperatureMin>
      <ComfyTemperatureMax>35</ComfyTemperatureMax>
      <MarketValue>1500</MarketValue>
      <LeatherAmount>30</LeatherAmount>
      <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.40</ArmorRating_Sharp>
    </statBases>
  <tools>
    <li>
      <label>left hand</label>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>21</power>
      <cooldownTime>2.4</cooldownTime>
      <linkedBodyPartsGroup>Hands</linkedBodyPartsGroup>
      <surpriseAttack>
        <extraMeleeDamages>
          <li>
            <def>Stun</def>
            <amount>5</amount>
          </li>
        </extraMeleeDamages>
      </surpriseAttack>
    </li>
    <li>
      <label>right blade</label>
      <capacities>
        <li>Blunt</li>
        <li>Cut</li>
        <li>Stab</li>
      </capacities>
      <power>26</power>
      <cooldownTime>2.6</cooldownTime>
      <linkedBodyPartsGroup>RatOgreBladeG</linkedBodyPartsGroup>
      <surpriseAttack>
        <extraMeleeDamages>
          <li>
            <def>Stun</def>
            <amount>5</amount>
          </li>
        </extraMeleeDamages>
      </surpriseAttack>
    </li>
    <li>
      <label>teeth</label>
      <capacities>
        <li>Bite</li>
      </capacities>
      <power>20.0</power>
      <cooldownTime>2</cooldownTime>
      <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
    </li>
  </tools>
    <race>
      <useMeatFrom>Alien_Skaven</useMeatFrom>
      <needsRest>true</needsRest>
      <intelligence>ToolUser</intelligence>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <body>RatOgre</body>
      <trainability>Simple</trainability>
      <predator>true</predator>
      <packAnimal>false</packAnimal>
      <baseHungerRate>1.5</baseHungerRate>
      <lifeExpectancy>50</lifeExpectancy>
      <baseBodySize>3</baseBodySize>
      <makesFootprints>true</makesFootprints>
      <baseHealthScale>2.5</baseHealthScale>
      <gestationPeriodDays>32</gestationPeriodDays>
      <wildness>0.75</wildness>
      <foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Light</leatherDef>
      <manhunterOnDamageChance>0.8</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.5</manhunterOnTameFailChance>
      <herdAnimal>false</herdAnimal>
      <nameOnTameChance>0.1</nameOnTameChance>
      <nameOnNuzzleChance>0.1</nameOnNuzzleChance>
      <herdMigrationAllowed>false</herdMigrationAllowed>
      <lifeStageAges>
        <li>
          <def>AnimalAdult</def>
          <minAge>0</minAge>
          <soundWounded>Pawn_Boomrat_Wounded</soundWounded>
          <soundDeath>Pawn_Boomrat_Death</soundDeath>
          <soundCall>Pawn_Boomrat_Call</soundCall>
          <soundAngry>Pawn_Boomrat_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>NoTradeSkaven_Ogre</li>
    </tradeTags>
  </ThingDef>

