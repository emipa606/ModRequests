<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <FactionDef>
    <defName>EshinFaction</defName>
    <label>Skaven traders</label>
    <description>Free standing clans of traders. These skaven tend to keep a neutral attitude compared to their kin in the Under-Empire.</description>
    <backstoryFilters>
      <li>
        <categories>
          <li>SkavenClanrat</li>
          <li>SkavenSlave</li>
          <li>SkavenEducated</li>
          <li>SkavenHireling</li>
          <li>SkavenTrader</li>
          <li>SkavenStormvermin</li>
          <li>SkavenMoulderPack</li>
          <li>SkavenMoulder</li>
          <li>SkavenSkryreVermin</li>
        </categories>
      </li>
    </backstoryFilters>
    <pawnSingular>Skaven</pawnSingular>
    <pawnsPlural>Skavens</pawnsPlural>
    <settlementGenerationWeight>0.5</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <canSiege>false</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <canMakeRandomly>true</canMakeRandomly>
    <colorSpectrum>
      <li>(0.356, 0.321, 0.325)</li>
    </colorSpectrum>
    <factionIconPath>Things/World/EshinIconA</factionIconPath>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <leaderTitle>Clan leader</leaderTitle>
    <techLevel>Medieval</techLevel>
    <allowedCultures><li>KTSkavenCulture</li></allowedCultures>
    <requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_TheistEmbodied</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Raider</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">MaleSupremacy</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">KTSkavenPrimacy</li>
    </requiredMemes>
    <disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">FleshPurity</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Guilty</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_Animist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">AnimalPersonhood</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">TreeConnection</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">NaturePrimacy</li>
    </disallowedMemes>
    <structureMemeWeights>
      <Structure_TheistEmbodied MayRequire="Ludeon.RimWorld.Ideology">2</Structure_TheistEmbodied>
      <Structure_TheistAbstract MayRequire="Ludeon.RimWorld.Ideology">0</Structure_TheistAbstract>
      <Structure_Ideological MayRequire="Ludeon.RimWorld.Ideology">0</Structure_Ideological>
      <Structure_Archist MayRequire="Ludeon.RimWorld.Ideology">0</Structure_Archist>
      <Structure_OriginChristian MayRequire="Ludeon.RimWorld.Ideology">0</Structure_OriginChristian>
      <Structure_OriginIslamic MayRequire="Ludeon.RimWorld.Ideology">0</Structure_OriginIslamic>
      <Structure_OriginHindu MayRequire="Ludeon.RimWorld.Ideology">0</Structure_OriginHindu>
      <Structure_OriginBuddhist MayRequire="Ludeon.RimWorld.Ideology">0</Structure_OriginBuddhist>
    </structureMemeWeights>
    <permanentEnemy>false</permanentEnemy>
    <fixedName>Clan traders</fixedName>
    <settlementNameMaker>NamerEshin</settlementNameMaker>
    <earliestRaidDays>1</earliestRaidDays>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>62</configurationListOrderPriority>
    <allowedArrivalTemperatureRange>-30~40</allowedArrivalTemperatureRange>
    <caravanTraderKinds>
      <li>Caravan_Skaven_BulkGoods</li>
      <li>Caravan_Skaven_MoulderPet</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_Skaven_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Skaven_Standard</li>
    </baseTraderKinds>
    <raidLootMaker>TribeRaidLootMaker</raidLootMaker>
    <raidLootValueFromPointsCurve>
      <points>
        <li>(35,     15)</li>
        <li>(100,   120)</li>
        <li>(1000,  500)</li>
        <li>(2000,  800)</li>
        <li>(4000, 1000)</li>
      </points>
    </raidLootValueFromPointsCurve>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>10</TraderClanratLow>
          <TraderClanratRanged>5</TraderClanratRanged>
          <TraderClanratHigh>4</TraderClanratHigh>
          <TraderClanratSkryreMid>1</TraderClanratSkryreMid>
          <TraderClanratHighGun>1</TraderClanratHighGun>
          <RatOgreBladeEshin>1</RatOgreBladeEshin>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Miners</kindDef>
        <commonality>1</commonality>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>3</TraderClanratLow>
          <TraderClanratRanged>2</TraderClanratRanged>
          <RatWolfEshin>1</RatWolfEshin>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Hunters</kindDef>
        <commonality>1</commonality>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>1</TraderClanratLow>
          <TraderClanratRanged>5</TraderClanratRanged>
          <RatWolfEshin>2</RatWolfEshin>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Loggers</kindDef>
        <commonality>1</commonality>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>2</TraderClanratLow>
          <TraderClanratRanged>3</TraderClanratRanged>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Farmers</kindDef>
        <commonality>1</commonality>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>3</TraderClanratLow>
          <TraderClanratRanged>2</TraderClanratRanged>
          <RatWolfEshin>1</RatWolfEshin>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <TraderClanratTrader>1</TraderClanratTrader>
        </traders>
        <carriers>
          <Muffalo>1</Muffalo>
        </carriers>
        <guards>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>10</TraderClanratLow>
          <TraderClanratRanged>5</TraderClanratRanged>
          <TraderClanratHigh>3</TraderClanratHigh>
          <TraderClanratHighGun>2</TraderClanratHighGun>
          <TraderClanratSkryreMid>2</TraderClanratSkryreMid>
          <RatOgreBladeEshin>1</RatOgreBladeEshin>
          <MoulderRatMid>2</MoulderRatMid>
          <RatOgreAbomEshin>2</RatOgreAbomEshin>
          <RatWolfEshin>3</RatWolfEshin>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>10</TraderClanratLow>
          <TraderClanratRanged>7</TraderClanratRanged>
          <TraderClanratHigh>5</TraderClanratHigh>
          <RatOgreBladeEshin>2</RatOgreBladeEshin>
          <TraderClanratHighGun>2</TraderClanratHighGun>
          <TraderClanratSkryreMid>2</TraderClanratSkryreMid>
          <TraderClanratTrader>1</TraderClanratTrader>
        </options>
      </li>
      <li>
        <!-- Normal fights -->
        <kindDef>Combat</kindDef>
        <commonality>40</commonality>
        <disallowedStrategies>
          <li>Siege</li>
        </disallowedStrategies>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratLow>10</TraderClanratLow>
          <TraderClanratRanged>7</TraderClanratRanged>
          <TraderClanratHigh>5</TraderClanratHigh>
          <TraderClanratHighGun>2</TraderClanratHighGun>
          <TraderClanratSkryreMid>2</TraderClanratSkryreMid>
          <RatOgreBladeEshin>1</RatOgreBladeEshin>
        </options>
      </li>
      <li>
        <!-- ranged only -->
        <kindDef>Combat</kindDef>
        <commonality>30</commonality>
        <disallowedStrategies>
          <li>Siege</li>
        </disallowedStrategies>
        <options>
          <TraderSlaveRatMissile>7</TraderSlaveRatMissile>
          <TraderClanratRanged>10</TraderClanratRanged>
          <TraderClanratHighGun>2</TraderClanratHighGun>
          <TraderClanratSkryreMid>2</TraderClanratSkryreMid>
          <TraderClanratTrader>1</TraderClanratTrader>
        </options>
      </li>
      <li>
        <!-- Elite -->
        <kindDef>Combat</kindDef>
        <commonality>20</commonality>
        <disallowedStrategies>
          <li>Siege</li>
        </disallowedStrategies>
        <options>
          <TraderClanratRanged>7</TraderClanratRanged>
          <TraderClanratHigh>5</TraderClanratHigh>
          <TraderClanratHighGun>2</TraderClanratHighGun>
          <TraderClanratSkryreMid>2</TraderClanratSkryreMid>
          <RatOgreBladeEshin>3</RatOgreBladeEshin>
          <TraderClanratTrader>1</TraderClanratTrader>
        </options>
      </li>
    </pawnGroupMakers>
  </FactionDef>

</Defs>