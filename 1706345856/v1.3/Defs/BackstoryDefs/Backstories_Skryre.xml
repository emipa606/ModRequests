<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <AlienRace.BackstoryDef>
    <defName>SkavenEngiSkryre</defName>
    <title>Skryre engineer</title>
    <titleShort>engineer</titleShort>
    <baseDescription>[PAWN_nameDef] works in the Warpforges of clan Skryre. Performing the duties of a engineer, [PAWN_pronoun] spends [PAWN_possessive] time constructing new arsenal for the clan.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Thin</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Crafting</defName>
        <amount>5</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>2</amount>
      </li>
    </skillGains>
    <forcedTraits>
      <li>
        <defName>FastLearner</defName>
        <chance>10</chance>
      </li>
      <li>
        <defName>Industriousness</defName>
        <degree>2</degree>
        <chance>10</chance>
      </li>
      <li>
        <defName>Industriousness</defName>
        <degree>1</degree>
        <chance>10</chance>
      </li>
    </forcedTraits>
    <forcedHediffs>
      <li>SkavenMoulderWarpSpine</li>
    </forcedHediffs>
    <spawnCategories>
      <li>SkavenSkryre</li>
      <li>SkavenEducated</li>
      <li>SkavenEngineer</li>
    </spawnCategories>
    <requiredWorkTags>
      <li>Intellectual</li>
      <li>Violent</li>
    </requiredWorkTags>
  </AlienRace.BackstoryDef>

  <AlienRace.BackstoryDef>
    <defName>SkavenTechSkryre</defName>
    <title>Skryre technician</title>
    <titleShort>technician</titleShort>
    <baseDescription>[PAWN_nameDef] works as general technician for clan Skryre. When not out in field work [PAWN_pronoun] maintain the Warpforges making sure that they are always operational.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Thin</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Intellectual</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>5</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>3</amount>
      </li>
    </skillGains>
    <forcedTraits>
      <li>
        <defName>Industriousness</defName>
        <degree>2</degree>
        <chance>10</chance>
      </li>
      <li>
        <defName>Industriousness</defName>
        <degree>1</degree>
        <chance>10</chance>
      </li>
    </forcedTraits>
    <spawnCategories>
      <li>SkavenSkryre</li>
      <li>SkavenEducated</li>
      <li>SkavenEngineer</li>
    </spawnCategories>
    <requiredWorkTags>
      <li>Intellectual</li>
      <li>Violent</li>
      <li>Crafting</li>
    </requiredWorkTags>
  </AlienRace.BackstoryDef>

  <AlienRace.BackstoryDef>
    <defName>SkavenSkryreAssistant</defName>
    <title>skryre assistant</title>
    <titleShort>assistant</titleShort>
    <baseDescription>[PAWN_nameDef] is a assistant to higher ups in clan Skryre. Often [PAWN_pronoun] find [PAWN_possessive]self out in the fields acting as the lab rat of the latest inventions, when [PAWN_pronoun] [PAWN_possessive]self isn't kneedeep in their own work.</baseDescription>
    <bodyTypeMale>Thin</bodyTypeMale>
    <bodyTypeFemale>Thin</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Intellectual</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>4</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>2</amount>
      </li>
    </skillGains>
    <forcedTraits>
      <li>
        <defName>FastLearner</defName>
        <chance>10</chance>
      </li>
      <li>
        <defName>Nerves</defName>
        <degree>-2</degree>
        <chance>10</chance>
      </li>
      <li>
        <defName>Nerves</defName>
        <degree>-1</degree>
        <chance>10</chance>
      </li>
    </forcedTraits>
    <spawnCategories>
      <li>SkavenSkryre</li>
      <li>SkavenEducated</li>
      <li>SkavenEngineer</li>
    </spawnCategories>
    <requiredWorkTags>
      <li>Crafting</li>
      <li>Intellectual</li>
      <li>Violent</li>
    </requiredWorkTags>
  </AlienRace.BackstoryDef>

