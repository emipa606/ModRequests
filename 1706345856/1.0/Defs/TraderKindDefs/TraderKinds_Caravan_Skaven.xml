<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Caravan_Skaven_BulkGoods</defName>
    <label>skaven salvage trader</label>
    <commonality>2</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>
          <min>750</min>
          <max>1200</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>
          <min>30</min>
          <max>40</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentSpacer</thingDef>
        <countRange>
          <min>-10</min>
          <max>2</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>
          <min>250</min>
          <max>400</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>
          <min>250</min>
          <max>400</max>
        </countRange>
      </li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
				<countRange>
					<min>250</min>
					<max>400</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Neutroamine</thingDef>
				<countRange>
					<min>70</min>
					<max>160</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Beer</thingDef>
				<totalPriceRange>
					<min>-100</min>
					<max>100</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chocolate</thingDef>
				<totalPriceRange>
					<min>10</min>
					<max>40</max>
				</totalPriceRange>
			</li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
				<excludedCategories><li>EggsFertilized</li></excludedCategories>
        <thingDefCountRange>
          <min>3</min>
          <max>3</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>150</min>
          <max>600</max>
        </totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>BuildingsFurniture</categoryDef>
        <thingDefCountRange>
          <min>3</min>
          <max>3</max>
        </thingDefCountRange>
        <countRange>
          <min>1</min>
          <max>1</max>
        </countRange>
      </li>
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>TradeSkaven_Ogre</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
          <li>TradeSkaven_Ogre</li>
        </tradeTagsBuy>
        <kindCountRange>
          <min>1</min>
          <max>3</max>
        </kindCountRange>
        <countRange>
          <min>1</min>
          <max>3</max>
        </countRange>
      </li>
      <li Class="StockGenerator_Clothes">
        <countRange>
          <min>10</min>
          <max>10</max>
        </countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
      </li>
			<li Class="StockGenerator_Category">
				<categoryDef>Textiles</categoryDef>
			</li>
			<li Class="StockGenerator_BuyArt" />
			<li Class="StockGenerator_BuyWeirdOrganic" />
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
          <li>Pemmican</li>
        </thingDefs>
      </li>
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>
          <min>2</min>
          <max>3</max>
        </countRange>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>
