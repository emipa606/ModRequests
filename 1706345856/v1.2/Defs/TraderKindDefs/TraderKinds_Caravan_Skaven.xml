<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Caravan_Skaven_BulkGoods</defName>
    <label>skaven salvage trader</label>
    <commonality>2</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>700~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>5~15</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Jade</thingDef>
        <countRange>50~100</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>200~400</countRange>
      </li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
        <countRange>50~200</countRange>
			</li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
				<excludedCategories><li>EggsFertilized</li></excludedCategories>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>40~100</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>BuildingsFurniture</categoryDef>
        <thingDefCountRange>1~4</thingDefCountRange>
        <countRange>1~4</countRange>
      </li>
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>TradeSkaven_Ogre</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
          <li>TradeSkaven_Ogre</li>
        </tradeTagsBuy>
          <kindCountRange>1~3</kindCountRange>
          <countRange>1~4</countRange>
      </li>
      <li Class="StockGenerator_Clothes">
        <countRange>2~8</countRange>
      </li>
      <li Class="StockGenerator_Armor">
        <countRange>2~4</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>200~800</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>2~5</countRange>
      </li>
    </stockGenerators>
  </TraderKindDef>
  
  <TraderKindDef>
    <defName>Caravan_Skaven_MoulderPet</defName>
    <label>skaven Moulder goods</label>
    <commonality>2</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>700~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Jade</thingDef>
        <countRange>50~100</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>5~15</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>200~400</countRange>
      </li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
        <countRange>50~200</countRange>
			</li>
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>TradeSkaven_Ogre</li>
          <li>TradeSkaven_Moulder</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
          <li>TradeSkaven_Ogre</li>
        </tradeTagsBuy>
          <kindCountRange>2~4</kindCountRange>
          <countRange>4~6</countRange>
      </li>
      <li Class="StockGenerator_Clothes">
        <countRange>2~8</countRange>
      </li>
      <li Class="StockGenerator_Armor">
        <countRange>2~4</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>200~800</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>2~5</countRange>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>
