<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Logann Race</li>
			<li>Logann Race (Continued)</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Wolvx"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Wolvx"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Wolvx"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Tail"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(0,0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Wolvx"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Tail"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(0,-0.42)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Wolvx"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Tail"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0.36,-0.50)</FemaleBB>
					</value>
				</li>
			</operations>
		</match>
	</Operation>




</Patch>