<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Star Wars Races</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Rodian"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Rodian"]/alienRace/generalSettings/alienPartGenerator</xpath>
					<value>
						<alienbodytypes>
							<li>FemaleBB</li>
							<li>Female</li>
							<li>Male</li>
							<li>Fat</li>
							<li>Thin</li>
							<li>Hulk</li>
						</alienbodytypes>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator</xpath>
					<value>
						<alienbodytypes>
							<li>FemaleBB</li>
							<li>Female</li>
							<li>Male</li>
							<li>Fat</li>
							<li>Thin</li>
							<li>Hulk</li>
						</alienbodytypes>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left lekku"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0,0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="StarWarsRaces_Togruta_Right_Lekku"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0,0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Twilek"]/alienRace/generalSettings/alienPartGenerator</xpath>
					<value>
						<alienbodytypes>
							<li>FemaleBB</li>
							<li>Female</li>
							<li>Male</li>
							<li>Fat</li>
							<li>Thin</li>
							<li>Hulk</li>
						</alienbodytypes>
					</value>
				</li>
			</operations>
		</match>
	</Operation>




</Patch>