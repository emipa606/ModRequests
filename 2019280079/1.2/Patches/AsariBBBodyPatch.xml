<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>RimEffect - Asari of the Rims Continued [1.1][1.2]</li>
			<li>xen.races.asari</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Asari"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Asari"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>

						<li>FemaleBB</li>

					</value>
				</li>
			</operations>
		</match>
	</Operation>




</Patch>