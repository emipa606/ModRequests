<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[O21] Tieflings</li>
			<li>neronix17.fr.race.tieflings</li>
			<li>[O21] Forgotten Realms</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="O21_Alien_Tiefling"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="O21_Alien_Tiefling"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="O21_Alien_Tiefling"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Head"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.840, 0.783)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="O21_Alien_Tiefling"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Head"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.420, 1.160)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="O21_Alien_Tiefling"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Head"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.850, 0.770)</FemaleBB>
					</value>
				</li>
			</operations>
		</match>
	</Operation>




</Patch>