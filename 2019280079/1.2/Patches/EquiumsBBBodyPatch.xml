<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Equiums Anything Goes</li>
			<li>Equiums</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Equium"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Equium"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Equium_Tail"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.4,0.1)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Equium_Tail"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.03,0.10)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="StarWarsRaces_Togruta"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Equium_Tail"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(-0.04,-0.01)</FemaleBB>
					</value>
				</li>
			</operations>
		</match>
	</Operation>




</Patch>