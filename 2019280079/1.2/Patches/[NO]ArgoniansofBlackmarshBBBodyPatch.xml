<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Argonians of Blackmarsh++++</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Never</success>
			<!-- notready need body tex -->
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Argonian"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Argonian"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>




</Patch>