<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Erin's Sephyr</li>
			<li>Erin.Sephyr</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ERN_Sephyr"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ERN_Sephyr"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ERN_Sephyr"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Waist"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ERN_Sephyr"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Waist"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, -0.70)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ERN_Sephyr"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Waist"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0.65, -0.2)</FemaleBB>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>




</Patch>