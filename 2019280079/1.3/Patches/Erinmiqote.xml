<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Erin's Miqo'te</li>
			<li>Erin.Miqote</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator</xpath>
					<value>
					<alienbodytypes>
						<li>FemaleBB</li>
						<li>Female</li>
						<li>Male</li>
						<li>Thin</li>
						<li>Fat</li>
					</alienbodytypes>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear"]/offsets/south/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, 0)</FemaleBB>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="MQT_MiqoteTail"]/offsets/north/bodyTypes</xpath>
					<value>
						<FemaleBB>(0, -0.2)</FemaleBB>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="MQT_Miqote"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="MQT_MiqoteTail"]/offsets/east/bodyTypes</xpath>
					<value>
						<FemaleBB>(0.21, -0.17)</FemaleBB>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>




</Patch>