<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Androids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ChjAndroid"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes[li="FemaleBB"]</xpath>
					<success>Invert</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName = "ChjAndroid"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
					<value>
						<li>FemaleBB</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>