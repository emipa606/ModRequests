<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <GeneDef>
    <defName>SZGene_MingShe_WindWing</defName>
    <label>Ming Snake Wind Shield</label>
    <description>The Snake Wind Shield is obtained by extracting the gene from the Snake Snake corpse. It inherits the Snake Snake's ability. Enemies within the range of the Snake Snake will receive continuous cutting damage. object) will be bounced.</description>
    <iconPath>AncientChineseBeast/MingShe/WindWingIcon</iconPath>
    <displayCategory>Archite</displayCategory>
    <marketValueFactor>1.5</marketValueFactor>
    <abilities>
      <li>SZ_MingShe_WindWing</li>
    </abilities>
    <descriptionHyperlinks>
      <AbilityDef>SZ_MingShe_WindWing</AbilityDef>
    </descriptionHyperlinks>
    <renderNodeProperties>
      <li>
        <texPath>AncientChineseBeast/MingShe/Wing</texPath>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <parentTagDef>Body</parentTagDef>
        <drawSize>2</drawSize>
        <drawData>
          <dataNorth>
            <offset>(0, 0.3, 0)</offset>
          </dataNorth>
          <dataSouth>
            <offset>(0, -0.3, 0)</offset>
          </dataSouth>
          <dataWest>
            <flip>true</flip>
          </dataWest>
        </drawData>
      </li>
    </renderNodeProperties>
    <biostatCpx>10</biostatCpx>
    <biostatMet>-4</biostatMet>
    <biostatArc>1</biostatArc>
    <symbolPack>
      <wholeNameSymbols>
        <li>
          <symbol>鸣蛇的祝福</symbol>
        </li>
        <li>
          <symbol>鸣蛇的恩赐</symbol>
        </li>
      </wholeNameSymbols>
    </symbolPack>
    <randomChosen>false</randomChosen>
  </GeneDef>

  <GeneDef>
    <defName>SZGene_MingShe_SoundWave</defName>
    <label>Ming Snake Sound Wave</label>
    <description>Extracted by genetic engineering, the sound waves of the Snake can make everyone who is hit by its sound waves feel dizzy.</description>
    <iconPath>AncientChineseBeast/MingShe/SoundWaveIcon</iconPath>
    <displayCategory>Archite</displayCategory>
    <marketValueFactor>1.5</marketValueFactor>
    <abilities>
      <li>SZ_MingShe_SoundWave</li>
    </abilities>
    <descriptionHyperlinks>
      <AbilityDef>SZ_MingShe_SoundWave</AbilityDef>
    </descriptionHyperlinks>
    <biostatCpx>10</biostatCpx>
    <biostatMet>-3</biostatMet>
    <biostatArc>1</biostatArc>
    <symbolPack>
      <wholeNameSymbols>
        <li>
          <symbol>鸣蛇的祝福</symbol>
        </li>
        <li>
          <symbol>鸣蛇的恩赐</symbol>
        </li>
      </wholeNameSymbols>
    </symbolPack>
    <randomChosen>false</randomChosen>
  </GeneDef>

  <GeneDef>
    <defName>SZGene_MingShe_Teeth</defName>
    <label>Snake-tooth</label>
    <description>The deadly fangs of the chirping snake, so poisonous that anyone who doubts its power will pay with their lives.</description>
    <iconPath>AncientChineseBeast/MingShe/TeethIcon</iconPath>
    <displayCategory>Archite</displayCategory>
    <marketValueFactor>1.5</marketValueFactor>
    <geneClass>AncientChineseBeast.Gene_Hediffs</geneClass>
    <descriptionHyperlinks>
      <HediffDef>SZ_QiongQi_Scratch</HediffDef>
    </descriptionHyperlinks>
    <modExtensions>
      <li Class="AncientChineseBeast.DefModExtension_GeneHediffs">
        <hediffs>
          <li>
            <bodyPartGroup>Teeth</bodyPartGroup>
            <hediff>SZ_MingShe_Teeth</hediff>
          </li>
        </hediffs>
      </li>
    </modExtensions>
    <biostatCpx>10</biostatCpx>
    <biostatMet>-2</biostatMet>
    <biostatArc>1</biostatArc>
    <symbolPack>
      <wholeNameSymbols>
        <li>
          <symbol>鸣蛇的祝福</symbol>
        </li>
        <li>
          <symbol>鸣蛇的恩赐</symbol>
        </li>
      </wholeNameSymbols>
    </symbolPack>
    <randomChosen>false</randomChosen>
  </GeneDef>
</Defs>