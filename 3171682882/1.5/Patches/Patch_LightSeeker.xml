<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Lost Psycasts - LightSeeker</li>     
        </mods>
        <match Class="PatchOperationAdd">
			<xpath>/Defs/VanillaPsycastsExpanded.PsycasterPathDef[defName="LPLS_LightSeeker"]</xpath>
			<value>
				<lockedReason>Lack of gene</lockedReason>
				<requiredGene>
					<GeneDef>Gene_LightSeeker</GeneDef>
				</requiredGene>
			</value>
		</match>
	</Operation>
</Patch>