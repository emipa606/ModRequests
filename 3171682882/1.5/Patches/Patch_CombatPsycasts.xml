<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Combat Psycasts</li>     
        </mods>
        <match Class="PatchOperationAdd">
			<xpath>/Defs/VanillaPsycastsExpanded.PsycasterPathDef[defName="CP_Combat"]</xpath>
