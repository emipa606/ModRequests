<?xml version="1.0" encoding="utf-8"?>
<Defs>
<AlienRace.RaceSettings>
    <defName>SWSR_SpongeBobRaceSettings</defName>
<pawnKindSettings>
	<alienslavekinds>
        <li>
            <kindDefs>
                <li>SWSR_SpongeBobishSlave</li>
            </kindDefs>
            <chance>10.0</chance>
        </li>
    </alienslavekinds>
    <alienrefugeekinds>
        <li>
            <kindDefs>
                <li>SWSR_SpongeBobishRefugee</li>
            </kindDefs>
            <chance>10.0</chance>
        </li>
    </alienrefugeekinds>
    <startingColonists>
        <li>
            <pawnKindEntries>
                <li>
                    <kindDefs>
                        <li>SWSR_SpongeBobishColonist</li>
                    </kindDefs>
                    <chance>20.0</chance>
                </li>
            </pawnKindEntries>
            <factionDefs>
                <li>PlayerColony</li> 
            </factionDefs>
        </li>
    </startingColonists>
    <alienwandererkinds>
        <li>
            <pawnKindEntries>
                <li>
                    <kindDefs>
                        <li>SWSR_SpongeBobishWanderer</li>
                    </kindDefs>
                    <chance>10.0</chance>
                </li>
            </pawnKindEntries>
            <factionDefs>
                <li>PlayerColony</li> 
            </factionDefs>
        </li>
    </alienwandererkinds>
</pawnKindSettings>
</AlienRace.RaceSettings>

<PawnKindDef ParentName="BasePlayerPawnKind">
    <defName>SWSR_SpongeBobishColonist</defName>
    <label>SpongeBob SquarePants</label>
    <race>SWSR_SpongeBob</race>
	<defaultFactionType>PlayerColony</defaultFactionType>
    <chemicalAddictionChance>0.06</chemicalAddictionChance>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelMoney>350~600</apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>50~800</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <initialResistanceRange>13~21</initialResistanceRange>
</PawnKindDef>

<PawnKindDef>
    <defName>SWSR_SpongeBobishSlave</defName>
    <label>SpongeBob SquarePants</label>
    <race>SWSR_SpongeBob</race>
	<combatPower>30</combatPower>
	<baseRecruitDifficulty>0.30</baseRecruitDifficulty>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>Offworld</li>
          <li>Outlander</li>
          <li>Outsider</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <defaultFactionType>OutlanderCivil</defaultFactionType>
    <itemQuality>Poor</itemQuality>
    <backstoryCryptosleepCommonality>0.10</backstoryCryptosleepCommonality>
    <isFighter>false</isFighter>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <gearHealthRange>0.2~0.8</gearHealthRange>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelMoney>0~350</apparelMoney>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>50~800</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <invNutrition>1</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
    <initialWillRange>0~0</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
</PawnKindDef>

<PawnKindDef ParentName="BasePlayerPawnKind">
    <defName>SWSR_SpongeBobishWanderer</defName>
    <label>SpongeBob SquarePants</label>
    <race>SWSR_SpongeBob</race>
	<defaultFactionType>PlayerColony</defaultFactionType>
    <chemicalAddictionChance>0.06</chemicalAddictionChance>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelMoney>350~600</apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>50~800</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <initialResistanceRange>13~21</initialResistanceRange>
</PawnKindDef>

<PawnKindDef ParentName="RefugeeBase">
    <defName>SWSR_SpongeBobishRefugee</defName>
    <label>SpongeBob SquarePants</label>
    <race>SWSR_SpongeBob</race>
	<combatPower>35</combatPower>
    <invNutrition>0</invNutrition>
    <gearHealthRange>0.2~0.6</gearHealthRange>
    <itemQuality>Poor</itemQuality>
    <apparelMoney>90~280</apparelMoney>
    <apparelAllowHeadgearChance>0.2</apparelAllowHeadgearChance>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <weaponMoney>60~200</weaponMoney>
    <weaponTags>
      <li>MedievalMeleeBasic</li>
      <li>NeolithicMeleeBasic</li>
    </weaponTags>
    <techHediffsMoney>50~50</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <techHediffsChance>0.15</techHediffsChance>
    <inventoryOptions>
      <skipChance>0.9</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>30~70</countRange>
        </li>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>1</countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>8~14</initialResistanceRange>
</PawnKindDef>
</Defs>