<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>SWSR_SpongeBob</defName>
    <label>spongebob</label>
    <description>SpongeBob is a childish, joyful, and clumsy sponge, who is good-natured and cherish hanging out with friends. He is a terrible doctor, so don't let him tend to your patients unless you absolutely need to. And dont put him in fire. He will quickly dry out and die.</description>
    <alienRace>
        <generalSettings>
			<maleGenderProbability>1</maleGenderProbability>
			<humanRecipeImport>true</humanRecipeImport>
			<allowHumanBios>true</allowHumanBios>
			<forcedRaceTraitEntries>
				<li>
                    <defName>NaturalMood</defName>
                    <degree>2</degree>
                </li>
                <li>
                    <defName>Gay</defName>
                    <chance>100</chance>
                </li>
                <li>
                    <defName>Neurotic</defName>
					<degree>1</degree>
                </li>
            </forcedRaceTraitEntries>
			<traitCount>1~2</traitCount>
			<minAgeForAdulthood>16</minAgeForAdulthood>
			<alienPartGenerator>
				<aliencrowntypes>
					<li>SpongeHead</li>
                </aliencrowntypes>
				<alienbodytypes>
					<li>SpongeBody</li>
				</alienbodytypes>
				<colorChannels>
    <li>
        <name>skin</name>
        <first Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>10</weight>
                    <only>RGBA(1.000, 1.000, 0.000, 1.000)</only>
                </li>
                <li>
                    <weight>10</weight>
                    <only>RGBA(0.500, 0.500, 0.000, 1.000)</only>
                </li>
                <li>
                    <weight>10</weight>
                    <only>RGBA(0.800, 0.800, 0.000, 1.000)</only>
                </li>
            </options>
        </first>
        <second Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>10</weight>
                    <only>RGBA(0.500, 1.000, 0.000, 1.000)</only>
                </li>
            </options>
        </second>
    </li>
</colorChannels>
			</alienPartGenerator>
        </generalSettings>
        <graphicPaths>
			<li>
				<body>Things/Pawn/SpongeBob/Bodies/</body>
				<head>Things/Pawn/SpongeBob/Heads/</head>
				<skeleton>Things/Pawn/SpongeBob/Bodies/Dessicated/Dessicated_SpongeBody</skeleton>
				<skull>Things/Pawn/SpongeBob/Heads/None_SpongeHead_Skull</skull>
				<stump>Things/Pawn/SpongeBob/Heads/None_SpongeHead_Stump</stump>
				<customDrawSize>(0.50, 0.70)</customDrawSize>
				<customPortraitDrawSize>(0.50, 0.70)</customPortraitDrawSize>
				<customHeadDrawSize>(1.0, 0.80)</customHeadDrawSize>
				<customPortraitHeadDrawSize>(1.0, 0.80)</customPortraitHeadDrawSize>
				<headOffset>(0.0, 0.0)</headOffset>
				<headOffsetDirectional>
					<north>(-0.02, 0.36)</north>
					<east>(0.10, 0.30)</east>
					<south>(0.02, 0.35)</south>
					<west>(-0.10, 0.30)</west>
				</headOffsetDirectional>
				<skinShader>CutoutSkin</skinShader>
			</li>
        </graphicPaths>
		<thoughtSettings>
				<cannotReceiveThoughts>
					<li>SoakingWet</li>
				</cannotReceiveThoughts>
        </thoughtSettings>
        <styleSettings>
				<li>
                    <key>HairDef</key>
                    <value>
                        <hasStyle>false</hasStyle>
                    </value>
                </li>
                <li>
                    <key>BeardDef</key>
                    <value>
						<styleTagsOverride>
							<li>NoBeard</li>
                        </styleTagsOverride>
                    </value>
                </li>
        </styleSettings>
        <relationSettings>
		<!-- Blank for now but add Bobs friends -->
        </relationSettings>
        <raceRestriction>
		<onlyUseRaceRestrictedApparel>true</onlyUseRaceRestrictedApparel>
			<apparelList>
				<li>Apparel_SquarePants</li>
			</apparelList>
			<whiteApparelList>
				<li>Apparel_AdvancedHelmet</li>
