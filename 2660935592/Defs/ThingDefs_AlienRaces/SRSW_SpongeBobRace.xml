        <compatibility>
			<isFlesh>false</isFlesh>
			<isSentient>true</isSentient> 
			<hasBlood>false</hasBlood>
        </compatibility>
    </alienRace>
	<statBases>
			<MarketValue>1200</MarketValue>
			<Flammability>2.0</Flammability>
			<MoveSpeed>4</MoveSpeed>
			<ComfyTemperatureMax>25</ComfyTemperatureMax>
			<ComfyTemperatureMin>1</ComfyTemperatureMin>
			<ImmunityGainSpeed>1.2</ImmunityGainSpeed>
			<CarryingCapacity>40</CarryingCapacity>
			<MeatAmount>0</MeatAmount>
			<LeatherAmount>0</LeatherAmount>
			<Mass>15</Mass>
			<FilthRate>0</FilthRate>
			<PsychicSensitivity>1.0</PsychicSensitivity>
			<ToxicSensitivity>0.25</ToxicSensitivity>
			<MeleeDodgeChance>1.0</MeleeDodgeChance>
			<SocialImpact>1</SocialImpact>
			<InjuryHealingFactor>5.0</InjuryHealingFactor>
			<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>3.0</ArmorRating_Blunt>
			<ArmorRating_Heat>0.0</ArmorRating_Heat>
			<GeneralLaborSpeed>1.2</GeneralLaborSpeed>
			<MedicalTendSpeed>0.5</MedicalTendSpeed>
			<MedicalTendQuality>0.5</MedicalTendQuality>
			<MedicalSurgerySuccessChance>0.5</MedicalSurgerySuccessChance>
			<RoyalFavorValue>4</RoyalFavorValue>
	</statBases>
	<comps>
	  <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>SpongeCubeA</eggFertilizedDef>
		<eggUnfertilizedDef>SpongeCubeB</eggUnfertilizedDef>
		<eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>1</eggLayIntervalDays>
        <eggCountRange>1</eggCountRange>
		<eggLayFemaleOnly>false</eggLayFemaleOnly>
      </li>
	</comps>
	<tools>
			<li>
				<label>left fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>4.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>5</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>4.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>5</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>teeth</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>15.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.07</chanceFactor>
				<soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
				<soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
	<race>
	  <thinkTreeMain>Humanlike</thinkTreeMain>
	  <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
	  <intelligence>Humanlike</intelligence>
	  <makesFootprints>true</makesFootprints>
	  <lifeExpectancy>130</lifeExpectancy>
	  <nameCategory>HumanStandard</nameCategory>
	  <hasGenders>True</hasGenders>
	  <body>Human</body>
	  <baseHungerRate>0.0</baseHungerRate>
	  <baseBodySize>1</baseBodySize>
	  <baseHealthScale>0.8</baseHealthScale>
	  <gestationPeriodDays>5</gestationPeriodDays>
	  <manhunterOnDamageChance>1.0</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.01</manhunterOnTameFailChance>
      <lifeStageAges>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>18</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <soundMeleeDodge>Pawn_MeleeDodge</soundMeleeDodge>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(50,100)</li>
          <li>(60,30)</li>
          <li>(70,18)</li>
          <li>(80,10)</li>
          <li>(90,3)</li>
          <li>(100,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
        <li>Human</li>
		<li>SWSR_SpongeBob</li>
      </hediffGiverSets>
    </race>
	
    <recipes>
