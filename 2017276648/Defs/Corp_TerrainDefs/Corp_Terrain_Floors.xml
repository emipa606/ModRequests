<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 

  <!-- ================= Tiles ================= -->

  <TerrainDef ParentName="FloorBase">
    <defName>Corp_PlasteelTile</defName>
    <label>plasteel tile</label>
    <renderPrecedence>244</renderPrecedence>
    <description>Sleek plasteel tiles for that high-tech spacer look.  Beautiful to look at, and almost as clean as sterile tiles!</description>
    <texturePath>Terrain/Surfaces/GenericFloorTile</texturePath>
    <color>(0.627, 0.698, 0.710)</color>
    <statBases>
      <WorkToBuild>800</WorkToBuild>
      <Beauty>8</Beauty>
      <Cleanliness>0.5</Cleanliness>
    </statBases>
    <costList>
      <Plasteel>7</Plasteel>
    </costList>
	<tags>
      <li>FineFloor</li>
    </tags>
    <constructEffect>ConstructMetal</constructEffect>
    <designationHotKey>Misc7</designationHotKey>
    <researchPrerequisites>
      <li>Smithing</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>3</constructionSkillPrerequisite>
  </TerrainDef>
  
