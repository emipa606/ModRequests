          <li>Cloth</li>
		  <li>Synthread</li>
		  <li>Hyperweave</li>
		  <li>DevilstrandCloth</li>
        </excludedThingDefs>
      </li>
	  
	  <!-- Slaves -->
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>-5~10</countRange>
      </li>
	  
	  <!-- Resources, Expensive-->
	  <li Class="StockGenerator_SingleDef">
