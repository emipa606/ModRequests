<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>CORP_Base_StandardOld</defName>
    <faction>CORP_Corporation</faction>
    <permitRequiredForTrading>Corp_TradeSettlement</permitRequiredForTrading>
    <stockGenerators>
      
	  <!-- Silver, the main currency -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>2000~8000</countRange>
      </li>
	  
	  <!-- Resources, Very Cheap-->
	  <li Class="StockGenerator_SingleDef">
        <thingDef>Hay</thingDef>
        <countRange>500~2000</countRange>
		<price>VeryCheap</price>
      </li>
	  
	  <!-- Resources, Cheap-->
	  <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>500~1000</countRange>
		<price>Cheap</price>
      </li>
	  <li Class="StockGenerator_Category">
        <categoryDef>StoneBlocks</categoryDef>
        <thingDefCountRange>10~10</thingDefCountRange>
