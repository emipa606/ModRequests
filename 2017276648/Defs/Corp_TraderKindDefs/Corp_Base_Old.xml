	  <li Class="StockGenerator_Category">
        <categoryDef>MeatRaw</categoryDef>
        <thingDefCountRange>1~5</thingDefCountRange>
        <countRange>200~300</countRange>
		<price>Cheap</price>
      </li>
	  
	  <!-- Resources, Normal-->
	  <li Class="StockGenerator_Category">
        <categoryDef>AnimalProductRaw</categoryDef>
