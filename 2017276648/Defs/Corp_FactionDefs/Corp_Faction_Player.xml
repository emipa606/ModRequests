<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <FactionDef ParentName="PlayerFactionBase">
    <defName>Corp_PlayerColony</defName>
    <label>Startup Company</label>
    <description>A new start-up company sent to this rimworld to set up production and trade.  They've lost contact with their employers and are now freelancers.</description>
    <isPlayer>true</isPlayer>
    <basicMemberKind>Corp_Colonist</basicMemberKind>
    <pawnSingular>employee</pawnSingular>
    <pawnsPlural>employees</pawnsPlural>
    <techLevel>Spacer</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>ImperialCommon</li>
        </categories>
		<commonality>0.05</commonality>
		</li>
		<li>
        <categories>
          <li>Outlander</li>
        </categories>
        <commonality>0.04</commonality>
      </li>
	  <li>
        <categories>
          <li>Offworld</li>
        </categories>
		<commonality>0.90</commonality>
      </li>
	  <li>
        <categories>
          <li>Pirate</li>
        </categories>
		<commonality>0.01</commonality>
      </li>
    </backstoryFilters>
    <factionIconPath>World/WorldObjects/Expanding/Town</factionIconPath>
    <playerInitialSettlementNameMaker>CORP_NamerSettlementCorporate</playerInitialSettlementNameMaker>
    <factionNameMaker>CORP_NamerFactionCorporate</factionNameMaker>
    <settlementNameMaker>CORP_NamerSettlementCorporate</settlementNameMaker>
	<pawnNameMaker>CORP_NamerPersonCorporate</pawnNameMaker>
    <pawnNameMakerFemale>CORP_NamerPersonCorporateFemale</pawnNameMakerFemale>
    <geneticVariance>1</geneticVariance>
    <startingResearchTags>
      <li>ClassicStart</li>
    </startingResearchTags>
    <startingTechprintsResearchTags>
      <li>ClassicStart</li>
      <li>ClassicStartTechprints</li>
    </startingTechprintsResearchTags>
    <hairTags>
      <li>Urban</li>
	  <li>Rural</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
		<li>Hyperweave</li>
		<li>Plasteel</li>
      </thingDefs>
    </apparelStuffFilter>
  </FactionDef>

</Defs>
