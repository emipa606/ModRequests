  <disableMainThemeSelector>false</disableMainThemeSelector>    <!-- [2019R2 min] : If true hide the theme selector in the main menu (bottom right), by default true -->
  <menuAlignment>right</menuAlignment>							<!-- The alignment of RimWorld's main menu (can be left, middle, right), by default right) -->
  
