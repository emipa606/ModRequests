    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workAmount>10000</workAmount>
    <allowMixingIngredients>true</allowMixingIngredients>
    <targetCountAdjustment>100</targetCountAdjustment>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
	  <li>Campfire</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MedicineHerbal</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>200</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MedicineHerbal</li>
		<li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <GY_MP>100</GY_MP>
    </products>
    <workSkill>Cooking</workSkill>
	<skillRequirements>
      <Cooking>8</Cooking>
    </skillRequirements>
  </RecipeDef>
