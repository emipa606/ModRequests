<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ResourceBase">
		<defName>Turn_ComponentMedical</defName>
		<label>medical component</label>
		<description>A collection of sterile circuits and machinery, designed to interface with organic anatomy.</description>
		<graphicData>
			<texPath>Things/Item/Resource/ComponentMedical</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>50</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<statBases>
			<MaxHitPoints>70</MaxHitPoints>
			<MarketValue>280</MarketValue>
			<Mass>0.6</Mass>
			<Flammability>0.6</Flammability>
			<DeteriorationRate>2.0</DeteriorationRate>
		</statBases>
		
		<!-- If false, this item can be a smelting product. -->
		<intricate>true</intricate>
		
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<tradeTags>
			<li>ExoticMisc</li>
		</tradeTags>
		<thingSetMakerTags><li>RewardStandardHighFreq</li></thingSetMakerTags>
	</ThingDef>

</Defs>