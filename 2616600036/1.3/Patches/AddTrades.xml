<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Traders that carry advanced components will carry medical components. -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TraderKindDef/stockGenerators[li/thingDef="ComponentSpacer"]</xpath>
		<value>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Turn_ComponentMedical</thingDef>
				<countRange>-5~3</countRange>
			</li>
		</value>
	</Operation>

</Patch>