<?xml version="1.0" encoding="utf-8" ?>
<Defs>


<ThingDef ParentName="BuildingBase">
    <defName>Building_ExecutionSpot</defName>
    <label>ExecutionSpot Spot</label>
    <description>Spot where prisoners are executed. Crowd can watch in front.</description>	
	<thingClass>ExecutionModByYuno.Building_SpotExecution</thingClass>
    <graphicData>
      <texPath>Things/Building/ExecutionSpot</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <statBases>
      <WorkToBuild>0</WorkToBuild>
    </statBases>
    <placeWorkers>
    </placeWorkers>
	    <comps>
      <li Class="ExecutionModByYuno.YunoExecutionDrawCompProp" />
    </comps>
	<tickerType>Rare</tickerType>
    <useHitPoints>false</useHitPoints>
		<scatterableOnMapGen>false</scatterableOnMapGen>
    <designationCategory>Misc</designationCategory>
    <passability>Standable</passability>
    <building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <sowTag>SupportPlantsOnly</sowTag>
      <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
      <ai_chillDestination>false</ai_chillDestination>
    </building>
  </ThingDef>

</Defs>
