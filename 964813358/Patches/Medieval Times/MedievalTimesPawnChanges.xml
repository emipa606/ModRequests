<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- ========== Remove Required Apparel Tags ========== -->

<Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>
    <li Class="PatchOperationRemove">
		<xpath>*/PawnKindDef/apparelRequired/li[.="MedTimes_Acc_WargodCrest_Art"]</xpath>
    </li>
  </operations>
</Operation>

<Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>
    <li Class="PatchOperationRemove">
		<xpath>*/PawnKindDef/apparelRequired/li[.="MedTimes_WarbourneShield"]</xpath>
    </li>
  </operations>
</Operation>

<Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>
    <li Class="PatchOperationReplace">
    <xpath>*/PawnKindDef/apparelRequired/li[.="MedTimes_Socks_Techno_Art"]</xpath>
		<value>
            <li>MedTimes_Socks_Tailored</li>
		</value>
    </li>
  </operations>
</Operation>
  
</Patch>

