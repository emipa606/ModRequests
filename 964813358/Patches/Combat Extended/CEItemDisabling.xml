<?xml version="1.0" encoding="utf-8" ?>

<Patch>

    <!-- ========== CE Apparel Disabling ========== -->

	<!-- Ballistic Shield -->

<Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>
    <li Class="PatchOperationRemove">
    <xpath>*/ThingDef[defName="Apparel_BallisticShield"]</xpath>
    </li>
  </operations>
</Operation>

	<!-- Flak Vest -->
	
<Operation Class="PatchOperationSequence">
  <success>Always</success>
