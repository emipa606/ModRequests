<?xml version="1.0" encoding="utf-8" ?>
<Defs>
_______________________________________________________________________
   <ThingDef ParentName="SculptureBase">
    <defName>IceSculpture</defName>
    <label>ice sculpture</label>
    <description>A delicate sculpture of ice that will melt over time.</description>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/IceSculpture</texPath>
    </graphicData>
	 <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0.35</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>4</Mass>
      <Beauty>20</Beauty>
      <WorkToMake>9000</WorkToMake>
    </statBases>
    <stuffCategories>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
	<comps>
	<li>
        <compClass>CompMelter</compClass>
      </li>
    </comps>
    <building>
      <expandHomeArea>false</expandHomeArea>
      <repairable>false</repairable>
      <isInert>true</isInert>
    </building>
  </ThingDef>

_______________________________________________________________________

</Defs>
