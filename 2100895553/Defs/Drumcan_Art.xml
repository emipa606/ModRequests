<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="SculptureBase">
    <defName>GoldenDrumcan</defName>
    <label>Golden Drumcan</label>
    <description>Ornamental drumcan made of gold. I can't press it.</description>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/Art/Drumcan-Gold</texPath>
      <drawSize>(1.8,1.8)</drawSize>
    </graphicData>
    <fillPercent>0.99</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>10000</MaxHitPoints>
      <Mass>150</Mass>
      <Beauty>1000</Beauty>
      <WorkToMake>10000</WorkToMake>
      <MarketValue>3000</MarketValue>
    </statBases>
    <costList>
      <Gold>30</Gold>
    </costList>
  </ThingDef>

  <ThingDef ParentName="SculptureBase">
    <defName>Drumcan</defName>
    <label>Drumcan</label>
    <description>Ornamental drumscan. I can't press it.</description>
    <castEdgeShadows>true</castEdgeShadows>
    <graphicData>
      <texPath>Things/Building/Joy/Drumcan</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1.8,1.8)</drawSize>
    </graphicData>
    <fillPercent>0.9</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>5000</MaxHitPoints>
      <Mass>150</Mass>
      <Beauty>100</Beauty>
      <WorkToMake>5000</WorkToMake>
  </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>300</costStuffCount>
  </ThingDef>

</Defs>
