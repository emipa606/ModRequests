<Defs>
    <SoundDef>
        <defName>RDR_DeadEye_Activated</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <muteWhenPaused>false</muteWhenPaused>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>RDR/DeadEyeStart</clipPath>
                    </li>
                </grains>
                <volumeRange>100</volumeRange>
                <distRange>10~30</distRange>
            </li>
        </subSounds>
    </SoundDef>
    
    <SoundDef>
        <defName>RDR_DeadEye_Deactivated</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <muteWhenPaused>false</muteWhenPaused>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>RDR/DeadEyeEnd</clipPath>
                    </li>
                </grains>
                <volumeRange>100</volumeRange>
                <distRange>10~30</distRange>
            </li>
        </subSounds>
    </SoundDef>
</Defs>