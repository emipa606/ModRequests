<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <IdeoIconDef>
    <defName>ReligionF</defName>
    <iconPath>UI/Ideoligions/ReligionE</iconPath>
    <memes>
      <li>Structure_OriginCommunism</li>
    </memes>
  </IdeoIconDef>
  
  <MemeDef>
    <defName>Structure_OriginCommunism</defName>
    <label>Communism</label>
    <description>Through the thunderstorms the sun of freedom shone for us,\nAnd Lenin illuminated the great path for us.\nWe were raised by Stalin - to be loyal to the people.\nHe inspired us to work and deeds.</description>
    <category>Structure</category>
    <iconPath>UI/IdeoCommunism/logo</iconPath>
    <impact>0</impact>
    <worshipRoomLabel>sanctuary</worshipRoomLabel>
    <randomizationSelectionWeightFactor>5</randomizationSelectionWeightFactor>
    <deityCount>0</deityCount>
    <consumableBuildings>
      <li>SacrificialFlag</li>
    </consumableBuildings>
    <renderOrder>-1</renderOrder>
    <generalRules>
      <rulesStrings>
        <li>memeLeaderNoun->supreme leader</li>
        <li>memeLeaderNoun->general Secretary</li>
        <li>memeLeaderAdjective->crazy</li>
        <li>memeLeaderAdjective->glorious</li>
        <li>memeLeaderAdjective->last</li>
        <li>memeLeaderAdjective->destructive</li>
        <li>memeLeaderAdjective->carnivorous</li>
        <li>memeLeaderAdjective->black tea</li>
        <li>memeLeaderAdjective->nuclear</li>
        <li>memeMoralist->thought police</li>
        <li>memeMoralist->kgb</li>
      </rulesStrings>
    </generalRules>
    <descriptionMaker>
      <patterns>
        <li>
          <def>IdeologicalFounder</def>
          <weight>1</weight>
        </li>
        <li>
          <def>IdeologicalLesson</def>
          <weight>3</weight>
        </li>
      </patterns>
      <constants>
        <li><key>structure_ideological</key><value>true</value></li>
      </constants>
    </descriptionMaker>
    <thingStyleCategories>
      <li>
        <category>Communism</category>
        <priority>4</priority>
      </li>
    </thingStyleCategories>
    <symbolPacks>
      <li>
        <ideoName>Communism</ideoName>
        <theme>communism</theme>
        <adjective>Communist</adjective>
        <member>Communist</member>
      </li>
    </symbolPacks>

  </MemeDef>
  
  
  
  
  
  
