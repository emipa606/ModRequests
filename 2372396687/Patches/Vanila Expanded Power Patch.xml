<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>Vanilla Furniture Expanded - Power</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>*/ThingDef[defName="VFE_PortableWoodFiredGenerator"]/comps/li[3]/fuelFilter/thingDefs</xpath>
					<value>
						<li>TP_Dried_Feces</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>*/ThingDef[defName="VFE_IndustrialWoodFiredGenerator"]/comps/li[3]/fuelFilter/thingDefs</xpath>
					<value>
						<li>TP_Dried_Feces</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>