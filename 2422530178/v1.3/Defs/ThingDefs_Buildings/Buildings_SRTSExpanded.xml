<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="NECShipBase" ParentName="BuildingBase" Abstract="True">
    <category>Building</category>
    <thingClass>Building</thingClass>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <selectable>true</selectable>
    <drawerType>MapMeshAndRealTime</drawerType>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <repairEffect>Repair</repairEffect>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <statBases>
      <SellPriceFactor>0.70</SellPriceFactor>
    </statBases>
    <tickerType>Normal</tickerType>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <stealable>false</stealable>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>Buildings</li>
    </thingCategories>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.90</fillPercent>
    <designationCategory>Ship</designationCategory>
    <killedLeavings>
      <ChunkSlagSteel>18</ChunkSlagSteel>
    </killedLeavings>
    <comps />
  </ThingDef>

  <ThingDef ParentName="NECShipBase">
    <defName>UNSC_Pelican</defName>
    <label>UNSC Pelican</label>
    <description>For over a century, Pelican dropships have been the UNSCâ€™s best and most prominent method of troop and materiel transportation in its various military endeavors. These workhorse craft are equally at home in orbit and atmosphere, and have even been pressed into use for short interplanetary trips. Available in a bewildering number of models and unique variants, Pelicans are likely to remain in service for the foreseeable future.</description>
    <graphicData>
      <texPath>UNSC_Pelican</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(11,13)</drawSize>
      <drawRotated>True</drawRotated>
    </graphicData>
    <size>(3,8)</size>
    <rotatable>True</rotatable>
    <statBases>
      <MaxHitPoints>3000</MaxHitPoints>
      <WorkToBuild>2400</WorkToBuild>
      <Flammability>0.4</Flammability>
      <Mass>20</Mass>
      <Beauty>15</Beauty>
    </statBases>
    <costList>
      <Steel>1800</Steel>
      <Gold>125</Gold>
      <Silver>400</Silver>
      <ComponentIndustrial>48</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="CompProperties_Transporter">
        <restEffectiveness>0.5</restEffectiveness>  <!-- How well (1.0 = 100%) passengers can rest, eat, heal while aboard -->
        <massCapacity>700</massCapacity> <!-- weight in kg -->
      </li>
      <li Class="SRTS.CompProperties_LaunchableSRTS">
        <travelSpeed>20</travelSpeed> <!-- Vanilla drop pod is 25 speed, min 1, max 100 -->
        <fuelPerTile>5</fuelPerTile> <!-- Drop pod cost is 2.25 per tile. min 1, max 6. Why'd we cap it at 6? I can't remember. -->
        <minPassengers>1</minPassengers>
        <maxPassengers>13</maxPassengers>
        <spaceFaring>true</spaceFaring>  <!-- For save our ship 2 -->
        <shuttleBayLanding>true</shuttleBayLanding>
      </li>
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>1200</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>600</initialConfigurableTargetFuelLevel>
        <fuelFilter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <initialFuelPercent>0.25</initialFuelPercent>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
        <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerPlant</compClass>
        <basePowerConsumption>-400</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable" />
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Glower">
        <glowRadius>12</glowRadius>
        <glowColor>(217,217,217,0)</glowColor>
      </li>
    </comps>
    <researchPrerequisites>
      <li>Reasearch_UNSCPelican</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>12</constructionSkillPrerequisite>
    <uiIconScale>0.75</uiIconScale>
    <killedLeavings>
      <ChunkSlagSteel>5</ChunkSlagSteel>
    </killedLeavings>
  </ThingDef>
