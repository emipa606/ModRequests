<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="BomberSkyfallerBase" ParentName="EtherealThingBase" Abstract="True">
    <thingClass>BomberSkyfaller</thingClass>
    <useHitPoints>false</useHitPoints>
    <drawOffscreen>true</drawOffscreen>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Skyfaller</altitudeLayer>
    <drawerType>RealtimeOnly</drawerType>
    <skyfaller />
  </ThingDef>
  
  <!-- UNSC Pelican -->

  <ThingDef ParentName="SkyfallerBase">
    <defName>UNSC_Pelican_Incoming</defName>
    <label>UNSC Pelican incoming</label>
    <thingClass>SRTS.SRTSIncoming</thingClass>
    <graphicData>
      <texPath>UNSC_Pelican</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(11,13)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Decelerate</movementType>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(8,12)</shadowSize>
      <anticipationSound>DropPod_Fall</anticipationSound>
      <anticipationSoundTicks>100</anticipationSoundTicks>
      <motesPerCell>0</motesPerCell>
    </skyfaller>
  </ThingDef>

  <ThingDef ParentName="SkyfallerBase">
    <defName>UNSC_Pelican_Leaving</defName>
    <label>UNSC Pelican leaving</label>
    <thingClass>SRTS.SRTSLeaving</thingClass>
    <graphicData>
      <texPath>UNSC_Pelican</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(11,13)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Accelerate</movementType>
      <reversed>true</reversed>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(8,12)</shadowSize>
      <anticipationSound>Sound_Launch</anticipationSound>
      <anticipationSoundTicks>-10</anticipationSoundTicks>
      <ticksToImpactRange><min>-40</min><max>-15</max></ticksToImpactRange>
    </skyfaller>
  </ThingDef>

  <ThingDef>
    <defName>UNSC_Pelican_Active</defName>
    <label>UNSC Pelican active</label>
    <category>Item</category>
    <thingClass>ActiveDropPod</thingClass>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>UNSC_Pelican</texPath>
