<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Production</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationRemove"> 
					<xpath>/Defs/ThingDef[  defName="VFE_TableMachiningLarge" or 
                                            defName="VFE_TableSmithyLarge" or 
                                            defName="VFE_TableStoveLarge" or 
                                            defName="VFE_TableTailorLarge" or 
                                            defName="VFE_ComponentFabricationBench" or 
                                            defName="VFE_TableButcherElectric" or 
                                            defName="VFE_TableDrugLabElectric" or 
                                            defName="VFE_FueledSmelter" or 
                                            defName="VFE_TableStonecutterElectric" 
                        ]/placeWorkers/li[text()="PlaceWorker_PreventInteractionSpotOverlap"]
                    </xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
