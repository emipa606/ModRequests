			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
            <li>AnimalProductRaw</li>
          </categories>
				</filter>
				<count>0.5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
		</ingredients>
		<products>
      <InsectJelly>20</InsectJelly>
    </products>
<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
  </RecipeDef>
</Defs>