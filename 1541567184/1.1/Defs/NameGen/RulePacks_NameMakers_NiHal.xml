<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonNiHal</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[fishspecies] [scientificfish]</li>
        <li>name->[names] [scientificfish]</li>
        <li>name->[names] '[nihalnicks]' [scientificfish]</li>
        <li>name->[malenihal] '[nihalnicks]' [scientificfish]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>malenihal</keyword>
          <path>NiHalNames/malenihal</path>
        </li>
        <li Class="Rule_File">
          <keyword>fishspecies</keyword>
          <path>NiHalNames/fishspecies</path>
        </li>
        <li Class="Rule_File">
          <keyword>scientificfish</keyword>
          <path>NiHalNames/scientificfish</path>
        </li>
        <li Class="Rule_File">
          <keyword>names</keyword>
          <path>NiHalNames/abstractfish</path>
        </li>
        <li Class="Rule_File">
          <keyword>nihalnicks</keyword>
          <path>NiHalNames/nihalnicks</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonNiHalF</defName>
