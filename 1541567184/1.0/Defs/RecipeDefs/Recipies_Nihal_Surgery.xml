<?xml version="1.0" encoding="UTF-8"?>

<Defs>  
 <RecipeDef ParentName="SurgeryFlesh">
      <defName>InstallNiHalPlasteelClaws</defName>
      <label>install plasteel claws</label>
      <description>Installs plasteel claws.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing plasteel claws.</jobString>
      <workAmount>1200</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>NiHalPlasteelClaws</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>NiHalPlasteelClaws</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>LeftHandClaws</li>
         <li>RightHandClaws</li>
      </appliedOnFixedBodyParts>
      <addsHediff>NiHalPlasteelClaws</addsHediff>
   </RecipeDef>

</Defs> 