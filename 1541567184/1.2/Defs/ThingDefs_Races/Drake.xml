<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="AnimalThingBase">
    <defName>Drake</defName>
    <label>drake</label>
    <description>Native to Rax'Thalaki, this dragon-like apex predator can use its meaty tail to smash or stab prey and its jaw can crush just about anything. Due to the aquatic nature of the planet, this creature is adept at swimming and has an interesting musculature. They don't survive in the cold, however.</description>
    <statBases>
      <MoveSpeed>6.5</MoveSpeed>
      <ComfyTemperatureMin>-5</ComfyTemperatureMin>
      <MarketValue>550</MarketValue>
      <ArmorRating_Sharp>0.7</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
      <ArmorRating_Heat>0.60</ArmorRating_Heat>	
    </statBases>
	<tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>15</power>
        <cooldownTime>1.3</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>15</power>
        <cooldownTime>1.3</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
	    <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>20</power>
        <cooldownTime>1.85</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>30</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
	  <li>
		<label>tail spear</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>20</power>
        <cooldownTime>1.85</cooldownTime>
        <linkedBodyPartsGroup>TailAttackToolStab</linkedBodyPartsGroup>
      </li>
      <li>
	  <label>tail</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>TailAttackToolBlunt</linkedBodyPartsGroup>
      </li>  
    </tools>
    <race>
      <wildBiomes>
		<TropicalRainforest>.15</TropicalRainforest>
		<TemperateForest>.08</TemperateForest>
		<AridShrubland>.08</AridShrubland>
      </wildBiomes>
      <body>DrakeBody</body>
      <herdAnimal>false</herdAnimal>
      <predator>true</predator>
      <baseBodySize>1.5</baseBodySize>
      <baseHungerRate>0.3</baseHungerRate>
      <baseHealthScale>2.4</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Drake</leatherDef>
	  <packAnimal>true</packAnimal>
	  <trainability>Advanced</trainability>
      <wildness>0.80</wildness>
      <manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <gestationPeriodDays>22.5</gestationPeriodDays>
      <lifeExpectancy>13</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_BigInsect_Pain</soundWounded>
          <soundDeath>Pawn_BigInsect_Death</soundDeath>
          <soundAngry>Pawn_BigInsect_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>StandardAnimal</li>
      <li>BadassAnimal</li>
      <li>AnimalUncommon</li>
      <li>AnimalFighter</li>
      <li>AnimalExotic</li>
    </tradeTags>
  </ThingDef>

