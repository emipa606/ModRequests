<?xml version="1.0" encoding="utf-8" ?>

<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Animals Expanded — Arid Shrubland</li>	
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>/Defs/PawnKindDef[defName = "AEXP_Giraffe"]</xpath>
				<value>
					<alternateGraphicChance>0.5</alternateGraphicChance>
					<alternateGraphics>
						<li><texPath>Things/Pawn/Animal/AEXP_Giraffe/GiraffeA</texPath></li>
						</alternateGraphics>
				</value>
			</li>
				
			</operations>
		</match>
	</Operation>
	
	
	
