<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef ParentName="CookMealBase">
		<defName>CookMealExceptional</defName>
		<label>cook excellent meal</label>
		<description>Cook a very complex meal from a combination of meat and plant ingredients. Much of the ingredients are wasted.</description>
		<jobString>Cooking excellent meal.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>AnimalProductRaw</li>
						<li>SeafoodRaw</li>
					</categories>
				</filter>
				<count>0.4</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
						<li>GreensRaw</li>
					</categories>
				</filter>
				<count>0.2</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>VegetablesRaw</li>
					</categories>
				</filter>
				<count>0.2</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>FruitsRaw</li>
						<li>MushroomsRaw</li>
					</categories>
				</filter>
				<count>0.2</count>
			</li>
		</ingredients>
		<products>
			<MealExceptional>1</MealExceptional>
		</products>
		<skillRequirements>
			<Cooking>9</Cooking>
		</skillRequirements>
	</RecipeDef>

</Defs>
