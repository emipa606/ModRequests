<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--========= Water ============-->
  
	<JobDef>
		<defName>GiveWaterToStoredWaterTaker</defName>
		<driverClass>Terra.JobDriver_StoredWaterTaker_GiveWater</driverClass>
		<reportString>filling water into TargetA.</reportString>
		<suspendable>false</suspendable>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>TakeWaterFromStoredWaterGiver</defName>
		<driverClass>Terra.JobDriver_StoredWaterGiver_TakeWater</driverClass>
		<reportString>taking water out of TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

  <!--========= General ============-->

	<JobDef>
		<defName>TakeItemOutOfItemProcessor</defName>
		<driverClass>Terra.JobDriver_ItemProcessor_TakeItem</driverClass>
		<reportString>taking item out of TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>FillItemProcessor</defName>
		<driverClass>Terra.JobDriver_ItemProcessor_FillItem</driverClass>
		<reportString>filling TargetA.</reportString>
		<suspendable>false</suspendable>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>
  
</Defs>