<Defs>

  <!--=========================== Basic crops ==============================-->

	<ThingDef ParentName="CultivatedPlantBase">
		<defName>Plant_Buckwheat</defName>
		<label>buckwheat plant</label>
		<description>A buckwheat plant. Buckwheat grows well even in harsher climate. It can also be used as a green manure.</description>
		<graphicData>
			<texPath>Things/Plant/Buckwheat/Buckwheat_Ripe</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<pathCost>10</pathCost>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<Flammability>0.8</Flammability>
			<Nutrition>0.18</Nutrition>
		</statBases>
		<plant>
			<fertilityMin>0.3</fertilityMin>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<growDays>5.9</growDays>
			<harvestYield>11</harvestYield>
			<harvestedThingDef>RawBuckwheat</harvestedThingDef>
			<harvestMinGrowth>0.75</harvestMinGrowth>
			<harvestWork>130</harvestWork>
			<sowWork>80</sowWork>
			<sowTags>
				<li>Ground</li>
				<li>RaisedBed</li>
				<li>Hydroponic</li>
			</sowTags>
			<visualSizeRange>
				<min>0.4</min>
				<max>1.2</max>
			</visualSizeRange>
		</plant>
		<modExtensions>
			<li Class="Terra.ModExt_Plant">
				<plantSize>Small</plantSize>
				<plantWasteYield>5</plantWasteYield>
				<fertilityConsumptionPerDay>0.0065</fertilityConsumptionPerDay>
				<moistureConsumptionPerDay>0.075</moistureConsumptionPerDay>
				<minMoisture>0.35</minMoisture>
				<minOptimalMoisture>0.45</minOptimalMoisture>
				<maxMoisture>0.9</maxMoisture>
				<minAliveTemperature>-6</minAliveTemperature>
				<minLeafTemperature>-6</minLeafTemperature>
				<minGrowthTemperature>3</minGrowthTemperature>
				<minGoodGrowthTemperature>10</minGoodGrowthTemperature>
				<minSunscaldTemperature>44</minSunscaldTemperature>
				<maxTemperature>44</maxTemperature>
				<ripeTemperatureThreshold>12</ripeTemperatureThreshold>
				<temperatureSensitivity>0.05</temperatureSensitivity>
				<suboptimalLightGrowthSensitivity>0.8</suboptimalLightGrowthSensitivity>
				<isGreenManure>true</isGreenManure>
				<manureFertilityFactor>1.5</manureFertilityFactor>
				<weedsMinGrassToAffectGrowth>0.4</weedsMinGrassToAffectGrowth>
				<weedsMaxGrassBeforeDamage>1.2</weedsMaxGrassBeforeDamage>
				<beeCanVisit>true</beeCanVisit>
				<beeVisitBlossomGrowthOffset>0.025</beeVisitBlossomGrowthOffset>
				<beeVisitingPeriodTicks>15000</beeVisitingPeriodTicks>
				<intercroppingBonus>
					<li>Plant_Corn</li>
					<li>Plant_Millet</li>
					<li>Plant_Healroot</li>
					<li>Plant_Psychoid</li>
				</intercroppingBonus>
				<intercroppingMalus>
					<li>Plant_Agave</li>
					<li>Plant_Devilstrand</li>
					<li>Plant_Smokeleaf</li>
				</intercroppingMalus>
				<growthSteps>
					<li>
						<state>Young</state>
						<max>0.65</max>
						<stepGraphicPath>Things/Plant/Buckwheat/Buckwheat_Young</stepGraphicPath>
					</li>
					<li>
						<state>Blossom</state>
						<max>0.75</max>
						<stepGraphicPath>Things/Plant/Buckwheat/Buckwheat_Blossom</stepGraphicPath>
						<beautyOffset>2</beautyOffset>
					</li>
					<li>
						<state>Unripe</state>
						<max>0.9</max>
						<stepGraphicPath>Things/Plant/Buckwheat/Buckwheat_Unripe</stepGraphicPath>
					</li>
					<li>
						<state>Ripe</state>
						<max>1</max>
						<stepGraphicPath>Things/Plant/Buckwheat/Buckwheat_Ripe</stepGraphicPath>
					</li>
				</growthSteps>
			</li>
		</modExtensions>
	</ThingDef>

