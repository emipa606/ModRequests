<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!--VFE - Settlers-->
  <Operation Class="JPTools.PatchOperationFindModById">
    <mods>
      <li>OskarPotocki.VanillaFactionsExpanded.SettlersModule</li>
    </mods>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/ThingDef[defName="Windmill"]/building</xpath>
      <value>
        <soundAmbient>WaterMill_Ambience</soundAmbient>
      </value>
    </match>
  </Operation>

</Patch>