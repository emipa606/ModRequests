<?xml version="1.0" encoding="UTF-8"?>

<Defs>


  <WorkGiverDef>
    <defName>DigAtDiggingSpot</defName>
    <label>dig at digging spot</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Mining</workType>
    <priorityInType>20</priorityInType>
    <fixedBillGiverDefs>
      <li>AM_DigSpot</li>
	  <li>AM_SandDigSpot</li>
    </fixedBillGiverDefs>
        <verb>dig</verb>
    <gerund>digging</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>BakeMasonry</defName>
    <label>bake various masonry goods</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Crafting</workType>
    <priorityInType>50</priorityInType>
    <fixedBillGiverDefs>
      <li>AM_FuelledKiln</li>
	  <li>AM_ElectricKiln</li>
    </fixedBillGiverDefs>
        <verb>bake</verb>
    <gerund>baking</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
</Defs>

