<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=============== Shells ====================-->

  <ThingDef ParentName="ResourceBase">
    <defName>ShockAbsorbeContainer</defName>
    <label>shock absorbe container</label>
    <description>Fired from container mortar.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Shell</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <color>(52,52,138)</color>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <tickerType>Normal</tickerType>
    <stackLimit>15</stackLimit>
    <statBases>
      <WorkToMake>800</WorkToMake>
      <MaxHitPoints>140</MaxHitPoints>
      <MarketValue>152</MarketValue>
      <Flammability>0.0</Flammability>
      <DeteriorationRate>2</DeteriorationRate>
    </statBases>
    <costList>
      <Plasteel>5</Plasteel>
    </costList>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <recipeMaker>
      <workSpeedStat>SmithingSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <recipeUsers>
        <li>ElectricSmithy</li>
        <li>FueledSmithy</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>

</Defs>