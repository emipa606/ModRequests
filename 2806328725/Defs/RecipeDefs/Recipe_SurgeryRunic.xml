	</RecipeDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
    <defName>WDD_InstallRunicLeg</defName>
    <label>install runic leg</label>
    <description>Install a runic leg.</description>
    <descriptionHyperlinks>
      <ThingDef>WDD_RunicLeg</ThingDef>
      <HediffDef>WDD_RunicLeg</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing runic leg.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WDD_RunicLeg</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WDD_RunicLeg</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Leg</li>
    </appliedOnFixedBodyParts>
    <addsHediff>WDD_RunicLeg</addsHediff>
  </RecipeDef>

