  <HediffDef ParentName="addedPartDryadic">
    <defName>WDD_DryadicConnector</defName>
    <label>dryadic brain connector</label>
    <labelNoun>a dryadic brain connector </labelNoun>
    <description>An installed dryadic brain connector.</description>
    <descriptionHyperlinks>
      <ThingDef>WDD_DryadicConnector</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>WDD_DryadicConnector</spawnThingOnRemoved>
    <addedPartProps>
      <partEfficiency>1</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <partIgnoreMissingHP>true</partIgnoreMissingHP>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>1</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>1</setMax>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>

  <HediffDef ParentName="addedPartDryadic">
    <hediffClass Inherit="False">Hediff_Implant</hediffClass>
    <defName>WDD_DryadicStimulator</defName>
    <label>dryadic stimulator</label>
    <labelNoun>a dryadic stimulator</labelNoun>
    <description>An installed dryadic stimulator.</description>
    <descriptionHyperlinks>
      <ThingDef>WDD_DryadicStimulator</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>WDD_DryadicStimulator</spawnThingOnRemoved>
    <addedPartProps>
      <partEfficiency>1.0</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Manipulation</capacity>
            <offset>0.1</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>0.1</offset>
          </li>
        </capMods>
        <totalBleedFactor>0.98</totalBleedFactor>
        <naturalHealingFactor>1.02</naturalHealingFactor>
        <restFallFactorOffset>-0.02</restFallFactorOffset>
        <hungerRateFactor>0.98</hungerRateFactor>
        <painFactor>0.98</painFactor>
        <statOffsets>
          <ImmunityGainSpeed>+0.02</ImmunityGainSpeed>
          <ShootingAccuracyPawn>+0.02</ShootingAccuracyPawn>
          <AimingDelayFactor>-0.02</AimingDelayFactor>
          <MeleeHitChance>+0.02</MeleeHitChance>
          <ConstructionSpeed>+0.02</ConstructionSpeed>
          <SmoothingSpeed>+0.02</SmoothingSpeed>
          <ConstructSuccessChance>+0.02</ConstructSuccessChance>
          <PlantWorkSpeed>+0.5</PlantWorkSpeed>
          <PlantHarvestYield>+0.2</PlantHarvestYield>
          <MiningSpeed>+0.02</MiningSpeed>
          <MiningYield>+0.02</MiningYield>
          <MedicalSurgerySuccessChance>+0.02</MedicalSurgerySuccessChance>
          <MedicalTendQuality>+0.02</MedicalTendQuality>
          <MedicalTendSpeed>+0.02</MedicalTendSpeed>
          <NegotiationAbility>+0.02</NegotiationAbility>
          <ResearchSpeed>+0.02</ResearchSpeed>
          <GlobalLearningFactor>+0.02</GlobalLearningFactor>
          <CookSpeed>+2</CookSpeed>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>
   
