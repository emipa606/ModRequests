<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Alpha Animals</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAddModExtension">
                    <xpath>/Defs/ThingDef[defName="AA_BlueMilk"]</xpath>
                    <value>
                        <li Class="DubsBadHygiene.WaterExt">
                            <water>0.80</water>
                            <SeekForThirst>false</SeekForThirst>
                        </li>
                    </value>
                </li>
				   <li Class="PatchOperationAddModExtension">
                    <xpath>/Defs/ThingDef[defName="AA_GowMilk"]</xpath>
                    <value>
                        <li Class="DubsBadHygiene.WaterExt">
                            <water>0.80</water>
                            <SeekForThirst>false</SeekForThirst>
                        </li>
                    </value>
                </li>
				  <li Class="PatchOperationAddModExtension">
                    <xpath>/Defs/ThingDef[defName="AA_NightMilk"]</xpath>
                    <value>
                        <li Class="DubsBadHygiene.WaterExt">
                            <water>0.80</water>
                            <SeekForThirst>false</SeekForThirst>
                        </li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>