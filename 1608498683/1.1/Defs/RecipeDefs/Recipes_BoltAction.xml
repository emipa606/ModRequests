<Defs>

<!-- ======= L85A2 ======= -->
	<RecipeDef>
    <defName>CPCraft_L115A3Sn_Weapon</defName>
    <label>make L115A3 SD</label>
    <description>make an L115A3 SD.</description>
    <jobString>making an L115A3 SD.</jobString>
    <workSkill>Crafting</workSkill>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
	<workAmount>45000</workAmount>
	<unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
	<recipeUsers>
      <li>RNBench_Marksman</li>
    </recipeUsers>
    <ingredients>
	  		<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>60</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
    </ingredients>
		<products>
			<RNGun_L115A3Sn>1</RNGun_L115A3Sn> <!-- === PRODUCT === -->
		</products>
	 <researchPrerequisite>PrecisionRifling</researchPrerequisite>
	<skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
  </RecipeDef>
  
