<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <AbilityDef ParentName="PsycastBase">
    <defName>OOAbility_MasterSpark</defName>
    <label>MasterSpark</label>
    <description>Unlock the launch masterspark button within seconds.</description>
    <level>5</level>
    <iconPath>Things/MasterSpark</iconPath>
    <writeCombatLog>True</writeCombatLog>
    <showPsycastEffects>False</showPsycastEffects>
    <targetRequired>False</targetRequired>
	<canUseAoeToGetTargets>False</canUseAoeToGetTargets>
	<!--showPsycastEffects>False</showPsycastEffects-->
	<warmupStartSound>Psycast_Neuroquake_CastStart</warmupStartSound>
    <warmupSound>Psycast_Neuroquake_CastLoop</warmupSound>
    <warmupPreEndSound>Psycast_Neuroquake_CastEnd</warmupPreEndSound>
    <warmupPreEndSoundSeconds>5</warmupPreEndSoundSeconds>
    <statBases>
      <Ability_EntropyGain>65</Ability_EntropyGain>
      <Ability_PsyfocusCost>0.04</Ability_PsyfocusCost>
      <Ability_Duration>10</Ability_Duration>
    </statBases>
    <verbProperties>
	  <warmupTime>1</warmupTime>
	  <warmupEffecter>Effect_OOAbility_liaoji</warmupEffecter>
	  <drawAimPie>False</drawAimPie>
      <requireLineOfSight>False</requireLineOfSight>
      <targetParams>
        <canTargetSelf>true</canTargetSelf>
        <canTargetPawns>false</canTargetPawns>
        <canTargetBuildings>false</canTargetBuildings>
        <canTargetAnimals>false</canTargetAnimals>
        <canTargetHumans>false</canTargetHumans>
        <canTargetMechs>false</canTargetMechs>
      </targetParams>
    </verbProperties>
    <comps>
      <li Class = "CompProperties_AbilityGiveHediff">
        <compClass>CompAbilityEffect_GiveHediff</compClass>
        <hediffDef>OOHediff_MasterSpark</hediffDef>
        <onlyBrain>True</onlyBrain>
		<onlyApplyToSelf>True</onlyApplyToSelf>
		<replaceExisting>true</replaceExisting>
        <psychic>True</psychic>
      </li>
	   <li Class="CompProperties_AbilityMoteOnTarget">
        <moteDefs>
          <li>Mote_PsycastSkipInnerExit</li>
          <li>Mote_PsycastSkipOuterRingExit</li>
        </moteDefs>
        <scale>1.55</scale>
        <sound>Psycast_Skip_Exit</sound>
        <preCastTicks>5</preCastTicks>
      </li>
    </comps>
  </AbilityDef>

<MVCF.ModDef>
	<defName>OOHediff_MasterSpark</defName>
	<Features>
            <Drawing>true</Drawing>
            <ApparelVerbs>true</ApparelVerbs>
            <IndependentFire>true</IndependentFire>
            <IntegratedToggle>true</IntegratedToggle>
			<RangedAnimals>true</RangedAnimals>
			<HediffVerbs>true</HediffVerbs>
	</Features>
</MVCF.ModDef>

<HediffDef ParentName="PsycastHediffBase">
  <defName>OOHediff_MasterSpark</defName>
  <label>MasterSpark</label>
  <description>Add the button to launch masterspark in a few seconds.For some reasons, the damage is concentrated in the far half.</description>
  <defaultLabelColor>(218,112,214)</defaultLabelColor>
  <labelNoun>MasterSpark</labelNoun>
  <isBad>false</isBad>
  <comps>
    <li Class="MVCF.Comps.HediffCompProperties_ExtendedVerbGiver">
      <verbs>
        <li>
        <verbClass>Verb_LaunchProjectileStatic</verbClass>
		<hasStandardCommand>true</hasStandardCommand>
		<forceNormalTimeSpeed>false</forceNormalTimeSpeed>
        <defaultProjectile>Bullet_OOStarWand_MasterSpark</defaultProjectile>
		<requireLineOfSight>false</requireLineOfSight>
		<onlyManualCast>True</onlyManualCast>
        <warmupTime>10</warmupTime>
		<warmupEffecter>Effect_OOStarWand_MasterSpark</warmupEffecter>
	    <minRange>24.9</minRange>
        <range>49.9</range>
        <!--burstShotCount>5</burstShotCount>
		<ticksBetweenBurstShots>120</ticksBetweenBurstShots-->
        <soundCast>PsychicShockLanceCast</soundCast>
        <soundCastTail>GunTail_Heavy</soundCastTail>
        <muzzleFlashScale>18</muzzleFlashScale>
		<targetParams>
			<canTargetPawns>False</canTargetPawns>
			<canTargetLocations>True</canTargetLocations>
        </targetParams>
		<rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
		<label>MasterSpark</label>
      </li>
	 </verbs>
	 <verbProps>
        <li>
          <label>MasterSpark</label>
        </li>
	 </verbProps>
	</li>
  </comps>
</HediffDef>

<!-- MasterSpark弹头 -->
	<VFECore.ExpandableProjectileDef ParentName="BaseBullet">
		<defName>Bullet_OOStarWand_MasterSpark</defName>
		<label>Bullet_OOStarWand_Spark</label>
		<thingClass>VFECore.FlamethrowProjectile</thingClass>     <!--爆炸和火焰弹头-->
		<graphicData>
			<texPath>Things/daoxian</texPath>                   <!--子弹纹理-->
			<texPathFadeOut>Things/EX</texPathFadeOut>     <!--弹丸淡出纹理-->
			<shaderType>MoteGlow</shaderType>	
		</graphicData>
		<projectile>
			<damageDef>OOStarAbilityArrow</damageDef>
			<speed>100</speed>
			<damageAmountBase>10</damageAmountBase>
			<explosionRadius>0.1</explosionRadius>
			<ai_IsIncendiary>true</ai_IsIncendiary>
			<flyOverhead>true</flyOverhead>
			<soundHitThickRoof>PsychicShockLanceCast</soundHitThickRoof>
		</projectile>
		<drawOffscreen>true</drawOffscreen>                         <!--必要-->
		<lifeTimeDuration>300</lifeTimeDuration>                    <!--总生命时间，计数器计算每个tickFrameRate属性-->
		<widthScaleFactor>0.3</widthScaleFactor>                    <!--缩放宽度-->
		<heightScaleFactor>0.32</heightScaleFactor>                    <!--缩放高度-->
		<startingPositionOffset>(0, 0, 0)</startingPositionOffset>    <!--定位起始位置-->
		<totalSizeScale>3.0</totalSizeScale>                         <!--将缩放子弹大小，必要-->
		<tickFrameRate>20</tickFrameRate>                           <!--每刻更改纹理的速度-->
		<finalTickFrameRate>10</finalTickFrameRate>                 <!--每刻改变淡出纹理的速度-->
		<tickDamageRate>20</tickDamageRate>							<!--确定通过哪个刻造成伤害-->
		<!--dealsDamageOnce>true</dealsDamageOnce-->
		<minDistanceToAffect>1</minDistanceToAffect>                <!--可以设置为忽略附近受影响的单元格（如果你不需要燃烧沙袋或其他建筑物，很有用）-->
	</VFECore.ExpandableProjectileDef>

