  <ThinkTreeDef>
    <defName>Forerunner</defName>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <!-- Downed -->
		<li Class="ThinkNode_Subtree">
          <treeDef>Downed</treeDef>
        </li>
		
		<li Class="ThinkNode_Subtree">
          <treeDef>ForerunnerBaseCombat</treeDef>
        </li>
        <!-- Do a queued job -->
        <li Class="ThinkNode_QueuedJob" />


        <!-- Lord -->
        <li Class="ThinkNode_JoinVoluntarilyJoinableLord">
          <dutyHook>HighPriority</dutyHook>
          <subNodes>
            <li Class="ThinkNode_Subtree">
              <treeDef>LordDuty</treeDef>
            </li>
          </subNodes>
        </li>
		<li Class="MoharAiJob.AiCorpse_JobGiver" />

        <!-- Idle -->
        <li Class="ThinkNode_Tagger">
          <tagToGive>Idle</tagToGive>
          <subNodes>
            <li Class="JobGiver_WanderAnywhere">
              <maxDanger>Deadly</maxDanger>
            </li>
          </subNodes>
        </li>
		
		<li Class="ThinkNode_ChancePerHour_Constant">
              <mtbDays>60</mtbDays>
              <subNodes>
                <li Class="ThinkNode_Tagger">
                  <tagToGive>Misc</tagToGive>
                  <subNodes>
                    <li Class="JobGiver_ExitMapRandom">
                      <defaultLocomotion>Walk</defaultLocomotion>
                    </li>
                  </subNodes>
                </li>
              </subNodes>
        </li>
        
        <!-- Idle error -->
        <li Class="JobGiver_IdleError"/>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
   <ThinkTreeDef>
    <defName>ForerunnerPassive</defName>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <!-- Downed -->
		<li Class="ThinkNode_Subtree">
          <treeDef>Downed</treeDef>
        </li>
		
		<li Class="ThinkNode_Subtree">
          <treeDef>ForerunnerBasePassive</treeDef>
        </li>
        <!-- Do a queued job -->
        <li Class="ThinkNode_QueuedJob" />

        <!-- Idle -->
        <li Class="ThinkNode_Tagger">
          <tagToGive>Idle</tagToGive>
          <subNodes>
            <li Class="JobGiver_WanderAnywhere">
              <maxDanger>Deadly</maxDanger>
            </li>
          </subNodes>
        </li>
		
		<li Class="ThinkNode_JoinVoluntarilyJoinableLord">
          <dutyHook>HighPriority</dutyHook>
          <subNodes>
            <li Class="ThinkNode_Subtree">
              <treeDef>LordDuty</treeDef>
            </li>
          </subNodes>
        </li>
		
		<li Class="ThinkNode_ChancePerHour_Constant">
              <mtbDays>60</mtbDays>
              <subNodes>
                <li Class="ThinkNode_Tagger">
                  <tagToGive>Misc</tagToGive>
                  <subNodes>
                    <li Class="JobGiver_ExitMapRandom">
                      <defaultLocomotion>Walk</defaultLocomotion>
                    </li>
                  </subNodes>
                </li>
              </subNodes>
        </li>
		
		<li Class="ThinkNode_ConditionalTrainableCompleted">
				<trainable>Release</trainable>
				<subNodes>
				<li Class="ThinkNode_Tagger">
					<tagToGive>TrainedAnimalBehavior</tagToGive>
				</li>
			</subNodes>
			</li>
			
        <!-- Idle error -->
        <li Class="JobGiver_IdleError"/>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
