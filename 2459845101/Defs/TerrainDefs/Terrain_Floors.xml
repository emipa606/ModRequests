<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef  ParentName="FloorBase">
    <defName>CHIP_ChipboardFloor</defName>
    <label>chipboard floor</label>
    <renderPrecedence>252</renderPrecedence>
    <description>Chipboard floor suitable for quickly constructed buildings. While it is very quick to lay and has an attractive veneer, it takes very little to ignite.</description>
    <texturePath>Terrain/ChipboardFloor</texturePath>
    <burnedDef>BurnedWoodPlankFloor</burnedDef>
    <statBases>
      <Beauty>1</Beauty>
	  <WorkToBuild>35</WorkToBuild>
      <Flammability>3.0</Flammability>
    </statBases>
    <costList>
      <CHIP_Chipboard>3</CHIP_Chipboard>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
  </TerrainDef>

</Defs>
