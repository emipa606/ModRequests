<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="BApparelBase" Abstract="True">
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <selectable>True</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>True</useHitPoints>
    <techLevel>Medieval</techLevel>
    <graphicData>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <drawGUIOverlay>true</drawGUIOverlay>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1</DeteriorationRate>
      <Beauty>5</Beauty>
      <SellPriceFactor>0.70</SellPriceFactor>
    </statBases>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Normal</tickerType>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
  </ThingDef>
	
  <ThingDef Name="GuPBetaApparelBase" ParentName="BApparelBase" Abstract="True">
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <defaultIngredientFilter>
        <categories>
          <li>Root</li>
        </categories>
        <disallowedThingDefs>
          <li>Gold</li>
          <li>Silver</li>
          <li>Plasteel</li>
          <li>Jade</li>
          <li>Uranium</li>
        </disallowedThingDefs>
      </defaultIngredientFilter>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <apparel>
      <gender>Female</gender>
      <tags>
        <li>RoyalCombatGear</li>
        <li>Psychic</li>
      </tags>
    </apparel>
    <equippedStatOffsets>
      <PsychicSensitivity>0.3</PsychicSensitivity>
      <PsychicEntropyRecoveryRate>3.5</PsychicEntropyRecoveryRate>
      <PsychicEntropyMax>3</PsychicEntropyMax>
    </equippedStatOffsets>
  </ThingDef>
      
