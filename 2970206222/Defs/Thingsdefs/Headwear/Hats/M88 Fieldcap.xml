<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--__________ M88 Fieldcap __________-->
  
  
  	<!-- M88 Fieldcap -->
  <ThingDef ParentName="HatMakeableBase">
    <defName>ODZ_METRO2033_M88Fieldcap</defName>
    <label>M88 fieldcap</label>
    <description>A matching fieldcap to go with m88 afghanka uniforms.</description>
    <graphicData>
      <texPath>Things/Headwear/Hats/M88Fieldcap/Fieldcap</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>	  
	  <drawSize>1.6</drawSize>		  
    </graphicData>
    <uiIconScale>1</uiIconScale>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <statBases>
			<WorkToMake>1200</WorkToMake>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>0.07</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.30</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.5</EquipDelay>
    </statBases>
    <recipeMaker>
      <researchPrerequisite>ODZ_METRO2033_Russia</researchPrerequisite>  
    </recipeMaker>
    <apparel>
	<useWornGraphicMask>true</useWornGraphicMask>	
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Headwear/Hats/M88Fieldcap/Fieldcap</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
	  <li>Soviet</li>
	  <li>Redline</li>
	  <li>Lowtier</li>
	  <li>Midtier</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

