<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <!--__________ Onskin ____________-->

 <!--__________ Black Tracksuit ____________-->


<!-- Tracksuit -->

  <ThingDef ParentName="ApparelMakeableBase">
    <defName>ODZ_METRO2033_TracksuitBlack</defName>
    <description>A tracksuit, nothing special, but it sure looks cool.</description>
    <label>black tracksuit</label>
    <recipeMaker>
      <researchPrerequisite>ODZ_METRO2033_Russia</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Onskin/Tracksuit/Black/Tracksuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
      <drawSize>1.7</drawSize>	  
    </graphicData>
      <costStuffCount>65</costStuffCount>
      <stuffCategories>
        <li>Fabric</li>
      </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <WorkToMake>2600</WorkToMake>
      <Mass>0.1</Mass>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.2</EquipDelay>
    </statBases>
    <apparel>
<useWornGraphicMask>true</useWornGraphicMask>		
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Legs</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Onskin/Tracksuit/Black/Tracksuit</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
		<li>Lowtier</li>
        <li>Civilian</li>		
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
