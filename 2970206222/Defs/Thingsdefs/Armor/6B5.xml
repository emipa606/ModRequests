<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
	<!--__________ 6b5 __________-->
  
  	<!-- 6b5 -->  
  <ThingDef ParentName="ArmorMachineableBase">
    <defName>ODZ_METRO2033_6b5Armor</defName>
    <label>6b5 vest</label>
    <description>A late period soviet design for spetsnaz units but later adopted by the new armed forces of the russian federation.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>ODZ_METRO2033_Russia</researchPrerequisite>  
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Armor/6b5/6b5</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.7</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>9000</WorkToMake>
      <MaxHitPoints>300</MaxHitPoints>
      <Mass>4</Mass>
      <Flammability>0.6</Flammability>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.38</ArmorRating_Blunt>
      <ArmorRating_Heat>0.27</ArmorRating_Heat>
      <Insulation_Cold>1</Insulation_Cold>
      <EquipDelay>4</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <AimingDelayFactor>-0.03</AimingDelayFactor>
	  <CarryingCapacity>20</CarryingCapacity>
	  <MedicalTendSpeed>0.05</MedicalTendSpeed>	
      <MoveSpeed>-0.15</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <costList>
      <Cloth>40</Cloth>
      <Steel>65</Steel>
    </costList>
    <apparel>
	<careIfWornByCorpse>true</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Armor/6b5/6b5</wornGraphicPath>
      <layers>
        <li>Middle</li>		
      </layers>
      <tags>
	  <li>Soviet</li>
	  <li>AFRF</li>
	  <li>Redline</li>
	  <li>Reich</li>
	  <li>Midtier</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
	    <colorGenerator Class="ColorGenerator_Options" Inherit="False">
      <options>
        <li>
          <weight>1</weight>
          <only>(130, 119, 88)</only>
        </li>           
		<li>
          <weight>1</weight>
          <only>(121, 122, 92)</only>
        </li>        
      </options>
    </colorGenerator>		
  </ThingDef>

