<?xml version="1.0" encoding="utf-8" ?>

<Defs>
  
<ThingDef ParentName="DryadAnimalThingBase">
    <defName>DryadSpiteRevenant</defName>
    <label>spite-revenant</label>
    <description>From the dark and deep woods comes the spite-revenant. Merciless, fast and deadly they come to do their duty and uphold that which once was.</description>
    <statBases>
      <PainShockThreshold>2.5</PainShockThreshold>
      <MoveSpeed>4.8</MoveSpeed>
      <ComfyTemperatureMin>-60</ComfyTemperatureMin>
      <ComfyTemperatureMax>70</ComfyTemperatureMax>
      <MarketValue>700</MarketValue>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <ArmorRating_Blunt>0.34</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
    </statBases>
  <tools>
    <li>
      <label>right shifting branch</label>
      <capacities>
        <li>Scratch</li>
        <li>Cut</li>
        <li>Blunt</li>
      </capacities>
      <power>11</power>
      <cooldownTime>1.8</cooldownTime>
      <linkedBodyPartsGroup>DryadRightBranchG</linkedBodyPartsGroup>
      <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
    </li>
    <li>
      <label>left shifting branch</label>
      <capacities>
        <li>Scratch</li>
        <li>Cut</li>
        <li>Blunt</li>
      </capacities>
      <power>11</power>
      <cooldownTime>1.8</cooldownTime>
      <linkedBodyPartsGroup>DryadLeftBranchG</linkedBodyPartsGroup>
      <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
    </li>
  </tools>
    <race>
      <useMeatFrom>Alien_Dryad</useMeatFrom>
      <needsRest>false</needsRest>
      <intelligence>ToolUser</intelligence>
      <thinkTreeMain>Mechanoid</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <body>TreeRevenant</body>
      <trainability>Intermediate</trainability>
      <predator>true</predator>
      <packAnimal>false</packAnimal>
      <baseHungerRate>0.0</baseHungerRate>
      <lifeExpectancy>1000</lifeExpectancy>
      <baseBodySize>1</baseBodySize>
      <bloodDef>DryadChlorophyll</bloodDef>
      <makesFootprints>false</makesFootprints>
      <baseHealthScale>1.5</baseHealthScale>
      <gestationPeriodDays>32</gestationPeriodDays>
      <wildness>0.5</wildness>
      <foodType>CarnivoreAnimal</foodType>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.75</manhunterOnTameFailChance>
      <herdAnimal>false</herdAnimal>
      <nameOnTameChance>0.1</nameOnTameChance>
      <nameOnNuzzleChance>0.1</nameOnNuzzleChance>
      <herdMigrationAllowed>false</herdMigrationAllowed>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.01</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.2</minAge>
          <soundWounded>Pawn_BigInsect_Pain</soundWounded>
          <soundDeath>Pawn_BigInsect_Death</soundDeath>
          <soundCall>Pawn_BigInsect_Call</soundCall>
          <soundAngry>Pawn_BigInsect_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>Dryad_Reliq</li>
    </tradeTags>
  </ThingDef>

