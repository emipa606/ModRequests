<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>DryadPlantCorpse</defName>
    <label>embed seed in corpse</label>
    <description>Have a dryad embed a seed in a corpse.</description>
    <jobString>Embedding seed.</jobString>
    <workAmount>1000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>DryadMilkSeed</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>CorpsesHumanlike</li>
      </categories>
      <thingDefs>
        <li>DryadMilkSeed</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <DryadCorpseRevenant>1</DryadCorpseRevenant>
    </products>
    <recipeUsers>
      <li>DryadCircle</li>
    </recipeUsers>
  </RecipeDef>

</Defs>