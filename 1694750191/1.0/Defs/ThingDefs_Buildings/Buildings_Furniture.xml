<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>DryadSleepingSpot</defName>
    <label>dryad resting rune</label>
    <description>A dryad resting at this spot will recover from injuries at a faster rate than normal. Needs to be in contact with the earth (diggable ground).</description>
    <thingClass>Building_Bed</thingClass>
    <graphicData>
      <texPath>Things/Building/DryadSleepingSpot/DryadSleepingSpot</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1.7)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <statBases>
      <WorkToBuild>200</WorkToBuild>
      <BedRestEffectiveness>1</BedRestEffectiveness>
      <Comfort>1</Comfort>
			<SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
    </statBases>
		<scatterableOnMapGen>false</scatterableOnMapGen>
    <useHitPoints>False</useHitPoints>
    <size>(1,1)</size>
    <uiIconScale>0.7</uiIconScale>
    <designationCategory>Furniture</designationCategory>
    <passability>Standable</passability>
    <drawGUIOverlay>True</drawGUIOverlay>
    <defaultPlacingRot>South</defaultPlacingRot>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <sowTag>SupportPlantsOnly</sowTag>
      <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
      <ai_chillDestination>false</ai_chillDestination>
      <bed_healPerDay>10</bed_healPerDay>
    </building>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <designationHotKey>Misc1</designationHotKey>
		<comps>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
		</comps>
  </ThingDef>

