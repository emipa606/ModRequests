  <ThingDef ParentName="BuildingBase">
    <defName>DryadSculptureSpot</defName>
    <label>dryad sculpture spot</label>
    <description>A place to sculpt dryadic themed sculptures. Needs to be in contact with the earth  (diggable ground).</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Building/DryadSculptureSpot/DryadSculptureSpot</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1.2)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <statBases>
      <WorkToBuild>100</WorkToBuild>
      <WorkTableWorkSpeedFactor>1</WorkTableWorkSpeedFactor>
    </statBases>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <useHitPoints>false</useHitPoints>
    <uiIconScale>1</uiIconScale>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>Standable</passability>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <workSpeedPenaltyOutdoors>false</workSpeedPenaltyOutdoors>
      <workSpeedPenaltyTemperature>false</workSpeedPenaltyTemperature>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
      <ai_chillDestination>false</ai_chillDestination>
    </building>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <designationHotKey>Misc1</designationHotKey>
    <researchPrerequisites>
      <li>DryadSculpture</li>
    </researchPrerequisites>
  </ThingDef>

