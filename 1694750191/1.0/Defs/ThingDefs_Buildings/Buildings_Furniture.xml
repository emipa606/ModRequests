  <ThingDef ParentName="BuildingBase">
    <defName>DryadResearchSpot</defName>
    <label>dryad research spot</label>
    <description>A place to research. Needs to be in contact with the earth  (diggable ground).</description>
    <thingClass>Building_ResearchBench</thingClass>
    <graphicData>
      <texPath>Things/Building/DryadRune/DryadMorai</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,3)</drawSize>
      <drawRotated>false</drawRotated>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <statBases>
      <WorkToBuild>100</WorkToBuild>
      <ResearchSpeedFactor>0.5</ResearchSpeedFactor>
