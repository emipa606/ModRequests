<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <WorkGiverDef>
    <defName>DoBillsUseDryadCircle</defName>
    <label>make things at dryad circle</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Art</workType>
    <priorityInType>100</priorityInType>
    <fixedBillGiverDefs>
      <li>DryadCircle</li>
    </fixedBillGiverDefs>
    <verb>sculpt</verb>
    <gerund>sculpting at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

