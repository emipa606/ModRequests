        </statOffsets>
		<capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>0.3</offset>
          </li>				  
        </capMods>
	  </li>      
	  <li>        
		<minSeverity>0.35</minSeverity>
		<label>Sammy Samsa, Majer(sic) League Boofball.</label>
		<painFactor>-0.15</painFactor>      
		<statOffsets>
		  <WorkSpeedGlobal>-0.75</WorkSpeedGlobal>
          <NegotiationAbility>-0.55</NegotiationAbility>
		  <ShootingAccuracyPawn>-5</ShootingAccuracyPawn>
		  <AimingDelayFactor>-0.10</AimingDelayFactor>		  
        </statOffsets>
		<capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>0.5</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>1</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>-1</offset>
          </li>
        </capMods>
      </li>	  
	  <li>
	   <label>salvia dance fight revolution</label>
	   <minSeverity>0.60</minSeverity>
	   <painFactor>-0.60</painFactor>       
		<statOffsets>
		  <WorkSpeedGlobal>-0.75</WorkSpeedGlobal>
          <NegotiationAbility>-0.75</NegotiationAbility>
		  <ShootingAccuracyPawn>-10</ShootingAccuracyPawn>
		  <AimingDelayFactor>-0.90</AimingDelayFactor>		  
        </statOffsets>
		<capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>2</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>2</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>-2</offset>
          </li>
        </capMods>
		<mentalStateGivers>
          <li>
            <mentalState>Berserk</mentalState>
            <mtbDays>0.4</mtbDays>
          </li>
		  <li>
            <mentalState>Tantrum</mentalState>
            <mtbDays>0.4</mtbDays>
          </li>
        </mentalStateGivers>		
	  </li>
	  <li>	  
	  <label>mushmarlow brain</label>
	   <minSeverity>0.97</minSeverity>
	   <painFactor>-0.60</painFactor>       
		<statOffsets>		  
          <NegotiationAbility>-0.75</NegotiationAbility>
		  <ShootingAccuracyPawn>-10</ShootingAccuracyPawn>
		  <AimingDelayFactor>-0.90</AimingDelayFactor>
		  <WorkSpeedGlobal>-0.90</WorkSpeedGlobal>
        </statOffsets>
		<capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>2</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>2</offset>
          </li>
		  <li>
