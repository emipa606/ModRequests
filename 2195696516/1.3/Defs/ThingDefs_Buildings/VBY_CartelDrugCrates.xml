<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ========================= Crashed ship parts ========================= -->

  <ThingDef Abstract="True" Name="VBY_BulkCratesDrugsBase" ParentName="BuildingBase">
     <!--
    <size>(6,3)</size>
	<tickerType>Normal</tickerType>
	-->
	<minifiedDef>MinifiedThing</minifiedDef>
	<thingCategories>
      <li>VBY_CrateCategories</li>
    </thingCategories>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.15</fillPercent>
	<pathCost>30</pathCost>
    <altitudeLayer>Item</altitudeLayer>
    <blockWind>false</blockWind>
    <receivesSignals>false</receivesSignals>
	<scatterableOnMapGen>false</scatterableOnMapGen>
    <rotatable>false</rotatable>
	<stealable>true</stealable>
	<castEdgeShadows>false</castEdgeShadows>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
	<soundDrop>Wood_Drop</soundDrop>   
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
	<resourcesFractionWhenDeconstructed>1</resourcesFractionWhenDeconstructed>
    <neverMultiSelect>false</neverMultiSelect>
	<socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>     
      <Flammability>0.7</Flammability>      
	  <Beauty>-5</Beauty>
	  <Comfort>0.15</Comfort>
	  <SellPriceFactor>0.50</SellPriceFactor>
    </statBases>	
	<tradeTags>      
	  <li>CartelDrugs</li>
    </tradeTags>
    <damageMultipliers>
      <li>
        <damageDef>Flame</damageDef>
        <multiplier>1.5</multiplier>
      </li>
    </damageMultipliers>
    <building>      
      <claimable>true</claimable>
      <roofCollapseDamageMultiplier>0.2</roofCollapseDamageMultiplier>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>  <!-- Customize  -->
	  <ai_chillDestination>false</ai_chillDestination>
      <artificialForMeditationPurposes>false</artificialForMeditationPurposes>
	  <isSittable>true</isSittable>
	  <allowAutoroof>false</allowAutoroof>
	  <alwaysDeconstructible>true</alwaysDeconstructible>
	  <expandHomeArea>false</expandHomeArea>
    </building>    
  </ThingDef>
  
  <ThingDef Abstract="True" Name="VBY_HookupCratesDrugsBase" ParentName="VBY_BulkCratesDrugsBase">
    <tradeTags Inherit="False">      
	  <li>CartelHookupDrugs</li>
    </tradeTags>
  </ThingDef>
	
  
