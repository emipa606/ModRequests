  <RecipeDef Name="VBY_UnpackDrugsBase" Abstract="True">
	<label>unpack drug bundles</label>
    <description>Remove drugs from their packaging.</description>
    <jobString>Removing drugs from their packaging.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>100</workAmount>
    <unfinishedThingDef>UnfinishedHealthItemProsthetic</unfinishedThingDef>
	<recipeUsers>
		<li>CraftingSpot</li>
        <li>DrugLab</li>
	</recipeUsers>
   </RecipeDef>
   
   
