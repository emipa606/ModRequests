<Defs>


	<ThingDef ParentName="DankPyon_RuinedBuildingBase">
		<defName>Seti_shedskinruin</defName>
		<label>shed snake skin</label>
		<description>The skin of a great serpent.</description>
		<graphicData>
			<texPath>Ruins/shedskinruin</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(2,1)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
				<fillPercent>0.2</fillPercent>
		<pathCost>42</pathCost>
		<defaultPlacingRot>South</defaultPlacingRot>
		<costList>
			<DankPyon_Leather_Lindwurm>20</DankPyon_Leather_Lindwurm>
		</costList>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Mass>10</Mass>
			<WorkToBuild>200</WorkToBuild>
			<Flammability>0.2</Flammability>
		</statBases>
		<size>(2,1)</size>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<castEdgeShadows>false</castEdgeShadows>
		<surfaceType>Item</surfaceType>
	</ThingDef>
	
