  <QuestEditor_Library.CaravanActionDef>
    <defName>setilocatedaer</defName>
    <label>Locate Daer</label>
    <description>Track down a Daer Lair.</description>
    <iconPath>WorldObjects/Locationdaer</iconPath>
	<conditions>
	<li Class="QuestEditor_Library.WorldCondition_TotalSkill">
		<skill>Animals</skill>
		<vaule>8</vaule>
	</li>
	</conditions>
    <actions>
      <li Class="QuestEditor_Library.WorldAction_GenerateQuest">
			<setCaravanTileAsTarget>true</setCaravanTileAsTarget>
			<quest>seti_daerquest</quest>
      </li>
    </actions>
  </QuestEditor_Library.CaravanActionDef>
  
