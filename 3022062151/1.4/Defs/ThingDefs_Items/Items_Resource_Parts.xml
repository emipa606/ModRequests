<Defs>
	<ThingDef ParentName="ResourceBase">
		<defName>Cog_Wood</defName>
		<label>Wooden Cogwheel</label>
		<description>a wooden cogwheel for creation of early vehicles.</description>
		<graphicData>
			<texPath>Things/Items/Parts/Cog_Wood</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<rotatable>false</rotatable>
		<stackLimit>20</stackLimit>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<statBases>
			<MarketValue>2.2</MarketValue>
			<Mass>0.8</Mass>
			<Flammability>0.05</Flammability>
			<MaxHitPoints>60</MaxHitPoints>
		</statBases>
	</ThingDef>

