<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="BuildingBase">
		<defName>EVP_Table_Carpentry</defName>
		<label>Carpentry table</label>
		<description>a table for carpentry.</description>    
		<thingClass>Building_WorkTable</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
      		<texPath>Things/Building/Production/TableCarpentry/TableCarpentry</texPath>
      		<graphicClass>Graphic_Multi</graphicClass>
      		<shaderType>CutoutComplex</shaderType>
      		<drawSize>(4,2)</drawSize>
    	</graphicData>
    	<altitudeLayer>Building</altitudeLayer>
    	<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Early_Early_Vehicles</designationCategory>
    	<size>(3,1)</size>
    	<pathCost>70</pathCost>
    	<hasInteractionCell>true</hasInteractionCell>
    	<interactionCellOffset>(0,0,-1)</interactionCellOffset>
    	<passability>PassThroughOnly</passability>
    	<surfaceType>Item</surfaceType>
    	<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    	<building>
			<uninstallWork>125</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<costStuffCount>100</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<researchPrerequisites>
      		<li>Early_Vehicle_Parts</li>
    	</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>EVP_Table_Carpentry_Large</defName>
		<label>Large Carpentry table</label>
		<description>a large table for carpentry.</description>    
		<thingClass>Building_WorkTable</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
      		<texPath>Things/Building/Production/LargeCarpentryTable/LargeCarpentryTable</texPath>
      		<graphicClass>Graphic_Multi</graphicClass>
      		<shaderType>CutoutComplex</shaderType>
      		<drawSize>(4,3)</drawSize>
    	</graphicData>
    	<altitudeLayer>Building</altitudeLayer>
    	<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>750</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Early_Early_Vehicles</designationCategory>
    	<size>(3,2)</size>
    	<pathCost>70</pathCost>
    	<hasInteractionCell>true</hasInteractionCell>
    	<interactionCellOffset>(0,0,-1)</interactionCellOffset>
    	<passability>PassThroughOnly</passability>
    	<surfaceType>Item</surfaceType>
    	<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    	<building>
			<uninstallWork>125</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<costStuffCount>200</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
    	<costList>
      		<Joint_Butt>4</Joint_Butt>
      		<Nail_Simple>2</Nail_Simple>
      		<Rope_Cloth>1</Rope_Cloth>
    	</costList>
		<researchPrerequisites>
      		<li>Large_Vehicle_Parts</li>
    	</researchPrerequisites>
	</ThingDef>

</Defs>