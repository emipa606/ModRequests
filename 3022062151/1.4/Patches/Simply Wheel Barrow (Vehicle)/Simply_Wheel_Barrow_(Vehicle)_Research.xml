<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Wheel Barrow (Vehicle)</li>
		</mods>
			
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName="MWB_TheWheel"]</xpath>
					<value>
						<prerequisites>
							<li>Early_Vehicle_Parts</li>
						</prerequisites>
						<tab>Early_Vehicle_Parts_Tab</tab>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="MWB_TheWheel"]/researchViewX</xpath>
					<value>
						<researchViewX>1</researchViewX>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="MWB_TheWheel"]/researchViewY</xpath>
					<value>
						<researchViewY>3</researchViewY>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>
 
