<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Wheel Barrow (Vehicle)</li>
		</mods>
	<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="MWB_WheelBarrowBluePrint"]/costList</xpath>
					<value>
            				<costList>
            					<WoodLog>60</WoodLog>
            					<Wheel_Wood>1</Wheel_Wood>
            					<Peg_Wood>2</Peg_Wood>
            					<Joint_Butt>4</Joint_Butt>
            				</costList>
					</value>
				</li>

			<li Class="PatchoperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="MWB_WheelBarrowCBluePrint"]/costList</xpath>
					<value>
            				<costList>
            					<WoodLog>60</WoodLog>
            					<Wheel_Wood>1</Wheel_Wood>
            					<Peg_Wood>2</Peg_Wood>
            					<Joint_Butt>4</Joint_Butt>
            				</costList>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="MWB_WheelBarrowBluePrint"]/designationCategory</xpath>
					<value>
						<designationCategory>Early_Early_Vehicles</designationCategory>
					</value>
				</li>

			<li Class="PatchoperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="MWB_WheelBarrowCBluePrint"]/designationCategory</xpath>
					<value>
						<designationCategory>Early_Early_Vehicles</designationCategory>
					</value>
				</li>

			</operations>
		</match>

	</Operation>

</Patch>