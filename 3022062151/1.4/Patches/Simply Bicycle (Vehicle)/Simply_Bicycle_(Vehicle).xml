<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Wheel Barrow (Vehicle)</li>
		</mods>
	<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="SIMB_SimpleBicycleBluePrint"]/costList</xpath>
					<value>
            				<costList>
            					<WoodLog>80</WoodLog>
            					<Wheel_Wood_Large>2</Wheel_Wood_Large>
            					<Nail_Simple>4</Nail_Simple>
            					<Cog_Wood>2</Cog_Wood>
            					<Cog_Wood_Large>1</Cog_Wood_Large>
            				</costList>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleBuildDef[defName="SIMB_SimpleBicycleBluePrint"]/designationCategory</xpath>
					<value>
						<designationCategory>Early_Early_Vehicles</designationCategory>
					</value>
				</li>

			</operations>
		</match>

	</Operation>

</Patch>