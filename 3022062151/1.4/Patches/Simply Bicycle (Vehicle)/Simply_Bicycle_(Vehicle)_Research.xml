<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Bicycle (Vehicle)</li>
		</mods>
			
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName="SIMB_cycling"]</xpath>
					<value>
						<tab>Early_Vehicle_Parts_Tab</tab>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="SIMB_cycling"]/researchViewX</xpath>
					<value>
						<researchViewX>2</researchViewX>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="SIMB_cycling"]/researchViewY</xpath>
					<value>
						<researchViewY>2</researchViewY>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>
 
