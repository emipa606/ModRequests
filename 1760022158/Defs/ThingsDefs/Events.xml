  <ThingDef ParentName="BuildingBase">
    <defName>OpenableContainer</defName>
    <label>Container</label>
    <description>Container containing resources</description>
    <thingClass>MoreEvents.Events.ShipCrash.Building_Container</thingClass>
    <graphicData>
      <texPath>Things/Buildings/Container</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <rect>(0.1,0.1,1.8,1.8)</rect>
      </damageData>
      <shadowData>
        <volume>(1.39,0.5,1.25)</volume>
      </shadowData>
    </graphicData>
    <size>(2,2)</size>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <blockWind>true</blockWind>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <WorkToBuild>3200</WorkToBuild>
      <MaxHitPoints>250</MaxHitPoints>
      <Flammability>0.5</Flammability>
    </statBases>
    <tickerType>Normal</tickerType>
	<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(1,0,0)</interactionCellOffset>
    <defaultPlacingRot>South</defaultPlacingRot>
		<building>
			<alwaysDeconstructible>true</alwaysDeconstructible>
			<claimable>false</claimable>
		</building>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>2.66</explosiveRadius>
        <explosiveDamageType>Flame</explosiveDamageType>
      </li>
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
  </ThingDef>

 
