<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <JobDef>
    <defName>NN_SetupChannel</defName>
    <driverClass>NoobertNebulous.JobDriver_SetupChannel</driverClass>
    <reportString>using streaming console to set up a vidtube channel.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>NN_OpenVidtube</defName>
    <driverClass>NoobertNebulous.JobDriver_OpenVidtube</driverClass>
    <reportString>using streaming console to open vidtube.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>NN_DeleteChannel</defName>
    <driverClass>NoobertNebulous.JobDriver_DeleteChannel</driverClass>
    <reportString>using streaming console to delete vidtube channel.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>NN_RecordVideo</defName>
    <driverClass>NoobertNebulous.JobDriver_RecordVideo</driverClass>
    <reportString>using streaming console to record a video.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

</Defs>
