<!-- Vault Gear Door SFX -->

  <SoundDef>
    <defName>VaultDoorOpenPowered</defName>  
    <context>MapOnly</context> 
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <name>False</name>      
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Misc/Door/VaultDoorOpen</clipPath>
