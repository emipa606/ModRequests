  <!-- 뺨 -->
  <GeneDef Name="papagoCheekBase" Abstract="True">
    <description>The cute cheek of papou. Parrot genes have been modified for humans. Carriers have a circular color on their cheeks.</description>
    <graphicData>
      <graphicPath>papago/cheek/cheek</graphicPath>
      <narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
      <useSkinShader>false</useSkinShader>
      <layer>PostHair</layer>
      <drawLoc>HeadLower</drawLoc>
      <drawOffsetNorth>(0, 0, 0)</drawOffsetNorth>
      <drawOffsetSouth>(0, 0, 0)</drawOffsetSouth>
      <drawOffsetEast>(0, 0, 0)</drawOffsetEast>
    </graphicData>
    <displayCategory>Cosmetic</displayCategory>
    
    <biostatCpx>0</biostatCpx>
    <endogeneCategory>Nose</endogeneCategory>
    <iconColor>(0.75, 0.75, 0.75)</iconColor>
    <randomChosen>true</randomChosen>
    <exclusionTags>
      <li>Cheek</li>
    </exclusionTags>
    <randomBrightnessFactor>0.12</randomBrightnessFactor>
  </GeneDef>
  
  <GeneDef ParentName="papagoCheekBase">
    <defName>papagoCheek0</defName>
    <label>Papou Cheek</label>
    <iconPath>papago/gene_cheek</iconPath>
    <iconColor>(255, 255, 255)</iconColor>
    <graphicData>
      <colorType>Hair</colorType>
    </graphicData>
  </GeneDef>
  
