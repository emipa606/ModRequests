  <!-- 귀 -->
  <GeneDef ParentName="GeneEarsBase" Name="papagoEarBase" Abstract="True">
    <description>The beautiful ear of papou. Parrot genes have been modified for humans.</description>
    <graphicData>
      <graphicPath>papago/ear/ear</graphicPath>
      <layer>PostHair</layer>
      <drawNorthAfterHair>false</drawNorthAfterHair>
      <!-- <useSkinShader>true</useSkinShader> -->
      <drawLoc>HeadTop</drawLoc>
      <drawOffsetNorth>(0, -0.1, 0)</drawOffsetNorth>
      <drawOffsetSouth>(0, 0, 0)</drawOffsetSouth>
      <drawOffsetEast>(0, 0, 0)</drawOffsetEast>
    </graphicData>
    <randomBrightnessFactor>0.12</randomBrightnessFactor>
    
  </GeneDef>
  
