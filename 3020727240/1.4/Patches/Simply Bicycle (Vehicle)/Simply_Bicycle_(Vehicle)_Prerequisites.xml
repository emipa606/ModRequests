<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Bicycle (Vehicle)</li>
		</mods>
				
			<match Class="PatchOperationReplace">
				<xpath>Defs/ResearchProjectDef[defName="SIMB_cycling"]/prerequisites</xpath>
				<value>
					<prerequisites>
						<li>MWB_TheWheel</li>
					</prerequisites>
				</value>
			</match>

	</Operation>

		<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Simply Bicycle (Vehicle)</li>
		</mods>
				
			<match Class="PatchOperationFindMod">
				<mods>
					<li>Early Vehicle Parts</li>
				</mods>

				<match Class="PatchOperationReplace">
				<xpath>Defs/ResearchProjectDef[defName="SIMB_cycling"]/prerequisites</xpath>
				<value>
					<prerequisites>
						<li>MWB_TheWheel</li>
						<li>Large_Vehicle_Parts</li>
					</prerequisites>
				</value>

				</match>
			</match>

	</Operation>
	
</Patch>
 
