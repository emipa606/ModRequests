<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- BinaryProcessor -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>BinaryProcessor</defName>
    <label>binary processor</label>
    <labelNoun>a binary processor</labelNoun>
    <description>An installed binary processor.</description>
    <descriptionHyperlinks><ThingDef>BinaryProcessor</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>BinaryProcessor</spawnThingOnRemoved>
<stages>
      <li>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>+0.10</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_ReactOnDamage">
        <damageDefIncoming>EMP</damageDefIncoming>
        <createHediff>BrainShock</createHediff>
      </li>
    </comps>
  </HediffDef>

  <ThingDef ParentName="BodyPartBionicExoBase">
    <defName>BinaryProcessor</defName>
    <label>binary processor</label>
    <description>A binary computer that syncs with the user's brain and processes some of the tasks independently.</description>
    <descriptionHyperlinks><RecipeDef>InstallBinaryProcessor</RecipeDef></descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Health/HealthItem</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.80</drawSize>
      <color>(189,169,118)</color>
    </graphicData>
    <thingSetMakerTags><li>RewardStandardLowFreq</li></thingSetMakerTags>
    <costList>
      <NeuroExoKit>1</NeuroExoKit>
      <ComponentSpacer>4</ComponentSpacer>
      <NeuroExoWire>10</NeuroExoWire>
	  <Plasteel>40</Plasteel>
	  <Synthread>50</Synthread>

    </costList>
    <recipeMaker>
      <researchPrerequisite>NeuralNetwork</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
    <defName>InstallBinaryProcessor</defName>
    <label>install binary processor</label>
    <description>Install a binary processor.</description>
    <descriptionHyperlinks>
      <ThingDef>BinaryProcessor</ThingDef>
      <HediffDef>BinaryProcessor</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing binary processor.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>BinaryProcessor</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>BinaryProcessor</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>BinaryProcessor</addsHediff>
  </RecipeDef>
  
  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>RemoveBinaryProcessor</defName>
    <label>remove binary processor</label>
    <description>Remove binary processor.</description>
    <descriptionHyperlinks>
      <ThingDef>BinaryProcessor</ThingDef>
      <HediffDef>BinaryProcessor</HediffDef>
    </descriptionHyperlinks>
    <jobString>Removing binary processor.</jobString>
    <removesHediff>BinaryProcessor</removesHediff>
  </RecipeDef>
  
