<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Geth Rocket Trooper Child -->
	<AlienRace.AlienBackstoryDef>
		<defName>Geth_RocketChild</defName>
		<title>Geth</title>
		<titleShort>Geth</titleShort>
		<baseDesc>A humanoid race of networked AIs that were created by the quarians 300 years ago as tools of labor and war. Having since won their freedom from their creators, the geth have proven they are able to reason, analyze situations, and deploy tactics as well as any organic race so long as they remain networked with other geth.</baseDesc>
		<bodyTypeMale>GethRocketTrooper</bodyTypeMale>
		<bodyTypeFemale>GethRocketTrooper</bodyTypeFemale>
		<slot>Childhood</slot>
		<spawnCategories>
			<li>GethRocketTrooperBackstory</li>
		</spawnCategories>
		<requiredWorkTags>None</requiredWorkTags>
		<shuffleable>True</shuffleable>
		<forcedTraitsChance>
			<li>
				<defName>GethNetworkedAI</defName>
				<chance>100</chance>
			</li>
			<li>
				<defName>GethNimble</defName>
				<chance>100</chance>
			</li>
			<li>
				<defName>GethAimAccuracy</defName>
				<chance>100</chance>
			</li>
		</forcedTraitsChance>
	</AlienRace.AlienBackstoryDef>
	<!-- Geth Rocket Trooper Adult -->
	<AlienRace.AlienBackstoryDef>
		<defName>Geth_RocketTrooper</defName>
		<title>Geth Rocket Trooper</title>
		<titleShort>Rocket Trooper</titleShort>
		<baseDesc>Geth Rocket Troopers bombard their foe's from long range using rockets.</baseDesc>
		<bodyTypeMale>GethRocketTrooper</bodyTypeMale>
		<bodyTypeFemale>GethRocketTrooper</bodyTypeFemale>
		<bodyTypeGlobal>GethRocketTrooper</bodyTypeGlobal>
		<slot>Adulthood</slot>
		<skillGains>
			<li>
				<key>Shooting</key>
				<value>10</value>
			</li>
			<li>
				<key>Melee</key>
				<value>4</value>
			</li>
			<li>
				<key>Intellectual</key>
				<value>6</value>
			</li>
		</skillGains>
		<spawnCategories>
