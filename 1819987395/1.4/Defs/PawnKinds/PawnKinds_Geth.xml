<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Legion -->
	<PawnKindDef>
		<defName>BaseLegion</defName>
		<race>Geth</race>
		<label>Legion</label>
		<factionLeader>true</factionLeader>
		<factionHostileOnDeath>true</factionHostileOnDeath>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>GethLegionBackstory</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<defaultFactionType>GethConsensus</defaultFactionType>
		<minGenerationAge>20</minGenerationAge>
		<destroyGearOnDrop>false</destroyGearOnDrop>
		<weaponMoney>9999~9999</weaponMoney>
		<aiAvoidCover>false</aiAvoidCover>
		<combatPower>120</combatPower>
		<isFighter>true</isFighter>
		<weaponTags>
			<li>GethM98Widow</li>
		</weaponTags>
		<biocodeWeaponChance>1.0</biocodeWeaponChance>
		<initialWillRange>1~3</initialWillRange>
		<initialResistanceRange>12~19</initialResistanceRange>
	</PawnKindDef>
