<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <DamageDef>
        <defName>YP_RangedEntangle</defName>
        <label>entangle</label>
        <workerClass>DamageWorker_AddInjury</workerClass>
        <externalViolence>true</externalViolence>
        <deathMessage>{0} has been entangled to death.</deathMessage>
        <hediff>NetGunshot</hediff>
        <hediffSolid>Crack</hediffSolid>
        <impactSoundType>Blunt</impactSoundType>
        <armorCategory>Blunt</armorCategory>
        <overkillPctToDestroyPart>0~0</overkillPctToDestroyPart>
        <isRanged>true</isRanged>
        <harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
        <makesAnimalsFlee>true</makesAnimalsFlee>
        <hasForcefulImpact>true</hasForcefulImpact>
        <defaultStoppingPower>10.0</defaultStoppingPower>
        <harmsHealth>false</harmsHealth>
        <makesBlood>false</makesBlood>
    </DamageDef>

</Defs>