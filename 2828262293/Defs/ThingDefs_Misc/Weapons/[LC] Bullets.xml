  <ThingDef ParentName="BaseBullet">
    <defName>LC_Bullet_QoH</defName>
    <label>QoH Bullet</label>
    <graphicData>
      <texPath>Things/Projectile/Star</texPath>
      <graphicClass>Graphic_Mote</graphicClass>
	  <color>(0.3,0.8,0.6,1)</color>
	  <drawSize>(0.3,0.3)</drawSize>
    </graphicData>
    <projectile>
      <damageDef>BulletHate</damageDef>
      <damageAmountBase>6</damageAmountBase>
      <stoppingPower>1</stoppingPower>
      <speed>25</speed>
    </projectile>
    <!-- Required If you want to perform a ranged attack or heal from the Bullet's impact point  -->
    <thingClass>LobotomyCorp.Weapon.LCBullet</thingClass>
    <!-- Required comps -->
    <comps>
      <li Class="LobotomyCorp.Weapon.CompProperties_BulletExtend">
        <!-- When <healingColonist> is true, if a bullet hits an ally, it does no damage and heals the injured area. -->
        <!-- The recovery target is randomly selected from the injured (uncut) part of the body. -->
        <healingColonist>true</healingColonist>
        <!-- The amount of recovery is the healValue, or 3  if no description is given. -->
        <healValue>3</healValue>
      </li>
    </comps>
    <modExtensions>
      <li Class = "LobotomyCorp.LCDamage">
        <convert>
          <li>
            <type>Projectile</type>
            <LCDamage>BulletHate</LCDamage>
            <noLCDamage>Bullet</noLCDamage>
          </li>
        </convert>
      </li>
    </modExtensions>
  </ThingDef>

  <!--ALEPH-->
  <ThingDef ParentName="BaseBullet">
    <defName>LC_Bullet_Paradise_Lost</defName>
    <label>Bullet Paradise Lost</label>
    <graphicData>
      <texPath>Things/Mote/Blank</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <projectile>
      <damageDef>PaleDmg</damageDef>
      <damageAmountBase>33</damageAmountBase>
      <stoppingPower>3</stoppingPower>
      <speed>100</speed>
	  <landedEffecter>Impact_Lost</landedEffecter>
	  <soundImpact>EGO_Lost_Impact</soundImpact>
    </projectile>
    <!-- Required If you want to perform a ranged attack or heal from the Bullet's impact point  -->
    <thingClass>LobotomyCorp.Weapon.LCBullet</thingClass>
    <!-- Required comps -->
    <comps>
      <li Class="LobotomyCorp.Weapon.CompProperties_BulletExtend">
        <!-- if raduis > 0, Damages anything within radius of the impact point. -->
        <radius>2</radius>
        <!-- It can also be defined in aoeType. In that case, you can probably make a square from the point of impact, but I don't assume much. -->
        <!-- You can use either <radius> on top or <aoeType> on the bottom. -->
        <!-- If you were to write a circular attack with range 3 in the same way in aoeType, it would look like this -->
        <!-- <aoeType>
          <multi>Circle</multi>
          <radius>3</radius>
        </aoeType> -->
      </li>
    </comps>
    <modExtensions>
      <li Class = "LobotomyCorp.LCDamage">
        <convert>
          <li>
            <type>Projectile</type>
            <LCDamage>PaleDmg</LCDamage>
            <noLCDamage>Bullet</noLCDamage>
          </li>
        </convert>
      </li>
    </modExtensions>
  </ThingDef>

