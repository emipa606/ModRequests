<Defs>
<!--Neolithic-->
<!--Rungu-->
<ThingDef ParentName="BaseMeleeWeapon_Blunt_Quality">
    <defName>YeetBlade_Rungu</defName>
    <label>rungu</label>
    <description>A simple throwing club, light enough to be thrown, but still deadly.</description>
    <graphicData>
      <texPath>Things/Rungu</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.2</uiIconScale>
    <techLevel>Neolithic</techLevel>
    <costStuffCount>30</costStuffCount>
    <statBases>
      <WorkToMake>1200</WorkToMake>
      <Mass>1.25</Mass>
      <AccuracyTouch>0.85</AccuracyTouch>
      <AccuracyShort>0.85</AccuracyShort>
      <AccuracyMedium>0.75</AccuracyMedium>
      <AccuracyLong>0.60</AccuracyLong>
      <RangedWeapon_Cooldown>1</RangedWeapon_Cooldown>
     </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <recipeMaker>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
    </recipeMaker>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <weaponTags>
      <li>NeolithicMeleeBasic</li>
    </weaponTags>
	<verbs>
      <li>
        <verbClass>Verb_LaunchProjectile</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Rungu_Thrown</defaultProjectile>
        <warmupTime>2</warmupTime>
        <range>15</range> 
        <soundCast>Interact_BeatFire</soundCast>
        <rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
      </li>
    </verbs>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>head</label>
        <labelUsedInLogging>false</labelUsedInLogging>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>
  <ThingDef ParentName="BaseProjectileNeolithic">
    <defName>Rungu_Thrown</defName>
    <label>rungu</label>
    <graphicData>
      <texPath>Things/Thrown_Projectile</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <projectile>
		<arcHeightFactor>1</arcHeightFactor>
      <flyOverhead>false</flyOverhead>
      <damageDef>Blunt</damageDef>
      <damageAmountBase>12</damageAmountBase>
      <stoppingPower>3</stoppingPower>
      <speed>50</speed>
    </projectile>
  </ThingDef>
  
