<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			
			<!-- ============================ Rocket Removal ========================== -->
			
			<!-- ===== weaponTags ===== -->
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Gun_TripleRocket" or defName="Gun_DoomsdayRocket"]/weaponTags</xpath>
			</li>
			
			<!-- ===== tradeTags ===== -->
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName = "Gun_TripleRocket" or defName="Gun_DoomsdayRocket"]/tradeTags</xpath>
			</li>
			
			<!-- ===== thingSetMakerTags ===== -->
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName = "Gun_TripleRocket" or defName="Gun_DoomsdayRocket"]/thingSetMakerTags</xpath>
			</li>
			
		</operations>
	</Operation>
</Patch>