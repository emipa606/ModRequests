    <ThingDef ParentName="BuildingBase">
    <defName>ResearchSubAIComputer</defName>
    <label>research sub ai computer</label>
	<description>An Sub AI computer powered by subpersona cores, capable of assisting in sensitive research. Gives a large boost to nearby research benches equal to your efforts.</description>
    <graphicData>
      <texPath>Things/ResearchSubAIComputer/ResearchSubAIComputer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>2.46875</drawSize>
      <linkFlags>
        <li>Custom6</li>
      </linkFlags>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
        <edgeTop>Damage/Edge</edgeTop>
        <edgeBot>Damage/Edge</edgeBot>
        <edgeLeft>Damage/Edge</edgeLeft>
        <edgeRight>Damage/Edge</edgeRight>
      </damageData>
    </graphicData>
	<uiIconPath>Things/ResearchSubAIComputer/ResearchSubAIComputer_MenuIcon</uiIconPath>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Mass>3</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>1.5</Beauty>
	  <Cleanliness>1.0</Cleanliness>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
    <tickerType>Normal</tickerType>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsSpecial</li>
    </thingCategories>
	<rotatable>true</rotatable>
    <researchPrerequisites>
      <li>SterileMaterials</li>
      <li>MultiAnalyzer</li>
    </researchPrerequisites>
    <size>(2,2)</size>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>250</costStuffCount>
    <costList>
      <Steel>120</Steel>
      <Plasteel>120</Plasteel>
	  <Silver>120</Silver>
      <Gold>120</Gold>
      <TechprofSubpersonaCore>1</TechprofSubpersonaCore>
    </costList>
	<designationCategory>Misc</designationCategory>
    <comps>
      <li Class="CompProperties_ThrownFleckEmitter">
        <fleck>SparkFlash</fleck>
        <offsetMin>(-0.6,0,0.3)</offsetMin>
        <offsetMax>(0.6,0,0.5)</offsetMax>
        <emissionInterval>75</emissionInterval>
        <scale>0.6~0.9</scale>
        <rotationRate>-15~15</rotationRate>
        <velocityX>0.3~0.7</velocityX>
        <velocityY>0.7~1.0</velocityY>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>3</glowRadius>
        <glowColor>(0,0,255,0)</glowColor>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>400</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <ResearchSpeedFactor>1.00</ResearchSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
		<maxDistance>10</maxDistance>
      </li>
    </comps>
	<placeWorkers>
		<li>PlaceWorker_ShowFacilitiesConnections</li>
	</placeWorkers>
    <constructionSkillPrerequisite>10</constructionSkillPrerequisite>
    <uiIconScale>0.85</uiIconScale>
  </ThingDef>
  
