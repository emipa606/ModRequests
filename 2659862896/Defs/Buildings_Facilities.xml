<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="BuildingBase">
    <defName>ResearchPad</defName>
    <label>research tablet</label>
	<description>A waist high table with a Tablet for performing sensitive research on. Gives a small boost to nearby research benches.</description>
    <graphicData>
      <texPath>Things/ResearchPad/ResearchPad</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>2.46875</drawSize>
      <linkFlags>
        <li>Custom6</li>
      </linkFlags>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
        <edgeTop>Damage/Edge</edgeTop>
        <edgeBot>Damage/Edge</edgeBot>
        <edgeLeft>Damage/Edge</edgeLeft>
        <edgeRight>Damage/Edge</edgeRight>
      </damageData>
    </graphicData>
	<uiIconPath>Things/ResearchPad/ResearchPad_MenuIcon</uiIconPath>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Mass>3</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>0.5</Beauty>
	  <Cleanliness>0.3</Cleanliness>
    </statBases>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsSpecial</li>
    </thingCategories>
	<rotatable>true</rotatable>
    <researchPrerequisites>
      <li>MicroelectronicsBasics</li>
    </researchPrerequisites>
    <size>(2,2)</size>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <costList>
      <Steel>40</Steel>
      <Plasteel>20</Plasteel>
	  <Silver>20</Silver>
      <Gold>10</Gold>
      <ComponentIndustrial>10</ComponentIndustrial>
    </costList>
	<designationCategory>Misc</designationCategory>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>100</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <ResearchSpeedFactor>0.1</ResearchSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
		<maxDistance>10</maxDistance>
      </li>
    </comps>
	<placeWorkers>
		<li>PlaceWorker_ShowFacilitiesConnections</li>
	</placeWorkers>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <uiIconScale>0.85</uiIconScale>
  </ThingDef>
    
