<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RecipeDef>
    <defName>FF_MakePrimitiveAmmo</defName>
    <label>make primitive ammunition</label>
    <description>Make primitive ammunition.</description>
    <jobString>Making primitive ammunition.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <workAmount>400</workAmount>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>CraftingSpot</li>
      <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Workbench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
            <li MayRequire="sihv.rombones">BoneItem</li>
            <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_DarkWoodLog</li>
            <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_ElvenWoodLog</li>
            <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_DryadWoodLog</li>
            <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_AncientWoodLog</li>
            <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Bone</li>
            <li MayRequire="LadyCookie.AestheticMaterials">RedWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">HighWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">DarkWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">EbonyWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">MahoganyWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">CactusWood</li>
            <li MayRequire="LadyCookie.AestheticMaterials">BambooWood</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <products>
      <FF_Ammunition_Primitive>75</FF_Ammunition_Primitive>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
        <li MayRequire="sihv.rombones">BoneItem</li>
        <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_DarkWoodLog</li>
        <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_ElvenWoodLog</li>
        <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_DryadWoodLog</li>
        <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_AncientWoodLog</li>
        <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Bone</li>
        <li MayRequire="LadyCookie.AestheticMaterials">RedWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">HighWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">DarkWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">EbonyWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">MahoganyWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">CactusWood</li>
        <li MayRequire="LadyCookie.AestheticMaterials">BambooWood</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>

  <RecipeDef>
    <defName>FF_MakeIndustrialAmmo</defName>
    <label>make industrial ammunition</label>
    <description>Make industrial ammunition.</description>
    <jobString>Making industrial ammunition.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>600</workAmount>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
            <li MayRequire="Argon.VMEu">VMEu_Lead</li>
            <li MayRequire="Argon.LVMEu">VMEu_Lead</li>
            <li MayRequire="Mlie.CuprosAlloys">CAL_Lead</li>
            <li MayRequire="test.TMaterials.Metals.1.0">carnap_lead</li>
            <li MayRequire="LadyCookie.AestheticMaterials">Lead</li>
            <li MayRequire="Wolf478.Wolfhaulcore.Metals">WDM_Lead</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <products>
      <FF_Ammunition_Industrial>75</FF_Ammunition_Industrial>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li MayRequire="Argon.VMEu">VMEu_Lead</li>
        <li MayRequire="Argon.LVMEu">VMEu_Lead</li>
        <li MayRequire="Mlie.CuprosAlloys">CAL_Lead</li>
        <li MayRequire="test.TMaterials.Metals.1.0">carnap_lead</li>
        <li MayRequire="LadyCookie.AestheticMaterials">Lead</li>
        <li MayRequire="Wolf478.Wolfhaulcore.Metals">WDM_Lead</li>
        <li>Chemfuel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <skillRequirements>
      <Crafting>4</Crafting>
    </skillRequirements>
    <researchPrerequisites>
      <li>Machining</li>
    </researchPrerequisites>
  </RecipeDef>

  <RecipeDef>
    <defName>FF_MakeSpacerAmmo</defName>
    <label>make charged-shot ammunition</label>
    <description>Make charged-shot ammunition.</description>
    <jobString>Making charged-shot ammunition.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>800</workAmount>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <products>
      <FF_Ammunition_Spacer>75</FF_Ammunition_Spacer>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <researchPrerequisites>
      <li>Fabrication</li>
    </researchPrerequisites>
  </RecipeDef>

</Defs>