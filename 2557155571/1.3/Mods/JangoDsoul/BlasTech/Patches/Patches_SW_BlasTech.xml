<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- ==========Wrist Weapons============ -->
  
<!-- B-2 Super Droid Wrist Blaster -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="B-2_Super_Droid_Wrist_Blaster"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="B-2_Super_Droid_Wrist_Blaster"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="B-2_Super_Droid_Wrist_Blaster"]/comps</xpath>
    <value>
      <li Class="Reloading.CompProperties_Reloadable">
        <compClass>Reloading.CompReloadable</compClass>
        <MaxShots>10</MaxShots> <!-- burstShotCount 1 -->
        <AmmoFilter>
          <thingDefs>
            <li>FF_Ammunition_Spacer</li>
          </thingDefs>
        </AmmoFilter>
        <ItemsPerShot>1</ItemsPerShot>
        <ReloadTimePerShot>0.1</ReloadTimePerShot>
          <GenerateAmmo>
            <FF_Ammunition_Spacer>30~50</FF_Ammunition_Spacer>
          </GenerateAmmo>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="B-2_Super_Droid_Wrist_Blaster"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Spacer_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Spacer_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

