  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[XND] Proper Shotguns (Continued)</li>
    </mods>

    <match Class="PatchOperationAdd">
      <xpath>Defs/thingDef[defName="GD_BSLauncher"]/comps</xpath>
      <value>
          <li Class="Reloading.CompProperties_Reloadable">
