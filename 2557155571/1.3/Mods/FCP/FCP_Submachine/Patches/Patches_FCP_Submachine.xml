<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Silenced .22 SMG -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="FCP_Gun_Silenced_22_SMG"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="FCP_Gun_Silenced_22_SMG"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="FCP_Gun_Silenced_22_SMG"]/comps</xpath>
    <value>
        <li Class="Reloading.CompProperties_Reloadable">
          <compClass>Reloading.CompReloadable</compClass>
          <MaxShots>180</MaxShots>
          <AmmoFilter>
            <thingDefs>
              <li>FF_Ammunition_Industrial</li>
            </thingDefs>
          </AmmoFilter>
          <ItemsPerShot>1</ItemsPerShot>
          <ReloadTimePerShot>0.005</ReloadTimePerShot>
          <ReloadSound>Standard_Reload</ReloadSound>
          <GenerateAmmo>
            <FF_Ammunition_Industrial>180~360</FF_Ammunition_Industrial>
          </GenerateAmmo>
        </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="FCP_Gun_Silenced_22_SMG"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Industrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Industrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

