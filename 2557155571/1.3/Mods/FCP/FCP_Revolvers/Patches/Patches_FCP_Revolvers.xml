<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- .357 Revolver -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="FCP_Gun_357_Magnum_Revolver"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="FCP_Gun_357_Magnum_Revolver"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="FCP_Gun_357_Magnum_Revolver"]/comps</xpath>
    <value>
        <li Class="Reloading.CompProperties_Reloadable">
          <compClass>Reloading.CompReloadable</compClass>
          <MaxShots>6</MaxShots>
          <AmmoFilter>
            <thingDefs>
              <li>FF_Ammunition_Industrial</li>
            </thingDefs>
          </AmmoFilter>
          <ItemsPerShot>1</ItemsPerShot>
          <ReloadTimePerShot>0.25</ReloadTimePerShot>
          <ReloadSound>Standard_Reload</ReloadSound>
          <GenerateAmmo>
            <FF_Ammunition_Industrial>20~30</FF_Ammunition_Industrial>
          </GenerateAmmo>
        </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="FCP_Gun_357_Magnum_Revolver"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Industrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Industrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

