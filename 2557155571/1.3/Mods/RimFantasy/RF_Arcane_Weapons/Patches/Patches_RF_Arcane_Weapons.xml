<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Diamond Bow -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="RF_DiamondBow"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="RF_DiamondBow"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="RF_DiamondBow"]/comps</xpath>
    <value>
        <li Class="Reloading.CompProperties_Reloadable">
          <compClass>Reloading.CompReloadable</compClass>
          <MaxShots>30</MaxShots>
          <AmmoFilter>
            <thingDefs>
              <li>FF_Ammunition_Preindustrial</li>
            </thingDefs>
          </AmmoFilter>
          <ItemsPerShot>1</ItemsPerShot>
          <ReloadTimePerShot>0.03</ReloadTimePerShot>
          <GenerateAmmo>
            <FF_Ammunition_Preindustrial>60~90</FF_Ammunition_Preindustrial>
          </GenerateAmmo>
        </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="RF_DiamondBow"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Preindustrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Preindustrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

