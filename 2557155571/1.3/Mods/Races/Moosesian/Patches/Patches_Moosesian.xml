<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Antkiller -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="WFF_Antkiller"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="WFF_Antkiller"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="WFF_Antkiller"]/comps</xpath>
    <value>
        <li Class="Reloading.CompProperties_Reloadable">
          <compClass>Reloading.CompReloadable</compClass>
          <MaxShots>10</MaxShots>
          <AmmoFilter>
            <thingDefs>
              <li>FF_Ammunition_Preindustrial</li>
            </thingDefs>
          </AmmoFilter>
          <ItemsPerShot>1</ItemsPerShot>
          <ReloadTimePerShot>0.2</ReloadTimePerShot>
          <ReloadSound>Standard_Reload</ReloadSound>
          <GenerateAmmo>
            <FF_Ammunition_Preindustrial>30~40</FF_Ammunition_Preindustrial>
          </GenerateAmmo>
        </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="WFF_Antkiller"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Preindustrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Preindustrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

