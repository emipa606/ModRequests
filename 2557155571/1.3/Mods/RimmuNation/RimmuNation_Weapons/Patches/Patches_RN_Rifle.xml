<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- DDM4 V11 -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="RNGun_DDM4V11_Rifle"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="RNGun_DDM4V11_Rifle"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="RNGun_DDM4V11_Rifle"]/comps</xpath>
    <value>
      <li Class="Reloading.CompProperties_Reloadable">
        <compClass>Reloading.CompReloadable</compClass>
        <MaxShots>32</MaxShots>
        <AmmoFilter>
          <thingDefs> <!-- 5.56×45mm NATO -->
            <li>FF_Ammunition_Industrial</li>
          </thingDefs>
        </AmmoFilter>
        <ItemsPerShot>1</ItemsPerShot>
        <ReloadTimePerShot>0.03</ReloadTimePerShot>
        <ReloadSound>Standard_Reload</ReloadSound>
        <GenerateAmmo>
          <FF_Ammunition_Industrial>64~96</FF_Ammunition_Industrial>
        </GenerateAmmo>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="RNGun_DDM4V11_Rifle"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Industrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Industrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

