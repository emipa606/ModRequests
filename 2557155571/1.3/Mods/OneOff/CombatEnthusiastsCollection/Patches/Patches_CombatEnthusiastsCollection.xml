<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- USFA Zip 22 -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="CEC_Zipper"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="CEC_Zipper"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="CEC_Zipper"]/comps</xpath>
    <value>
      <li Class="Reloading.CompProperties_Reloadable">
        <compClass>Reloading.CompReloadable</compClass>
        <MaxShots>10</MaxShots> <!-- burstShotCount 1 -->
        <AmmoFilter>
          <thingDefs> <!-- 22 LR -->
            <li>FF_Ammunition_Industrial</li>
          </thingDefs>
        </AmmoFilter>
        <ItemsPerShot>1</ItemsPerShot>
        <ReloadTimePerShot>0.4</ReloadTimePerShot>
		<ReloadSound>Standard_Reload</ReloadSound>
          <GenerateAmmo>
            <FF_Ammunition_Industrial>30~40</FF_Ammunition_Industrial>
          </GenerateAmmo>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="CEC_Zipper"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Industrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Industrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

