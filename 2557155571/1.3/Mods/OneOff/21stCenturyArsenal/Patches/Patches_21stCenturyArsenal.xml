<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Desert Tech HTI -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="Axi_AMR_HTI_21CA"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="Axi_AMR_HTI_21CA"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Axi_AMR_HTI_21CA"]/comps</xpath>
    <value>
      <li Class="Reloading.CompProperties_Reloadable">
        <compClass>Reloading.CompReloadable</compClass>
        <MaxShots>7</MaxShots> <!-- burstShotCount 1 -->
        <AmmoFilter>
          <thingDefs> <!-- 50 Action Express -->
            <li>FF_Ammunition_Industrial</li>
          </thingDefs>
        </AmmoFilter>
        <ItemsPerShot>1</ItemsPerShot>
        <ReloadTimePerShot>0.2</ReloadTimePerShot>
		<ReloadSound>Standard_Reload</ReloadSound>
          <GenerateAmmo>
            <FF_Ammunition_Industrial>28~35</FF_Ammunition_Industrial>
          </GenerateAmmo>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="Axi_AMR_HTI_21CA"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Industrial_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Industrial_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

