      <xpath>Defs/ThingDef[defName="Axi_S_M26MASS_21CA"]/comps</xpath>
      <value>
          <li Class="Reloading.CompProperties_Reloadable">
            <compClass>Reloading.CompReloadable</compClass>
            <MaxShots>30</MaxShots> <!-- pelletCount 6 -->
