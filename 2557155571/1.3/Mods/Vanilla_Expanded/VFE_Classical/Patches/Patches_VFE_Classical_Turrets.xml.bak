<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- See Languages folder for text replacement, this was done for ease of translators -->

<!-- Scorpion Turret -->

  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="VFEC_Turret_Scorpion"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs</xpath>
    <value>
      <thingDefs>
	    <li>FF_Ammunition_Primitive</li>
	  </thingDefs>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="VFEC_Turret_Scorpion"]/comps/li[@Class="CompProperties_Refuelable"]/fuelMultiplier</xpath>
    <value>
      <fuelMultiplier>1</fuelMultiplier>
    </value>
  </Operation>

</Patch>