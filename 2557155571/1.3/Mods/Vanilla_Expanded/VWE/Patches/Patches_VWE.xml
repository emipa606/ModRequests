<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- ========== Neolithic ========== -->

<!-- Long Bow -->

  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="VWE_Bow_Long"]/comps</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName="VWE_Bow_Long"]</xpath>
      <value>
        <comps />
      </value>
    </nomatch>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="VWE_Bow_Long"]/comps</xpath>
    <value>
      <li Class="Reloading.CompProperties_Reloadable">
        <compClass>Reloading.CompReloadable</compClass>
        <MaxShots>20</MaxShots>
        <AmmoFilter>
          <thingDefs>
            <li>FF_Ammunition_Primitive</li>
          </thingDefs>
        </AmmoFilter>
        <ItemsPerShot>1</ItemsPerShot>
        <ReloadTimePerShot>0.15</ReloadTimePerShot>
          <GenerateAmmo>
            <FF_Ammunition_Primitive>20~30</FF_Ammunition_Primitive>
          </GenerateAmmo>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="VWE_Bow_Long"]</xpath>
    <value>
      <li Class="AnimalBehaviours.AnimalStatExtension">
        <statToAdd>
          <li>FF_Ammo_Type</li>	
        </statToAdd>
        <statValues>
          <li>FF_Primitive_Ammo</li>
        </statValues>
        <statDescriptions>
          <li>FF_Primitive_Desc</li>
        </statDescriptions>
      </li>
    </value>
  </Operation>

