<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   <!-- ============================ Outlander Villager Base ========================== -->
   <PawnKindDef Name="WoWOutlanderVillagerBase" Abstract="True">
      <defName>WoWOutlanderVillagerBase</defName>
      <label>villager</label>
      <combatPower>35</combatPower>
      <itemQuality>Poor</itemQuality>
      <isFighter>false</isFighter>
      <gearHealthRange>
         <min>0.2</min>
         <max>2.0</max>
      </gearHealthRange>
      <weaponTags>
         <li>Gun</li>
         <li>MedievalMeleeBasic</li>
      </weaponTags>
      <weaponMoney>
         <min>65</min>
         <max>250</max>
      </weaponMoney>
      <apparelMoney>
         <min>200</min>
         <max>400</max>
      </apparelMoney>
      <apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
      <techHediffsChance>0.06</techHediffsChance>
      <techHediffsMoney>
         <min>50</min>
         <max>600</max>
      </techHediffsMoney>
      <techHediffsTags>
         <li>Poor</li>
         <li>Simple</li>
      </techHediffsTags>
      <defaultFactionType>OutlanderCivil</defaultFactionType>
      <chemicalAddictionChance>0.05</chemicalAddictionChance>
      <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
      <invNutrition>2.55</invNutrition>
      <apparelTags>
         <li>IndustrialBasic</li>
         <li>IndustrialAdvanced</li>
      </apparelTags>
      <inventoryOptions>
         <skipChance>0.70</skipChance>
         <subOptionsChooseOne>
            <li>
               <choiceChance>90</choiceChance>
               <thingDef>Silver</thingDef>
               <countRange>
                  <min>30</min>
                  <max>100</max>
               </countRange>
            </li>
         </subOptionsChooseOne>
      </inventoryOptions>
   </PawnKindDef>
