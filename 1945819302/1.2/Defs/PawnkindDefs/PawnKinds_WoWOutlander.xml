   <!-- ============================ Town Trader Base ========================== -->
   <PawnKindDef Name="WoWOutlanderTownTraderBase" Abstract="True">
      <defName>WoWOutlanderTownTraderBase</defName>
      <label>town trader</label>
      <trader>true</trader>
      <combatPower>40</combatPower>
      <isFighter>false</isFighter>
      <gearHealthRange>
         <min>1</min>
         <max>1</max>
      </gearHealthRange>
      <itemQuality>Normal</itemQuality>
      <weaponTags>
         <li>Gun</li>
      </weaponTags>
      <weaponMoney>
         <min>200</min>
         <max>200</max>
      </weaponMoney>
      <apparelMoney>
         <min>400</min>
         <max>600</max>
      </apparelMoney>
      <apparelTags>
         <li>IndustrialMilitaryBasic</li>
         <li>IndustrialMilitaryAdvanced</li>
         <li>BeltDefensePop</li>
      </apparelTags>
      <apparelRequired>
         <li>Apparel_BowlerHat</li>
      </apparelRequired>
      <techHediffsChance>0.15</techHediffsChance>
      <techHediffsMoney>
         <min>600</min>
         <max>1500</max>
      </techHediffsMoney>
      <techHediffsTags>
         <li>Poor</li>
         <li>Simple</li>
         <li>Advanced</li>
      </techHediffsTags>
      <defaultFactionType>OutlanderCivil</defaultFactionType>
      <chemicalAddictionChance>0.05</chemicalAddictionChance>
      <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
      <invNutrition>2.55</invNutrition>
      <inventoryOptions>
         <skipChance>0.70</skipChance>
         <subOptionsChooseOne>
            <li>
               <choiceChance>90</choiceChance>
               <thingDef>Silver</thingDef>
               <countRange>
                  <min>30</min>
                  <max>100</max>
               </countRange>
            </li>
         </subOptionsChooseOne>
      </inventoryOptions>
   </PawnKindDef>
