	<success>Always</success>
	<operations>
		<li Class="PatchOperationTest">
			<xpath>*/ThingDef[defName = "SleepingSpot"]/comps/li[@Class="CompProperties_AffectedByFacilities"]</xpath>
			<success>Invert</success>
		</li>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "SleepingSpot"]/comps</xpath>
			<value>
				<li Class="CompProperties_AffectedByFacilities" />
			</value>
