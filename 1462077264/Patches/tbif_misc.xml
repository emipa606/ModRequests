<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- Add links to other things.
This determines what shows up (each <li>) when you click a thing (defName) in the world.
-->

<!-- OrbitalTradeBeacon -->
<Operation Class="PatchOperationSequence">
	<success>Always</success>
	<operations>
		<li Class="PatchOperationTest">
			<xpath>*/ThingDef[defName = "OrbitalTradeBeacon"]/comps</xpath>
			<success>Invert</success>
		</li>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "OrbitalTradeBeacon"]</xpath>
			<value>
				<comps />
			</value>
		</li>
	</operations>
</Operation>
