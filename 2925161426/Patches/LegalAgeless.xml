<?xml version="1.0" encoding="utf-8" ?>
<Patch>
		
		<Operation Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName = "Ageless"]/description</xpath>
			<value>
				
				<description>Carriers of this gene have archites in the bloodstream which continuously reverse the process of aging. Starting at the age of 13, carriers begin to biologically age slower. By 22, the aging process stops completely.</description>
			
			</value>
			
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/GeneDef[defName = "Ageless"]/biologicalAgeTickFactorFromAgeCurve/points</xpath>
			<value>
				<li>(13, 1)</li>
				<li>(22, 0)</li>
			</value>
			
	</Operation>
		
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/GeneDef[defName = "Ageless"]/biologicalAgeTickFactorFromAgeCurve/points/li</xpath>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/GeneDef[defName = "Ageless"]/biologicalAgeTickFactorFromAgeCurve/points</xpath>
			<value>
				<li>(13, 1)</li>
				<li>(22, 0)</li>
			</value>
			
	</Operation>
	
</Patch>

