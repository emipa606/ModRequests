	<!-- Spiced Tofu -->
	<RecipeDef>
		<defName>MakeSpicedTofu</defName>
		<label>make spiced tofu</label>
		<description>Create spiced tofu. Much stronger flavour than tofu alone, is used as a dish seasoning.</description>
		<jobString>Making spiced tofu.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>250</workAmount>
		<recipeUsers>
			<li>VCE_CondimentPrepTable</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Tofu</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>VCE_RawSpices</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Tofu</li>
				<li>VCE_RawSpices</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<SpicedTofu>5</SpicedTofu>
		</products>
		<researchPrerequisite>TofuProduction</researchPrerequisite>
	</RecipeDef>
	<!-- TofuSoup -->
	<RecipeDef ParentName="CookMealBase">
		<defName>MakeTofuSoup</defName>
		<label>prepare tofu soup (fine)</label>
		<description>Prepare an uncooked tofu soup assembled with tofu and vegetables.</description>
		<jobString>Preparing tofu soup.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<recipeUsers>
			<li>ElectricStove</li>	
			<li>FueledStove</li>			
		</recipeUsers>
		<fixedIngredientFilter Inherit="False">
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
			<thingDefs>
				<li>Tofu</li>
				<li>CannedTofu</li>
			</thingDefs>
		</fixedIngredientFilter>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Tofu</li>
						<li>CannedTofu</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<defaultIngredientFilter Inherit="False">
			<thingDefs>
				<li>Tofu</li>
				<li>CannedTofu</li>
			</thingDefs>
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
		</defaultIngredientFilter>
		<products>
			<UncookedTofuSoup>1</UncookedTofuSoup>
		</products>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
		<researchPrerequisite>
			<li>TofuProduction</li>
		</researchPrerequisite>
	</RecipeDef>
	<!-- Deep-fried Tofu -->
