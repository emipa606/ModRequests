	</RecipeDef>
	<!-- Canned tofu -->
	<RecipeDef>
		<defName>MakeCannedTofu</defName>
		<label>can tofu x20</label>
		<description>Safely store tofu in a steel can. Canning food extends its shelf life almost indefinitely.</description>
		<jobString>Canning tofu.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<recipeUsers>
			<li>VCE_CanningMachine</li>					
		</recipeUsers>	
		<ingredientValueGetterClass>VanillaCookingExpanded.IngredientValueGetter_NutritionWithExtraIngredient</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Tofu</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Tofu</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<CannedTofu>20</CannedTofu>
		</products>
		<researchPrerequisite>TofuProduction</researchPrerequisite>
	</RecipeDef>
</Defs>