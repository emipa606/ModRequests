<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--SoyMilk-->
	<RecipeDef>
		<defName>MakeSoyMilk</defName>
		<label>process soybeans into soy milk</label>
		<description>Process soybeans(50) into soy milk(20).</description>
		<jobString>Processing soybeans into soy milk.</jobString>
		<workAmount>450</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>VCE_RawSoybean</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<products>
			<SoyMilk>20</SoyMilk>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>VCE_RawSoybean</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>TofuProduction</researchPrerequisite>
	</RecipeDef>
	<!--TofuStove-->
	<RecipeDef>
		<defName>MakeTofu</defName>
		<label>process soy milk into tofu</label>
		<description>Process soy milk(20) into tofu(20).</description>
		<jobString>Processing soy milk into tofu.</jobString>
		<workAmount>450</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SoyMilk</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<products>
			<Tofu>20</Tofu>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SoyMilk</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>TofuProduction</researchPrerequisite>
	</RecipeDef>
