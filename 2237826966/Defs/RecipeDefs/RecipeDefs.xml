	<!-- Mapo Tofu-->
	<RecipeDef ParentName="CookMealBase">
		<defName>MakeMapoTofu</defName>
		<label>cook mapo tofu (lavish)</label>
		<description>Cook mapo tofu, an incredibly spicy lavish meal. Requires meat, raw or canned tofu, and spiced tofu.</description>
		<jobString>Cooking mapo tofu.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>VanillaCookingExpanded.IngredientValueGetter_NutritionWithExtraIngredient</ingredientValueGetterClass>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<fixedIngredientFilter>
		  <categories>
			 <li>MeatRaw</li>
		  </categories>
		  <disallowedCategories>
			<li>PlantFoodRaw</li>
			<li>VCE_Condiments</li>
			<li>AnimalProductRaw</li>
			<li>VCEF_RawFishCategory</li>
		  </disallowedCategories>
		  <disallowedThingDefs>
			<li>Meat_Human</li>
			<li>Meat_Megaspider</li>
		  </disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
		  <categories>
			<li>MeatRaw</li>
		  </categories>
		  <thingDefs>
			<li>SpicedTofu</li>
			<li>Tofu</li>
			<li>CannedTofu</li>
		  </thingDefs>
		</defaultIngredientFilter>
		<ingredients>
		  <li>
			<filter>
			  <categories>
				<li>MeatRaw</li>
			  </categories>
			  <disallowedThingDefs>
				<li>Tofu</li>
				<li>CannedTofu</li>
			  </disallowedThingDefs>
			</filter>
			<count>.5</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>Tofu</li>
				<li>CannedTofu</li>
			  </thingDefs>
			</filter>
			<count>.5</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>SpicedTofu</li>
			  </thingDefs>
			</filter>
			<count>1.0</count>
		  </li>
		</ingredients>
		<products>
			<MapoTofu>1</MapoTofu>
		</products>
		<skillRequirements>
			<Cooking>8</Cooking>
		</skillRequirements>
		<researchPrerequisite>TofuProduction</researchPrerequisite>
	</RecipeDef>
	<!-- MapoTofuBulk -->
