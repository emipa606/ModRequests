<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef>
		<defName>ConsumedSpicedTofu</defName>
		<label>consumed spiced tofu (from meal)</label>
		<makesSickThought>false</makesSickThought>
		<description>Consumed some spiced tofu, known for minor healing abilities and pain relief.</description>
		<tendable>false</tendable>
		<isBad>false</isBad>
		<defaultLabelColor>(92,62,198)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<comps>
			<li Class="VanillaCookingExpanded.HediffCompProperties_WhileHavingThoughts">
				<thoughtDefs>
					<li>ConsumedSpicedTofu</li>
				</thoughtDefs>
			</li>
		</comps>
		<stages>
			<li>
				<painFactor>0.90</painFactor>
				<statOffsets>
					<ImmunityGainSpeed>0.20</ImmunityGainSpeed>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>
</Defs>

