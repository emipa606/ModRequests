<Defs>


    <ItemProcessor.CombinationDef>
		<defName>VCE_SoyMilkIntoCheese</defName>
		<building>VCE_CheesePress</building>
		<items>
			<li>SoyMilk</li>
		</items>	
		<amount>
			<li>50</li>
		</amount>
		<result>TofuCheese</result>	
		<yield>25</yield>
		<useQualityIncreasing>true</useQualityIncreasing>
			<awfulQualityAgeDaysThreshold>5</awfulQualityAgeDaysThreshold>
			<poorQualityAgeDaysThreshold>8</poorQualityAgeDaysThreshold>
			<normalQualityAgeDaysThreshold>12</normalQualityAgeDaysThreshold>
			<goodQualityAgeDaysThreshold>17</goodQualityAgeDaysThreshold>
			<excellentQualityAgeDaysThreshold>20</excellentQualityAgeDaysThreshold>
			<masterworkQualityAgeDaysThreshold>25</masterworkQualityAgeDaysThreshold>
			<legendaryQualityAgeDaysThreshold>45</legendaryQualityAgeDaysThreshold>
	</ItemProcessor.CombinationDef>

	<ItemProcessor.CombinationDef>
		<defName>TofuSoupRecipe</defName>
		<building>VCE_ElectricPot</building>
		<items>
			<li>UncookedTofuSoup</li>
		</items>	
		<amount>
			<li>1</li>
		</amount>
		<result>CookedTofuSoup</result>	
		<yield>10</yield>
		<useQualityIncreasing>false</useQualityIncreasing>
		<singleTimeIfNotQualityIncreasing>2</singleTimeIfNotQualityIncreasing>
	</ItemProcessor.CombinationDef>
</Defs>