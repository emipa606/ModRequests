			<li>Gloomy Extra - Kitchen</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="TableAdvancedElectricStove"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
					<value>
						<li>SM_Sink</li>
						<li>SM_KitchenCupboard_A</li>
						<li>SM_KitchenCupboard_B</li>
						<li>SM_KitchenCupboard_C</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>