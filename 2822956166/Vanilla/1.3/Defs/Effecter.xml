  <!--============================下面是全息影像============================-->
  <!--机枪模块·全息影像-->
  <ThingDef ParentName="MoteBase">
    <defName>MYDE_Battlecruiser_GunModule</defName>
    <graphicData>
      <texPath>Effect/Module/GunModule</texPath>
      <shaderType>Transparent</shaderType>
      <graphicClass>MYDE_Battlecruiser.Graphic_Mote_NoAngle</graphicClass>
    </graphicData>
    <altitudeLayer>Projectile</altitudeLayer>
    <rotatable>false</rotatable>
    <mote>
      <fadeInTime>2.4</fadeInTime>
      <!--淡入时间-->
      <solidTime>0</solidTime>
      <!--持续时间-->
      <fadeOutTime>2.6</fadeOutTime>
      <!--淡出时间-->
    </mote>
  </ThingDef>
  <EffecterDef>
    <defName>MYDE_Battlecruiser_GunModuleII</defName>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SprayerTriggered</subEffecterClass>
        <moteDef>MYDE_Battlecruiser_GunModule</moteDef>
        <positionRadius>0</positionRadius>
        <!--生成位置，一格是1。指以格子中心为圆点，半径0.25的圆-->
        <burstCount>1~1</burstCount>
        <!--生成数量-->
        <scale>1</scale>
        <spawnLocType>OnSource</spawnLocType>
        <positionOffset>(0,0,0.5)</positionOffset>
      </li>
    </children>
  </EffecterDef>
