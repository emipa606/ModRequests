<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--星港-->
  <ThingDef ParentName="BuildingBase">
    <defName>MYDE_Battlecruiser_Base</defName>
    <label>Starport</label>
    <!--<label>星港</label>-->
    <!--<description>Starport，</description>-->
    <description>Starport is used for the maintenance and command of "Battlecruiser" and for issuing Yamato Cannon orders.

Fuel or ammunition storage is completed independently by each module. Starport monitors the material storage every 10s at most. (It is not allowed to replenish ammunition and add modules to the "Battlecruiser" in flight)

The "Battlecruiser" can be started only when the fuel module has fuel.

Battlecruiser:
1.With the initial armor of 3000, the enemy will try to attack the "Battlecruiser". The damaged Battlecruiser can be repaired by repairing "Starport".
2.The configuration of "Battlecruiser" can be modified by placing various modules in 2 spaces around "Starport".
3.The order to recall "Battlecruiser" can return most of the materials and energy. The "Battlecruiser" with armor of 0 will generate mixed explosion according to the remaining materials.
4.Includes 10 orders.</description>
    <category>Building</category>
    <graphicData>
      <texPath>Building/Base</texPath>
      <!--<texPath>Nothing/Nothing</texPath>-->
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
      <allowFlip>false</allowFlip>
      <drawSize>(5.3,5.3)</drawSize>
      <!--<shadowData><volume>(2,6,0.7)</volume><offset>(0,0,-0.7)</offset></shadowData>-->
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>30</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>1</fillPercent>
    <!--<blockLight>true</blockLight>-->
    <size>(5,5)</size>
    <statBases>
      <MaxHitPoints>3000</MaxHitPoints>
      <Flammability>0</Flammability>
      <WorkToBuild>3000</WorkToBuild>
      <Mass>100</Mass>
      <Beauty>10</Beauty>
    </statBases>
    <selectable>true</selectable>
    <building>
      <destroySound>BuildingDestroyed_Metal_Small</destroySound>
    </building>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <placeWorkers>
      <li>PlaceWorker_CoolerSimple</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <comps>
      <li Class="MYDE_Battlecruiser.CompProperties_FuelSystem">
        <showFuelGizmo>true</showFuelGizmo>
        <minimumFueledThreshold>1</minimumFueledThreshold>
        <!--<fuelIconPath>UI/Overlays/Barrel</fuelIconPath>-->
      </li>
      <li Class="MYDE_Battlecruiser.CompProperties_LaunchUAV"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_CrossMap"></li>
      <!--<li Class="MYDE_Battlecruiser.CompProperties_RecallUAV"><ShowLabel>Recall</ShowLabel></li>-->
      <!--<li Class="MYDE_Battlecruiser.CompProperties_LaunchYaMaTo"><ShowLabel>Yamato Cannon</ShowLabel></li>-->
      <li Class="MYDE_Battlecruiser.CompProperties_Starport_GunWeaponControl"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_Starport_RocketWeaponControl"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_Starport_LaserWeaponControl"></li>
      <!--<li Class="MYDE_Battlecruiser.CompProperties_FlyingStarport"></li>-->
      <li Class="MYDE_Battlecruiser.CompProperties_StopMode"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_TargetMode"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_AttackDownSwitch"></li>
      <li Class="MYDE_Battlecruiser.CompProperties_EndingModuleMode"></li>
      <li Class="CompProperties_Glower">
        <glowRadius>20</glowRadius>
        <glowColor>(217,217,208,0)</glowColor>
      </li>
    </comps>
    <designationHotKey>Misc4</designationHotKey>
    <castEdgeShadows>true</castEdgeShadows>
    <thingClass>MYDE_Battlecruiser.Building_Battlecruiser_Starport</thingClass>
    <designationCategory>MYDE_Battlecruiser_BuildingList</designationCategory>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
    <costList>
      <Steel>300</Steel>
      <Plasteel>150</Plasteel>
      <ComponentIndustrial>30</ComponentIndustrial>
    </costList>
    <researchPrerequisites>
      <li>MYDE_Battlecruiser_Research_Starport</li>
    </researchPrerequisites>
  </ThingDef>
  <!--无人机基站·辅助模块·燃料模块-->
  <ThingDef ParentName="BuildingBase">
    <defName>MYDE_Battlecruiser_Base_Stuffer_Chemfuel</defName>
    <label>Fuel Module</label>
    <!--<label>燃料模块</label>-->
    <description>Provides fuel for "Battlecruiser", and each module can provide 30s flight time. 
(60 Chemfuel is required for filling)</description>
    <category>Building</category>
    <graphicData>
      <texPath>AuxiliaryModule/Chemfuel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
      <allowFlip>false</allowFlip>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Standable</passability>
    <pathCost>30</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>1</fillPercent>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
      <Flammability>0</Flammability>
      <WorkToBuild>1500</WorkToBuild>
      <Mass>10</Mass>
      <Beauty>0</Beauty>
    </statBases>
    <selectable>true</selectable>
    <building>
      <destroySound>BuildingDestroyed_Metal_Small</destroySound>
    </building>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <placeWorkers>
      <li>PlaceWorker_CoolerSimple</li>
      <li>MYDE_Battlecruiser.PlaceWorker_NearBase</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <comps>
      <li Class="CompProperties_Refuelable">
        <fuelLabel>Fuel</fuelLabel>
        <fuelGizmoLabel>Fuel</fuelGizmoLabel>
        <fuelFilter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <fuelCapacity>30</fuelCapacity>
        <initialFuelPercent>1</initialFuelPercent>
        <autoRefuelPercent>0.99</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <minimumFueledThreshold>1</minimumFueledThreshold>
        <fuelMultiplier>0.5</fuelMultiplier>
        <!--这是1个材料提供多少能量-->
        <factorByDifficulty>true</factorByDifficulty>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <!--<fuelIconPath>UI/Overlays/Barrel</fuelIconPath>-->
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>30</initialConfigurableTargetFuelLevel>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
    </comps>
    <designationHotKey>Misc4</designationHotKey>
    <thingClass>Building</thingClass>
    <designationCategory>MYDE_Battlecruiser_BuildingList</designationCategory>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
    <costList>
      <Steel>75</Steel>
      <Chemfuel>60</Chemfuel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <researchPrerequisites>
      <li>MYDE_Battlecruiser_Research_Starport</li>
    </researchPrerequisites>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>MYDE_Battlecruiser_Root</li>
    </thingCategories>
  </ThingDef>
