<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<DesignatorDropdownGroupDef>
		<defName>OverWallCooler</defName>
	</DesignatorDropdownGroupDef>
	
	<ThingDef ParentName="Cooler" Name="Cooler_Over">
		<defName>Cooler_Over</defName>
		<label>over-wall cooler</label>
		<designatorDropdown>OverWallCooler</designatorDropdown>
		<graphicData>
			<texPath>Cooler_Over</texPath>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<blockWind>false</blockWind>
		<fillPercent>.6</fillPercent>
		<coversFloor>false</coversFloor>
		<blockLight>false</blockLight>
		<staticSunShadowHeight>.6</staticSunShadowHeight>
		<building>
			<canPlaceOverWall>false</canPlaceOverWall>
			<isEdifice>false</isEdifice>
		</building>
		<designationHotKey>Misc6</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="Cooler_Over">
		<defName>Cooler_Over2W</defName>
		<label>over-wall cooler (wide)</label>
		<graphicData>
			<texPath>Cooler_Over2W</texPath>
			<drawSize>(1,2)</drawSize>
		</graphicData>
		<size>(1,2)</size>
	</ThingDef>

	<ThingDef ParentName="Vent">
		<defName>Vent_Over</defName>
		<label>over-wall vent</label>
		<graphicData>
			<texPath>Vent_Over</texPath>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<blockWind>false</blockWind>
		<fillPercent>.6</fillPercent>
		<coversFloor>false</coversFloor>
		<blockLight>false</blockLight>
		<castEdgeShadows>false</castEdgeShadows>
		<building>
			<canPlaceOverWall>false</canPlaceOverWall>
			<isEdifice>false</isEdifice>
		</building>
		<designationHotKey>Misc7</designationHotKey>
	</ThingDef>
	
</Defs>