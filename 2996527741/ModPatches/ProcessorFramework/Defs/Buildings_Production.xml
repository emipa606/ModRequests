<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>Insect_Hotel</defName>
		<label>insect hotel</label>
		<description>A simple construction perfectly designed for insect nests, allowing the conversion of raw food into insect meat. Should be built outdoors.</description>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>false</blockWind>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<destroySound>BuildingDestroyed_Wood_Small</destroySound>
		</building>
		<drawerType>MapMeshAndRealTime</drawerType>
		<castEdgeShadows>true</castEdgeShadows>
		<constructEffect>ConstructWood</constructEffect>
		<costList>
			<WoodLog>30</WoodLog>
		</costList>
		<designationCategory>Production</designationCategory>
		<fillPercent>.4</fillPercent>
		<graphicData>
			<texPath>Buildings/Insect_Hotel/InsectHotel</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<drawSize>(1.2,1.2)</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<minifiedDef>MinifiedThing</minifiedDef>
		<passability>PassThroughOnly</passability>
		<pathCost>35</pathCost>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<recipes/>
		<rotatable>true</rotatable>
		<surfaceType>None</surfaceType>
		<statBases>
			<Beauty>-5</Beauty>
			<Cleanliness>-15</Cleanliness>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<thingClass>Building</thingClass>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<tickerType>Rare</tickerType>
		<comps>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>25</capacity>
				<barOffset>(0, -.38)</barOffset>
				<independentProcesses>false</independentProcesses>
				<showProductIcon>false</showProductIcon>
				<dropIngredients>false</dropIngredients>
				<processes>
					<li>AttractInsects</li>
				</processes>
</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>Insect_Condo</defName>
		<label>insect condo</label>
		<description>An advanced enclosure designed for the sanitary mass production of insect meat.</description>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>false</blockWind>
		<drawerType>MapMeshAndRealTime</drawerType>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
		</building>
		<castEdgeShadows>true</castEdgeShadows>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<costList>
			<Steel>30</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<designationCategory>Production</designationCategory>
		<fillPercent>.4</fillPercent>
		<graphicData>
			<texPath>Buildings/Insect_Condo/Insect_Condo</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<drawSize>(1.2,1.2)</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<minifiedDef>MinifiedThing</minifiedDef>
		<passability>PassThroughOnly</passability>
		<pathCost>35</pathCost>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<recipes/>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
			<li>MicroelectronicsBasics</li>
		</researchPrerequisites>
		<rotatable>true</rotatable>
		<surfaceType>None</surfaceType>
		<statBases>
			<Beauty>-2</Beauty>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<thingClass>Building</thingClass>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<tickerType>Rare</tickerType>
		<comps>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>10</capacity>
				<dropIngredients>false</dropIngredients>
				<barOffset>(0, -.38)</barOffset>
				<showProductIcon>false</showProductIcon>
				<processes>
					<li>BreedInsects</li>
				</processes>
</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
	</ThingDef>
</Defs>
