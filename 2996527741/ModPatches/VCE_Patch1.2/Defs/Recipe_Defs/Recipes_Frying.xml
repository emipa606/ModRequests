<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>Insect_DeepFryMeats</defName>
		<label>deep fry insect meats x20</label>
		<description>Deep fry large insect meat chunks. Deep fried food is very tasty, but not very good for your health.</description>
		<jobString>Deep frying meats.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<recipeUsers>
			<li>VCE_DeepFrier</li>
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Meat_Megaspider</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<disallowedThingDefs>
				<li>VCE_DeepFriedBigMeat</li>
				<li>VCE_DeepFriedFish</li>			
			</disallowedThingDefs>			
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>Meat_Megaspider</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>VCE_DeepFriedBigMeat</li>
				<li>VCE_DeepFriedFish</li>
			</disallowedThingDefs>
			<disallowedCategories>
			</disallowedCategories>
		</defaultIngredientFilter>
		<products>
			<Insect_DeepFriedInsectMeat>4</Insect_DeepFriedInsectMeat>
		</products>
	</RecipeDef>
</Defs>
