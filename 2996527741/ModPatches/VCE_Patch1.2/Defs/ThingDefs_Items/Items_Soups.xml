<Defs>  
	<ThingDef ParentName="RoughPlantBase">
		<defName>Insect_UncookedSoupSimple</defName>
		<label>uncooked insect simple soup</label>
		<description>An uncooked simple insect soup. This needs to be cooked in an electric pot to be edible.</description>
		<graphicData>
			<texPath>Things/Items/Uncookedsoup/Insect_UncookedSoupSimple</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>96.8</MarketValue>
			<Flammability>1.3</Flammability>
			<WorkToMake>1200</WorkToMake>
			<Mass>4</Mass>
		</statBases>
		<tradeability>None</tradeability>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>4</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
			<li Class="CompProperties_Ingredients" />				
		</comps>
		<thingSetMakerTags/>
		<thingCategories>
			<li>VCE_UncookedSoups</li>
		</thingCategories>
		<ingestible>
			<preferability>NeverForNutrition</preferability>			
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
		<defName>Insect_CookedSoupSimple</defName>
		<label>insect simple soup</label>
		<description>A simple insectsoup slow cooked from a few ingredients.</description>
		<stackLimit>10</stackLimit>
		<graphicData>
			<texPath>Things/Meals/Insect_SoupSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>12</MarketValue>
			<Nutrition>0.8</Nutrition>
			<Mass>0.4</Mass>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Ingest_Drink</ingestSound>
		</ingestible>

	</ThingDef>

