  <ThingDef ParentName="BenchBase">
    <defName>TableMachiningExplosive</defName>
    <label>Explosives Machining Table</label>
    <description>A fairly advanced and very compact working station for making single-use rocket launchers.</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/TableMachiningExplosive</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <costList>
    <Plasteel>300</Plasteel>
      <ComponentIndustrial>8</ComponentIndustrial>
      <ComponentSpacer>2</ComponentSpacer>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>4800</WorkToBuild>
      <MaxHitPoints>120</MaxHitPoints>
      <Flammability>0.6</Flammability>
      <Cleanliness>-2</Cleanliness>
    </statBases>
    <size>(3,1)</size>
    <designationCategory>Production</designationCategory>
