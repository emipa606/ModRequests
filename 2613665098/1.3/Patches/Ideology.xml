<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Ideology</li>
        </mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName="FemaleModifierDef"]/stages/li/statFactors</xpath>
					<value>
					<SuppressionPower>0.5</SuppressionPower>
					</value>
    			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName="FemaleModifierDef"]/stages/li/statFactors</xpath>
					<value>
					<CertaintyLossFactor>3</CertaintyLossFactor>
					</value>
    			</li>
		</operations>
	</match>
</Operation>

</Patch>