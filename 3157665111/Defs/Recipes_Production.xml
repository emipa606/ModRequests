<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Make_ComponentIndustrial_AJN_Silver</defName>
    <label>make component</label>
    <description>Make a component (costs more resources and takes longer than a Fabrication Bench).</description>
    <jobString>Making component.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>25000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	<recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>11.4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentIndustrial>1</ComponentIndustrial>
    </products>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <displayPriority>0</displayPriority>
  </RecipeDef>

