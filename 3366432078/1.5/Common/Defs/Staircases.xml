<!-- Low quality upstairs -->
  <ThingDef Abstract="True" Name="StaircaseBedLowQualityBase" ParentName="StaircaseBedBase">
    <modExtensions>
      <li Class="SecondFloor.SecondFloorModExtension">
        <RemoveSoakingWet>true</RemoveSoakingWet>
        <RemoveSleptOutside>true</RemoveSleptOutside>
        <RemoveSleptInCold>true</RemoveSleptInCold>
        <RemoveSleptInBarracks>true</RemoveSleptInBarracks>
        <RemoveSleepDisturbed>false</RemoveSleepDisturbed>
        <ideologySecondFloorAssignmentAllowed>true</ideologySecondFloorAssignmentAllowed>
        <RemoveSharedBed>true</RemoveSharedBed>
        <RemoveSleptInBedroom>true</RemoveSleptInBedroom>
        <customHediff>SF_WarmTemperature</customHediff>
      </li>
    </modExtensions>
    <statBases>
      <MaxHitPoints>450</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>0</Beauty>
      <BeautyOutdoors>0</BeautyOutdoors>
      <WorkToBuild>80000</WorkToBuild>
      <Mass>400</Mass>
      <BedRestEffectiveness>0.85</BedRestEffectiveness>
      <Comfort>.6</Comfort>
      <ImmunityGainSpeedFactor>1.05</ImmunityGainSpeedFactor>
      <SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
      <MedicalTendQualityOffset>0.00</MedicalTendQualityOffset>
      <BirthRitualQualityOffset MayRequire="Ludeon.RimWorld.Biotech">2</BirthRitualQualityOffset>
    </statBases>
    <techLevel>Neolithic</techLevel>
    <researchPrerequisites>
      <li>PassiveCooler</li>
    </researchPrerequisites>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
    </stuffCategories>
    <comps>
      <li Class="SecondFloor.CompProperties_GiveThoughtStairs">
        <thoughtDef>SF_Low_Quality_Room</thoughtDef>
      </li>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li MayRequire="Ludeon.RimWorld.Ideology">SleepAccelerator</li>
        </linkableFacilities>
      </li>
      <li>
        <compClass>SecondFloor.CompAssignBedHediff</compClass>
      </li>
    </comps>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
  </ThingDef>

