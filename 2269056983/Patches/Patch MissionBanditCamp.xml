<?xml version="1.0" encoding="UTF-8"?>

<Patch>
   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Royalty</li>
      </mods>
      <match Class="PatchOperationSequence">
         <success>Always</success>
         <operations>
            <li Class="PatchOperationReplace">
				<xpath>Defs/QuestScriptDef[defName="Mission_BanditCamp"]/root[@Class="QuestNode_Root_Mission_BanditCamp"]/siteFaction</xpath>
				<value>
					<siteFaction>KenshiSkeletonFaction</siteFaction>
				</value>
            </li>
         </operations>
      </match>
   </Operation>
</Patch>