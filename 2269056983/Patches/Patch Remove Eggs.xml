<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Patch unfertilized chicken eggs -->
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="EggChickenUnfertilized"]</xpath>
		<value>
			<ThingDef ParentName="EggUnfertBase">
			<defName>EggChickenUnfertilized</defName>
			<label>swampraptor egg (unfert.)</label>
			<description>An unfertilized swampraptor egg. It can be eaten raw, but it's much, much better cooked.</description>
			<graphicData>
			  <color>(88,120,88)</color>
			</graphicData>
			<statBases>
			  <MarketValue>45.0</MarketValue>
			  <Mass>2.25</Mass>
			  <Flammability>0.5</Flammability>
			</statBases>
		  </ThingDef>
		</value>
	</Operation>
	
