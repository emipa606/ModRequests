<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="AnimalThingBase">
    <defName>KenshiSwampraptor</defName>
    <label>swamp raptor</label>
    <description>Swamp Raptors are bi-pedal reptilian creatures with round bodies and large jaws. They are omnivores and will attempt to eat crops growing in any farms that they come across, as well as any corpses.</description>
    <statBases>
      <MoveSpeed>3.5</MoveSpeed>
      <MarketValue>150</MarketValue>
      <ComfyTemperatureMin>-55</ComfyTemperatureMin>
      <ComfyTemperatureMax>65</ComfyTemperatureMax>
	  <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
      <ArmorRating_Heat>0.10</ArmorRating_Heat>
    </statBases>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.7</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggSwampraptorFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>10</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>1</eggProgressUnfertilizedMax>
        <eggCountRange>
          <min>2</min>
          <max>3</max>
        </eggCountRange>
      </li>
    </comps>
    <race>
      <body>KenshiQuadrupedAnimalWithClawsTailAndJowl</body>
      <predator>false</predator>
      <maxPreyBodySize>0.35</maxPreyBodySize>
      <baseBodySize>0.25</baseBodySize>
      <baseHungerRate>0.07</baseHungerRate>
      <baseHealthScale>0.8</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal, OmnivoreRoughAnimal</foodType>
      <leatherDef>Leather_Swampraptor</leatherDef>
      <meatLabel>raptorflesh</meatLabel>
	  	  <bloodDef>Filth_BloodInsect</bloodDef>
	  <meatColor>(222,189,126)</meatColor>
      <wildness>0.75</wildness>
      <petness>0.05</petness>
      <manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.10</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <lifeExpectancy>20</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.15</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>KenshiAnimalUncommon</li>
      <li>KenshiAnimalFighter</li>
    </tradeTags>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
    <defName>KenshiSwampraptor</defName>
    <label>swamp raptor</label>
    <race>KenshiSwampraptor</race>
    <combatPower>65</combatPower>
	<canArriveManhunter>true</canArriveManhunter>
    <ecoSystemWeight>0.25</ecoSystemWeight>
	<wildGroupSize>
      <min>3</min>
      <max>6</max>
	</wildGroupSize>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Swampraptor</texPath>
          <drawSize>1</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Dessicated_Swampraptor</texPath>
          <drawSize>1</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Swampraptor</texPath>
          <drawSize>1.25</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Dessicated_Swampraptor</texPath>
          <drawSize>1.25</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Swampraptor</texPath>
          <drawSize>1.5</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Swampraptor/Dessicated_Swampraptor</texPath>
          <drawSize>1.5</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>
  </Defs>