<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>Med_ElectricalCable</defName>
    <label>electrical cable</label>
    <description>An assembly of electrical wires, bundled inside a protective sheath. Electrical cables may be used for transmission of electrical power or signals.</description>
    <graphicData>
      <texPath>Items/ElectricalCable</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
	<allowedArchonexusCount>100</allowedArchonexusCount>
	<soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>150</stackLimit>
	<healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <MarketValue>1.6</MarketValue>
      <Mass>0.2</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  
  <ThingDef ParentName="ResourceBase">
    <defName>Med_Pipe</defName>
    <label>pipe</label>
    <description>A solid hollow cylinder meant for transporting fluids and flowing solids. Pipes are typically used in plumbing, i.e to move sewage, water and air around.</description>
    <graphicData>
      <texPath>Items/Pipe</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
	<allowedArchonexusCount>50</allowedArchonexusCount>
	<soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>75</stackLimit>
	<healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <MarketValue>1.8</MarketValue>
      <Mass>0.5</Mass>
      <Flammability>0.0</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  
</Defs>
