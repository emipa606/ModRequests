   <RecipeDef ParentName="SurgeryBionic">
      <defName>InstallCombinedRib</defName>
      <label>install combined rib replacement</label>
      <description>Installs a combined rib replacement.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing a combined rib replacement.</jobString>
      <workAmount>2000</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>CombinedRib</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>CombinedRib</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>Rib1</li>
         <li>Rib2</li>
         <li>Rib3</li>
         <li>Rib4</li>
         <li>Rib5</li>
         <li>Rib6</li>
         <li>Rib7</li>
         <li>Rib8</li>
         <li>Rib9</li>
         <li>Rib10</li>
         <li>Rib11</li>
         <li>Rib12</li>
      </appliedOnFixedBodyParts>
      <addsHediff>CombinedRib</addsHediff>
      <researchPrerequisite>RibReplacements</researchPrerequisite>
   </RecipeDef>

</Defs>