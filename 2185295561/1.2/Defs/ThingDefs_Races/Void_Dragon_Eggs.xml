<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef Abstract="True" Name="VoidDergEggFertBase" ParentName="OrganicProductBase">
		<tickerType>Normal</tickerType>
		<socialPropernessMatters>true</socialPropernessMatters>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/EggBirdSmall</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<Mass>0.15</Mass>
			<MaxHitPoints>20</MaxHitPoints>
			<DeteriorationRate>2</DeteriorationRate>
			<Flammability>0.7</Flammability>
			<FoodPoisonChanceFixedHuman>100</FoodPoisonChanceFixedHuman>
		</statBases>
		<thingCategories>
			<li>EggsDragon</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>15</daysToRotStart>
				<rotDestroys>true</rotDestroys>
				<disableIfHatcher>true</disableIfHatcher>
			</li>
			<li Class="CompProperties_TemperatureRuinable">
				<minSafeTemperature>-100</minSafeTemperature>
				<maxSafeTemperature>200</maxSafeTemperature>
				<progressPerDegreePerTick>0.00003</progressPerDegreePerTick>
			</li>
		</comps>
		<stackLimit>1</stackLimit>
	</ThingDef>

<ThingDef ParentName="VoidDergEggFertBase">
    <defName>EggRedNebulaDragonFertilized</defName>
    <label>red nebula dragon egg (fert.)</label>
    <description>A bright red egg swirling with strange energies.</description>
    <graphicData>
      <texPath>Things/Pawn/Eggs/RedNebulaEgg</texPath>
      <drawSize>1.5</drawSize>
	</graphicData>
	<statBases>
		<Beauty>26</Beauty>
		<Mass>12.25</Mass>
		<Flammability>0</Flammability>
		<MarketValue>10000</MarketValue>
	</statBases>
	<stuffProps>
		<commonality>0</commonality>
	</stuffProps>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>60</hatcherDaystoHatch>
        <hatcherPawn>Red_Nebula_Dragon</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
