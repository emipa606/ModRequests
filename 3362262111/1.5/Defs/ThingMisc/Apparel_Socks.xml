<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Short_Socks</defName>
		<label>short socks</label>
		<description>Comfortable and warm short socks.</description>
		<graphicData>
			<texPath>Things/Footwear/ShortSocks</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
		<generateCommonality>10</generateCommonality>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<WorkToMake>1000</WorkToMake>
			<Mass>0.02</Mass>
			<StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.4</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.08</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.0</EquipDelay>
		</statBases>
		<thingCategories Inherit="False">
			<li>Socks</li>
		</thingCategories>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>Feet</li>
			</bodyPartGroups>
			<layers>
				<li>SocksLayer</li>
			</layers>
			<tags>
				<li>IndustrialBasic</li>
				<li>IndustrialMilitaryAdvanced</li>
				<li>Royal</li>
				<li>RoyalTier2</li>
				<li>RoyalTier3</li>
				<li>RoyalTier4</li>
				<li>RoyalTier5</li>
				<li>RoyalTier6</li>
				<li>RoyalTier7</li>
				<li>IndustrialAdvanced</li>
				<li>IndustrialMilitaryBasic</li>
				<li>IndustrialMilitary</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_StandardApparel"/>
	</ThingDef>
	
