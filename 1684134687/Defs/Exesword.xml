<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>SangvisMelee_Executioner</defName>
    <label>Executioner Sword</label>
    <description>A hi-tech zhanmadao developed for Model SP524 "Executioner". Good for slicing through butter, cheese, birthday cakes, cinnamon rolls, crackers, cows, thrumbos, bones, bullets, human organs, solid steel plates, tanks, trees, other swords, ...awkward silences, crashed ship parts, mechanoids, and various other things.</description>
    <graphicData>
      <texPath>Exesword</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <weaponTags>
      <li>Exotic</li>
    </weaponTags>
    <costList>
	  <Plasteel>65</Plasteel>
	  <Steel>35</Steel>
      <ComponentSpacer>5</ComponentSpacer>
	</costList>
    <statBases>
      <WorkToMake>50000</WorkToMake>
      <Mass>3.6</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>36</power>
        <cooldownTime>1.5</cooldownTime>
      </li>
      <li>
        <label>blade</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>44</power>
		<armorPenetration>0.99</armorPenetration>
        <cooldownTime>1.7</cooldownTime>
      </li>
      <li>
        <label>edge</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>44</power>
		<armorPenetration>0.99</armorPenetration>
        <cooldownTime>1.7</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
	  <researchPrerequisite>ChargedShot</researchPrerequisite>
	  <skillRequirements>
        <Crafting>15</Crafting>
      </skillRequirements>
	  <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>

</Defs>