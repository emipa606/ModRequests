<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="XmlExtensions.PatchOperationAdd">
		<xpath>Defs/XmlExtensions.SettingsMenuDef[modId="majestic.framework"]//li[tag="toggleResize"]/buttons</xpath>
		<value>
			<li>
				<label>CMT Half Size</label>
				<value>sizeCMTkk</value>
				<tooltip>kamikadza13's CMT Resize</tooltip>
			</li>
		</value>
	</Operation>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>majestic.framework</modId>
		<key>toggleResize</key>
		<defaultValue>sizeVanilla</defaultValue>
		<apply>
			<Operation Class="XmlExtensions.PatchByCase">
				<value>{toggleResize}</value>
				<cases>
					<li>
						<value>sizeCMTkk</value>
						<apply>
							<!--The stats that are being patched will appear in the settings menu in the same order listed here-->
