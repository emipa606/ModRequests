<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Renimon Default Base -->
	<PawnKindDef Abstract="True" Name="RenimonPawnKind">
    <combatPower>30</combatPower>
	<baseRecruitDifficulty>0.75</baseRecruitDifficulty>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>False</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
  </PawnKindDef>
  
  <!-- Renimon Player Base -->
	<PawnKindDef ParentName="RenimonPawnKind">
    <defName>RRP_PlayerSpacer_Renimon_Pawn</defName>
    <label>Digimon</label>
    <race>RRP_PawnRace_Renamon</race>
    <defaultFactionType>RRP_PlayerRenamonTechnocracy</defaultFactionType>
    <apparelTags>
      <li>TestingBasicTrue</li>
    </apparelTags>
    <apparelMoney>200~1000</apparelMoney>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>10~15</initialResistanceRange>
  </PawnKindDef>
 
 	<!-- Renimon NPC Unit Base -->
	<PawnKindDef Abstract="True" Name="RRP_NpcSpacer_Renimon_Pawn">
	<defaultFactionType>RRP_RenamonTechnocracy</defaultFactionType>
    <combatPower>30</combatPower>
	<baseRecruitDifficulty>0.85</baseRecruitDifficulty>
    <apparelIgnoreSeasons>False</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
    <label>Digimon</label>
    <race>RRP_PawnRace_Renamon</race>
    <apparelTags>
      <li>TestingBasicTrue</li>
    </apparelTags>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>5~7</initialResistanceRange>
    <isFighter>true</isFighter>
    <invNutrition>3</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
  </PawnKindDef>
  
 <!-- Renimon NPC Melee unit -->
 <PawnKindDef ParentName="RRP_NpcSpacer_Renimon_Pawn">
    <defName>RRP_NpcSpacer_Renimon_Pawn_Melee</defName>
    <label>CQC Specialist</label>
	<race>RRP_PawnRace_Renamon</race>
    <combatPower>80</combatPower>
    <minGenerationAge>18</minGenerationAge>
    <maxGenerationAge>900</maxGenerationAge>
	<itemQuality>Normal</itemQuality>
    <apparelMoney>
      <min>500</min>
      <max>1750</max>
    </apparelMoney>
    <apparelRequired>
      <li>RRP_Apparel_Sakurya_Relaxwear</li>
    </apparelRequired>
    <weaponMoney>
      <min>250</min>
      <max>450</max>
    </weaponMoney>
    <weaponTags>
      <li>MedievalMeleeDecent</li>
      <li>MedievalMeleeAdvanced</li>
    </weaponTags>
    <skills>
      <li>
        <skill>Melee</skill>
        <range>5~10</range>
      </li>
    </skills>
    <techHediffsMoney>
      <min>25</min>
      <max>150</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <combatEnhancingDrugsCount>
      <min>0</min>
      <max>1</max>
    </combatEnhancingDrugsCount>
  </PawnKindDef>
  
 <!-- Renimon NPC Ranged unit -->
 <PawnKindDef ParentName="RRP_NpcSpacer_Renimon_Pawn">
    <defName>RRP_NpcSpacer_Renimon_Pawn_Ranger</defName>
    <label>Ranger Specialist</label>
	<race>RRP_PawnRace_Renamon</race>
    <combatPower>70</combatPower>
    <itemQuality>Normal</itemQuality>
    <minGenerationAge>25</minGenerationAge>
    <maxGenerationAge>700</maxGenerationAge>
    <apparelMoney>
      <min>750</min>
      <max>1000</max>
    </apparelMoney>
    <apparelRequired>
      <li>RRP_Apparel_Sakurya_PowerArmour</li>
    </apparelRequired>
    <weaponMoney>
      <min>150</min>
      <max>1450</max>
    </weaponMoney>
    <weaponTags>
      <li>Gun</li>
    </weaponTags>
    <disallowedTraits>
      <li>Brawler</li>
    </disallowedTraits>
    <skills>
      <li>
        <skill>Shooting</skill>
        <range>4~10</range>
      </li>
    </skills>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <combatEnhancingDrugsCount>
      <min>0</min>
      <max>1</max>
    </combatEnhancingDrugsCount>
    <inventoryOptions>
      <skipChance>0.8</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>25</min>
            <max>50</max>
          </countRange>
        </li>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>
            <min>0</min>
            <max>1</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
 
 <!-- Renimon NPC Leader unit -->
