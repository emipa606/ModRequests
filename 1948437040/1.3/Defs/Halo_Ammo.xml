<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="MakeableShellBase">
        <defName>rce_RocketAmmo</defName>
        <label>SPNKR Rocket</label>
        <description>The M19 102mm high-explosive, shaped-charge surface-to-surface missile (M19 SSM) is a munition for the M41 SSR MAV/AW Rocket Launcher and M39 rocket turret employed on the back of the M12A1 Rocket Warthog.</description>
        <graphicData>
            <texPath>Things/Item/Ammo/Rocket_AmmoBox</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <stackLimit>2</stackLimit>
        <costList>
            <Steel>10</Steel>
            <Chemfuel>5</Chemfuel>
			<ComponentIndustrial>1</ComponentIndustrial>
        </costList>
		<recipeMaker>
			<researchPrerequisite>UnscHeavyWeapons</researchPrerequisite>
		<recipeUsers Inherit="False">
			<li>rce_UNSCWorkbench</li>
		</recipeUsers>			
		</recipeMaker>
        <thingCategories Inherit="False">
            <li>Manufactured</li>
        </thingCategories>
        <tradeTags Inherit="False" />
        <comps>
            <li Class="CompProperties_Explosive">
                <explosiveRadius>2.9</explosiveRadius>
                <explosiveDamageType>Bomb</explosiveDamageType>
                <requiredDamageTypeToExplode>Flame</requiredDamageTypeToExplode>
            </li>
        </comps>
    </ThingDef>
	
