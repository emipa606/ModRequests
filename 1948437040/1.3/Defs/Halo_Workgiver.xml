      <li>rce_UNSCWorkbench</li>
	
    </fixedBillGiverDefs>
    <verb>creating things</verb>
    <gerund>creating things at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
  
  
  
  
  

