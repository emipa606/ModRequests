<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>Lamp-pot</defName>
    <label>lamp-pot</label>
    <description>Plant decorative flowers here to improve the mood of people nearby. This high-tech flower pot has a built in lamp which already comes powered because of the energy cells needed in construction.</description>
    <thingClass>Building_PlantGrower</thingClass>
    <graphicData>
      <texPath>Furn/Lamp-pot2</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
      <shadowData>
        <volume>(0.55, 0.35, 0.3)</volume>
        <offset>(0, 0, -0.28)</offset>
      </shadowData>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <uiIconScale>0.70</uiIconScale>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.20</fillPercent>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>450</WorkToBuild>
      <Mass>3</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>50</Beauty>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>50</costStuffCount>
    <costList>
      <ComponentIndustrial>3</ComponentIndustrial>
      <Energx>6</Energx>
      <Eftam>20</Eftam>
    </costList>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Glower">
        <glowRadius>9</glowRadius>
        <glowColor>(217,217,208,0)</glowColor>
      </li>
    </comps>
    <fertility>1</fertility>
    <building>
      <defaultPlantToGrow>Plant_Daylily</defaultPlantToGrow>
      <sowTag>Decorative</sowTag>
    </building>
    <rotatable>false</rotatable>
    <uiIconOffset>(0, -0.05)</uiIconOffset>
    <researchPrerequisites>
      <li>Autonomousfurniture</li>
    </researchPrerequisites>
  </ThingDef>
