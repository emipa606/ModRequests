  <!--============================ Furniture ===============================-->
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>EliteDresserMeh</defName>
    <label>Elite dresser</label>
    <description>A high-tech dresser. Gives a large comfort bonus to all nearby beds while also producing a sound that helps a person reach REM sleep faster and keeps them there longer for a better sleep. Placing more than one dresser near the same bed has no effect.</description>
    <graphicData>
      <texPath>Furn2/Dresser</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,1)</drawSize>
      <shaderType>CutoutComplex</shaderType>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.45</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
    <pathCost>50</pathCost>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costList>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Eftam>40</Eftam>
      <Energx>5</Energx>
    </costList>
    <defaultPlacingRot>North</defaultPlacingRot>
    <costStuffCount>50</costStuffCount>
    <statBases>
      <MaxHitPoints>180</MaxHitPoints>
      <Mass>15</Mass>
      <WorkToBuild>4000</WorkToBuild>
      <Flammability>0.5</Flammability>
      <Beauty>45</Beauty>
    </statBases>
    <size>(2,1)</size>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <Comfort>0.25</Comfort>
          <BedRestEffectiveness>0.25</BedRestEffectiveness>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <maxDistance>4</maxDistance>
      </li>
    </comps>
    <researchPrerequisites>
      <li>Autonomousfurniture</li>
    </researchPrerequisites>
  </ThingDef>
  <!--============================ Furniture ===============================-->
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>IonizerMeh</defName>
    <label>ionizer</label>
    <description>Ionizes a room with mechanites in order to keep it as clean as possible. Great for kitchens and hospitals and for making a room clean and beautiful.</description>
    <graphicData>
      <texPath>Furn2/Ionisator</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.55, 0.35, 0.3)</volume>
        <offset>(0, 0, -0.28)</offset>
      </shadowData>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.20</fillPercent>
    <statBases>
      <MaxHitPoints>350</MaxHitPoints>
      <WorkToBuild>6000</WorkToBuild>
      <Mass>18</Mass>
      <Flammability>0.7</Flammability>
      <Cleanliness>70</Cleanliness>
      <Beauty>35</Beauty>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costList>
      <Mechanites>1</Mechanites>
      <Eftam>200</Eftam>
      <Energx>8</Energx>
    </costList>
    <costStuffCount>100</costStuffCount>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Glower">
        <glowRadius>6</glowRadius>
        <glowColor>(100,100,250,0)</glowColor>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>3</glowRadius>
        <glowColor>(252,201,201,0)</glowColor>
      </li>
    </comps>
    <rotatable>false</rotatable>
    <uiIconOffset>(0,0.15)</uiIconOffset>
    <uiIconScale>0.70</uiIconScale>
    <researchPrerequisites>
      <li>Autonomousfurniture</li>
    </researchPrerequisites>
  </ThingDef>
</Defs>