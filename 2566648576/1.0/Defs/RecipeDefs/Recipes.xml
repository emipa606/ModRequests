<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <RecipeDef>
    <defName>Extractgasn</defName>
    <label>Extract N-16 gas</label>
    <description>Extract N-16 gas. Extraction of 8 units of the gas requires 2 units of steel for drilling. Extraction speed is highly dependant on "Mining" skill.</description>
    <jobString>Extracting N-16 gas</jobString>
    <workAmount>4100</workAmount>
    <recipeUsers>
      <li>Ancientborehole</li>
    </recipeUsers>
    <workSpeedStat>MiningSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Gasnenkj>10</Gasnenkj>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <workSkill>Mining</workSkill>
  </RecipeDef>
  <RecipeDef>
    <defName>MakeEftam</defName>
    <label>Produce eftam x50</label>
    <description>Make eftam by synthesis of steel and stone blocks under the influence of N-16 gas.</description>
    <jobString>Making eftam</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>3000</workAmount>
    <recipeUsers>
      <li>Gasconverter</li>
    </recipeUsers>
    <targetCountAdjustment>5</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>StoneBlocks</li>
          </categories>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gasnenkj</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Gasnenkj</li>
      </thingDefs>
      <categories>
        <li>StoneBlocks</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <Eftam>50</Eftam>
    </products>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
