<Patch>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Base_Neolithic_Standard"]/stockGenerators</xpath>
    <value>

      <!-- Belts -->
      <li Class="StockGenerator_Category">
        <categoryDef>PW_Belts</categoryDef>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>2~3</countRange>
      </li>

      <!-- Medicine -->
      <li Class="StockGenerator_Category">
        <categoryDef>PW_HealingItem</categoryDef>
        <excludedThingDefs>
          <li>PW_RareCandy</li>
          <li>PW_HealPowder</li>
        </excludedThingDefs>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>10~25</countRange>
      </li>

      <!-- Stones -->
      <li Class="StockGenerator_Category">
        <categoryDef>PW_EvolutionStone</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>1~2</countRange>
      </li>

      <!-- Fossils -->
      <li Class="StockGenerator_Category">
        <categoryDef>PW_Fossils</categoryDef>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>

      <!-- Eggs -->
      <li Class="PokeWorld.StockGenerator_PokemonEgg">
        <categoryDef>PW_Eggs</categoryDef>
        <thingDefCountRange>1~3</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>

      <!-- Pokemon -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>PW_PokemonCommon</li>
          <li>PW_PokemonStarter</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>PW_PokemonUncommon</li>
          <li>PW_PokemonRare</li>
          <li>PW_PokemonExotic</li>
        </tradeTagsBuy>
        <kindCountRange>3~5</kindCountRange>
        <countRange>4~8</countRange>
      </li>

    </value>
  </Operation>

