<?xml version="1.0" encoding="utf-8"?>
<Patch>
<!--Medieval Prosthetics Mod ID: 2101545242  -->
  <!-- Removing a tab -->
  <!--
  <Operation Class="PatchOperationRemove">
      <xpath>/Defs/ResearchProjectDef[defName = "Target"]</xpath>
  </Operation>
  -->
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Medieval Prosthetics</li>
    </mods>
<match Class = "PatchOperationSequence">
	<Operations>
	<li Class="PatchOperationRemove">
      <xpath>/Defs/ResearchTabDef[defName = "MedPros"]</xpath>
	</li>
  
  <!-- Changing a tab -->
  <!--
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ResearchProjectDef[defName = "target"]/tab</xpath>
    <value>
      <tab>"Destination"</tab>
    </value>
  </Operation>
  -->
  
  <!-- MedPros Research-->
	<li Class="PatchOperationsequence">            
	<Operations>
		<li Class="PatchOperationReplace">
			<xpath>/Defs/ResearchProjectDef[defName = "CrudeLimbs"]/tab</xpath>
			<value>
				<tab>MedicalResearch</tab>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="CrudeLimbs"]/researchViewX</xpath>
            <value>
				<researchViewX>3.00</researchViewX>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="CrudeLimbs"]/researchViewY</xpath>
            <value>
				<researchViewY>3.15</researchViewY>
			</value>
		</li>
	</Operations>
</li>
	<li Class="PatchOperationsequence">            
	<Operations>
		<li Class="PatchOperationReplace">
			<xpath>/Defs/ResearchProjectDef[defName = "ArtificialLimbs"]/tab</xpath>
			<value>
				<tab>MedicalResearch</tab>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="ArtificialLimbs"]/researchViewX</xpath>
            <value>
				<researchViewX>4.00</researchViewX>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="ArtificialLimbs"]/researchViewY</xpath>
            <value>
				<researchViewY>3.15</researchViewY>
			</value>
		</li>
	</Operations>
</li>
	<li Class="PatchOperationsequence">            
	<Operations>
		<li Class="PatchOperationReplace">
			<xpath>/Defs/ResearchProjectDef[defName = "MechanicalLimbs"]/tab</xpath>
			<value>
				<tab>MedicalResearch</tab>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="MechanicalLimbs"]/researchViewX</xpath>
            <value>
				<researchViewX>5.00</researchViewX>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="MechanicalLimbs"]/researchViewY</xpath>
            <value>
				<researchViewY>3.15</researchViewY>
			</value>
		</li>
	</Operations>
  </li>
	<li Class="PatchOperationsequence">            
	<Operations>
		<li Class="PatchOperationReplace">
			<xpath>/Defs/ResearchProjectDef[defName = "AmbroisenLimbs"]/tab</xpath>
			<value>
				<tab>MedicalResearch</tab>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="AmbroisenLimbs"]/researchViewX</xpath>
            <value>
				<researchViewX>6.00</researchViewX>
			</value>
		</li>
		<li Class="PatchOperationReplace">                    
            <xpath>Defs/ResearchProjectDef[defName="AmbroisenLimbs"]/researchViewY</xpath>
            <value>
				<researchViewY>3.15</researchViewY>
			</value>
		</li>
	</Operations>
	</li>
</Operations>
</match>
</Operation>
</Patch>