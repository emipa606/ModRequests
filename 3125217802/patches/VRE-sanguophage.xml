<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Sanguophage</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
  <ThingDef ParentName="BuildingBase">
    <thingClass>valkyrim.livingwalls.LivingwallVRES</thingClass>
    <defName>livingwallhemogen</defName>
    <label>Bioformed Living Wall</label>
    <description>A wall made of flesh and bone, requires input from the Hemogen network and if its available the nutrient paste network.</description>
    <category>Building</category>
    <graphicData>
      <texPath>VRESLivingWall/VRESLivingWall_atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkType>CornerFiller</linkType>
      <linkFlags>
        <li>Wall</li>
        <li>Rock</li>
      </linkFlags>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
        <edgeTop>Damage/Edge</edgeTop>
        <edgeBot>Damage/Edge</edgeBot>
        <edgeLeft>Damage/Edge</edgeLeft>
        <edgeRight>Damage/Edge</edgeRight>
      </damageData>
    </graphicData>
    <uiIconPath>VRESLivingWall/VRESLivingWall_MenuIcon</uiIconPath>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>135</WorkToBuild>
      <Flammability>0.4</Flammability>
      <MeditationFocusStrength>0.22</MeditationFocusStrength>
    </statBases>
    <researchPrerequisites>
      <li>Deathrest</li>
    </researchPrerequisites>
    <costList>
      <HemogenPack>1</HemogenPack>
    </costList>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <coversFloor>true</coversFloor>
    <placingDraggableDimensions>1</placingDraggableDimensions>
    <tickerType>Normal</tickerType>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <holdsRoof>true</holdsRoof>
    <designationCategory>Structure</designationCategory>
    <staticSunShadowHeight>1.0</staticSunShadowHeight>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
	  <soundImpactDefault>BulletImpact_Wood</soundImpactDefault>
    <fertility>0</fertility>
    <building>
      <isInert>true</isInert>
      <isPlaceOverableWall>true</isPlaceOverableWall>
      <ai_chillDestination>false</ai_chillDestination>
      <blueprintGraphicData>
        <texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
      </blueprintGraphicData>
    </building>
    <comps>

      <li Class="PipeSystem.CompProperties_RefillWithPipes">
        <pipeNet>VRE_HemogenNet</pipeNet>
        <thing>HemogenPack</thing>
      </li>
      <li Class="valkyrim.livingwalls.CompPropertieslivingwalltileVRES">
				<fuelConsumptionRate>.1</fuelConsumptionRate>
				<fuelCapacity>1.0</fuelCapacity>
        <tier>3.0</tier>
        <fuelFilter>
          <thingDefs>
            <li>HemogenPack</li>
          </thingDefs>
				</fuelFilter>
			</li>
    </comps>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>2</multiplier>
      </li>
      <li>
        <damageDef>Thump</damageDef>
        <multiplier>4</multiplier>
      </li>
    </damageMultipliers>
  </ThingDef>
  	</value>
          </li>
  			</operations>
		</match>
  </Operation>
</Patch>