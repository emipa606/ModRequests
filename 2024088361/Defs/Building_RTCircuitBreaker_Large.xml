<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<DesignatorDropdownGroupDef>
		<defName>ss_fuse_gr</defName>
	</DesignatorDropdownGroupDef>

	<ThingDef>
		<defName>Building_RTCircuitBreaker_12k</defName>
		<label>circuit breaker (12k)</label>
		<description>A set of automatic circuit breakers. Handles up to 12000 Wd discharge, will flick off if tripped.</description>
		<designationCategory>Power</designationCategory>
		<designatorDropdown>ss_fuse_gr</designatorDropdown>

		<category>Building</category>
		<thingClass>Building</thingClass>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsPower</li>
		</thingCategories>

		<graphicData>
			<texPath>Building_RTCircuitBreaker_12k</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1, 2)</drawSize>
			<damageData>
				<rect>(0, 0, 1, 2)</rect>
			</damageData>
		</graphicData>
		<drawerType>MapMeshAndRealTime</drawerType>
		<repairEffect>Repair</repairEffect>
		<constructEffect>ConstructMetal</constructEffect>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<castEdgeShadows>true</castEdgeShadows>

		<researchPrerequisites>
			<li>ResearchProject_RTCircuitBreakers</li>
			<li>LargeBatteries</li>
		</researchPrerequisites>
		<size>(1, 2)</size>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<selectable>True</selectable>
		<rotatable>True</rotatable>
		<fillPercent>0.30</fillPercent>
		<pathCost>30</pathCost>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>1500</WorkToMake>
			<Flammability>1.0</Flammability>
			<Mass>30</Mass>
			<Beauty>-10</Beauty>
		</statBases>
		<leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
		<costList>
			<Steel>150</Steel>
			<Plasteel>10</Plasteel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>

		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTransmitter</compClass>
				<transmitsPower>True</transmitsPower>
			</li>
			<li Class="CompProperties_Flickable" />
			<li Class="RT_Fuse.CompProperties_RTFuse">
				<compClass>RT_Fuse.CompRTFuse</compClass>
				<surgeMitigation>12000</surgeMitigation>
				<breakdownOnTrip>false</breakdownOnTrip>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
	</ThingDef>
