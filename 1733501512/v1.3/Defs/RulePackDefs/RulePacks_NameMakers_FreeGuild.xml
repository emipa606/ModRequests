<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerFGPeople</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[Male] [sur]</li>
        <li>name->[Female] [sur]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>Male</keyword>
          <path>NameBanks/Names_FGMale</path>
        </li>
        <li Class="Rule_File">
          <keyword>Female</keyword>
          <path>NameBanks/Names_FGFemale</path>
        </li>
        <li Class="Rule_File">
          <keyword>sur</keyword>
          <path>NameBanks/Names_FGFamily</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
