<?xml version="1.0" encoding="utf-8"?>
<Defs>
<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Hellhound</defName>
    <label>hellhound</label>
    <description>A strong, wolf-like human who have an affinity to fire. not suited to working with plants, or research however. It is also very uncommon for them to not be a pyromainiac.</description>
    <statBases>
      <MarketValue>3500</MarketValue>
      <MoveSpeed>9.0</MoveSpeed>
      <ComfyTemperatureMin>-84</ComfyTemperatureMin>
      <ComfyTemperatureMax>77</ComfyTemperatureMax>
      <LeatherAmount>70</LeatherAmount>
      <RoyalFavorValue>5</RoyalFavorValue>
      <Flammability>0</Flammability>
      <CarryingCapacity>100</CarryingCapacity>
      <ResearchSpeed>0.6</ResearchSpeed>
      <MiningSpeed>1.25</MiningSpeed>
      <SocialImpact>0.75</SocialImpact>
      <PlantWorkSpeed>0.25</PlantWorkSpeed>
      <PlantHarvestYield>0.35</PlantHarvestYield>
      <ConstructionSpeed>1.0</ConstructionSpeed>
    </statBases>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10.1</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>21</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10.1</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>21</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>12.1</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
        <soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
        <soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4.5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>

    <alienRace>
        <generalSettings>
            <maleGenderProbability>0</maleGenderProbability>
            <humanRecipeImport>true</humanRecipeImport>
            <allowHumanBios>true</allowHumanBios>
            <forcedRaceTraitEntries>
                <li>
                    <defName>Pyromaniac</defName>
                    <chance>70</chance>
                </li>
                <li>
                    <defName>Tough</defName>
                    <chance>100</chance>
                </li>
				<li>
                    <defName>Bloodlust</defName>
                    <chance>25</chance>
                </li>
				<li>
                    <defName>Psychopath</defName>
                    <chance>35</chance>
                </li>
            </forcedRaceTraitEntries>
            <alienPartGenerator>
                <aliencrowntypes>
                    <li>Average_Normal</li>
                </aliencrowntypes>
            <alienbodytypes>
                <li>Female</li>
            </alienbodytypes>
            <useGenderedHeads>false</useGenderedHeads>
            <useGenderedBodies>false</useGenderedBodies>
					<bodyAddons>
						<li>
							<path>Hellhound/Head/HellhoundFeatures</path>
							<bodyPart>Head</bodyPart>
							<inFrontOfBody>true</inFrontOfBody>
							<drawnInBed>true</drawnInBed>
							<drawSize>1</drawSize>
							<offsets>
							<south>
								<bodyTypes>
								<Female>(-0,0.34)</Female>
								</bodyTypes>
							</south>
							<north>
								<bodyTypes>
								<Female>(-0.0,0.34)</Female>
								</bodyTypes>
							</north>
							<east>
								<bodyTypes>
								<Female>(-0.1,0.34)</Female>
								</bodyTypes>
							</east>
							</offsets>
						</li>
					</bodyAddons>
			<colorChannels>
                        <li>                           
                            <name>skin</name>
                            <first Class="ColorGenerator_Options">
                                <options><!--Skin Colors--> 
                                    <li>
                                        <weight>5</weight>
                                        <only>RGBA(1.0,1.0,1.0,1.0)</only>
                                    </li>
                                </options>
                            </first>
                            <second Class="ColorGenerator_Options">
                                <options><!--Eye Colors-->
                                    <li>
                                        <weight>1</weight>
                                        <only>RGBA(1.0,1.0,1.0,1.0)</only>
                                    </li>
                                </options>
                            </second>
                        </li>
                        <li>
                            <name>hair</name>
                            <first Class="ColorGenerator_Options">
                                <options><!--Hair Colors-->
                                    <li>
                                        <weight>5</weight>
                                        <only>RGBA(0.27,0.29,0.37,1.0)</only>
                                    </li>
                                </options>
                            </first>
                            <second Class="ColorGenerator_Options"><!--Hair Accessory Colors-->
                                <options>
                                    <li>
                                        <weight>1</weight>
                                        <only>RGBA(0.27,0.29,0.37,1.0)</only>
                                    </li>
                                </options>
                            </second>
                        </li>
                    </colorChannels>
            </alienPartGenerator>
        </generalSettings>
        <graphicPaths>
            <li>
                <body>Hellhound/Body/</body>
                <head>Hellhound/Head/</head>
            </li>
        </graphicPaths>
            <styleSettings>
                <li>
                    <key>HairDef</key>
                    <value>
                        <hasStyle>true</hasStyle>
                    </value>
                </li>
                <li>
                    <key>BeardDef</key>
                    <value>
                        <hasStyle>false</hasStyle>
                    </value>
                </li>
                <li>
                    <key>TattooDef</key>
                    <value>
                        <hasStyle>true</hasStyle>
                    </value>
                </li>
            </styleSettings>
        <compatibility>
            <isFlesh>true</isFlesh>
            <isSentient>true</isSentient> 
            <hasBlood>true</hasBlood>
        </compatibility>
    </alienRace>

    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>80</lifeExpectancy>
      <leatherDef>Leather_Fox</leatherDef>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
      <baseBodySize>1.3</baseBodySize>
      <baseHealthScale>1.6</baseHealthScale>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>35</gestationPeriodDays>
      <meatMarketValue>0.9</meatMarketValue>
      <manhunterOnDamageChance>1.0</manhunterOnDamageChance>
      <manhunterOnTameFailChance>40</manhunterOnTameFailChance>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.01, 0.02)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1.2</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>4</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>13</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>18</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <soundMeleeDodge>Pawn_MeleeDodge</soundMeleeDodge>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(50,100)</li>
          <li>(60,30)</li>
          <li>(70,18)</li>
          <li>(80,10)</li>
          <li>(90,3)</li>
          <li>(100,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
      <li>CureScaria</li>
      <li MayRequire="Ludeon.RimWorld.Royalty">CureBloodRot</li>
      <li MayRequire="Ludeon.RimWorld.Royalty">CureAbasia</li>
    </recipes>

</AlienRace.ThingDef_AlienRace>
</Defs>