<?xml version="1.0" encoding="utf-8" ?>
<!-- This file contains the labels for buttons that are not in the settings window. -->
<LanguageData>
  <!-- The previous button label. -->
	<PreviousButton>Previous</PreviousButton>
  <!-- The previous button shorthand label. -->
	<PreviousButtonShort>Prev.</PreviousButtonShort>
  <!-- The next button label. -->
	<NextButton>Next</NextButton>
  <!-- The play/pause label. This only shows when the song is paused. -->
	<PlayButton>Play</PlayButton>
  <!-- The play/pause label. This only shows when the song is playing. -->
	<PauseButton>Pause</PauseButton>
  <!-- The hide button label, for switching to the mini display. -->
	<SwitchButton>Miniplayer</SwitchButton>
  <!-- For when the mini button will actually hide the player. -->
  <HideButton>Hide Player</HideButton>
</LanguageData>