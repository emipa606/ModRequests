  <TraitDef>
    <defName>GWT_Simple</defName>
    <commonality>1</commonality>
    <degreeDatas>
      <li>
        <label>simple</label>
        <description>{PAWN_nameDef} was never great at complex tasks, though they were gifted in doing the few things they could reasonably understand very well. {PAWN_nameDef} has no need for learning as a child.</description>
        <statFactors>
          <!-- Optional. Modifies pawn stats by multiplier -->
          <MoveSpeed>1.10</MoveSpeed>
        </statFactors>
        <statOffsets>
          <GeneralLaborSpeed>1</GeneralLaborSpeed>
          <GlobalLearningFactor>-0.5</GlobalLearningFactor>
        </statOffsets>
      </li>
    </degreeDatas>
    <requiredWorkTags>
      <li>ManualDumb</li>
      <li>Hauling</li>
      <li>Cleaning</li>
    </requiredWorkTags>
  </TraitDef>
