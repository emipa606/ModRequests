<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
        <li>Paniel the Automata</li>
    </mods>
    <match Class="PatchOperationSequence">
        <operations>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Paniel_Race"]/comps</xpath>
                <value>
                    <li Class="HumanResources.CompProperties_Knowledge">
                        <innateKnownTechs>
                            <li>PNRP_SelfRepairKit</li>
                            <li>Electricity</li>
                        </innateKnownTechs>
                        <innateKnownWeapons>
                            <!--Industrial-->
                            <li>PN_Shovel</li>
                            <li>PN_Chainsword</li>
                            <li>PN_Revolver</li>
                            <li>PN_Rifle</li>
                            <li>PN_Machinegun</li>
                            <li>PN_Cannon</li>
                            <li>PN_PrototypeRailGun</li>
                            <!--Royal-->
                            <li>PN_Chainlongsword</li>
                            <li>PN_WarHammer</li>
                            <li>PN_TwinPistol</li>
                            <li>PN_RoyalRifle</li>
                            <li>PN_RoyalHeavySlug</li>
                            <li>PN_RoyalMachinegun</li>
                            <li>PN_RoyalLSW</li>
                        </innateKnownWeapons>
                    </li>
                </value>
            </li>            
        </operations>
    </match>
  </Operation>
</Patch>