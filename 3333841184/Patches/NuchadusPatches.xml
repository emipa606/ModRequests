<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/AbilityDef[defName="VFEI2_ChemfuelSpit"]/verbProperties/warmupTime</xpath>
		<value>
			<warmupTime>1</warmupTime>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/AbilityDef[defName="VFEI2_ChemfuelSpit"]/comps</xpath>
		<value>
		<comps>
			<li Class="CompProperties_AbilityLaunchProjectile">
				<projectileDef>VFEI2_ChemfuelSpit_Bomb_Boss</projectileDef>
			</li>
		</comps>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Titantick"]/comps</xpath>
		<value>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Regeneration_1</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Titantick"]</xpath>
		<value>
			<verbs>
				<li>
					<verbClass>Verb_Shoot</verbClass>
					<warmupTime>6.0</warmupTime>
					<forcedMissRadius>1</forcedMissRadius>
					<defaultProjectile>VFEI2_ChemfuelSpit_Bomb_Boss</defaultProjectile>
					<range>60</range>
					<minRange>5.9</minRange>
					<targetParams>
					<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<burstShotCount>1</burstShotCount>
					<soundCast>VFEI2_FireSpew_Resolve</soundCast>
					<hasStandardCommand>true</hasStandardCommand>
					<muzzleFlashScale>0</muzzleFlashScale>
				</li>
			</verbs>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Titantick"]/statBases</xpath>
		<value>
					<ImmunityGainSpeed>5</ImmunityGainSpeed>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Titantick"]/statBases/ArmorRating_Blunt</xpath>
		<value>
					<ArmorRating_Blunt>0.35</ArmorRating_Blunt>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Titantick"]/statBases/ArmorRating_Sharp</xpath>
		<value>
					<ArmorRating_Sharp>0.60</ArmorRating_Sharp>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_Titantick"]/race/wildness</xpath>
			<value>
					<wildness>0</wildness>
			</value>
    </Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Hellbeetle"]/comps</xpath>
		<value>
			<comps>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Blazing_Body</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
			</comps>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Hellbeetle"]</xpath>
		<value>
			<verbs>
				<li>
					<verbClass>Verb_Shoot</verbClass>
					<warmupTime>1.0</warmupTime>
					<forcedMissRadius>2</forcedMissRadius>
					<defaultProjectile>Bullet_HellbeetleInfernoCannon</defaultProjectile>
					<range>10</range>
					<minRange>1.9</minRange>
					<targetParams>
					<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<ticksBetweenBurstShots>5</ticksBetweenBurstShots>
					<burstShotCount>5</burstShotCount>
					<soundCast>InfernoCannon_Fire</soundCast>
					<hasStandardCommand>true</hasStandardCommand>
					<muzzleFlashScale>0</muzzleFlashScale>
				</li>
			</verbs>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Fuelmite"]/comps</xpath>
		<value>
			<comps>
			<li Class="AnimalBehaviours.CompProperties_AnimalProduct">
				<resourceDef>Chemfuel</resourceDef>
				<gatheringIntervalDays>10</gatheringIntervalDays>
				<resourceAmount>15</resourceAmount>
				<customResourceString>VFEI2_Chemfuel</customResourceString>
				<hideDisplayOnWildAnimals>true</hideDisplayOnWildAnimals>
			</li>
			</comps>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Fuelmite"]</xpath>
		<value>
			<verbs>
				<li>
					<verbClass>Verb_Shoot</verbClass>
					<warmupTime>3.0</warmupTime>
					<forcedMissRadius>1</forcedMissRadius>
					<defaultProjectile>VFEI2_ChemfuelSpit_Bomb</defaultProjectile>
					<range>40</range>
					<minRange>2.9</minRange>
					<targetParams>
					<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<burstShotCount>1</burstShotCount>
					<soundCast>VFEI2_FireSpew_Resolve</soundCast>
					<hasStandardCommand>true</hasStandardCommand>
					<muzzleFlashScale>0</muzzleFlashScale>
				</li>
			</verbs>
		</value>
	</Operation>
	
</Patch>