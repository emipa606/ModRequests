<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_BlackEmpress"]/comps</xpath>
		<value>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Regeneration_1</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_BlackEmpress"]/race/wildness</xpath>
			<value>
					<wildness>0</wildness>
			</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/AbilityDef[defName="AA_GreaterVileSpit"]/cooldownTicksRange</xpath>
			<value>
					<cooldownTicksRange>0</cooldownTicksRange>
			</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/AbilityDef[defName="AA_GreaterVileSpit"]/verbProperties</xpath>
			<value>
			<verbProperties>
			<verbClass>Verb_CastAbility</verbClass>
			<range>20.9</range>
			<warmupTime>3</warmupTime>
			<soundCast>VFEI2_FireSpew_Resolve</soundCast>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
			</verbProperties>
			</value>
	</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_BlackQueen"]</xpath>
		<value>
			<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<accuracyTouch>1</accuracyTouch>
				<accuracyShort>1</accuracyShort>
				<accuracyMedium>1</accuracyMedium>
				<accuracyLong>1</accuracyLong>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>AA_BlackHiveBolt</defaultProjectile>
				<warmupTime>1</warmupTime>
				<burstShotCount>5</burstShotCount>
				<range>40</range>
				<soundCast>AA_PoisonBolt</soundCast>
				<muzzleFlashScale>0</muzzleFlashScale>
				<label>venomous spit</label>
				<commonality>.8</commonality>
			</li>
		</verbs>
		</value>
	</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="AA_MammothWorm"]/tools</xpath>
			<value>
			<tools>
			<li>
				<label>body</label>
				<capacities>
					<li>AA_ExtraDamageMechanoidsTusk</li>
				</capacities>
				<power>30</power>
				<cooldownTime>1.5</cooldownTime>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>100</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>AA_ExtraDamageMechanoidsTusk</li>
				</capacities>
				<label>tusks</label>
				<power>30</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>TuskAttackTool</linkedBodyPartsGroup>
			</li>
		</tools>
			</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="AA_MammothWorm"]/race/baseHealthScale</xpath>
			<value>
					<baseHealthScale>8.0</baseHealthScale>
			</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="AA_MammothWorm"]/comps</xpath>
		<value>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Crushing</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</value>
	</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="AA_Ravager"]/tools</xpath>
			<value>
			<tools>
			<li>
				<label>triple maw</label>
				<capacities>
					<li>AA_ExtraDamageMechanoidsClaws</li>
				</capacities>
				<power>10</power>
				<cooldownTime>0.25</cooldownTime>
				<linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>AA_ExtraDamageMechanoidsClaws</li>
				</capacities>
				<power>10</power>
				<cooldownTime>0.25</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
			</value>
	</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="AA_Ravager"]</xpath>
