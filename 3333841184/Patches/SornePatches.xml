<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Empress"]/comps/li[@Class="VFEInsectoids.CompProperties_Hive"]/insectTypes</xpath>
		<value>
					<li>
						<insect>VFEI2_RoyalMegascarab</insect>
						<insectType>Worker</insectType>
					</li>
					<li>
						<insect>VFEI2_RoyalSpelopede</insect>
						<insectType>Defender</insectType>
					</li>
					<li>
						<insect>VFEI2_RoyalMegaspider</insect>
						<insectType>Defender</insectType>
					</li>			
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Empress"]/comps</xpath>
		<value>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Regeneration_1</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Strong_Crushing</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Empress"]/statBases/ArmorRating_Blunt</xpath>
		<value>
					<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Empress"]/statBases/ArmorRating_Sharp</xpath>
		<value>
					<ArmorRating_Sharp>0.75</ArmorRating_Sharp>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Empress"]/statBases</xpath>
		<value>
					<ImmunityGainSpeed>5</ImmunityGainSpeed>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_Empress"]/race/baseHealthScale</xpath>
			<value>
					<baseHealthScale>10</baseHealthScale>
			</value>
    </Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_Empress"]/race/wildness</xpath>
			<value>
					<wildness>0</wildness>
			</value>
    </Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Patriarch"]/comps</xpath>
		<value>
			<li Class="AnimalBehaviours.CompProperties_AnimalProduct">
				<resourceDef>InsectJelly</resourceDef>
				<gatheringIntervalDays>1</gatheringIntervalDays>
				<resourceAmount>100</resourceAmount>
				<customResourceString>VFEI2_InsectJelly</customResourceString>
				<hideDisplayOnWildAnimals>true</hideDisplayOnWildAnimals>
			</li>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Regeneration_1</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
			<li Class="AnimalBehaviours.CompProperties_InitialHediff">
				<hediffname>VFEI2_Strong_Crushing</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Patriarch"]/statBases/ArmorRating_Blunt</xpath>
		<value>
					<ArmorRating_Blunt>0.6</ArmorRating_Blunt>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Patriarch"]/statBases/ArmorRating_Sharp</xpath>
		<value>
					<ArmorRating_Sharp>0.9</ArmorRating_Sharp>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_Patriarch"]/statBases</xpath>
		<value>
					<ImmunityGainSpeed>5</ImmunityGainSpeed>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_Patriarch"]/race/baseHealthScale</xpath>
			<value>
					<baseHealthScale>10</baseHealthScale>
			</value>
    </Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_Patriarch"]/race/wildness</xpath>
			<value>
					<wildness>0</wildness>
			</value>
    </Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_Patriarch"]/tools</xpath>
		<value>
			<tools>
			<li>
				<label>head claw</label>
				<capacities>
					<li>Cut</li>
				</capacities>
				<armorPenetration>0.25</armorPenetration>
				<power>10</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
			</li>
			<li>
				<label>front claw</label>
				<capacities>
					<li>Cut</li>
				</capacities>
				<armorPenetration>0.8</armorPenetration>
				<power>40</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>10</power>
				<armorPenetration>0.25</armorPenetration>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_RoyalSpelopede"]</xpath>
		<value>
			<verbs>
				<li>
				<verbClass>Verb_Shoot</verbClass>
				<accuracyTouch>1</accuracyTouch>
				<accuracyShort>1</accuracyShort>
				<accuracyMedium>1</accuracyMedium>
				<accuracyLong>1</accuracyLong>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>Bullet_Thorn</defaultProjectile>
				<warmupTime>1</warmupTime>
				<burstShotCount>1</burstShotCount>
				<range>30</range>
				<soundCast>VFEI2_InsectThornpod_Release</soundCast>
				<muzzleFlashScale>0</muzzleFlashScale>
			</li>
			</verbs>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="VFEI2_RoyalMegaspider"]/statBases/MoveSpeed</xpath>
		<value>
					<MoveSpeed>3.6</MoveSpeed>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_RoyalMegaspider"]/race/baseHealthScale</xpath>
			<value>
					<baseHealthScale>6</baseHealthScale>
			</value>
    </Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="VFEI2_RoyalMegaspider"]/tools</xpath>
		<value>
			<li>
				<label>right claw</label>
				<capacities>
					<li>VFEI2_TeramantisStun</li>
				</capacities>
				<armorPenetration>0.5</armorPenetration>
				<power>20</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>VFEI2_ClawAttackTool</linkedBodyPartsGroup>
			</li>
			<li>
				<label>left claw</label>
				<capacities>
					<li>VFEI2_TeramantisStun</li>
				</capacities>
				<armorPenetration>0.5</armorPenetration>
				<power>20</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>VFEI2_ClawAttackTool</linkedBodyPartsGroup>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName ="VFEI2_RoyalMegaspider"]/race/body</xpath>
			<value>
					<body>VFEI2_ClawedInsectoid</body>
			</value>
    </Operation>
	
</Patch>