<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<HediffDef>
		<defName>InkPoorlyDrawn</defName>
		<label>ink loss</label>
		<description>Loss of coolant in internal piping, resulting in reduced cooling capacity and overheating of internals.</description>
		<maxSeverity>1.0</maxSeverity>
	<defaultLabelColor>(0.6, 0.6, 0.9)</defaultLabelColor>
      <stages>
        <li>
          <label>initial</label>
          <becomeVisible>false</becomeVisible>
        </li>
		
		
        <li>
          <minSeverity>0.1</minSeverity>
          <label>initial</label>
		  <statOffsets>
           <ComfyTemperatureMax>-5</ComfyTemperatureMax>
		  </statOffsets>
        </li>
		
		
        <li>
          <minSeverity>0.3</minSeverity>
          <label>minor</label>
		  <statOffsets>
           <ComfyTemperatureMax>-15</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.05</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.05</offset>
              </li>
            </capMods>
        </li>
		
		
        <li>
          <minSeverity>0.6</minSeverity>
          <label>serious</label>
		  <statOffsets>
		<ComfyTemperatureMax>-30</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Consciousness</capacity>
                <offset>-0.3</offset>
              </li>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.1</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.1</offset>
              </li>
            </capMods>
        </li>
		
		
        <li>
          <minSeverity>0.9</minSeverity>
          <label>extreme</label>
		  <statOffsets>
         <ComfyTemperatureMax>-60</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Consciousness</capacity>
                <offset>-0.6</offset>
              </li>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.30</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.30</offset>
              </li>
            </capMods>
        </li>
	</stages>
	</HediffDef>
</Defs>