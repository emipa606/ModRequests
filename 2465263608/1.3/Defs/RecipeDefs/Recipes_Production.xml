<Defs>

<!-- ##### CHEMFUEL ##### -->
<!-- Chemfuel with dark oil -->
<RecipeDef>
	<defName>VMEu_MakeChemfuelFromDarkOil</defName>
	<label>make chemfuel from dark oil</label>
	<description>A somewhat long process that extracts huge amounts of chemfuel from dark oil.\n\nThis process makes dark oil a little less usable afterwards.</description>
	<jobString>Refining chemfuel from dark oil.</jobString>
	<effectWorking>Cremate</effectWorking>
	<soundWorking>Recipe_Cremate</soundWorking>
	<workAmount>3500</workAmount>
	<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<recipeUsers>
		<li>BiofuelRefinery</li>
	</recipeUsers>
	<ingredients>
	  <li>
		<filter>
		  <thingDefs>
			<li>VMEu_DarkOil</li>
		  </thingDefs>
		</filter>
		<count>20</count>
	  </li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
		<li>VMEu_DarkOil</li>
	  </thingDefs>
	</fixedIngredientFilter>
	<products>
	  <Chemfuel>100</Chemfuel>
	</products>
</RecipeDef>
				  

