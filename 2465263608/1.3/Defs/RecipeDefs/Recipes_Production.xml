<!-- Asphalt production -->
<RecipeDef>
	<defName>VMEu_MakeAsphaltMix</defName>
	<label>make asphalt mix</label>
	<description>Make a dark, thik mix from the rest of already used dark oil and grounded stone.\n\nAsphalt is great for paths, and is very fast to walk through.</description>
	<jobString>making asphalt mix.</jobString>
	<effectWorking>Cremate</effectWorking>
	<soundWorking>Recipe_Cremate</soundWorking>
	<workAmount>1200</workAmount>
	<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<recipeUsers>
		<li>FueledSmithy</li>
		<li>ElectricSmithy</li>
	</recipeUsers>
	<ingredients>
	  <li>
		<filter>
		  <categories>
			<li>StoneChunks</li>
		  </categories>
		</filter>
		<count>1</count>
	  </li>
	  <li>
		<filter>
		  <thingDefs>
			<li>VMEu_DarkOil</li>
		  </thingDefs>
		</filter>
		<count>1</count>
	  </li>
	</ingredients>
	<fixedIngredientFilter>
	  <categories>
		<li>StoneChunks</li>
	  </categories>
	  <thingDefs>
		<li>VMEu_DarkOil</li>
	  </thingDefs>
	</fixedIngredientFilter>
	<products>
	  <VMEu_AsphaltMix>20</VMEu_AsphaltMix>
	</products>
	<researchPrerequisite>BiofuelRefining</researchPrerequisite>
</RecipeDef>
