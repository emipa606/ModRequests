<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<DesignatorDropdownGroupDef>
		<defName>MAG_ArchotechFloors</defName>
	</DesignatorDropdownGroupDef>
	<TerrainDef Name="FloorPlasteel">
		<defName>FloorArchotechUwU</defName>
		<label>archotech floor</label>
		<designatorDropdown>MAG_ArchotechFloors</designatorDropdown>
		<description>An archotech floor tile. Very clean and sturdy.</description>
		<texturePath>Terrain/Floors/SexyArchotechFloorPanels</texturePath>
		<layerable>true</layerable>
		<affordances>
			<li>Light</li>
			<li>Medium</li>
			<li>Heavy</li>
		</affordances>
		<tags>
			<li>Floor</li>
			<li>FineFloor</li>
		</tags>
		<statBases>
			<Cleanliness>1.2</Cleanliness>
			<WorkToBuild>12000</WorkToBuild>
			<MarketValue>20</MarketValue>
			<Beauty>20</Beauty>
		</statBases>
		<costList>
			<Plasteel>3</Plasteel>
			<ArchotechScrap>1</ArchotechScrap>
		</costList>
		<designationCategory>Floors</designationCategory>
		<fertility>0</fertility>
		<constructEffect>ConstructMetal</constructEffect>
		<filthAcceptanceMask></filthAcceptanceMask>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<researchPrerequisites>
			<li>ArchoTechStart</li>
		</researchPrerequisites>
	</TerrainDef>

