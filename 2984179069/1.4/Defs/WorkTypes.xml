<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<WorkGiverDef>
		<defName>DoBillsSexyArchotechWorkbench</defName>
		<label>fabricate archotech things</label>
		<giverClass>WorkGiver_DoBill</giverClass>
		<workType>Smithing</workType>
		<priorityInType>50</priorityInType>
		<fixedBillGiverDefs>
			<li>ArchBench</li>
		</fixedBillGiverDefs>
		<verb>making things</verb>
		<gerund>making archotech items at</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<prioritizeSustains>true</prioritizeSustains>
	</WorkGiverDef>

	<WorkGiverDef>
		<defName>MAG_CarryToArchotechGeneExtractor</defName>
		<label>carry to archotech gene extractor</label>
		<giverClass>MoreArchotechGarbage.WorkGiver_CarryToArchoGeneExtractor</giverClass>
		<workType>Hauling</workType>
		<verb>extract genes</verb>
		<gerund>extracting genes from</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<canBeDoneByMechs>false</canBeDoneByMechs>
	</WorkGiverDef>

	<WorkGiverDef MayRequire="DanielWedemeyer.GeneRipper">
		<defName>MAG_CarryToArchotechGeneRipper</defName>
		<label>carry to archotech gene ripper</label>
		<giverClass>MoreArchotechGarbage.WorkGiver_CarryToArchoGeneRipper</giverClass>
		<workType>Hauling</workType>
		<verb>extract genes</verb>
		<gerund>extracting genes from</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<canBeDoneByMechs>false</canBeDoneByMechs>
	</WorkGiverDef>
	
	<WorkGiverDef MayRequire="DanielWedemeyer.GeneRipper">
		<defName>MAG_CarryCorpseToArchotechGeneRipper</defName>
		<label>carry to archotech gene ripper</label>
		<giverClass>MoreArchotechGarbage.WorkGiver_CarryCorpseToArchoGeneRipper</giverClass>
		<workType>Hauling</workType>
		<verb>extract genes</verb>
		<gerund>extracting genes from</gerund>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<canBeDoneByMechs>false</canBeDoneByMechs>
	</WorkGiverDef>
</Defs>