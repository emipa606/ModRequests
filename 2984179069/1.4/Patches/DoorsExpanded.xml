<Patch>
	<!--<color>(104,109,80)</color>-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Doors Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<ThingDef ParentName="AbstractHeronDoorBase">
							<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
							<defName>PH_MAG_DoorDouble</defName>
							<thingClass>DoorsExpanded.Building_DoorExpanded</thingClass>
							<designationCategory>Structure</designationCategory>
							<!-- label and description are handled by CompProperties_PostProcessText -->
							<statBases>
								<WorkToBuild>2000</WorkToBuild>
								<DoorOpenSpeed>7.5</DoorOpenSpeed>
								<Flammability>0</Flammability>
								<Beauty>2</Beauty>
								<Cleanliness>2</Cleanliness>
							</statBases>
							<costList>
								<Steel>30</Steel>
								<Plasteel>20</Plasteel>
								<ArchotechScrap>20</ArchotechScrap>
							</costList>
							<researchPrerequisites>
								<li>ArchoTechStart</li>
							</researchPrerequisites>
							<useHitPoints>false</useHitPoints>
							<size>(2,1)</size>
							<graphicData>
								<texPath>Things/Building/Door/DoorDouble_Mover</texPath>
								<color>(104,109,80)</color>
								<graphicClass>Graphic_Single</graphicClass>
								<drawSize>(2,1)</drawSize>
								<damageData>
									<!--  no damage marks because they don't move with the door
        <rect>(0,0.12,1,0.76)</rect>-->
									<enabled>false</enabled>
								</damageData>
							</graphicData>
							<uiIconPath>Things/Building/Door/DoorDouble_MenuIcon</uiIconPath>
							<uiIconColor>(104,109,80)</uiIconColor>
							<comps>
								<li Class="DoorsExpanded.CompProperties_PostProcessText">
									<defaultLabelAndDescriptionFrom>MAG_ArchotechDoor</defaultLabelAndDescriptionFrom>
									<appendSizeToLabel>true</appendSizeToLabel>
								</li>
								<li Class="DoorsExpanded.CompProperties_DoorExpanded">
									<doorType>Standard</doorType>
								</li>
							</comps>
							<tickerType>Normal</tickerType>
							<category>Building</category>
						</ThingDef>

						<ThingDef ParentName="AbstractHeronDoorBase">
							<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
							<thingClass>DoorsExpanded.Building_DoorExpanded</thingClass>
							<defName>PH_MAG_DoorTriple</defName>
							<designationCategory>Structure</designationCategory>
							<!-- label and description are handled by CompProperties_PostProcessText -->
							<statBases>
								<WorkToBuild>3000</WorkToBuild>
								<DoorOpenSpeed>7.5</DoorOpenSpeed>
								<Flammability>0</Flammability>
								<Beauty>3</Beauty>
								<Cleanliness>3</Cleanliness>
							</statBases>
							<costList>
								<Steel>45</Steel>
								<Plasteel>30</Plasteel>
								<ArchotechScrap>30</ArchotechScrap>
							</costList>
							<researchPrerequisites>
								<li>ArchoTechStart</li>
							</researchPrerequisites>
							<useHitPoints>false</useHitPoints>
							<size>(3,1)</size>
							<graphicData>
								<texPath>Things/Building/Door/DoorTriple_Mover</texPath>
								<color>(104,109,80)</color>
								<graphicClass>Graphic_Single</graphicClass>
								<drawSize>(3,1)</drawSize>
								<damageData>
									<!--  no damage marks because they don't move with the door
        <rect>(0,0.12,1,0.76)</rect>-->
									<enabled>false</enabled>
								</damageData>
							</graphicData>
							<uiIconPath>Things/Building/Door/DoorTriple_MenuIcon</uiIconPath>
							<uiIconColor>(104,109,80)</uiIconColor>
							<comps>
								<li Class="DoorsExpanded.CompProperties_PostProcessText">
									<defaultLabelAndDescriptionFrom>MAG_ArchotechDoor</defaultLabelAndDescriptionFrom>
									<appendSizeToLabel>true</appendSizeToLabel>
								</li>
								<li Class="DoorsExpanded.CompProperties_DoorExpanded">
									<doorType>Standard</doorType>
								</li>
							</comps>
							<tickerType>Normal</tickerType>
							<category>Building</category>
						</ThingDef>

					</value>
				</li>

			</operations>
		</match>
	</Operation>






</Patch>