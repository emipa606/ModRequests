<!-- With this, archotech floors will have the same infestation-blocking properties as armored floors (if Armored Floors is active) -->
<Patch>
	

	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Mechanoid Bench 3</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "SalvageCorpseMechanoidArchotech"]/products</xpath>
					<value>
						<MechanoidBench_MechaComponent>1</MechanoidBench_MechaComponent>
					</value>
				</li>					
			</operations>
		</match>
	</Operation>

	
</Patch>