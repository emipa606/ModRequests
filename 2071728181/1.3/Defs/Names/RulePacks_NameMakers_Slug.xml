<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonSlug</defName>
    <rulePack>

		<rulesStrings>
			<li>name->[FirstName] '[slugAdj]' [SlugName]</li>
			<li>name->[FirstName] '[badAdj]' [SlugName]</li>
			<li>name->[FirstName] '[SlugName2]' [SlugName]</li>
			<li>name->[FirstName] '[badass]' [SlugName]</li>
			<li>name->[FirstName] '[SlugName]' [roman]</li>
	
			<li>name->[color] '[SlugName2]' [SlugName]</li>

			<li>name->[slugAdj] '[SlugName2]' [SlugName]</li>
			
			<li>name->[FirstName] [SlugName]</li>
		</rulesStrings>
	  
		<rulesRaw>
			<li Class="Rule_File">
				<keyword>badAdj</keyword>
				<path>SlugNames/BadAdj</path>
			</li>
			<li Class="Rule_File">
			  <keyword>slugAdj</keyword>
			  <path>SlugNames/SlugAdj</path>
			</li>
			<li Class="Rule_File">
			  <keyword>SlugName</keyword>
			  <path>SlugNames/SlugNames</path>
			</li>
			<li Class="Rule_File">
			  <keyword>SlugNames</keyword>
			  <path>SlugNames/SlugNames2</path>
			</li>
			
			<li Class="Rule_File">
			  <keyword>FirstName</keyword>
			  <path>Common/UnisexFirstNames</path>
			</li>

			<li Class="Rule_File">
			  <keyword>animal</keyword>
			  <path>Words/Nouns/Animals</path>
			</li>
			<li Class="Rule_File">
			  <keyword>badass</keyword>
			  <path>Words/Nouns/People_Badass</path>
			</li>
			<li Class="Rule_File">
			  <keyword>negative</keyword>
			  <path>Words/Nouns/Concepts_Badass</path>
			</li>
			<li Class="Rule_File">
			  <keyword>roman</keyword>
			  <path>Words/Misc/Numbers_SmallRoman</path>
			</li>
			<li Class="Rule_File">
			  <keyword>color</keyword>
			  <path>Words/Nouns/Colors</path>
			</li>
		</rulesRaw>
	  
    </rulePack>
  </RulePackDef>
  
</Defs>