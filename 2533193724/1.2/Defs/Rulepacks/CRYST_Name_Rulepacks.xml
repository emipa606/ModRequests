<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
<!--■■■■■■■■■■■■■■■■■■PAWN NAMES■■■■■■■■■■■■■■■■■■■■■■■-->
  
  
  <RulePackDef>
    <defName>CRYST_NAMER_BROOCH_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>CRYSTBroochFirst->Names/CRYST_BroochFirst</li>
        <li>CRYSTBroochLast->Names/CRYST_NamesLast</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[CRYSTBroochFirst] [CRYSTBroochLast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  
  <RulePackDef>
    <defName>CRYST_NAMER_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>CRYSTnamefirst->Names/CRYST_NamesFirst</li>
        <li>CRYSTnamelast->Names/CRYST_NamesLast</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[CRYSTnamefirst] [CRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>CRYST_NAMER_MALE_DEF</defName>
    <include>
      <li>CRYST_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[CRYSTnamefirst] [CRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>CRYST_NAMER_FEMALE_DEF</defName>
    <include>
      <li>CRYST_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[CRYSTnamefirst] [CRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
</Defs>