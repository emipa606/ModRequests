<?xml version="1.0" encoding="utf-8" ?>
  
<Patch>
  
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>CCP's Stone Tweaks</li>
    </mods>
    
    <match Class="PatchOperationSequence">
      <success>Always</success>
      
      <operations>
        <!--■BLUEGRANITE CHUNK■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "ChunkBlueGranite"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_BLUEGRANITE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
        <!--■GREENMARBLE CHUNK■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "ChunkGreenMarble"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_GREENMARBLE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
        <!--■PINKMARBLE CHUNK■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "ChunkPinkMarble"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_PINKMARBLE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
        <!--■■■■■■■■■■■■■■■■■■ BLOCKS ■■■■■■■■■■■■■■■■■■■■■■■-->
        
        <!--■BLUEGRANITE BLOCKS■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "BlocksBlueGranite"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_BLUEGRANITE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
        <!--■GREENMARBLE BLOCKS■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "BlocksGreenMarble"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_GREENMARBLE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
        <!--■PINKMARBLE BLOCKS■-->
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/ThingDef[defName = "BlocksPinkMarble"]</xpath>
          <value>
            <ingestible>
              <tasteThought>CRYST_THOUGHT_FOOD_CORE_CHUNKS_PINKMARBLE_DEF</tasteThought>
            </ingestible>
          </value>
        </li>
        
   
      </operations>
    </match>
  </Operation>
   
  </Patch>
  