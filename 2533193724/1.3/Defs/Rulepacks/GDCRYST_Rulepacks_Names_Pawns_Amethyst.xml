<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!--■■■■■■■■■■■■■■■■■
    NAMES RULEPACK: Amethyst
    1. Cleavling
    2. Conceiver
    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■ NAMES RULEPACK: Amethyst  ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■
    ■■■■■■■■■■■■■■■■■■1. CLEAVLING ■■■■■■■■■■■■■■■■■■■■■■■-->
  <RulePackDef>
    <defName>GDCRYST_NAMER_AmethystCleavlingSmall</defName>
    <rulePack>
      <rulesFiles>
        <li>GDCRYSTnameshort->Names/GDCRYST_NamesShort</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[GDCRYSTnameshort] [GDCRYSTnameshort]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>GDCRYST_NAMER_AmethystCleavlingSmallMale</defName>
    <include>
      <li>GDCRYST_NAMER_AmethystCleavlingSmall</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[GDCRYSTnameshort] [GDCRYSTnameshort]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>GDCRYST_NAMER_AmethystCleavlingSmallFemale</defName>
