    </rulePack>
  </RulePackDef>
  
  <!--■■■■■■■■■■■■■■■■■■2. CONCEIVER ■■■■■■■■■■■■■■■■■■■■■■■-->
  <RulePackDef>
    <defName>GDCRYST_NAMER_DEF</defName>
    <rulePack>
      <rulesFiles>
        <li>GDCRYSTnamefirst->Names/GDCRYST_NamesFirst</li>
        <li>GDCRYSTnamelast->Names/GDCRYST_NamesLast</li>
      </rulesFiles>
      <rulesStrings>
        <li>r_name->[GDCRYSTnamefirst] [GDCRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>GDCRYST_NAMER_MALE_DEF</defName>
    <include>
      <li>GDCRYST_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[GDCRYSTnamefirst] [GDCRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>GDCRYST_NAMER_FEMALE_DEF</defName>
    <include>
      <li>GDCRYST_NAMER_DEF</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[GDCRYSTnamefirst] [GDCRYSTnamelast]</li>
      </rulesStrings>
    </rulePack>
