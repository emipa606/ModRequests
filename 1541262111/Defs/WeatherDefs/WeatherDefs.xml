<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <WeatherDef>
    <defName>NoneSpace</defName>
    <label>none</label>
    <description>It's a normal "day" in space. That means a whole lot of nothing.</description>
    <repeatable>true</repeatable>
    <favorability>Good</favorability>
    <perceivePriority>0</perceivePriority>
    <temperatureRange>
      <min>-9999</min>
      <max>-999</max>
    </temperatureRange>
    <durationRange>
      <min>200000</min>
      <max>400000</max>
    </durationRange>
    <!-- Colors are Clear -->
    <skyColorsDay>
      <sky>(1,1,1)</sky>
      <shadow>(0,0,0)</shadow>
      <overlay>(1,1,1)</overlay>
      <saturation>1.25</saturation>
    </skyColorsDay>
    <skyColorsDusk>
      <sky>(1,1,1)</sky>
      <shadow>(0,0,0)</shadow>
      <overlay>(1,1,1)</overlay>
      <saturation>1.25</saturation>
    </skyColorsDusk>
    <skyColorsNightEdge>
      <sky>(1,1,1)</sky>
      <shadow>(0,0,0)</shadow>
      <overlay>(1,1,1)</overlay>
      <saturation>1.25</saturation>
    </skyColorsNightEdge>
    <skyColorsNightMid>
      <sky>(1,1,1)</sky>
      <shadow>(0,0,0)</shadow>
      <overlay>(1,1,1)</overlay>
      <saturation>1.25</saturation>
    </skyColorsNightMid>
  </WeatherDef>

</Defs>
