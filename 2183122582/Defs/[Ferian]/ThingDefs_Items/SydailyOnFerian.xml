<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="AF_GoodsBase">
    <defName>SydailyOnFerian</defName>
    <label>菲狐的斯黛莉水晶</label>
    <description>这是菲狐族头部的斯黛莉水晶，等待着一次全新的冒险。
	\n已经失去了原来的颜色，能隐约听见它在说话，好像在骂我们呢。</description>
	
        <graphicData>
            <texPath>Goods/SydailyOnFerian</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1.82</drawSize>
            <shaderType>Transparent</shaderType>
        </graphicData>
		
    <statBases>
      <MaxHitPoints>20</MaxHitPoints>
      <Flammability>0</Flammability>
      <MarketValue>8000</MarketValue>
      <Mass>9999999</Mass>
      <WorkToMake>0</WorkToMake>
      <Beauty>0</Beauty>
    </statBases>
	
            <useHitPoints>true</useHitPoints><!-- from ferian -->  
            <destroyable>false</destroyable>
			
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>2.55</glowRadius>
        <glowColor>(255,255,255,0)</glowColor>
      </li>
    </comps>
	
    <tradeability>Sellable</tradeability>
    <thingSetMakerTags>
      <li>Ferian_tag</li>
    </thingSetMakerTags>
  </ThingDef>
  
</Defs>
