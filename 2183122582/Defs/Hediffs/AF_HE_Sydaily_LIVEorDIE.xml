    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.53,0.71,1.00)</defaultLabelColor>
    <initialSeverity>0.001</initialSeverity>
    <stages>
      <li>
				<partEfficiencyOffset>0.4</partEfficiencyOffset><!-- congratulations -->
      </li>
    </stages>
