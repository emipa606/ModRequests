    <selectable>True</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>false</useHitPoints>
    <techLevel>Archotech</techLevel>
	
    <graphicData>
      <onGroundRandomRotateAngle>0</onGroundRandomRotateAngle>
      <shaderType>Transparent</shaderType>
