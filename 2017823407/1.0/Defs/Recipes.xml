<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <RecipeDef ParentName="SurgeryFlesh">
        <defName>InstallAstartePart</defName>
        <label>Continue Astarte GeneSeed Transformation</label>
        <description>GeneSeed Organ implantation.</description>
        <workerClass>GeneSeed.GeneSeedRecipe_InstallNaturalBodyPart</workerClass>
        <jobString>implantation of Gene Seed organ.</jobString>
        <workAmount>2500</workAmount>
        <skillRequirements>
            <Medicine>5</Medicine>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>GeneSeed</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <appliedOnFixedBodyParts>
            <li>SecondaryHeart</li>
            <li>Ossmodula</li>
            <li>Biscopea</li>
            <li>Haemastamen</li>
            <li>LarramansOrgan</li>

            <li>CatalepseanNode</li>
            <li>Preomnor</li>
            <li>Omophagea</li>
            <li>MultiLung</li>
            <li>Occulobe</li>

            <li>LymansEar</li>
            <li>SusanMembrane</li>
            <li>Melanochrome</li>
            <li>OoliticKidney</li>
            <li>Neuroglottis</li>

            <li>Mucranoid</li>
            <li>BetchersGland</li>
            <li>ProgenoidGlands</li>
            <li>TheBlackCarapace</li>
        </appliedOnFixedBodyParts>
    </RecipeDef>


    <RecipeDef ParentName="SurgeryFlesh">
        <defName>CheatAstartePart</defName>
        <label>Finish GeneSeed Transformation</label>
        <description>GeneSeed quickened implantation.</description>
        <workerClass>GeneSeed.GeneSeedRecipe_MakeAwesome</workerClass>
        <jobString>implantation of all Gene Seed.</jobString>
        <workAmount>2500</workAmount>
        <skillRequirements>
            <Medicine>5</Medicine>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>GeneSeed</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <appliedOnFixedBodyParts>
            <li>SecondaryHeart</li>
            <li>Ossmodula</li>
            <li>Biscopea</li>
            <li>Haemastamen</li>
            <li>LarramansOrgan</li>

            <li>CatalepseanNode</li>
            <li>Preomnor</li>
            <li>Omophagea</li>
            <li>MultiLung</li>
            <li>Occulobe</li>

            <li>LymansEar</li>
            <li>SusanMembrane</li>
            <li>Melanochrome</li>
            <li>OoliticKidney</li>
            <li>Neuroglottis</li>

            <li>Mucranoid</li>
            <li>BetchersGland</li>
            <li>ProgenoidGlands</li>
            <li>TheBlackCarapace</li>
        </appliedOnFixedBodyParts>
    </RecipeDef>
</Defs>