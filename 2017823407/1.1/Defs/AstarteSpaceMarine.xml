<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef Name="AstarteAdaptusCore" ParentName="HumanRace">
        <defName>AstarteAdaptusCore</defName>
        <label>AstarteAdaptusCore</label>
        <description>A human, modified by gene engineering and designed evolutionary pressures.</description>
        
        <comps>
            <li Class="CompProperties_Milkable">
                <milkDef>GeneSeed</milkDef>
                <milkIntervalDays>2</milkIntervalDays>
                <milkAmount>1</milkAmount>
            </li>
        </comps>
        <race>
            <thinkTreeMain>Humanlike</thinkTreeMain>
            <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
            <intelligence>Humanlike</intelligence>
            <makesFootprints>true</makesFootprints>
            <leatherDef>Leather_Human</leatherDef>
            <nameCategory>HumanStandard</nameCategory>
            <body>AstarteSpaceMarine</body>
            <foodType>OmnivoreRoughAnimal</foodType>
            <gestationPeriodDays>35</gestationPeriodDays>
            <meatMarketValue>0.8</meatMarketValue>
            <manhunterOnDamageChance>0.20</manhunterOnDamageChance>
            <manhunterOnTameFailChance>0.02</manhunterOnTameFailChance>
            <litterSizeCurve Inherit="false">
                <points>
                    <li>(0.5, 0)</li>
                    <li>(1, 1)</li>
                    <li>(1.01, 0.02)</li>
                    <li>(3.5, 0)</li>
                </points>
            </litterSizeCurve>
            <lifeStageAges Inherit="false">
                <li>
                    <def>HumanlikeBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>HumanlikeToddler</def>
                    <minAge>1.2</minAge>
                </li>
                <li>
                    <def>HumanlikeChild</def>
                    <minAge>4</minAge>
                </li>
                <li>
                    <def>HumanlikeTeenager</def>
                    <minAge>8</minAge>
                </li>
                <li>
                    <def>HumanlikeAdult</def>
                    <minAge>1400</minAge>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
            <specialShadowData>
                <volume>(0.3, 0.8, 0.4)</volume>
                <offset>(0,0,-0.3)</offset>
            </specialShadowData>
            <ageGenerationCurve Inherit="false">
                <points>
                    <li>(8,0)</li>
                    <li>(10,100)</li>
                    <li>(40,100)</li>
                    <li>(60,30)</li>
                    <li>(70,18)</li>
                    <li>(80,10)</li>
                    <li>(900,3)</li>
                    <li>(5000,0)</li>
                </points>
            </ageGenerationCurve>
            <hediffGiverSets Inherit="false">
                <!--li>OrganicStandard</li-->
                <!--li>Human</li-->
            </hediffGiverSets>
        </race>
        <recipes Inherit="false">
            <li>CheatAstartePart</li>
            <li>InstallAstartePart</li>
            <li>ExciseCarcinoma</li>
            <li>AdministerMechSerumHealer</li>
            <li>RemoveBodyPart</li>
            <li>Euthanize</li>
            <li>Anesthetize</li>
        </recipes>
    </ThingDef>
    
    <ThingDef Name="AstarteSpaceMarine" ParentName="AstarteAdaptusCore">
        <defName>AstarteSpaceMarine</defName>
        <label>Astarte Space Marine</label>
        <description>A human, modified by gene engineering and designed evolutionary pressures.</description>
        <statBases>
            <MarketValue>17500</MarketValue>
            <MoveSpeed>6.2</MoveSpeed>
            <Flammability>0.2</Flammability>
            <ComfyTemperatureMin>-16</ComfyTemperatureMin>
            <ComfyTemperatureMax>36</ComfyTemperatureMax>
            <LeatherAmount>100</LeatherAmount>
        </statBases>
        <comps>
            <li Class="CompProperties_Milkable">
                <milkDef>GeneSeed</milkDef>
                <milkIntervalDays>2</milkIntervalDays>
                <milkAmount>1</milkAmount>
            </li>
        </comps>
        <tools>
            <li>
                <label>left fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>15.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>right fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>15.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>18</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>teeth</label>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>19.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <chanceFactor>0.07</chanceFactor>
            </li>
            <li>
                <label>head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>14</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
            <li>
                <label>caustic spit</label>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>80</power>
                <cooldownTime>3</cooldownTime>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.02</chanceFactor>
            </li>
        </tools>
        <race>
            <lifeExpectancy>8000</lifeExpectancy>
            <baseBodySize>3</baseBodySize>
            <baseHealthScale>10</baseHealthScale>
        </race>
    </ThingDef>

