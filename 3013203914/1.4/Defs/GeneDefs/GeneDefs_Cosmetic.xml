<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
	<!-- |||  Leo Ears ||| -->
	<GeneDef ParentName="GeneEarsBase">
	<defName>JAR_LeoEars</defName>
		<label>leo ears</label>
		<description>The carriers of this gene grow conal forward facing ears on the top of their head. Ideal for hunters who stalk their prey.</description>
		<biostatCpx>0</biostatCpx>
		<displayCategory>Cosmetic</displayCategory>
		<displayOrderInCategory>200</displayOrderInCategory>
		<iconPath>UI/GeneIcons/Gene_LeoEars</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/Leo/Ears/Leo_Ears</graphicPath>
			<colorType>Skin</colorType>
			<useSkinShader>false</useSkinShader>
		</graphicData>
		<symbolPack>
		<prefixSymbols>
			<li><symbol>cat</symbol></li>
			<li><symbol>feline</symbol></li>
			<li><symbol>leo</symbol></li>
			<li><symbol>lion</symbol></li>
			<li><symbol>kitten</symbol></li>
		</prefixSymbols>
		<suffixSymbols>
			<li><symbol>ear</symbol></li>
			<li><symbol>listener</symbol></li>
			<li><symbol>receiver</symbol></li>
		</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- ||| Tigris Ears ||| -->
	<GeneDef ParentName="GeneEarsBase">
	<defName>JAR_TigrisEars</defName>
		<label>tigris ears</label>
		<description>The carriers of this gene grow conal forward facing ears on the top of their head. Ideal for hunters who stalk their prey.</description>
		<biostatCpx>0</biostatCpx>
		<displayCategory>Cosmetic</displayCategory>
		<displayOrderInCategory>200</displayOrderInCategory>
		<iconPath>UI/GeneIcons/Gene_TigrisEars</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/Tigris/Ears/Tigris_Ears</graphicPath>
			<colorType>Skin</colorType>
			<useSkinShader>false</useSkinShader>
		</graphicData>
		<symbolPack>
		<prefixSymbols>
			<li><symbol>cat</symbol></li>
			<li><symbol>feline</symbol></li>
			<li><symbol>tigris</symbol></li>
			<li><symbol>tiger</symbol></li>
			<li><symbol>kitten</symbol></li>
		</prefixSymbols>
		<suffixSymbols>
			<li><symbol>ear</symbol></li>
			<li><symbol>listener</symbol></li>
			<li><symbol>receiver</symbol></li>
		</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<!-- ||| Leo Tails ||| -->
	<GeneDef ParentName="GeneTailBase">
		<defName>JAR_LeoTail</defName>
		<label>leo tail</label>
		<description>the carriers of this gene sprout a long, semi-prehensile addition to their spine, covered in uniform thin fur. Its original purpose of balance is somewhat redundant thanks to the inner ear.</description>
		<iconPath>UI/GeneIcons/Gene_LeoTail</iconPath>
		<iconColor>(0.50, 0.50, 0.50)</iconColor>
		<displayOrderInCategory>1000</displayOrderInCategory>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/Leo/Tails/LeoSkin_Tail</graphicPath>
			<colorType>Skin</colorType>
			<useSkinShader>false</useSkinShader>
			<drawOffsetNorth>(0, 0.45, -0.25)</drawOffsetNorth>
			<drawOffsetSouth>(-0.05, 0, -0.15)</drawOffsetSouth>
			<drawOffsetEast>(-0.35, -0.1, -0.25)</drawOffsetEast>
		</graphicData>
		<symbolPack>
		<prefixSymbols>
			<li><symbol>cat</symbol></li>
			<li><symbol>feline</symbol></li>
			<li><symbol>leo</symbol></li>
			<li><symbol>lion</symbol></li>
			<li><symbol>kitten</symbol></li>
		</prefixSymbols>
		<suffixSymbols>
			<li><symbol>tail</symbol></li>
			<li><symbol>trail</symbol></li>
		</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- ||| Tigris Tails ||| -->
	<GeneDef ParentName="GeneTailBase">
		<defName>JAR_TigrisTail</defName>
		<label>tigris tail</label>
		<description>the carriers of this gene sprout a long, semi-prehensile addition to their spine, covered in striped thick fur. Its original purpose of balance is somewhat redundant thanks to the inner ear.</description>
		<iconPath>UI/GeneIcons/Gene_TigrisTail</iconPath>
		<iconColor>(0.50, 0.50, 0.50)</iconColor>
		<displayOrderInCategory>1000</displayOrderInCategory>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/Tigris/Tails/TigrisSkin_Tail</graphicPath>
			<colorType>Skin</colorType>
			<useSkinShader>false</useSkinShader>
			<drawOffsetNorth>(0, 0.45, -0.25)</drawOffsetNorth>
			<drawOffsetSouth>(-0.05, 0, -0.15)</drawOffsetSouth>
			<drawOffsetEast>(-0.35, -0.1, -0.25)</drawOffsetEast>
		</graphicData>
		<symbolPack>
		<prefixSymbols>
			<li><symbol>cat</symbol></li>
			<li><symbol>feline</symbol></li>
			<li><symbol>Tigris</symbol></li>
			<li><symbol>Tiger</symbol></li>
			<li><symbol>kitten</symbol></li>
		</prefixSymbols>
		<suffixSymbols>
			<li><symbol>tail</symbol></li>
			<li><symbol>trail</symbol></li>
		</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	
	<!-- |||  Tigris Tuft ||| -->
	<GeneDef ParentName="GeneHairStyleBase">
    <defName>JAR_Tigris_Tuft</defName>
    <label>Tigris Tuft</label>
    <labelShortAdj>Tuft</labelShortAdj>
    <description>Carriers of this gene grow thick furry facial sideburns as hair, even women.</description>
    <iconPath>UI/GeneIcons/Gene_TigrisTuft</iconPath>
    <displayOrderInCategory>60</displayOrderInCategory>
    <womenCanHaveBeards>true</womenCanHaveBeards>
	<displayCategory>Cosmetic</displayCategory>
	<biostatCpx>0</biostatCpx>
    <hairTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>JAR_TigrisBeard</li>
      </tags>
    </hairTagFilter>
  </GeneDef>
  
  <!-- |||  Leo Tuft ||| -->
  <GeneDef ParentName="GeneHairStyleBase">
    <defName>JAR_Leo_Tuft</defName>
    <label>Leo Mane</label>
    <labelShortAdj>Lion mane</labelShortAdj>
    <description>Male carriers of this gene experience rapid hair growth and head, growing a mane that covers their head.</description>
    <iconPath>UI/GeneIcons/Gene_LeoMane</iconPath>
    <displayOrderInCategory>60</displayOrderInCategory>
	<displayCategory>Cosmetic</displayCategory>
	<biostatCpx>0</biostatCpx>
    <hairTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>JAR_LeoBeard</li>
      </tags>
    </hairTagFilter>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>beard</symbol></li>
        <li><symbol>bush</symbol></li>
        <li><symbol>lumber</symbol></li>
        <li><symbol>bristle</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>beard</symbol></li>
        <li><symbol>bush</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>
</Defs>