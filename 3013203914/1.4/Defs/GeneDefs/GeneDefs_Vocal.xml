<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- ||| Leo Voice ||| -->
	<GeneDef ParentName="GeneVoiceBase">
		<defName>JAR_LeoVoice</defName>
		<label>leo voice</label>
		<description>Carriers have a feline voice like that of a lion.</description>
		<iconPath>UI/GeneIcons/Gene_LeoVoice</iconPath>
		<displayOrderInCategory>125</displayOrderInCategory>
		<soundCall>JAR_Leo_Call</soundCall>
		<soundDeath>JAR_Leo_Death</soundDeath>
		<soundWounded>JAR_Leo_Wounded</soundWounded>
		<symbolPack>
			<wholeNameSymbols>
				<li><symbol>roarer</symbol></li>
				<li><symbol>meower</symbol></li>
				<li><symbol>mewuler</symbol></li>
			</wholeNameSymbols>
			<prefixSymbols>
				<li><symbol>feline</symbol></li>
				<li><symbol>leo</symbol></li>
				<li><symbol>lion</symbol></li>
				<li><symbol>fe</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>scream</symbol></li>
				<li><symbol>roar</symbol></li>
				<li><symbol>meow</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- ||| Tigris Voice ||| -->
	<GeneDef ParentName="GeneVoiceBase">
		<defName>JAR_TigrisVoice</defName>
		<label>tigris voice</label>
		<description>Carriers have a feline voice like that of a Tiger.</description>
		<iconPath>UI/GeneIcons/Gene_LeoVoice</iconPath>
		<displayOrderInCategory>125</displayOrderInCategory>
		<soundCall>JAR_Tigris_Call</soundCall>
		<soundDeath>JAR_Tigris_Death</soundDeath>
		<soundWounded>JAR_Tigris_Wounded</soundWounded>
		<symbolPack>
			<wholeNameSymbols>
				<li><symbol>roarer</symbol></li>
				<li><symbol>meower</symbol></li>
				<li><symbol>mewuler</symbol></li>
			</wholeNameSymbols>
			<prefixSymbols>
				<li><symbol>feline</symbol></li>
				<li><symbol>Tigris</symbol></li>
				<li><symbol>Tiger</symbol></li>
				<li><symbol>fe</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>scream</symbol></li>
				<li><symbol>roar</symbol></li>
				<li><symbol>meow</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

</Defs>