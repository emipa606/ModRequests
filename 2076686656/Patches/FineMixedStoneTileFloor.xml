<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationAdd">
		  <xpath>Defs</xpath>
		  <value>
			  <TerrainDef ParentName="FineTileStoneBase">
				<defName>FineTileMixedStone</defName>
				<label>fine mixed stone tile</label>
				<description>Exquisite mixed stone tiles, made with no compromises, for expressing economic status. Slow to construct.</description>

				<researchPrerequisites>
				  <li>Stonecutting</li>
				</researchPrerequisites>

				<costList>
				  <MixedStoneBlocks>20</MixedStoneBlocks>
				</costList>
				
				<tags>
				  <li>FineFloor</li>
				</tags>

				<texturePath>Terrain/TileStoneFine</texturePath>
			  </TerrainDef>
		  </value>
		</match>
	</Operation>
</Patch>
