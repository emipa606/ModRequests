<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- Slabs -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="SchoolDesk"]/graphicData/texPath</xpath>
					<value>
						<texPath>Things/Building/Childcare/SchoolDesk/SchoolDesk</texPath>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="SchoolDesk"]/graphicData</xpath>
					<value>
						<shaderType>CutoutComplex</shaderType>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
