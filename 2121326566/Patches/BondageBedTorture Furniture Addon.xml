<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- move to hospital tab -->
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="sd_medicaddons.PatchOperationFindMod">
				<modName>BondageBedTorture Furniture Addon</modName>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "SR_Building_OperatingBondageTable"]</xpath>
				<value>
					<designationCategory>HE_Hospital</designationCategory>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>