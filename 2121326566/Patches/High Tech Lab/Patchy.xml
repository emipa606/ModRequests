<?xml version="1.0"?>

<Patch>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="LabStation"]/graphicData/drawSize</xpath>
			<value>
			<drawSize>4,3.1</drawSize>
			</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="LabStation"]/graphicData</xpath>
			<value>
			<drawOffset>(0.0, 0.0, 0.3)</drawOffset>
			</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="LabStation"]</xpath>
			<value>
				<building>
					<paintable>true</paintable>
				</building>
			</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="LabTerminal"]/graphicData/drawSize</xpath>
			<value>
			<drawSize>2,2.25</drawSize>
			</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="LabTerminal"]/graphicData</xpath>
			<value>
			<drawOffset>(0.0, 0.0, 0.1)</drawOffset>
			</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="LabTerminal"]</xpath>
			<value>
				<building>
					<paintable>true</paintable>
				</building>
			</value>
	</Operation>
	

	

	
</Patch>