<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<ThingDef ParentName="BasedHygieneMom" Name="DubsSwimmingPoolBase">
		<designationCategory>Hygiene</designationCategory>
		<description>Hatchery for the artificial breeding, hatching, and rearing of freshwater fish.</description>
		<thingClass>DubsBadHygiene.Building_Pool</thingClass>
		<defName>JWL_FishHatchery</defName>
		<label>fish hatchery</label>
		<stuffCategories>
			<li>Metallic</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>600</costStuffCount>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<uiIconPath>UI/Hatchery1</uiIconPath>
		<graphicData>
			<drawSize>(11,7)</drawSize>
			<texPath>Things/Buildings/Hatchery1</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>Cutout</shaderType>
		</graphicData>
		<statBases>
			<MaxHitPoints>1500</MaxHitPoints>
			<WorkToBuild>20000</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>2</Beauty>
			<Mass>100</Mass>
		</statBases>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
		<comps>
			<li Class="DubsBadHygiene.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<fillPercent>0.15</fillPercent>
		<size>(9, 5)</size>
		<passability>PassThroughOnly</passability>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<pathCost>0</pathCost>
		<rotatable>true</rotatable>
		<researchPrerequisites>
			<li MayRequire="dbhlitemode">SwimmingPools</li>
		</researchPrerequisites>
		<tickerType>Normal</tickerType>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
	</ThingDef>

</Defs>
