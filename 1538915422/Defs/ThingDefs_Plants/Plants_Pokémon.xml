  <ThingDef ParentName="PlantBase">
    <defName>Plant_Chesto</defName>
    <label>Chesto plant</label>
    <description>This Berry's thick skin and fruit are very tough and dry tasting. However, every bit of it can be eaten.</description>
    <graphicData>
      <texPath>Things/Plants/ChestoPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<statBases>
	  <MaxHitPoints>110</MaxHitPoints>
	  <Nutrition>0.2</Nutrition>
    </statBases>
