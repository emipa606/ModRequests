  <DesignatorDropdownGroupDef>
  <defName>PokePlush</defName>
  </DesignatorDropdownGroupDef>

	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>LaprasPlush</defName>
		<label>Lapras Plush</label>
		<description>A plushy version of a pokémon. How cute!</description>
		<thingClass>Building_Art</thingClass>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>30</pathCost>
		<useHitPoints>true</useHitPoints>
		<minifiedDef>MinifiedThing</minifiedDef>
		<rotatable>false</rotatable>
		<fillPercent>0.20</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<graphicData>
			<texPath>Things/Buildings/PlushLapras/PlushLapras</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>2.2</drawSize>
		</graphicData>
		<uiIconScale>0.7</uiIconScale>
		<costList>
			<Cloth>20</Cloth>
		</costList>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<WorkToBuild>400</WorkToBuild>
			<Mass>2</Mass>
			<Beauty>10</Beauty>
		</statBases>
		<comps>
		</comps>
		<designatorDropdown>PokePlush</designatorDropdown>
	</ThingDef>

	
