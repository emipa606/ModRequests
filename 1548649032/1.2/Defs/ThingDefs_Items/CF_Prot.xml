    <techLevel>Industrial</techLevel>
    <costList>
      <Steel>15</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <statBases>
      <WorkToMake>5000</WorkToMake>
      <Mass>4</Mass>
    </statBases>
	<recipeMaker>
      <researchPrerequisite>SimpleProtsAnimalsCF</researchPrerequisite>
    </recipeMaker>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
  </ThingDef>

