  <ThingDef ParentName="ResourceBase">
    <defName>MiniComponentSpacer</defName>
    <label>Advanced mini component</label>
    <description>Mini-Advanced computing and energy-directing machinery for animal prosthetic.</description>
    <graphicData>
      <texPath>Things/Item/Resource/ComponentSpacer</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>0.75</drawSize>
	  <color>(0.9, 0.9, 0.5)</color>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>40</MaxHitPoints>
      <MarketValue>65</MarketValue>
      <Mass>0.2</Mass>
      <Flammability>0.6</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags><li>RewardStandardHighFreq</li></thingSetMakerTags>
  </ThingDef>
  
  <RecipeDef>
    <defName>Make_MiniComponentSpacer</defName>
    <label>make advanced Mini component x3</label>
    <description>Make an advanced Mini component.</description>
    <jobString>Making advanced Mini component.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>8000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	    <recipeUsers>
      <li>FabricationBench</li>
      <li>AnimalLab</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>0.3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ComponentIndustrial</li>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Gold</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <MiniComponentSpacer>3</MiniComponentSpacer>
    </products>
    <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>



</Defs>
