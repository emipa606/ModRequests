<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="OrganicProductBase">
		<defName>MeatRotten</defName>
		<label>rotten meat</label>
		<description>Rotten flesh obtained from a creature's rotting corpse. It has degraded so badly that it is no longer possible to tell what creature it came from.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MeatRotten</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>0.1</MarketValue>
			<Mass>0.03</Mass>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<DeteriorationRate>8</DeteriorationRate>
			<Nutrition>0.02</Nutrition>
			<FoodPoisonChanceFixedHuman>0.90</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
		</comps>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<foodType>Meat</foodType>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<tasteThought>AteMeatRotten</tasteThought>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
		<thingCategories>
			<li>MeatRawRotten</li>
		</thingCategories>
	</ThingDef>

</Defs>