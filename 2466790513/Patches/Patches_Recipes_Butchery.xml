<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Prevents regular butchering bills from taking desiccated and rotten corpses -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="ButcherCorpseFlesh"]/fixedIngredientFilter/specialFiltersToDisallow</xpath>
		<value>
			<li>AllowDesiccated</li>
		</value>
	</Operation>

</Patch>