<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
        <mods>
            	<li>Vanilla Ideology Expanded - Memes and Structures</li>
        </mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/MemeDef[defName="VME_Structure_Shintaoism"]</xpath>
    			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/structureMemeWeights/VME_Structure_Shintaoism</xpath>
    			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/FactionDef[@Name="TribeBase"]/structureMemeWeights/VME_Structure_Shintaoism</xpath>
    			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/StyleCategoryDef[defName="VME_Shintaoism"]/label</xpath>
					<value>
						<label>oriental</label>
					</value>
    			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/StyleCategoryDef[defName="VME_Shintaoism"]/iconPath</xpath>
					<value>
						<iconPath>UI/StyleCategories/Oriental</iconPath>
					</value>
    			</li>

	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~ Oriental carpets and floors ~~~~~~~~~~~~~~~~~~~~~~~~~~-->

			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[@Name="VME_ShintaoFineStoneTiles"]/description</xpath>
					<value>
						<description>Fine stone tiles in an oriental style for expressing economic status. Slow to construct.</description>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoFineTileSandstone"]/label</xpath>
					<value>
						<label>oriental fine sandstone tile</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoFineTileGranite"]/label</xpath>
					<value>
						<label>oriental fine granite tile</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoFineTileLimestone"]/label</xpath>
					<value>
						<label>oriental fine limestone tile</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoFineTileSlate"]/label</xpath>
					<value>
						<label>oriental fine slate tile</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoFineTileMarble"]/label</xpath>
					<value>
						<label>oriental fine marble tile</label>
					</value>
			</li>

			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetRed"]/label</xpath>
					<value>
						<label>oriental red carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetGreen"]/label</xpath>
					<value>
						<label>oriental green carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetBlue"]/label</xpath>
					<value>
						<label>oriental blue carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetCream"]/label</xpath>
					<value>
						<label>oriental cream carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetDark"]/label</xpath>
					<value>
						<label>oriental dark carpet</label>
					</value>
			</li>

			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetFineBurgundy"]/label</xpath>
					<value>
						<label>fine oriental burgundy carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetFineIndigo"]/label</xpath>
					<value>
						<label>fine oriental indigo carpet</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/TerrainDef[defName="VME_ShintaoCarpetFineWhite"]/label</xpath>
					<value>
						<label>fine oriental white carpet</label>
					</value>
			</li>

		
			<li Class="PatchOperationReplace">
				<xpath>/Defs/DesignatorDropdownGroupDef[defName="VME_Floor_ShintaoFineStoneTiles"]/label</xpath>
					<value>
						<label>oriental fine stone tiles</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/DesignatorDropdownGroupDef[defName="VME_Floor_ShintaoCarpet"]/label</xpath>
					<value>
						<label>oriental carpets</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/DesignatorDropdownGroupDef[defName="VME_Floor_ShintaoCarpetFine"]/label</xpath>
					<value>
						<label>oriental fine carpets</label>
					</value>
			</li>

		
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="VME_TaoistSlab_Medium"]/label</xpath>
					<value>
						<label>oriental slab (medium)</label>
					</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="VME_TaoistSlab_Broad"]/label</xpath>
					<value>
						<label>oriental slab (broad)</label>
					</value>
			</li>
		</operations>
	</match>
</Operation>
<Operation Class="PatchOperationFindMod">
        <mods>
            	<li>More Religious Origins</li>
        </mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/MemeDef[defName="Structure_OriginDaoist"]/iconPath</xpath>
					<value>
						<iconPath>UI/StyleCategories/VME_ThingStyle_Taoist</iconPath>
					</value>
    			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/StyleCategoryDef[defName="Daoist"]/iconPath</xpath>
					<value>
						<iconPath>UI/StyleCategories/VME_ThingStyle_Taoist</iconPath>
					</value>
    			</li>
		</operations>
	</match>
</Operation>

</Patch>