<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>AutumnMushroom</defName>
    <label>Autumn Mushroom</label>
    <description>Mushrooms gathered by moving mysterious mushrooms. A little toxic color is mixed, but it is surprisingly delicious.</description>
    <graphicData>
      <texPath>Things/Item/AutumnMushroom</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>2.2</MarketValue>
    </statBases>
    <ingestible>
      <tasteThought>AutumnHarvest</tasteThought>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>AutumnFruit</defName>
    <label>Autumn Fruit</label>
    <description>Various fragrant fruits collected by bats. All are sweet and delicious.</description>
    <graphicData>
      <texPath>Things/Item/AutumnFruit</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>2.4</MarketValue>
			<Mass>0.027</Mass>
		</statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>14</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tasteThought>AutumnHarvest</tasteThought>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>


  
</Defs>
