<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <ThingDef>
    <defName>RimGingerbread</defName>
    <label>Rim-Gingerbread</label>
    <description>A bread with Rim-Ginger. </description>
    <graphicData>
      <texPath>Things/Item/Resource/RimGingerbread</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.77</drawSize>
    </graphicData>
     <rotatable>false</rotatable>
     <stackLimit>75</stackLimit>
     <soundInteract>Standard_Drop</soundInteract>
     <soundDrop>Standard_Drop</soundDrop>
     <thingClass>ThingWithComps</thingClass>
     <category>Item</category>
     <resourceReadoutPriority>Middle</resourceReadoutPriority>
     <useHitPoints>true</useHitPoints>
     <selectable>true</selectable>
     <altitudeLayer>Item</altitudeLayer>
     <drawGUIOverlay>true</drawGUIOverlay>
     <pathCost>15</pathCost>
     <socialPropernessMatters>true</socialPropernessMatters>
     <alwaysHaulable>true</alwaysHaulable>
     <comps>
       <li Class="CompProperties_Forbiddable"/>
     </comps>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>4.0</MarketValue>
      <Mass>0.075</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>8</DeteriorationRate>
      <Nutrition>0.08</Nutrition>
    </statBases>
    <thingCategories>
      <li>Foods</li>
    </thingCategories>
    <ingestible>
      <preferability>DesperateOnly</preferability>
      <joyKind>IngestingGingerbread</joyKind>
      <foodType>Processed</foodType>
      <joy>0.12</joy>
      <maxNumToIngestAtOnce>4</maxNumToIngestAtOnce>
      <ingestSound>Meal_Eat</ingestSound>
      <chairSearchRadius>25</chairSearchRadius>
    </ingestible>
  </ThingDef>

</Defs>
