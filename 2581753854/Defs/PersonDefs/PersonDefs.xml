	<Darknet.Defs.PersonDef Abstract="True" Name="Darknet_PersonDef_CybercrimeBase">
		<personClass>Darknet.Defs.Person_Cybercrime</personClass>
		<duration>420000~900000</duration>
		<factionDef>Darknet_FactionDef_Cybercrime</factionDef>
