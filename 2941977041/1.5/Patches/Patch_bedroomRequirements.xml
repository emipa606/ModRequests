            <li>Village_MediumHouse_Masked</li>
            <li>Village_SmallHouse_Masked</li>
            <li>Village_SmallHouse_PastelBlue</li>
            <li>Village_MediumHouse_PastelBlue</li>
            <li>Village_LargeHouse_PastelBlue</li>
            <li>Village_SmallHouse_PastelPink</li>
            <li>Village_MediumHouse_PastelPink</li>
            <li>Village_LargeHouse_PastelPink</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>


</Patch>