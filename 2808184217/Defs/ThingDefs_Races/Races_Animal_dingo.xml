<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ====================================================== -->

  <ThingDef ParentName="AnimalThingBase"  ADogSaidBody="LegsTail">
    <defName>Dingo</defName>
    <label>dingo</label>
    <description>The dingo is a medium-sized canine that possesses a lean, hardy body adapted for speed, agility, and stamina. Dingoes were never totally domesticated and conserved some wolf behaviour from their ancestors. They are well adapted for arid conditions, getting some of their water from their prey. Might eat your babies.</description>
    <statBases>
      <MoveSpeed>5.0</MoveSpeed>
      <MarketValue>250</MarketValue>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
	   <ComfyTemperatureMax>60</ComfyTemperatureMax>
    </statBases>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>9.7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>9.7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>14.2</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.7</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>QuadrupedAnimalWithPawsAndTail</body>
	  <predator>true</predator>
	  <manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
	  <manhunterOnDamageChance>0.05</manhunterOnDamageChance>
      <petness>1</petness>
      <baseBodySize>0.75</baseBodySize>
      <baseHungerRate>0.5</baseHungerRate>
      <baseHealthScale>1.05</baseHealthScale>
      <foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Dog</leatherDef>
      <nameOnTameChance>1</nameOnTameChance>
      <trainability>Advanced</trainability>
      <wildness>0.4</wildness>
      <nuzzleMtbHours>24</nuzzleMtbHours>
	  <wildBiomes>
				<AridShrubland>0.2</AridShrubland>
				<Desert>0.15</Desert>
				<ExtremeDesert>0.10</ExtremeDesert>
			</wildBiomes>
      <gestationPeriodDays>10</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(2, 1.7)</li>
          <li>(2.5, 0.7)</li>
          <li>(3, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeExpectancy>12</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_Dog_Injured</soundWounded>
          <soundDeath>Pawn_Dog_Death</soundDeath>
          <soundCall>Pawn_Dog_Call</soundCall>
          <soundAngry>Pawn_Dog_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalCommon</li>
      <li>AnimalPet</li>
    </tradeTags>
  </ThingDef>
  
  <PawnKindDef ParentName="AnimalKindBase">
    <defName>Dingo</defName>
    <label>dingo</label>
    <labelPlural>dingoes</labelPlural>
    <race>Dingo</race>
    <combatPower>75</combatPower>
    <ecoSystemWeight>0.7</ecoSystemWeight>
    <lifeStages>
      <li>
        <label>dingo puppy</label>
        <labelPlural>dingo puppies</labelPlural>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/dingo/dingo</texPath>
          <drawSize>0.7</drawSize>
          <shaderType>CutoutComplex</shaderType>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Labrador/Dessicated_Labrador</texPath>
          <drawSize>0.7</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/dingo/dingo</texPath>
          <drawSize>1</drawSize>
          <shaderType>CutoutComplex</shaderType>
          <shadowData>
            <volume>(0.25, 0.3, 0.25)</volume>
            <offset>(0,0,-0.15)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Labrador/Dessicated_Labrador</texPath>
          <drawSize>1</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/dingo/dingo</texPath>
          <drawSize>1.5</drawSize>
          <shaderType>CutoutComplex</shaderType>
          <shadowData>
            <volume>(0.3, 0.4, 0.3)</volume>
            <offset>(0,0,-0.15)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Labrador/Dessicated_Labrador</texPath>
          <drawSize>1.5</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>
  
  
</Defs>
