<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef ParentName="FactionBase">
    <defName>Hippies</defName>
    <label>Hippies</label>
    <description>Un groupe de hippies.</description>
    <pawnSingular>hippie</pawnSingular>
    <pawnsPlural>hippies</pawnsPlural>
    <listOrderPriority>10</listOrderPriority>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>false</canSiege>
    <canStageAttacks>false</canStageAttacks>
    <leaderTitle>gourou</leaderTitle>
    <factionIconPath>hippie</factionIconPath>
    <colorSpectrum>
      <li>(255, 255, 255)</li>
      <li>(255, 255, 255)</li>
      <li>(255, 255, 255)</li>
    </colorSpectrum>
	<fixedName>Communauté Hippie</fixedName>
    <settlementNameMaker>nomcoloniehippies</settlementNameMaker>
    <allowedCultures><li>Hippie</li></allowedCultures>
    <structureMemeWeights>
      <Structure_OriginHindu MayRequire="Ludeon.RimWorld.Ideology">1</Structure_OriginHindu>
      <Structure_OriginBuddhist MayRequire="Ludeon.RimWorld.Ideology">1</Structure_OriginBuddhist>
      <Structure_Animist MayRequire="Ludeon.RimWorld.Ideology">1</Structure_Animist>
      <Structure_TheistAbstract MayRequire="Ludeon.RimWorld.Ideology">1</Structure_TheistAbstract>
      <Structure_Ideological MayRequire="Ludeon.RimWorld.Ideology">1</Structure_Ideological>
    </structureMemeWeights>
    <requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">HighLife</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">PeaceLove</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">TreeConnection</li>
    </requiredMemes>
    <disallowedPrecepts>
      <li MayRequire="Ludeon.RimWorld.Ideology">ApparelDesired_Soft_Subordinate</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">ApparelDesired_Strong_Subordinate</li>
    </disallowedPrecepts>
    <techLevel>Industrial</techLevel>
    <backstoryCategories>
      <li>Outlander</li>
    </backstoryCategories>
    <geneticVariance>1</geneticVariance>
    <caravanTraderKinds>
      <li>Caravan_hippie</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_hippie</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_hippie</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-40</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <!-- Normal fights, ranged & melee -->
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <options>
	  <Hippie>5</Hippie>
	  <Hippie_chef>1</Hippie_chef>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <Hippie_trader>1</Hippie_trader>
        </traders>
        <carriers>
          <Alpaca>25</Alpaca>
          <Donkey>25</Donkey>
          <Goat>25</Goat>
          <Sheep>25</Sheep>
        </carriers>
        <guards>
          <Hippie>10</Hippie>
	  <Hippie_chef>1</Hippie_chef>
        </guards>
      </li>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <Hippie>2</Hippie>
	  <Hippie_chef>1</Hippie_chef>
        </options>
      </li>
      <li>
        <!-- Base defense -->
        <kindDef>Settlement</kindDef>
        <options>
	  <Hippie>10</Hippie>
	  <Hippie_chef>1</Hippie_chef>
        </options>
      </li>
    </pawnGroupMakers>
    <settlementTexturePath>peace</settlementTexturePath>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>5</configurationListOrderPriority>
  </FactionDef>



