<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
		<mods>
        <li>XenoDiversity Precept</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/MemeDef[defName = "PeaceLove"]/requireOne</xpath>
					<value>
						<li>
							<li>XenoDiversity_Approved</li>
							<li>XenoDiversity_Respected</li>
							<li>XenoDiversity_Exalted</li>
						</li>
					</value>
				</li>		
			</operations>
		</match>
	</Operation>
</Patch>