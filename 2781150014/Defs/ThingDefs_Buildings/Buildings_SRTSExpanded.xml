<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="NECShipBase">
    <defName>SRTSDQuadreme</defName> 
    <label>Quadreme</label> 
    <description>A large airship designed after the great Roman ships of war. Has the capacity to carry large compliment of soldiers. Is equipped with decent armor to protect its users. 

		Passenger cap: 1 - 30 Speed: Fair Fuel cap: 5000 Combustable Fuel efficiency : Fair
		----------------------------
		Cargo cap: 7000kg</description> 
    <graphicData>
      <texPath>Quadreme</texPath> 
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(25,25)</drawSize> 
    </graphicData>
    <size>(9,9)</size> 
    <statBases>
      <MaxHitPoints>500</MaxHitPoints> 
      <WorkToBuild>30000</WorkToBuild> 
      <Flammability>0.7</Flammability> 
      <Mass>50</Mass> 
      <Beauty>75</Beauty> 
    </statBases>
    <costList> 
      <Steel>1000</Steel>
      <Cloth>600</Cloth>
      <Silver>800</Silver>
      <WoodLog>2000</WoodLog>
    </costList>
    <comps> 
      <li Class="CompProperties_Transporter">
        <restEffectiveness>2</restEffectiveness>
        <massCapacity>20000</massCapacity>
      </li>
      <li Class="SRTS.CompProperties_LaunchableSRTS"> 
        <travelSpeed>2</travelSpeed>
        <fuelPerTile>10</fuelPerTile>
        <minPassengers>1</minPassengers>
        <maxPassengers>30</maxPassengers>
      	<spaceFaring>false</spaceFaring>
      	<shuttleBayLanding>true</shuttleBayLanding>
      </li>
      <li Class="CompProperties_Refuelable"> 
        <fuelCapacity>5000</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>500</initialConfigurableTargetFuelLevel>
        <fuelFilter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </fuelFilter>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <initialFuelPercent>0.25</initialFuelPercent>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
        <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
      </li>
    </comps>
    <researchPrerequisites> 
      <li>SRTSD_Dirigible</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite> 
    <uiIconScale>0.55</uiIconScale> 
    <killedLeavings>
      <ChunkSlagSteel>5</ChunkSlagSteel>
    </killedLeavings>
  </ThingDef>

