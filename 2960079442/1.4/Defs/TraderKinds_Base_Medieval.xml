<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>IT_Base_MedievalStandard</defName>
    <stockGenerators>

      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>800~3000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Hay</thingDef>
        <countRange>50~200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <price>Expensive</price>
        <countRange>3~10</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>300~500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>300~500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Uranium</thingDef>
        <countRange>40~75</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Jade</thingDef>
        <price>Expensive</price>
        <countRange>0~20</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>200~300</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Gold</thingDef>
        <countRange>20~120</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <price>Cheap</price>
        <countRange>50~250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>15~30</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>300~700</totalPriceRange>
        <excludedThingDefs>
          <li>Leather_Human</li>
        </excludedThingDefs>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>ResourcesRaw</categoryDef>
        <thingDefCountRange>3~3</thingDefCountRange>
        <totalPriceRange>200~600</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories>
          <li>EggsFertilized</li>
        </excludedCategories>
        <thingDefCountRange>3~5</thingDefCountRange>
        <totalPriceRange>300~800</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>PlantFoodRaw</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>100~150</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>FoodMeals</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>12~50</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSimple</thingDef>
        <countRange>0~10</countRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
        </thingDefs>
      </li>

      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>400~1000</totalPriceRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>PsychiteTea</thingDef>
        <countRange>0~50</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Luciferium</thingDef>
        <countRange>-2~5</countRange>
      </li>

      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>2</count>
            <chance>0.5</chance>
          </li>
          <li>
            <count>3</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>

      <!-- Weapons-->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>WeaponRanged</tradeTag>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <countRange>2~4</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>WeaponsMelee</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>4~4</thingDefCountRange>
        <countRange>1~2</countRange>
      </li>
      <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_Category">
        <categoryDef>WeaponsMelee</categoryDef>
        <maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MeleeWeapon_Spear</thingDef>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MeleeWeapon_LongSword</thingDef>
        <countRange>1~1</countRange>
      </li>
      <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_SingleDef">
        <thingDef>MeleeWeapon_Warhammer</thingDef>
        <countRange>1~1</countRange>
      </li>

      <!-- Apparel-->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>BasicClothing</tradeTag>
        <apparelTag>IT_MedievalApparel</apparelTag>
        <countRange>4~5</countRange>
      </li>
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Clothing</tradeTag>
        <apparelTag>IT_MedievalApparel</apparelTag>
        <countRange>1~2</countRange>
      </li>
      <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_MarketValue">
        <tradeTag>IT_MedievalTrade_Royalty</tradeTag>
        <apparelTag>IT_MedievalApparel</apparelTag>
        <countRange>1~2</countRange>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology" Class="StockGenerator_MarketValue">
        <tradeTag>IT_MedievalTrade_Ideology</tradeTag>
        <apparelTag>IT_MedievalApparel</apparelTag>
        <countRange>3~5</countRange>
      </li>
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Armor</tradeTag>
        <apparelTag>IT_MedievalApparel_ArmorMid</apparelTag>
        <countRange>2~3</countRange>
      </li>
      <li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_MarketValue">
        <tradeTag>Armor</tradeTag>
        <apparelTag>IT_MedievalApparel_Biotech</apparelTag>
        <countRange>2~3</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Apparel_SimpleHelmet</thingDef>
        <countRange>1~2</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Apparel_PlateArmor</thingDef>
        <countRange>1~2</countRange>
      </li>
      <!-- Implants -->
      <li Class="StockGenerator_Category">
        <categoryDef>BodyPartsNatural</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <maxTechLevelGenerate>Industrial</maxTechLevelGenerate>
        <price>Expensive</price>
        <categoryDef>BodyPartsProsthetic</categoryDef>
        <thingDefCountRange>2~2</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>

      <!-- Buildings -->
      <li Class="StockGenerator_Category">
        <categoryDef>BuildingsFurniture</categoryDef>
        <thingDefCountRange>3~4</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Art</tradeTag>
        <countRange>5~7</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Telescope</thingDef>
        <countRange>0~1</countRange>
      </li>

      <!-- Exotic -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>1~3</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ThrumboHorn</thingDef>
        <countRange>-5~1</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ElephantTusk</thingDef>
        <countRange>1~3</countRange>
      </li>

      <!-- Slaves -->
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>1~2</countRange>
      </li>

      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>3~4</kindCountRange>
        <countRange>2~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>

      <!-- Archite capsules -->
      <!-- none -->

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>ImplantEmpireCommon</tag></li>

    </stockGenerators>
  </TraderKindDef>

</Defs>
