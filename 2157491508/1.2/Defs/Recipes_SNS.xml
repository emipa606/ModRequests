<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<RecipeDef Abstract="True" Name="CosmicCall_Part" ParentName="SNS_Constructor_Item">
    <description>Turn raw Cosmic Matter into one of the mysterious components needed to realize The Call of the Cosmos.</description>
		<workAmount>45000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SNS_CosmicMatter</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
    </ingredients>
	</RecipeDef>

	<RecipeDef ParentName="CosmicCall_Part">
    <defName>SNS_Production_CosmicCall_Component_I</defName>
    <label>Pattern Fracture of Greatness</label>
    <products>
			<SNS_CosmicCall_Component_I>1</SNS_CosmicCall_Component_I>
    </products>
  </RecipeDef>

