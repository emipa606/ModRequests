<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="UF_UndergroundFoodPlantBase">
		<defName>UF_PlantVanetike</defName>
		<label>Vanetike</label>
		<description>This xenofungus procudes a sweet, juicy flesh which is used as a vegetable substitute.\n\nThis plant only grows in dark environments and will die if exposed to light.</description>
		<statBases>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/Vanetike</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<harvestedThingDef>UF_RawVanetike</harvestedThingDef>
			<harvestYield>11</harvestYield>
			<sowMinSkill>3</sowMinSkill>
			<growDays>6</growDays>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.05</max>
			</visualSizeRange>
			<purpose>Food</purpose>
			<sowResearchPrerequisites>
				<li>UF_XenofungusBasic</li>
			</sowResearchPrerequisites>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(204, 238, 76)</glowColor>
			</li>
		</comps>
	</ThingDef>



</Defs>