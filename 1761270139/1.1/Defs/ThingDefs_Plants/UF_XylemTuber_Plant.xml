<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="UF_UndergroundFoodPlantBase">
    <defName>UF_PlantXylemTuber</defName>
    <label>Xylem tuber</label>
    <description>A tough, fibrous xenofungus which yields a small amount of material that can be used as wood.\n\nThis plant only grows in dark environments and will die if exposed to light.</description>
    <statBases>
      <Nutrition>0.15</Nutrition>
    </statBases>
    <graphicData>
		<texPath>Plants/XylemTuber</texPath>
		<graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <ingestible />
    <plant>
		<harvestedThingDef>WoodLog</harvestedThingDef>
		<harvestTag>Wood</harvestTag>
		<harvestYield>5</harvestYield>
		<sowMinSkill>6</sowMinSkill>
		<growDays>7.5</growDays>
		<visualSizeRange>
			<min>0.3</min>
			<max>1.4</max>
		</visualSizeRange>
		<sowWork>800</sowWork>
		<harvestWork>800</harvestWork>
		<sowResearchPrerequisites>
				<li>UF_XenofungusCultivation</li>
			</sowResearchPrerequisites>
	</plant>
	<comps>
		<li Class="CompProperties_Glower">
			<glowRadius>1</glowRadius>
			<glowColor>(72, 43, 72)</glowColor>
		</li>
	</comps>
  </ThingDef>


</Defs>