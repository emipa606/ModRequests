<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="UF_UndergroundFoodPlantBase">
    <defName>UF_PlantDeepDevilstrand</defName>
    <label>Deep Devilstrand</label>
    <description>A unique strain of Devilstrand that grows without light, but which grows at a slightly slower rate.\n\nThis plant only grows in dark environments and will die if exposed to light.</description>
    <statBases>
      <Nutrition>0.15</Nutrition>
    </statBases>
    <graphicData>
		<texPath>Plants/DeepDevilstrand</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <ingestible />
    <plant>
		<harvestedThingDef>DevilstrandCloth</harvestedThingDef>
		<harvestYield>6</harvestYield>
		<sowMinSkill>10</sowMinSkill>
		<dieIfLeafless>false</dieIfLeafless>
		<topWindExposure>0.0</topWindExposure>
		<growDays>24</growDays>
		<visualSizeRange>
			<min>0.3</min>
			<max>1.0</max>
		</visualSizeRange>
		<sowWork>400</sowWork>
		<sowResearchPrerequisites>
				<li>UF_DeepDevilstrand</li>
			</sowResearchPrerequisites>
	</plant>
	<comps>
		<li Class="CompProperties_Glower">
			<glowRadius>1</glowRadius>
			<glowColor>(90, 5, 5)</glowColor>
		</li>
	</comps>
  </ThingDef>


</Defs>