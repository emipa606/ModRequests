<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase" Name="UF_NeutrophyriaBase" Abstract="True">
    <graphicData>
      <texPath>Resource/UF_Neutrophyria</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <MarketValue>0.05</MarketValue>
      <Mass>0.03</Mass>
      <Flammability>2.0</Flammability>
      <DeteriorationRate>3.4</DeteriorationRate>
    </statBases>
    <stackLimit>75</stackLimit>
    <thingCategories>
      <li>PlantMatter</li>
    </thingCategories>
  </ThingDef>

  <ThingDef ParentName="UF_NeutrophyriaBase">
    <defName>UF_RawNeutrophyria</defName>
    <label>Raw Neutrophyria</label>
    <description>The raw liquid of the neutrophyria xenofungus. Can be processed into neutroamine at a drug lab.</description>
    <statBases>
      <MarketValue>8</MarketValue>
    </statBases>
	<tickerType>Rare</tickerType>
	<comps>
		<li Class="CompProperties_Rottable">
				<daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
		</li>
	</comps>
  </ThingDef>

</Defs>
