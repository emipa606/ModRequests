<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="UF_UndergroundFoodPlantBase">
    <defName>UF_PlantLaeti</defName>
    <label>Laeti</label>
    <description>A fibrous vine-like xenofungus, utterly inedible. The fibers it produces can be woven into different fabrics.\n\nThis plant only grows in dark environments and will die if exposed to light.</description>
    <statBases>
      <Nutrition>0.15</Nutrition>
    </statBases>
    <graphicData>
		<texPath>Plants/UF_Laeti</texPath>
		<graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <ingestible />
    <plant>
		<harvestedThingDef>UF_RawLaeti</harvestedThingDef>
		<harvestYield>8</harvestYield>
		<sowMinSkill>8</sowMinSkill>
		<growDays>10</growDays>
		<visualSizeRange>
			<min>0.3</min>
			<max>1.75</max>
		</visualSizeRange>
		<sowWork>310</sowWork>
		<sowResearchPrerequisites>
				<li>UF_AdvancedXenofungusCultivation</li>
			</sowResearchPrerequisites>
	</plant>
	<comps>
		<li Class="CompProperties_Glower">
			<glowRadius>1</glowRadius>
			<glowColor>(92, 249, 132)</glowColor>
		</li>
	</comps>
  </ThingDef>


</Defs>