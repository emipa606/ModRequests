<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>GloomyFurniture</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
    <operations>
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "RGK_Chair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "RGK_Chair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
    <li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "RGK_Chair"]/comps</xpath>
		<value>
		<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/GloomyFurniture/RGK_Chair1_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3.3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
	</li>

		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "RGKDiningChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "RGKDiningChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "RGKDiningChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/GloomyFurniture/RGK_Chair3_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3.3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
	</li>

		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "GL_Bench"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "GL_Bench"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "GL_Bench"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/GloomyFurniture/Bench_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(2.3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
	</li>

    <li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "RGK_Chair" or defName = "RGKDiningChair" or defName = "GL_Bench"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
	</li>
    </operations>
    </match>
</Operation>

</Patch>