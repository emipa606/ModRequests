<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>More Furniture</li>
		<li>More Furniture [1.0]</li>
		<li>More Furniture [1.1 + 1.2]</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
 
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/SquareChair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
		</li>

		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>	
	<li  Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/ComfyChair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
		</value>
		    </li>
		
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchRight"]</xpath>
		<value>
	<comps>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/CouchR_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
    </comps>
		</value>
		    </li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchLeft"]</xpath>
		<value>
	<comps>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/CouchL_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
    </comps>
		</value>
		    </li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchCenter"]</xpath>
		<value>
	<comps>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/CouchM_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
    </comps>
		</value>
		    </li>
		
	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchCorner"]</xpath>
		<value>
	<comps>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/Anon2MoreFurniture/CouchC_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
    </comps>
		</value>
		    </li>

	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair" or 
						defName = "Anon2CushionedChair" or 
						defName = "Anon2CouchRight" or 
						defName = "Anon2CouchLeft" or 
						defName = "Anon2CouchCenter" or 
						defName = "Anon2CouchCorner"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
	</li>
	</operations>
    </match>
</Operation>

</Patch>