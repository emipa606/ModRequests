<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Furniture Expanded</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>

		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Seat_SquareChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Seat_SquareChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
         <li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_SquareChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/VanillaFurnitureExp/SquareChair/Chair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(1,1)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
		</value>
		    </li>
		
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Seat_ModernChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Seat_ModernChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>		
		<li  Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_ModernChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/VanillaFurnitureExp/ModernChair/OfficeChair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <drawSize>(1,1)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
		</li>
		
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Seat_RoyalChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Seat_RoyalChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>			
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_RoyalChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/VanillaFurnitureExp/RoyalChair/RoyalChair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <drawSize>(1,1)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
      </li>
		</value>
		    </li>
		
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Seat_RoyalArmchair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Seat_RoyalArmchair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>		
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_RoyalArmchair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/VanillaFurnitureExp/RoyalArmchair/RoyalArmchair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <drawSize>(2.6,2.6)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
		    </li>

 	<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_SquareChair" or 
						defName = "Seat_ModernChair" or 
						defName = "Seat_RoyalChair" or 
						defName = "Seat_RoyalArmchair"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
	</li>

 </operations>
    </match>
</Operation>

</Patch>