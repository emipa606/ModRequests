<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Furniture Expanded - Spacer Module</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Seat_AdvChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Seat_AdvChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>	

        <li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_AdvChair"]/comps</xpath>
		<value>
        <li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
          <graphicData>
            <texPath>Things/Building/Furniture/VanillaFurnitureSpacer/AdvancedChair/AdvancedChair_back</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <drawSize>(1,1)</drawSize>
          </graphicData>
          <altitudeLayer>MoteOverhead</altitudeLayer>
		</li>
		</value>
		</li>

		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Seat_AdvChair"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
		</li>

	</operations>
    </match>
</Operation>

</Patch>