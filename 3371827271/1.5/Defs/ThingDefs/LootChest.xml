<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="BuildingBase">
        <defName>WAW_LootChest</defName>
        <label>loot chest</label>
        <thingClass>Building</thingClass>
        <category>Building</category>
        <description>A chest that can hold lots of things.</description>
        <graphicData>
          <texPath>Things/LootChest</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <drawSize>(1.9,1.9)</drawSize>
        </graphicData>
        <size>(1,1)</size>
        <rotatable>false</rotatable>
        <useHitPoints>false</useHitPoints>
        <tickerType>Normal</tickerType>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <castEdgeShadows>false</castEdgeShadows>
        <fillPercent>0.5</fillPercent>
        <uiOrder>2200</uiOrder>
        <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
        <comps>
          <li Class="CompProperties_Transporter">
            <massCapacity>999999.99</massCapacity>
            <max1PerGroup>true</max1PerGroup>
            <restEffectiveness>0.8</restEffectiveness>
            <canChangeAssignedThingsAfterStarting>true</canChangeAssignedThingsAfterStarting>
          </li>
          <li Class="WarfareAndWarbands.Warband.CompProperties_LootChest">
          </li>
        </comps>
        <inspectorTabs>
          <li>ITab_ContentsTransporter</li>
        </inspectorTabs>
        <uiIconScale>0.65</uiIconScale>
      </ThingDef>
      
</Defs>