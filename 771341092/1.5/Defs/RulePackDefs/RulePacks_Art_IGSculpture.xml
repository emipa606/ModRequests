<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ArtName_IGSculptures</defName>
		<rulePack>
		  <rulesStrings>
			<li>art_name->[TGSFirst] [TGSSecond]</li>
		  </rulesStrings>
		  <rulesRaw>
			<li Class="Rule_File">
				<keyword>TGSFirst</keyword>
				<path>NameBanks/TGSFirst</path>
			</li>
			<li Class="Rule_File">
				<keyword>TGSSecond</keyword>
				<path>NameBanks/TGSSecond</path>
			</li>
		  </rulesRaw>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>ArtDescription_IGSculptures</defName>
		<rulePack>
			<rulesStrings>
				<li>art_description_root->This [art] brings [word1] honor to the [word2]</li>
				<li>art_description_root->This [art] brings [word1] honor to the [word2]</li>
				<li>art_description_root->This [art] brings [word1] honor to the [word2]</li>
				
				<li>art->work</li>
				<li>art->piece</li>
				<li>art->construct</li>
				<li>art->sculpture</li>
				
				<li>word1->great</li>
				<li>word1->splendid</li>
				<li>word1->tremendous</li>
				<li>word1->immense</li>
				<li>word1->considerable</li>
				
				<li>word2->Emperor</li>
				<li>word2->Imperium</li>
				<li>word2->Imperial Guard</li>
				<li>word2->Mechanicus</li>
				<li>word2->Space Marines</li>
				
			</rulesStrings>
		</rulePack>
	</RulePackDef>

</Defs>