	<!--QX==================== Imperial Guard Core Mod - WH40k SCiFi Lamp =========================QX-->
	
	<ThingDef ParentName="IG_BuildingBase" >
		<defName>IG_WH40kSciFiLamp</defName>
		<label>Imperial Lamp</label>
		<description>Imperial Lamp, built to standard specification.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<selectable>true</selectable>
		<fillPercent>0.20</fillPercent>
		<pathCost>8</pathCost>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<designationCategory>IG_ImperialMenuA</designationCategory>
		<rotatable>false</rotatable>
		<uiIconOffset>(0, 0.03)</uiIconOffset>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/IGCore/Building/Lighting/WH40kLamp/WH40kLamp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.3, 0.6, 0.3)</volume>
				<offset>(0,0,-0.1)</offset>
			</shadowData>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<costList>
			<Steel>20</Steel>
		</costList>
		<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>300</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>2</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>16</glowRadius>
				<glowColor>(172,175,175,0)</glowColor>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>10</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
		<researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>
	
	
	
	
	
	
	
	
	
	
	
	
