	<!--QX==================== Imperial Guard Core Mod - Gold Wall Light =========================QX-->

	<ThingDef ParentName="IG_WallLightBase">
		<defName>IG_WallLightGOLD</defName>
		<label>Imperial Wall Light Gold</label>
		<description>Imperial full height waterproof light fixture designed to be mounted on a wall, fills the halls of the righteous with a warming glow in the name of the Eternal Emperor.</description>
		<graphicData>
			<texPath>Things/IGCore/Building/Lighting/WallLights/IGWallLightGOLD</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>2,2</drawSize>
		</graphicData>
		<uiIconPath>Things/IGCore/Building/Lighting/WallLights/IGWallLightGOLD_UI</uiIconPath>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>15</glowRadius>
				<glowColor>(204,172,0,0)</glowColor>
			</li>
		</comps>
		<designatorDropdown>Imperial_Light_Wall</designatorDropdown>
	</ThingDef>
	
