<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--QX==================== Imperial Guard Core Mod - Unfinished BASE =========================QX-->

	<ThingDef Name="IG_UnfinishedBase" Abstract="True">
		<thingClass>UnfinishedThing</thingClass>
		<category>Item</category>
		<label>unfinished thing</label>
		<graphicData>
			<texPath>Things/IGCore/Unfinished/GenericIcon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<useHitPoints>true</useHitPoints>
		<isUnfinishedThing>true</isUnfinishedThing>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<selectable>true</selectable>
		<tradeability>Never</tradeability>
		<drawerType>RealtimeOnly</drawerType>
		<!-- For CompForbiddable -->
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<DeteriorationRate>2.0</DeteriorationRate>
			<Beauty>-4</Beauty>
			<Mass>1</Mass> <!-- ingredients are added to this value -->
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
		<thingCategories>
			<li>IG_Unfinished</li>
		</thingCategories>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
	</ThingDef>
	
