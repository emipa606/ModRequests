<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--QX==================== Imperial Guard Core Mod - Adeptus Mechanicus Trait =========================QX-->

	<TraitDef>
		<defName>IGTraitMechanicus</defName>
		<commonality>0.05</commonality>
		<degreeDatas>
			<li>
				<label>Imperial-Mechanicus</label>
				<description>[PAWN_nameDef] has been blessed by the Mechanicus to be overall better than regular humans at all tasked related to research, craft and mining. [PAWN_pronoun] is blessed for a harsh working environment.</description>
				<statOffsets>
					<WorkSpeedGlobal>0.25</WorkSpeedGlobal>
					<GlobalLearningFactor>1.3</GlobalLearningFactor>
					<MoveSpeed>0.10</MoveSpeed>
					<MentalBreakThreshold>-0.15</MentalBreakThreshold>
					<ImmunityGainSpeed>0.2</ImmunityGainSpeed>
					<CarryingCapacity>150</CarryingCapacity>
					<TradePriceImprovement>0.25</TradePriceImprovement>
					<SocialImpact>0.1</SocialImpact>
					<ConstructionSpeed>0.10</ConstructionSpeed>
				</statOffsets>
				<skillGains>
					<li>
						<key>Crafting</key>
						<value>3</value>
					</li>
					<li>
						<key>Construction</key>
						<value>3</value>
					</li>
					<li>
						<key>Mining</key>
						<value>3</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<conflictingTraits>
			<li>IGTraitAstraMilitarum</li>
			<li>IGTraitAssassinorum</li>
			<li>IGTraitInquisition</li>
			<li>IGTraitImperialMedic</li>
			<li>IG_TraitAstarte</li>
		</conflictingTraits>
	</TraitDef>

	<!--QX==================== Imperial Guard Core Mod - Astra Militarum Trait =========================QX-->	

	<TraitDef>
		<defName>IGTraitAstraMilitarum</defName>
		<commonality>0.05</commonality>
		<degreeDatas>
			<li>
				<label>Imperial-AstraMilitarum</label>
				<description>[PAWN_nameDef] has been blessed by the Emperor to be overall better than regular humans at all tasked related to combat to better serve the Emperor against Chaos. [PAWN_pronoun] is blessed with general combat excellence.</description>
