					<MeleeHitChance>3.00</MeleeHitChance>
				</statFactors>
				<statOffsets>
					<MeleeDodgeChance>0.25</MeleeDodgeChance>
					<AimingDelayFactor>-0.15</AimingDelayFactor>
					<ShootingAccuracyPawn>0.15</ShootingAccuracyPawn>
					<GlobalLearningFactor>0.15</GlobalLearningFactor>
					<MoveSpeed>0.10</MoveSpeed>
					<MentalBreakThreshold>-0.15</MentalBreakThreshold>
					<ImmunityGainSpeed>0.15</ImmunityGainSpeed>
					<CarryingCapacity>100</CarryingCapacity>
					<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Shooting</key>
						<value>3</value>
					</li>
					<li>
						<key>Melee</key>
						<value>3</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<conflictingTraits>
			<li>IGTraitMechanicus</li>
			<li>IGTraitAssassinorum</li>
			<li>IGTraitInquisition</li>
			<li>IGTraitImperialMedic</li>
			<li>IG_TraitAstarte</li>
