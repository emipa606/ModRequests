<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--QX==================== Imperial Guard Core Mod -  Decoration Arbites Statue =========================QX-->
	
	<ThingDef ParentName="IG_SculptureBase">
		<defName>StatueArbites</defName>
		<label>Imperial Statue to Arbites</label>
		<description>A Statue of the Imperium depicting the illustrious Arbites.</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<texPath>Things/IGCore/Building/Decorations/StatueArbitesMk2</texPath>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<fillPercent>0.5</fillPercent>
		<rotatable>false</rotatable>
		<size>(2,2)</size>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<Beauty>110</Beauty>
			<Mass>25</Mass>
			<WorkToMake>20000</WorkToMake>
		</statBases>
		<stuffCategories>
			<li>Metallic</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>350</costStuffCount>
	</ThingDef>
	
