<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	    <mods>
		    <li>Optimization: Meats (1.2)</li>
	    </mods>
        <match Class="PatchOperationSequence">
            <operations>
                
                <!-- Chicken -->
                    <li Class="PatchOperationReplace">
                        <xpath>Defs/ThingDef[defName = "KIT_SyntheticChicken"]/plant/harvestedThingDef</xpath>
                        <value>
                            <harvestedThingDef>Meat_Cow</harvestedThingDef>
                        </value>
                    </li>

                <!-- Muffalo -->
                    <li Class="PatchOperationReplace">
                        <xpath>Defs/ThingDef[defName = "KIT_SyntheticMuffalo"]/plant/harvestedThingDef</xpath>
                        <value>
                            <harvestedThingDef>Meat_Cow</harvestedThingDef>
                        </value>
                    </li>

            
            </operations>
        </match>
    </Operation>

</Patch>
