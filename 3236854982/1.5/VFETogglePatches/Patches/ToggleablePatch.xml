<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<!--mods>
			<li>Vanilla Expanded Framework</li>
		</mods-->
		<enabled>False</enabled>
		<label>Disable Apparel switching for certain outfit</label>
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="BinahLibrarian"]/comps/li[@Class="VanillaApparelExpanded.CompProperties_SwitchApparel"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="BinahArbiter"]/comps/li[@Class="VanillaApparelExpanded.CompProperties_SwitchApparel"]</xpath>
			</li>
		</operations>
	</Operation>
</Patch>