<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef Name="RabbitArmorBase" ParentName="ArmorMachineableBase" Abstract="True">
		<techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
