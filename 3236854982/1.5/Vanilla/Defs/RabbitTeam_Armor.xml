			<MeleeWeapon_CooldownMultiplier>-0.25</MeleeWeapon_CooldownMultiplier>
			<MeleeDodgeChance>15</MeleeDodgeChance>
			<MeleeHitChance>10</MeleeHitChance>
			<MoveSpeed>0.8</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ArmorRabbitTeam</li>
		</thingCategories>
		<costList>
			<ComponentSpacer>3</ComponentSpacer>
			<Plasteel>80</Plasteel>
			<DevilstrandCloth>40</DevilstrandCloth>
			<Uranium>10</Uranium>
		</costList>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
			<soundWear>Wear_PowerArmor</soundWear>
			<soundRemove>Remove_PowerArmor</soundRemove>
		</apparel>
		<thingSetMakerTags>
			<li>RewardStandardHighFreq</li>
		</thingSetMakerTags>
	</ThingDef>

