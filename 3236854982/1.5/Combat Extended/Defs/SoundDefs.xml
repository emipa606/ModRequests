<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<SoundDef>
		<defName>RabbitTeamGun_Interact</defName>
		<context>MapOnly</context>
		<maxSimultaneous>1</maxSimultaneous>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>RabbitTeam/RabbitTeamGun_Reload</clipPath>
					</li>
				</grains>
				<volumeRange>50~55</volumeRange>
			</li>
		</subSounds>
	</SoundDef>

	<SoundDef>
		<defName>RabbitTeamGun_Fire</defName>
		<context>MapOnly</context>
		<maxSimultaneous>1</maxSimultaneous>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>RabbitTeam/RabbitTeamGun_Fire</clipPath>
					</li>
				</grains>
				<volumeRange>50~55</volumeRange>
				<pitchRange>0.90~1.00</pitchRange>
			</li>
		</subSounds>
	</SoundDef>

	<SoundDef>
		<defName>RabbitTeamKnife_Attack</defName>
		<context>MapOnly</context>
		<maxSimultaneous>1</maxSimultaneous>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>RabbitTeam/RabbitTeam_Knife</clipFolderPath>
					</li>
				</grains>
				<volumeRange>5~10</volumeRange>
			</li>
		</subSounds>
	</SoundDef>

</Defs>