<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef ParentName="RockBase">
    <defName>CRFT_MineableSmokeleaf</defName>
    <label>compacted smokeleaf</label>
    <description>Ancient smokeleaf, compacted over time. Still fresh, can be mined for smokeleaf.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(57,70,52)</color>
      <colorTwo>(71,104,72)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>SmokeleafLeaves</mineableThing>
      <mineableYield>40</mineableYield>
      <mineableScatterCommonality>0.01</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>5~10</mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
</Defs>