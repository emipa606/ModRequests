			<thingDefs>
				<li>SmokeleafLeaves</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
			<categories>
				
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>			
			<thingDefs>
				<li>SmokeleafLeaves</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
			<categories>
				
			</categories>
		</defaultIngredientFilter>
		<products>
			<CRFT_SmokeleafVape>5</CRFT_SmokeleafVape>
		</products>
		<skillRequirements>
			<Crafting>6</Crafting>
		</skillRequirements>
	</RecipeDef>
