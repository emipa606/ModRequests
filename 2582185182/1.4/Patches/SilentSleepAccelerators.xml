<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- ========== Patch - metasynthetic.SilentSleepAccelerator ========== -->
    <!-- Targeting Ideology\Defs\SoundDefs\SleepAccelerator_SoundDefs.xml -->
    <Operation Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
            <li Class="PatchOperationReplace">
                <xpath>Defs/SoundDef[
                    defName = "SleepAcceleratorActive_Operating"
                    ]/subSounds/li/volumeRange
                </xpath>
                <value>
                    <volumeRange>0~0</volumeRange>
                </value>
            </li>
        </operations>
    </Operation>

</Patch>
