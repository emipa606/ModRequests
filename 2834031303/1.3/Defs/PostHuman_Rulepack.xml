<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<RulePackDef>
    <defName>AP_NamerFactionPostHumanUtility</defName>
    <rulePack>
      <rulesStrings>
		<li>PostHumanNameFirst->[QuName1]</li>
		<li>PostHumanNameLast->[QuName2]</li>
      </rulesStrings>
	  
      <rulesFiles>
             <li>QuName1->WordParts/Names_Qu1</li>
             <li>QuName2->WordParts/Names_Qu2</li>
        </rulesFiles>
    </rulePack>
</RulePackDef>
	
  <RulePackDef>
        <defName>AP_NamerPersonPostHuman</defName>
		<include>
		<li>AP_NamerFactionPostHumanUtility</li>
		</include>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=3)->[PostHumanNameFirst] [PostHumanNameLast]</li>
				<li>r_name(p=3)->[PostHumanNameFirst] [PostHumanNameLast]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
