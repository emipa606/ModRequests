<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
		<defName>AP_KillerFolk</defName>
		<label>Killer Folk</label>
		<description>A predatory race sharing roots with humanity. They were once wild predators that evolved and gained sapience. Their claws and saber teeth used to be larger and stronger. Killer Folks have religions centered around war and hunting as a part of their lifestyle. They developed factory farming, organized state structures, industrial civilizations, and new technology. However, they still kept part of their violent behavior. Apart from that, they aren't much different from standard humans.</description>
		<alienRace>
			<generalSettings>
				<immuneToAge>false</immuneToAge>
				<forcedRaceTraitEntries>
					<li>
						<defName>Bloodlust</defName>
						<chance>20</chance>
					</li>
				</forcedRaceTraitEntries>
				<maxDamageForSocialfight>20</maxDamageForSocialfight>
				<allowHumanBios>false</allowHumanBios>
				<humanRecipeImport>true</humanRecipeImport>
				<alienPartGenerator>
					<borderScale>1</borderScale>
					<atlasScale>1</atlasScale>
					<aliencrowntypes>
						<li>Average_Normal</li>
					</aliencrowntypes>
					<alienbodytypes>
						<li>Male</li>
					</alienbodytypes>
					<useGenderedHeads>false</useGenderedHeads>
					<useGenderedBodies>false</useGenderedBodies>
					<colorChannels>
						<li>
							<name>skin</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>10</weight>
										<only>RGBA(1,1,1)</only>
									</li>
									<li>
										<weight>5</weight>
										<only>RGBA(227, 205, 198)</only>
									</li>
									<li>
										<weight>5</weight>
										<only>RGBA(249, 246, 237)</only>
									</li>
								</options>
							</first>
						</li>
					</colorChannels>
				</alienPartGenerator>
			</generalSettings>
			<graphicPaths>
				<li>
					<body>Things/KillerFolk/Body/</body>
					<head>Things/KillerFolk/Head/</head>
					<skeleton>Things/KillerFolk/Body/Dessicated</skeleton>
				</li>
				<li>
					<lifeStageDefs>
						<li>AP_KF_Adult</li>
					</lifeStageDefs>
					<customDrawSize>(1, 1)</customDrawSize><!--(1.05, 1.05)-->
					<customHeadDrawSize>(1, 1)</customHeadDrawSize><!--(1.15, 1.15)-->
					<body>Things/KillerFolk/Body/</body>
					<head>Things/KillerFolk/Head/</head>
					<skeleton>Things/KillerFolk/Body/Dessicated</skeleton>
				</li>
			</graphicPaths>
			<styleSettings>
				<li>
					<key>HairDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
				<li>
					<key>TattooDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
				<li>
					<key>BeardDef</key>
					<value>
						<hasStyle>false</hasStyle>
					</value>
				</li>
			</styleSettings>
		</alienRace>
		<statBases>
			<MarketValue>2000</MarketValue>
			<MoveSpeed>4.8</MoveSpeed>
			<Mass>50</Mass>
			<Flammability>1</Flammability>
			<ComfyTemperatureMin>12</ComfyTemperatureMin>
			<ComfyTemperatureMax>30</ComfyTemperatureMax>
			<LeatherAmount>75</LeatherAmount>
			<InjuryHealingFactor>1</InjuryHealingFactor>
			<CarryingCapacity>40</CarryingCapacity>
			<IncomingDamageFactor>1</IncomingDamageFactor>
			<PainShockThreshold>1</PainShockThreshold>
			<PawnBeauty>1</PawnBeauty>
			<AimingDelayFactor>1</AimingDelayFactor>
			<FoodPoisonChance>0</FoodPoisonChance>
			<ToxicSensitivity>0.9</ToxicSensitivity>
			<SocialImpact>1</SocialImpact>
			<GeneralLaborSpeed>1</GeneralLaborSpeed>
			<ShootingAccuracyPawn>1</ShootingAccuracyPawn>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>14</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>8</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>1</chanceFactor>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>16</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>8</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>1</chanceFactor>
			</li>
			<li>
				<label>razor teeth</label>
				<capacities>
					<li>Stab</li>
				</capacities>
				<power>10</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>2</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>0.8</chanceFactor>
			</li>
		</tools>
		<race>
			<thinkTreeMain>Humanlike</thinkTreeMain>
			<thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
			<intelligence>Humanlike</intelligence>
			<makesFootprints>true</makesFootprints>
			<lifeExpectancy>140</lifeExpectancy>
			<leatherDef>Leather_Human</leatherDef>
			<nameCategory>HumanStandard</nameCategory>
			<nameGenerator>AP_NamerPersonPostHuman</nameGenerator>
			<body>AP_KF_Body</body>
			<hasGenders>true</hasGenders>
			<baseBodySize>1.2</baseBodySize>
			<baseHealthScale>1.2</baseHealthScale>
			<baseHungerRate>1.3</baseHungerRate>
			<foodType>CarnivoreAnimal</foodType>
			<!--<gestationPeriodDays>30</gestationPeriodDays>-->
			<manhunterOnDamageChance>0</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0</manhunterOnTameFailChance>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(1.01, 0.02)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeStageAges>
				<li>
					<def>AP_KF_Adult</def>
					<minAge>20</minAge>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
			<specialShadowData>
				<volume>(0.3, 0.8, 0.4)</volume>
				<offset>(0,0,-0.3)</offset>
			</specialShadowData>
			<ageGenerationCurve>
				<points>
					<li>(14,0)</li>
					<li>(24,80)</li>
					<li>(30,80)</li>
					<li>(60,70)</li>
					<li>(110,60)</li>
					<li>(120,40)</li>
					<li>(140,20)</li>
					<li>(150,0)</li>
				</points>
			</ageGenerationCurve>
			<hediffGiverSets>
				<li>OrganicStandard</li>
			</hediffGiverSets>
		</race>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<showImageInInfoCard>true</showImageInInfoCard>
				<ImageToShowInInfoCard>Art/Art_Killer</ImageToShowInInfoCard>
			</li>
		</modExtensions>
	</AlienRace.ThingDef_AlienRace>
	<AlienRace.RaceSettings>
		<defName>AP_KF_Settings</defName>
		<pawnKindSettings>
			<startingColonists>
				<li>
					<pawnKindEntries>
						<li>
							<kindDefs>
								<li>AP_KillerFolkColonistPawnKind</li>
							</kindDefs>
							<chance>5.0</chance>
						</li>
					</pawnKindEntries>
					<factionDefs>
						<li>PlayerColony</li>
					</factionDefs>
				</li>
			</startingColonists>
		</pawnKindSettings>
	</AlienRace.RaceSettings>
	<LifeStageDef>
		<defName>AP_KF_Adult</defName>
		<label>adult</label>
		<visible>false</visible>
		<reproductive>false</reproductive>
	</LifeStageDef>
</Defs>