  <ThingDef ParentName="BuildingBase">
    <defName>ST_Barricade</defName>
    <label>Mobile Barricade</label>
    <description>A kind of mobile bunker, which can be rapidly deployed and dismantled. When it is destroyed, it will blast the front area.</description>
    <size>(3,1)</size>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/ST_Barricade</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>Transparent</shaderType>
      <drawRotated>false</drawRotated>
      <drawSize>(3,1)</drawSize>
    </graphicData>
    <rotatable>true</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>42</pathCost>
    <fillPercent>0.80</fillPercent>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <statBases>
      <Beauty>-3</Beauty>
      <MaxHitPoints>900</MaxHitPoints>
      <WorkToBuild>400</WorkToBuild>
      <Flammability>0</Flammability>
      <Mass>3.1</Mass>
    </statBases>
    <selectable>true</selectable>
    <costList>
      <Steel>30</Steel>
      <Chemfuel>10</Chemfuel>
      <!-- 材料 -->
    </costList>
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <clearBuildingArea>true</clearBuildingArea>
    <building>
      <isEdifice>true</isEdifice>
      <expandHomeArea>false</expandHomeArea>
    </building>
    <designationCategory>Security</designationCategory>
    <comps>
      <li Class="MYDE_ExGTUrbanCombat.CompProperties_BarricadeExplosion">
        <explosiveRadius>1.9</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <!--这个是伤害，除了下面的不同，其他的所有字段都跟原版的CompProperties_Explosive相同-->
        <damageAmountBase>10</damageAmountBase>
        <!--这个是爆炸间隔-->
        <ExplosiveRange_Num>2</ExplosiveRange_Num>
        <!--这个是最大爆炸距离-->
        <ExplosiveRange_Max>9</ExplosiveRange_Max>
      </li>
    </comps>
  </ThingDef>
