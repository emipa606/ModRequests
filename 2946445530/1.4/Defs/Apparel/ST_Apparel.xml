<Defs>
  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>ST_AdvancedHelmet</defName>
    <label>bulletproof helmet</label>
    <description>A heavyweight combat helmet with a complex design, reinforced with plasteel.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
      <displayPriority>205</displayPriority>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/ST_helmet_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>2.1</uiIconScale>
    <statBases>
      <WorkToMake>8000</WorkToMake>
      <MaxHitPoints>600</MaxHitPoints>
      <Mass>5</Mass>
      <Flammability>0.3</Flammability>
      <ArmorRating_Sharp>0.92</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.3</ArmorRating_Blunt>
      <ArmorRating_Heat>0.5</ArmorRating_Heat>
      <Insulation_Cold>2.4</Insulation_Cold>
