<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods><li>Fertile Fields [1.1]</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="ItW_FungiponicsBasin"]/costList</xpath>
					<match Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="ItW_FungiponicsBasin"]/costList</xpath>
						<value>
							<PileofDirt>2</PileofDirt>
						</value>
					</match>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="ItW_FungiponicsBasin"]</xpath>
						<value>
							<costList>
								<PileofDirt>2</PileofDirt>
							</costList>
						</value>
					</nomatch>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="ItW_FungiponicsBasin"]/designationCategory</xpath>
					<value>
						<designationCategory>RFF_Farm</designationCategory>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>