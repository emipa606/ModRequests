<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>SeedsPlease</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_BrightbellsSeed</defName>
							<label>brightbells spores</label>
							<sources>
								<li>ItW_Brightbells</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_BrightbellsSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_GleamcapSeed</defName>
							<label>gleamcap spores</label>
							<sources>
								<li>ItW_Gleamcap</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_GleamcapSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ItW_ExtractSeed_Gleamcap</defName>
							<label>extract gleamcap spores</label>
							<description>Extract spores from gleamcap salve.</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>ItW_GleamcapSalve</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>ItW_GleamcapSalve</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<ItW_GleamcapSeed>3</ItW_GleamcapSeed>
							</products>
						</RecipeDef>
					</value>
				</li>				
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_ShimmershroomSeed</defName>
							<label>shimmershroom spores</label>
							<sources>
								<li>ItW_Shimmershroom</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_ShimmershroomSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ItW_ExtractSeed_Shimmershroom</defName>
							<label>extract shimmershroom spores</label>
							<description>Extract spores from shimmershroom.</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>ItW_RawShimmershroom</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>ItW_RawShimmershroom</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<ItW_ShimmershroomSeed>3</ItW_ShimmershroomSeed>
							</products>
						</RecipeDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_GlowbulbSeed</defName>
							<label>glowbulb spores</label>
							<sources>
								<li>ItW_Glowbulb</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_GlowbulbSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ItW_ExtractSeed_Glowbulb</defName>
							<label>extract glowbulb spores</label>
							<description>Extract spores from glowbulb.</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>ItW_RawGlowbulb</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>ItW_RawGlowbulb</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<ItW_GlowbulbSeed>3</ItW_GlowbulbSeed>
							</products>
						</RecipeDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_GreyfieldsSeed</defName>
							<label>greyfields spores</label>
							<sources>
								<li>ItW_Greyfields</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_GreyfieldsSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ItW_ExtractSeed_Greyfields</defName>
							<label>extract greyfields spores</label>
							<description>Extract spores from greyfields.</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>ItW_RawGreyfields</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>ItW_RawGreyfields</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<ItW_GreyfieldsSeed>3</ItW_GreyfieldsSeed>
							</products>
						</RecipeDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>ItW_RoyalBracketSeed</defName>
							<label>royal bracket spores</label>
							<sources>
								<li>ItW_RoyalBracket</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seeds/ItW_RoyalBracketSeeds</texPath>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>
						</SeedsPlease.SeedDef>
					</value>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defname="ItW_RawRoyalBracket"]</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs</xpath>
						<value>
							<RecipeDef ParentName="ExtractSeed">
								<defName>ItW_ExtractSeed_RoyalBracket</defName>
								<label>extract royal bracket spores</label>
								<description>Extract spores from royal bracket.</description>
								<ingredients>
									<li>
										<filter>
											<thingDefs>
												<li>ItW_RawRoyalBracket</li>
											</thingDefs>
										</filter>
										<count>4</count>
									</li>
								</ingredients>
								<fixedIngredientFilter>
									<thingDefs>
										<li>ItW_RawRoyalBracket</li>
									</thingDefs>
								</fixedIngredientFilter>
								<products>
									<ItW_RoyalBracketSeed>3</ItW_RoyalBracketSeed>
								</products>
							</RecipeDef>
						</value>
					</match>
					<nomatch Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationAdd">
								<xpath>/Defs</xpath>
								<value>
										<!-- ROYAL BRACKET -->
									<!-- Mushroom that can be made into sauce -->
									<ThingDef ParentName="PlantFoodRawBase">
										<defName>ItW_RawRoyalBracket</defName>
										<label>raw royal bracket</label>
										<description>Raw royal bracket, ready to be cooked into food or processed into royal sauce.</description>
										<graphicData>
											<texPath>Things/Item/Resource/PlantFoodRaw/ItW_RawRoyalBracket</texPath>
											<graphicClass>Graphic_Single</graphicClass>
										</graphicData>
										<statBases>
											<MarketValue>2.0</MarketValue>
											<Mass>0.03</Mass>
										</statBases>
										<ingestible>
											<foodType>VegetableOrFruit</foodType>
											<specialThoughtDirect>AteRawFood</specialThoughtDirect>
										</ingestible>
										<comps>
											<li Class="CompProperties_Rottable">
												<daysToRotStart>10</daysToRotStart>
											</li>
										</comps>
									</ThingDef> 
								</value>
							</li>
							
							<li Class="PatchOperationReplace">
							<xpath>/Defs/ThingDef[defName="ItW_RoyalBracket"]/plant/harvestedThingDef</xpath>
								<value>
									<harvestedThingDef>ItW_RawRoyalBracket</harvestedThingDef>
								</value>
							</li>
							<li Class="PatchOperationAdd">
								<xpath>/Defs</xpath>
								<value>
									<RecipeDef ParentName="ExtractSeed">
										<defName>ItW_ExtractSeed_RoyalBracket</defName>
										<label>extract royal bracket spores</label>
										<description>Extract spores from royal bracket.</description>
										<ingredients>
											<li>
												<filter>
													<thingDefs>
														<li>ItW_RawRoyalBracket</li>
													</thingDefs>
												</filter>
												<count>4</count>
											</li>
										</ingredients>
										<fixedIngredientFilter>
											<thingDefs>
												<li>ItW_RawRoyalBracket</li>
											</thingDefs>
										</fixedIngredientFilter>
										<products>
											<ItW_RoyalBracketSeed>3</ItW_RoyalBracketSeed>
										</products>
									</RecipeDef>
								</value>
							</li>
						</operations>
					</nomatch>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>