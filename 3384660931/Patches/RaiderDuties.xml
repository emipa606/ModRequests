<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationInsert">
		<!-- Before JobGiver_AITrashBuildingsDistant with attackAllInert -->
		<xpath>Defs/DutyDef[defName = "AssaultColony"]/thinkNode[@Class="ThinkNode_Priority"]/subNodes/li[@Class="JobGiver_AITrashBuildingsDistant"]</xpath>
		<order>Append</order>
		<value>
			<li Class="MultiFloors.JobGiver_AssaultColonyThroughStairs"/>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<!-- Before JobGiver_WanderNearBreacher and after JobGiver_AIFightEnemies -->
		<xpath>Defs/DutyDef[defName = "Breaching"]/thinkNode[@Class="ThinkNode_Priority"]/subNodes/li[@Class="JobGiver_WanderNearBreacher"]</xpath>
		<order>Prepend</order>
		<value>
			<li Class="MultiFloors.JobGiver_AssaultColonyThroughStairs"/>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<!-- Before JobGiver_AITrashBuildingsDistant with attackAllInert -->
		<xpath>Defs/DutyDef[defName = "Kidnap"]/thinkNode[@Class="ThinkNode_Priority"]/subNodes/li[@Class="JobGiver_Kidnap"]</xpath>
		<order>Append</order>
		<value>
			<li Class="MultiFloors.JobGiver_TakeKidnapeeToGround"/>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<!-- Before JobGiver_AITrashBuildingsDistant with attackAllInert -->
		<xpath>Defs/DutyDef[defName = "Steal"]/thinkNode[@Class="ThinkNode_Priority"]/subNodes/li[@Class="JobGiver_Steal"]</xpath>
		<order>Append</order>
		<value>
			<li Class="MultiFloors.JobGiver_StealToGround"/>
		</value>
	</Operation>
</Patch>