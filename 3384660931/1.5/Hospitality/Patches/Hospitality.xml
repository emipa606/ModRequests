<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- so they will switch floor -->
			<li Class="PatchOperationInsert" MayRequire="Orion.Hospitality">
				<xpath>Defs/DutyDef[defName = "Relax"]/thinkNode[@Class="ThinkNode_Priority"]/subNodes/li[@Class="Hospitality.ThinkNode_InGuestArea"]</xpath>
				<order>Prepend</order>
				<value>
					<li Class="MultiFloorsHospitalityCompat.JobGiver_GoToLivingMap"/>
				</value>
			</li>
			<!-- configure where they should go -->
			<li Class="PatchOperationInsert" MayRequire="Orion.Hospitality">
				<xpath>Defs/PawnTableDef[defName = "Guests"]/columns/li[6]</xpath>
				<order>Prepend</order>
				<value>
					<li>MF_HospitalityGuestLevel</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>