<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>MF_RFFuelPipeConnector</defName>
		<label>chemfuel pipe connector</label>
		<description>Connect chemfuel pipes on two levels together. (For chemfuel pipe network in Rimefeller)</description>
		<rotatable>false</rotatable>
		<statBases>
      		<WorkToBuild>30</WorkToBuild>
    	</statBases>
		<costList>
            <Steel>15</Steel>
        </costList>
		<researchPrerequisites>
            <li>ChemfuelRefining</li>
		</researchPrerequisites>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<tickerType>Rare</tickerType>
        <neverMultiSelect>true</neverMultiSelect>
        <designationCategory>MultiFloors</designationCategory>
		<constructEffect>ConstructMetal</constructEffect>
		<graphicData>
            <texPath>Buildings/MF_PipeConnector</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(1.25, 1.25)</drawSize>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
            </damageData>
            <drawRotated>false</drawRotated>
            <allowFlip>false</allowFlip>
        </graphicData>
        <comps>
            <li Class="MultiFloorsRimefellerCompat.CompProperties_FuelTrader" />
        </comps>
	</ThingDef>
</Defs>