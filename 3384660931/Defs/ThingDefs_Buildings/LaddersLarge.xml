<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="MF_StairEntranceBase">
		<defName>MF_LadderLUpA</defName>
		<label>large ladder up</label>
		<description>A sturdy, wide ladder for ascending to a higher level. Requires an indoor, roofed room with at least 25 cells. Cannot be built underground or in the same room as another upstairs.</description>
		<rotatable>true</rotatable>
		<size>(1, 2)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>50</costStuffCount>
		<statBases>
      		<WorkToBuild>2000</WorkToBuild>
    	</statBases>
		<uiOrder>3020</uiOrder>
		<graphicData>
			<graphicClass>Graphic_Multi</graphicClass>
			<texPath>Buildings/LadderLarge/LadderLUp</texPath>
			<drawSize>(2, 2)</drawSize>
			<drawOffsetWest>(0, 0, 0.5)</drawOffsetWest>
			<drawOffsetEast>(0, 0, 0.5)</drawOffsetEast>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<isUpstairs>true</isUpstairs>
				<connectedToExitDef>MF_LadderLExitA</connectedToExitDef>
			</li>
		</modExtensions>
	</ThingDef>
	<ThingDef ParentName="MF_StairEntranceBase">
		<defName>MF_LadderLDownA</defName>
		<label>large ladder down</label>
		<description>A sturdy, wide ladder leading to a lower level. Requires an indoor room with at least 25 cells. Cannot be built on upper levels or in the same room as another downstairs.</description>
		<rotatable>true</rotatable>
		<size>(1, 2)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>50</costStuffCount>
		<statBases>
      		<WorkToBuild>2000</WorkToBuild>
    	</statBases>
		<uiOrder>3020</uiOrder>
		<graphicData>
			<graphicClass>Graphic_Multi</graphicClass>
			<texPath>Buildings/LadderLarge/LadderLDown</texPath>
			<drawSize>(2, 2)</drawSize>
			<drawOffsetNorth>(0, 0, -0.1)</drawOffsetNorth>
			<drawOffsetSouth>(0, 0, -0.1)</drawOffsetSouth>
			<drawOffsetWest>(-0.05, 0, 0.45)</drawOffsetWest>
			<drawOffsetEast>(0.05, 0, 0.45)</drawOffsetEast>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<isUpstairs>false</isUpstairs>
				<connectedToExitDef>MF_LadderLExitA</connectedToExitDef>
			</li>
		</modExtensions>
	</ThingDef>
	<ThingDef ParentName="MF_StairExitBase">
		<defName>MF_LadderLExitA</defName>
		<label>large ladder exit</label>
		<description>The endpoint of a large ladder.</description>
		<rotatable>true</rotatable>
		<size>(1, 2)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<graphicData>
			<graphicClass>Graphic_Multi</graphicClass>
			<texPath>Buildings/LadderLarge/LadderLUp</texPath>
			<drawSize>(2, 2)</drawSize>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<upstairsEntranceDef>MF_LadderLUpA</upstairsEntranceDef>
				<downstairsEntranceDef>MF_LadderLDownA</downstairsEntranceDef>
			</li>
		</modExtensions>
	</ThingDef>
</Defs>