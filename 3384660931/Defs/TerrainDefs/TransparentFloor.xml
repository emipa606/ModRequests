<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<TerrainDef ParentName="FloorBase">
		<defName>MF_TransparentFloor</defName>
		<label>transparent floor</label>
		<renderPrecedence>240</renderPrecedence>
		<description>Transparent floor. It reveals the level below. Cannot be built in basements.</description>
		<texturePath>Terrains/TransparentFloor</texturePath>
		<!-- so it replaces the roofed ground -->
		<layerable>false</layerable>
		<edgeType>Fade</edgeType>
		<color>(255,255,255)</color>
		<isPaintable>false</isPaintable>
		<statBases>
			<WorkToBuild>50</WorkToBuild>
			<Beauty>0</Beauty>
		</statBases>
		<uiOrder>3000</uiOrder>
		<pathCost>100000</pathCost>
		<altitudeLayer>Terrain</altitudeLayer>
		<passability>Impassable</passability>
		<avoidWander>true</avoidWander>
		<extinguishesFire>true</extinguishesFire>
		<holdSnow>false</holdSnow>
		<takeFootprints>false</takeFootprints>
		<takeSplashes>false</takeSplashes>
	</TerrainDef>
</Defs>