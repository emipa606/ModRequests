<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ResearchTabDef>
		<defName>ArchotechBionics_ResearchTab</defName>
		<label>Archotech Bionics</label>
	</ResearchTabDef>

    <ResearchProjectDef>
		<defName>ArchotechBionics</defName>
		<label>Archotech Bionics</label>
		<description>Archotech Bionics are notoriously hard to reverse engineer, but should you succeed, a whole new world of Bionic engineering awaits you.</description>
		<baseCost>6000</baseCost>
		<techLevel>Archotech</techLevel>
		<prerequisites>
			<li>AdvancedFabrication</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>0</researchViewX>
		<researchViewY>1</researchViewY>
		<tab>ArchotechBionics_ResearchTab</tab>
	</ResearchProjectDef>
	
	<ResearchProjectDef>
		<defName>ArchotechBionicsAdvancedLimbs</defName>
		<label>Advanced Archotech Limbs</label>
		<description>Regular Archotech Limbs are the most basic form of Archotech Bionics, more advanced and specialised versions exist.</description>
		<baseCost>8000</baseCost>
		<techLevel>Archotech</techLevel>
		<prerequisites>
			<li>ArchotechBionics</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>1</researchViewX>
		<researchViewY>1</researchViewY>
		<tab>ArchotechBionics_ResearchTab</tab>
	</ResearchProjectDef>
	
	<ResearchProjectDef>
		<defName>ArchotechBionicsAdvancedOrgans</defName>
		<label>Advanced Archotech Organs</label>
		<description>Archotech Organs are the peak of Bionic engineering and offer the best performance out of all artificial body parts.</description>
		<baseCost>8000</baseCost>
		<techLevel>Archotech</techLevel>
		<prerequisites>
			<li>ArchotechBionics</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>1</researchViewX>
		<researchViewY>2</researchViewY>
		<tab>ArchotechBionics_ResearchTab</tab>
	</ResearchProjectDef>

	<ResearchProjectDef>
		<defName>ArchotechBionicsAdvancedSkeleton</defName>
		<label>Advanced Archotech Skeletal Enhancement</label>
		<description>Enhancing the Skeletal structure to Archotech standards allows one to use Archotech Limbs to their full potential.</description>
		<baseCost>4000</baseCost>
		<techLevel>Archotech</techLevel>
		<prerequisites>
			<li>ArchotechBionics</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>1</researchViewX>
		<researchViewY>0</researchViewY>
		<tab>ArchotechBionics_ResearchTab</tab>
	</ResearchProjectDef>
	
	<ResearchProjectDef>
		<defName>ArchotechBionicsAdvancedImplants</defName>
		<label>Advanced Archotech Implants</label>
		<description>Archotech Implants enhance the base performance of a body part to almost incomprehnsible standards.</description>
		<baseCost>10000</baseCost>
		<techLevel>Archotech</techLevel>
		<prerequisites>
			<li>ArchotechBionicsAdvancedOrgans</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>2</researchViewX>
		<researchViewY>2</researchViewY>
		<tab>ArchotechBionics_ResearchTab</tab>
	</ResearchProjectDef>

