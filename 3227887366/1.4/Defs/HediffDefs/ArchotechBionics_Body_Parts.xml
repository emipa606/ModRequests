  <!-- Archotech Warrior Leg -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>ArchotechWarriorLeg</defName>
    <label>archotech warrior leg</label>
    <labelNoun>an archotech warrior leg</labelNoun>
    <description>An installed archotech warrior leg.</description>
    <descriptionHyperlinks><ThingDef>ArchotechWarriorLeg</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>ArchotechWarriorLeg</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.5</partEfficiency>
    </addedPartProps>
	<comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>spike</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>22</power> <!-- Same Power, 30% more armor penetration and 0.6 seconds faster than normal knee spike -->
            <armorPenetration>0.69</armorPenetration>
            <cooldownTime>2.0</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
        </tools>
      </li>
    </comps>
	<stages>
      <li>
		<statOffsets>
			<MeleeDodgeChance>5</MeleeDodgeChance>
		</statOffsets>
	  </li>
	</stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartArchotechMakeableBase">
    <defName>ArchotechWarriorLeg</defName>
    <label>archotech warrior leg</label>
    <description>A rare advanced version of the Archotech Leg mainly used by the archotech combat forces in melee combat. At first glance it looks and feels like a regular human leg but it allows for a vastly superior mobility and has an inbuilt monoblade knee spike that pops out at will.</description>
    <descriptionHyperlinks><RecipeDef>InstallArchotechWarriorLeg</RecipeDef></descriptionHyperlinks>
	<thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
	<costList>
     <Plasteel>30</Plasteel>
	 <Hyperweave>20</Hyperweave>
	 <Jade>10</Jade>
	 <Gold>1</Gold>
	 <Uranium>3</Uranium>
     <ComponentSpacer>6</ComponentSpacer>
    </costList>
    <recipeMaker>
      <researchPrerequisite>ArchotechBionicsAdvancedLimbs</researchPrerequisite>
    </recipeMaker>
    <statBases>
      <MarketValue>4800</MarketValue>
	  <WorkToMake>68000</WorkToMake>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
