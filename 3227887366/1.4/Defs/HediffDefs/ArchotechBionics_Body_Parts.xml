  <!-- Archotech Warrior Arm -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>ArchotechWarriorArm</defName>
    <label>archotech warrior arm</label>
    <labelNoun>an archotech warrior arm</labelNoun>
    <description>An installed archotech warrior arm.</description>
    <descriptionHyperlinks><ThingDef>ArchotechWarriorArm</ThingDef></descriptionHyperlinks>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>blade</label>
            <capacities>
              <li>Cut</li>
            </capacities>
            <power>18</power> <!-- Same AP, higher armor penetration and 0.3 seconds faster then regular elbow blade -->
			<armorPenetration>0.69</armorPenetration>
            <cooldownTime>1.7</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
		  <li>
            <label>claw</label>
            <capacities>
              <li>Scratch</li>
            </capacities>
            <power>15</power>  <!-- same AP, higher armor penetration and 0.3 seconds faster than regular hand talon -->
            <armorPenetration>0.35</armorPenetration>
            <cooldownTime>1.2</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>ArchotechWarriorArm</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.5</partEfficiency>
    </addedPartProps>
	<stages>
      <li>
        <statOffsets>
			<RangedCooldownFactor>-0.15</RangedCooldownFactor>
			<ShootingAccuracyPawn>+5</ShootingAccuracyPawn>
			<MeleeHitChance>+5</MeleeHitChance>
		</statOffsets>
      </li>
	</stages>
  </HediffDef>

