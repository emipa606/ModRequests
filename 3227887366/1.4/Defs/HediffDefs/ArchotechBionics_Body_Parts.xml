      <solid>true</solid>
      <partEfficiency>1.5</partEfficiency>
    </addedPartProps>
  </HediffDef>

  <ThingDef ParentName="BodyPartArchotechMakeableBase">
    <defName>ArchotechEar</defName>
    <label>archotech ear</label>
    <description>An Archotech Ear offers an overall enhancement of the regular body part to meet the quality standards of Archotech.</description>
