	<thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
	<costList>
		<Plasteel>15</Plasteel>
		<Jade>5</Jade>
		<Gold>1</Gold>
		<Uranium>1</Uranium>
		<ComponentSpacer>4</ComponentSpacer>
    </costList>
    <statBases>
      <MarketValue>2300</MarketValue>
	  <WorkToMake>50000</WorkToMake>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
