  <ThingCategoryDef>
    <defName>PNRoyalHeadgear</defName>
    <label>Royal Headgear</label>
    <parent>PNHeadgear</parent>
  </ThingCategoryDef>

  <ThingDef Abstract="True" Name="PN_RoyalApparelBase" ParentName="ApparelMakeableBase">
    <apparel>
      <tags Inherit="false">
        <li>PN_RoyalApparel</li>
        <li>Royal</li>
        <li>RoyalTier2</li>
        <li>RoyalTier3</li>
        <li>RoyalTier4</li>
        <li>RoyalTier5</li>
        <li>RoyalTier6</li>
        <li>RoyalTier7</li>
      </tags>
