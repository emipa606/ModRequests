<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<KeyBindingCategoryDef>
		<defName>ColonistTabs</defName>
		<label>Colonist Tabs</label>
		<description>Inventory</description>
	</KeyBindingCategoryDef>
	<KeyBindingDef>
		<category>ColonistTabs</category>
		<defName>ToggleGear</defName>
		<label>Toggles the selected colonists gear tab</label>
		<defaultKeyCodeA>I</defaultKeyCodeA>
	</KeyBindingDef>
	<KeyBindingDef>
		<category>ColonistTabs</category>
		<defName>ToggleBio</defName>
		<label>Toggles the selected colonists bio tab</label>
		<defaultKeyCodeA>B</defaultKeyCodeA>
	</KeyBindingDef>
	<KeyBindingDef>
		<category>ColonistTabs</category>
		<defName>ToggleNeeds</defName>
		<label>Toggles the selected colonists needs tab</label>
		<defaultKeyCodeA>N</defaultKeyCodeA>
	</KeyBindingDef>
	<KeyBindingDef>
		<category>ColonistTabs</category>
		<defName>ToggleHealth</defName>
		<label>Toggles the selected colonists health tab</label>
		<defaultKeyCodeA>H</defaultKeyCodeA>
	</KeyBindingDef>
</Defs>
