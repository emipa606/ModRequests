<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Biomes! Islands</li>
        </mods>
        <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
            <li Class="PatchOperationAdd"> 
                <xpath>/Defs/BiomeDefs[defName="BiomesIslands_TropicalIsland"]/wildAnimals</xpath>
                <value>
                    <li>
         <MD_MilkDragon>0.001</MD_MilkDragon>
                    </li>                        
                </value>
            </li>
            <li Class="PatchOperationAdd"> 
                <xpath>/Defs/BiomeDefs[defName="BiomesIslands_Atoll"]/wildAnimals</xpath>
                <value>
                    <li>
        <MD_MilkDragon>0.001</MD_MilkDragon>
                    </li>                        
                </value>
            </li>
			
        </operations>
        </match>
    </Operation>

</Patch>