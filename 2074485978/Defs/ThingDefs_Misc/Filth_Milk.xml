<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseFilth">
    <defName>TRAHS_Filth_VanillaMilkratMilk</defName>
    <label>vanilla milkrat milk</label>
    <statBases>
      <Beauty>-20</Beauty>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>TRAHS/Things/Filth/Filth_Milk</texPath>
      <color>(200, 200, 200, 100)</color>
	  <drawSize>2</drawSize>
    </graphicData>
    <pathCost>5</pathCost>
    <filth>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>90</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <allowsFire>false</allowsFire>
    </filth>
  </ThingDef>

