<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		
		<!-- rulepacks - ideo global -->	
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerIdeoGlobal"]/rulePack/rulesStrings</xpath>
				<value>
					<li>globalAdjective->Original</li>
					<li>globalAdjective->Traditional</li>
					<li>globalAdjective->Modern</li>
					<li>globalAdjective->Contemporary</li>
					<li>globalAdjective->Western</li>
					<li>globalAdjective->Eastern</li>
					<li>globalAdjective->Northern</li>
					<li>globalAdjective->Southern</li>
					<li>globalAdjective->First</li>
					<li>globalAdjective->Second</li>
					<li>globalAdjective->Late</li>
					<li>globalAdjective->Standard</li>
					<li>globalAdjective->Conventional</li>
					<li>globalAdjective->Common</li>
					<li>globalAdjective->Classical</li>
				</value>
			</li>
			
		<!-- rulepacks - spacer culture -->	
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="CultureSpacer"]/rulePack/rulesStrings</xpath>
				<value>
					<li>globalAdjective->Planetary</li>
					<li>globalAdjective->Galactic</li>
					<li>globalAdjective->Cosmic</li>
					<li>globalHyphenPrefix->cosmo</li>
					<li>globalHyphenPrefix->star</li>
					<li>globalHyphenPrefix->space</li>
				</value>
			</li>
			
		<!-- rulepacks - ideo corunan -->	
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RulePackDef[defName="NamerIdeoCorunan"]/rulePack/rulesStrings</xpath>
				<value>
					<li>globalAdjective(p=15)->Ancestral</li>
					<li>globalAdjective(p=15)->Ceremonial</li>
				</value>
			</li>
			
		</operations>
	</Operation>

</Patch>