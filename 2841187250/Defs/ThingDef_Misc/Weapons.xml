<?xml version="1.0" encoding="utf-8"?>
<Defs>



 <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>MeleeWeapon_AncientSword</defName>
    <label>ancient longsword</label>
    <description>The ancient weapon of Pharaoh, the ancient longsword can be used for slashing or stabbing or showing off.It's very difficult and expensive to make.</description>
    <graphicData>
      <texPath>Things/Weapons/Schwert</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  	  	  <shaderType>CutoutComplex</shaderType>
			    <drawSize>1.4</drawSize>
    </graphicData>
    <techLevel>Medieval</techLevel>
    <costStuffCount>200</costStuffCount>
    <statBases>
      <WorkToMake>25000</WorkToMake>
      <Mass>2</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>25</power>
        <cooldownTime>2.6</cooldownTime>
      </li>
      <li>
        <label>edge</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>25</power>
        <cooldownTime>2.6</cooldownTime>
      </li>
    </tools>
	    <recipeMaker>
				<workSkill>Crafting</workSkill>
      <skillRequirements>
        <Crafting>10</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>CraftingSpot</li>
		<li MayRequire="Sovereign.Samsucksfrozenchicken">Legacy_Bench</li>

      </recipeUsers>
    </recipeMaker>
	
  </ThingDef>
  
  
  
  
  
