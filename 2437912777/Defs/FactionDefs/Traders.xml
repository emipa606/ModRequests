<Defs>

  <TraderKindDef>
    <defName>Caravan_Bee_Wax</defName>
    <label>Wax trader</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>750~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>100~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>RawFlowers</thingDef>
        <countRange>100~850</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>BeeBread</thingDef>
        <countRange>20~60</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>0~10</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Human</thingDef>
        <countRange>0~250</countRange>
      </li>	  
      <li Class="StockGenerator_Category">
        <categoryDef>RB_BeeswaxCategory</categoryDef>
        <thingDefCountRange>2~5</thingDefCountRange>
        <totalPriceRange>1500~8000</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>RB_Honeys</categoryDef>
        <thingDefCountRange>0~3</thingDefCountRange>
        <totalPriceRange>10~200</totalPriceRange>
      </li>	  
      <li Class="StockGenerator_Category">
        <categoryDef>RB_Combs</categoryDef>
        <thingDefCountRange>0~3</thingDefCountRange>
        <totalPriceRange>0.1~3</totalPriceRange>
      </li>		  
      <li Class="StockGenerator_Tag">
        <tradeTag>T1Stinger</tradeTag>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>T2Stinger</tradeTag>
        <thingDefCountRange>0~1</thingDefCountRange>
        <countRange>-1~1</countRange>
      </li>	  	  

      
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
      </li>
      
      <!-- Techprints -->
      <!-- none -->
      
      <!-- Weapons -->
      <!-- none -->
      
      <!-- Apparel -->

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <!-- none -->
      
      <!-- Slaves -->
     <li Class="StockGenerator_Slaves">
        <countRange>0~2</countRange>
      </li>
      
      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>1~2</kindCountRange>
        <countRange>0~2</countRange>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>
