        <thingDefCountRange>3~9</thingDefCountRange>
        <totalPriceRange>300~1500</totalPriceRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Chocolate</li>
          <li>Kibble</li>
        </thingDefs>
      </li>
	  
      <li Class="StockGenerator_Category">
        <categoryDef>RB_BeesAndQueens</categoryDef>
        <thingDefCountRange>0~2</thingDefCountRange>
        <totalPriceRange>0.1~2</totalPriceRange>
      </li>	  
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Human</thingDef>
        <countRange>0~800</countRange>
      </li>	  
      <li Class="StockGenerator_Category">
        <categoryDef>BodyPartsNatural</categoryDef>
        <thingDefCountRange>0~6</thingDefCountRange>
        <totalPriceRange>10~1000</totalPriceRange>
      </li>		  
      <li Class="StockGenerator_Category">
        <categoryDef>RB_BeeswaxCategory</categoryDef>
        <thingDefCountRange>2~5</thingDefCountRange>
        <totalPriceRange>1000~8000</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>RB_Honeys</categoryDef>
        <thingDefCountRange>0~3</thingDefCountRange>
        <totalPriceRange>10~500</totalPriceRange>
      </li>	  
      <li Class="StockGenerator_SingleDef">
        <thingDef>RawFlowers</thingDef>
        <countRange>20~1000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>BeeBread</thingDef>
        <countRange>20~200</countRange>
      </li>	  
      
      <li Class="StockGenerator_Tag">
        <tradeTag>T1Stinger</tradeTag>
        <thingDefCountRange>1~2</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>T2Stinger</tradeTag>
        <thingDefCountRange>0~3</thingDefCountRange>
        <countRange>0~3</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>T3Stinger</tradeTag>
        <thingDefCountRange>0~3</thingDefCountRange>
        <countRange>-2~1</countRange>
      </li>	  
	  
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>400~1000</totalPriceRange>
      </li>
      
      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>2</count>
            <chance>0.5</chance>
          </li>
          <li>
            <count>3</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>
      
      <!-- Weapons-->
      
      <!-- Apparel-->
      
      <!-- Implants -->
      <!-- none -->
      
      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>0~3</thingDefCountRange>
        <countRange>-1~1</countRange>
      </li>

      <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_SingleDef">
        <thingDef>AnimusStone</thingDef>
        <countRange>-3~1</countRange> <!-- 25% chance of appearing -->
      </li>

      <!-- Slaves -->
      <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>1~8</countRange>
      </li>

      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalUncommon</li>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>2~4</kindCountRange>
        <countRange>1~5</countRange>
      </li>

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>  
  
  </Defs>
  