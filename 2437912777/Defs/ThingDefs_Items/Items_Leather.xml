    <description>A rare and high value leather made from the backs of honey badgers. While not good for insulation it is instead higly valued for its high beauty and extreme sharp protection blocking any stings.</description>
    <graphicData>
      <texPath>Things/Items/HoneyBadgerLeather</texPath>	
      <color>(255,255,255)</color>
    </graphicData>
    <statBases>
      <MarketValue>32</MarketValue>
      <StuffPower_Armor_Sharp>8.5</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.40</StuffPower_Armor_Blunt>
      <StuffPower_Insulation_Cold>8</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>10</StuffPower_Insulation_Heat>
    </statBases>
    <minRewardCount>10</minRewardCount>	
    <stuffProps>
      <color>(46,31,31)</color>
      <commonality>0.00005</commonality>
      <statFactors>
        <MaxHitPoints>6.0</MaxHitPoints>
        <Beauty>15</Beauty>
      </statFactors>
    </stuffProps>
  </ThingDef>
  
</Defs>