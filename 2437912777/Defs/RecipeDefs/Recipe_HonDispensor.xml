<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef ParentName="CookMealBase">
    <defName>BG_BlendHonMeal</defName>
    <label>blend horrid honey meal</label>
    <description>Blend a synthetic honey to be spread on bread.</description>
    <jobString>Blending.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>FoodRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
    </ingredients>
    <products>
      <BG_HorridHoneyMeal>1</BG_HorridHoneyMeal>
    </products>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>BG_HoneyPasteDispenser</li>
    </recipeUsers>		
  </RecipeDef>
  
