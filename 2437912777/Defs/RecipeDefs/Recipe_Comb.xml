<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>RB_CentrifugeSmortComb</defName>
		<label>extract from intellectual comb</label>
		<description>Centrifuges a intellectual comb to extract honey, wax and shreds of big brained intelligence.</description>
		<jobString>Extracting honey, wax and brains</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>RB_Honey_Centrifuge</li>	
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RB_Smort_Honeycomb</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>

		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>RB_Smort_Honeycomb</li>

			</thingDefs>
		</fixedIngredientFilter>
		<specialProducts>
			<li>Butchery</li>
		</specialProducts>
		<skillRequirements>
			<Crafting>3</Crafting>
		</skillRequirements>
	</RecipeDef>
	
