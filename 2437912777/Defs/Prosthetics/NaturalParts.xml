<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="BodyPartInsectBase">
    <defName>NaturalBeeWing</defName>
    <label>bee wing</label>
    <description>A biological bee wing.</description>
    <descriptionHyperlinks><RecipeDef>InstallNatBeeWing</RecipeDef></descriptionHyperlinks>
    <statBases>
      <MarketValue>120</MarketValue>
      <Mass>1</Mass>
      <DeteriorationRate>4.0</DeteriorationRate>	  
    </statBases>
    <thingCategories>
      <li>BodyPartsNatural</li>
    </thingCategories>
    <costList>
      <UraniumH>20</UraniumH>
      <RB_Bee_Temperate_Drone>1</RB_Bee_Temperate_Drone>
      <RB_BaseHoney>200</RB_BaseHoney>	  
    </costList>		
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
    <defName>InstallNatBeeWing</defName>
    <label>install bee wing</label>
    <description>Install a biological wing.</description>
    <descriptionHyperlinks><ThingDef>NaturalBeeWing</ThingDef></descriptionHyperlinks>
    <jobString>Installing wing.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>NaturalBeeWing</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>NaturalBeeWing</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>BeeWing</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>
  
  
