<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef ParentName="SurgeryFlesh">
		<defName>Recipe_FuelInjection</defName>
		<label>fuel injection</label>
		<description>Inject fuel into an individual to energize their fuel system. Does nothing if they do not have an active fuel system.</description>
		<jobString>injecting fuel cell into TargetA.</jobString>
		<workAmount>800</workAmount>
		<hideBodyPartNames>true</hideBodyPartNames>
		<targetsBodyPart>false</targetsBodyPart>
		<anesthetize>false</anesthetize>
		<workerClass>GS_Core.Recipe_FuelInjection</workerClass>
		<surgeryOutcomeEffect IsNull="True" /> <!-- Always succeeds -->
		<ingredients>
			<li>
			<filter>
				<thingDefs>
				<li>GS_Fuel</li>
				</thingDefs>
			</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
			<li>GS_Fuel</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>
	
</Defs>
