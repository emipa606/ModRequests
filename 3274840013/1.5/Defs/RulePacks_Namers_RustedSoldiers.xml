<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NAT_NamerRustedPawn</defName>
    <rulePack>
      <rulesStrings>

        <li>nnumber(p=6)->I</li>
        <li>nnumber(p=6)->II</li>
        <li>nnumber(p=4)->III</li>
        <li>nnumber(p=6)->IV</li>
        <li>nnumber(p=6)->V</li>
		   <li>nnumber(p=6)->VI</li>
        <li>nnumber(p=3)->VII</li>
        <li>nnumber(p=2)->VIII</li>
        <li>nnumber(p=4)->IX</li>
        <li>nnumber(p=4)->X</li>
		   <li>nnumber(p=4)->XI</li>
        <li>nnumber(p=2)->XII</li>
        <li>nnumber(p=2)->XIII</li>
        <li>nnumber->XIV</li>
        <li>nnumber(p=2)->XV</li>
		   <li>nnumber->XVI</li>
        <li>nnumber->XVII</li>
		   <li>nnumber->XVIII</li>
        <li>nnumber->XIX</li>
        <li>nnumber->XX</li>
        <li>nnumber->XXI</li>
        <li>nnumber->XXII</li>
        <li>nnumber->XXIII</li>
        <li>nnumber->XXIV</li>
        <li>nnumber->XXV</li>
	 	   <li>nnumber->XXVI</li>

      </rulesStrings>
      <rulesFiles>
        <li>nat_prefix->Names/NAT_RustedSoldier_Prefix</li>
		   <li>nat_postfix->Names/NAT_RustedSoldier_Postfix</li>
		   <li>nat_solid->Names/NAT_RustedSoldier_Solid</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NAT_NamerRustedSoldier</defName>
    <include>
      <li>NAT_NamerRustedPawn</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=10)->[nat_prefix][nat_postfix]</li>
        <li>r_name->[nat_solid]</li>
		   <li>r_name(p=6)->[nat_prefix][nat_postfix] [nnumber]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NAT_NamerRustedDumb</defName>
    <include>
      <li>NAT_NamerRustedPawn</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=10)->[nat_prefix][nat_postfix_dumb]</li>
        <li>r_name(p=3)->[nat_solid]</li>
		   <li>r_name(p=6)->[nat_postfix][nat_prefix]</li>
      </rulesStrings>
      <rulesFiles>
		   <li>nat_postfix_dumb->Names/NAT_RustedSoldier_PostfixDumb</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NAT_NamerRustedOfficer</defName>
    <include>
      <li>NAT_NamerRustedPawn</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=10)->[nat_prefix]-[nat_prefix][nat_postfix]</li>
		   <li>r_name(p=4)->[nat_prefix]-[nat_prefix][nat_postfix] [nnumber]</li>
        <li>r_name(p=6)->[nat_prefix][nat_postfix] [nnumber]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NAT_NamerEliteRust</defName>
    <include>
      <li>NAT_NamerRustedPawn</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=6)->[nat_twisted][nat_postfix]</li>
        <li>r_name(p=5)->[nat_twisted][nat_twisted]</li>
		   <li>r_name(p=7)->[nat_prefix][nat_twisted]</li>
        <li>r_name(p=3)->[nat_twisted][nat_twisted][nat_postfix]</li>
        <li>r_name->[nat_twisted][nat_twisted][nat_twisted]</li>
		   <li>r_name(p=4)->[nat_prefix][nat_twisted][nat_twisted]</li>
      </rulesStrings>
      <rulesFiles>
		   <li>nat_twisted->Names/NAT_Twisted</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
