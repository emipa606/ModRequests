<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThinkTreeDef>
    <defName>NAT_ReworkedCollector</defName>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <!-- Despawned -->
        <li Class="ThinkNode_Subtree">
          <treeDef>Despawned</treeDef>
        </li>

        <!-- Downed -->
        <li Class="ThinkNode_Subtree">
          <treeDef>Downed</treeDef>
        </li>
        <li Class="ThinkNode_Subtree">
          <treeDef>BurningResponse</treeDef>
        </li>

        <li Class="NAT.ThinkNode_ConditionalCollectorState">
          <state>Wait</state>
          <subNodes>
            <li Class="NAT.JobGiver_CollectorWait_Reworked" />
          </subNodes>
        </li>
        <li Class="NAT.ThinkNode_ConditionalCollectorState">
          <state>Collect</state>
          <subNodes>
            <li Class="NAT.JobGiver_CollectorSteal" />
          </subNodes>
        </li>
        <li Class="NAT.ThinkNode_ConditionalCollectorState">
          <state>Escape</state>
          <subNodes>
            <li Class="NAT.JobGiver_CollectorEscape" />
          </subNodes>
        </li>
        <li Class="NAT.ThinkNode_ConditionalCollectorState">
          <state>Rage</state>
          <subNodes>
            <li Class="NAT.JobGiver_CollectorRage" />
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
</Defs>