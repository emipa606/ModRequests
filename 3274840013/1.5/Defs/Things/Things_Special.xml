      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/NAT_RustedSculpture</texPath>
	  <drawSize>(2.8,2.8)</drawSize>
    </graphicData>
    <fillPercent>0.45</fillPercent>
    <rotatable>false</rotatable>
    <tradeability>None</tradeability>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>50</Mass>
      <Beauty>-5</Beauty>
    </statBases>
    <comps Inherit="False">
      <!-- From ArtBuildingBase to override art comp -->
      <li>
        <compClass>CompQuality</compClass>
      </li>
      <li Class="CompProperties_MeditationFocus">
        <statDef>MeditationFocusStrength</statDef>
        <focusTypes>
          <li>Artistic</li>
        </focusTypes>
        <offsets>
          <li Class="FocusStrengthOffset_Quality">
            <curve>
              <points>
                <li>(0,0.12)</li> <!-- awful -->
                <li>(1,0.16)</li> <!-- poor -->
                <li>(2,0.20)</li> <!-- normal -->
                <li>(3,0.22)</li> <!-- good -->
                <li>(4,0.24)</li> <!-- excellent -->
                <li>(5,0.26)</li> <!-- masterwork -->
                <li>(6,0.28)</li> <!-- legendary -->
              </points>
            </curve>
          </li>
          <li Class="FocusStrengthOffset_BuildingDefsWithQuality">
            <defs>
              <li>SculptureSmall</li>
              <li>SculptureLarge</li>
              <li>SculptureGrand</li>
            </defs>
            <radius>9.9</radius>
            <maxBuildings>8</maxBuildings>
            <focusPerQuality>
              <points>
                <li>(0,0.0)</li> <!-- awful -->
                <li>(1,0.0)</li> <!-- poor -->
                <li>(2,0.01)</li> <!-- normal -->
                <li>(3,0.01)</li> <!-- good -->
                <li>(4,0.01)</li> <!-- excellent -->
                <li>(5,0.02)</li> <!-- masterwork -->
                <li>(6,0.02)</li> <!-- legendary -->
              </points>
            </focusPerQuality>
            <explanationKey>MeditationFocusPerSculpture</explanationKey>
            <explanationKeyAbstract>MeditationFocusPerSculptureAbstract</explanationKeyAbstract>
          </li>
        </offsets>
      </li>
      <li Class="CompProperties_Art">
        <nameMaker>NAT_NamerArtRustedSculpture</nameMaker>
        <descriptionMaker>NAT_ArtDescription_RustedSculpture</descriptionMaker>
        <canBeEnjoyedAsArt>true</canBeEnjoyedAsArt>
      </li>
    </comps>
    <recipeMaker>
      <researchPrerequisite>NAT_RustedSoldiers</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Artistic</workSkill>
      <unfinishedThingDef>UnfinishedSculpture</unfinishedThingDef>
      <effectWorking>Sculpt</effectWorking>
      <soundWorking>Recipe_Sculpt</soundWorking>
      <recipeUsers>
