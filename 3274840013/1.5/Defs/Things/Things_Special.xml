  <ThingDef ParentName="BuildingBase">
    <defName>NAT_RustedSculpture_Officer</defName>
    <label>rusted sculpture(officer)</label>
	<genericMarketSellable>false</genericMarketSellable>
	<tradeability>None</tradeability>
	<minifiedDef>MinifiedThing</minifiedDef>
	<thingClass>Building_Art</thingClass>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <useHitPoints>true</useHitPoints>
    <rotatable>false</rotatable>
    <thingCategories>
      <li>BuildingsArt</li>
    </thingCategories>
	<inspectorTabs>
      <li>ITab_Art</li>
    </inspectorTabs>
	<tradeTags>
      <li>Art</li>
    </tradeTags>
    <statBases>
	  <MaxHitPoints>270</MaxHitPoints>
      <Beauty>-10</Beauty>
      <Flammability>1.0</Flammability>
      <MeditationFocusStrength>0.0</MeditationFocusStrength>
      <Mass>170</Mass>
    </statBases>
	<fillPercent>0.55</fillPercent>
	<size>(2, 2)</size>
	<graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/NAT_RustedSculpture_Large</texPath>
	  <drawSize>(3,3)</drawSize>
    </graphicData>
    <comps>
      <li Class="NAT.CompProperties_RustedSculpture">
        <pawnKind>NAT_RustedFieldMarshal</pawnKind>
		<activeTicks>1</activeTicks>
        <ticksToActivate>210</ticksToActivate>
        <activateTexPath>UI/NAT_ActivateSoldier</activateTexPath>
        <jobString>actvate</jobString>
		<activateLabelString>Order activating</activateLabelString>
		<activateDescString>Order a pawn to activate rusted soldier.</activateDescString>
		<guiLabelString>Choose who should activate rusted soldier.</guiLabelString>
		<activatingString>activating rusted soldier</activatingString>
		<activatingStringPending>activating rusted soldier</activatingStringPending>
        <showMustBeActivatedByColonist>false</showMustBeActivatedByColonist>
        <targetingParameters>
          <canTargetBuildings>false</canTargetBuildings>
          <canTargetAnimals>false</canTargetAnimals>
          <canTargetMechs>false</canTargetMechs>
          <onlyTargetControlledPawns>true</onlyTargetControlledPawns>
        </targetingParameters>
      </li>
      <li>
        <compClass>CompQuality</compClass>
      </li>
      <li Class="CompProperties_MeditationFocus">
        <statDef>MeditationFocusStrength</statDef>
        <focusTypes>
          <li>Artistic</li>
        </focusTypes>
        <offsets>
          <li Class="FocusStrengthOffset_Quality">
            <curve>
              <points>
                <li>(0,0.12)</li> <!-- awful -->
                <li>(1,0.16)</li> <!-- poor -->
                <li>(2,0.20)</li> <!-- normal -->
                <li>(3,0.22)</li> <!-- good -->
                <li>(4,0.24)</li> <!-- excellent -->
                <li>(5,0.26)</li> <!-- masterwork -->
                <li>(6,0.28)</li> <!-- legendary -->
              </points>
            </curve>
          </li>
          <li Class="FocusStrengthOffset_BuildingDefsWithQuality">
            <defs>
              <li>SculptureSmall</li>
              <li>SculptureLarge</li>
              <li>SculptureGrand</li>
            </defs>
            <radius>9.9</radius>
            <maxBuildings>8</maxBuildings>
            <focusPerQuality>
              <points>
                <li>(0,0.0)</li> <!-- awful -->
                <li>(1,0.0)</li> <!-- poor -->
                <li>(2,0.01)</li> <!-- normal -->
                <li>(3,0.01)</li> <!-- good -->
                <li>(4,0.01)</li> <!-- excellent -->
                <li>(5,0.02)</li> <!-- masterwork -->
                <li>(6,0.02)</li> <!-- legendary -->
              </points>
            </focusPerQuality>
            <explanationKey>MeditationFocusPerSculpture</explanationKey>
            <explanationKeyAbstract>MeditationFocusPerSculptureAbstract</explanationKeyAbstract>
          </li>
        </offsets>
      </li>
      <li Class="CompProperties_Art">
        <nameMaker>NAT_NamerArtRustedSculpture</nameMaker>
        <descriptionMaker>NAT_ArtDescription_RustedSculpture</descriptionMaker>
        <canBeEnjoyedAsArt>true</canBeEnjoyedAsArt>
      </li>
    </comps>
    <costList>
      <Bioferrite>210</Bioferrite>
    </costList>
  </ThingDef>
  
