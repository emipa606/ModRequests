<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NAT_NamerArtRustedSculpture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <!-- General -->
        <li>r_art_name->[AdjectiveAny] [soldierName]</li>
        <li>r_art_name->[adjectiveRusted] Sculpture</li>
		<li>r_art_name->[adjectiveRusted] [soldierName]</li>
        <li>r_art_name->[soldierName] of [ConceptAny]</li>

        <!-- Adjectives for Unsettling -->
        <li>soldierName->Soldier</li>
		<li>soldierName->Warrior</li>
		<li>soldierName->Fighter</li>
		<li>soldierName->Infantryman</li>
		<li>soldierName->Officer</li>
		<li>soldierName->Gunner</li>
        
        <li>adjectiveRusted->bioferrite</li>
		<li>adjectiveRusted->rusted</li>
		<li>adjectiveRusted->old</li>
		<li>adjectiveRusted->creepy</li>
		<li>adjectiveRusted->clunking</li>
		<li>adjectiveRusted->rusty</li>
		<li>adjectiveRusted->tough</li>
		<li>adjectiveRusted->warped</li>
		<li>adjectiveRusted->twisted</li>
		<li>adjectiveRusted->corrupted</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NAT_ArtDescription_RustedSculpture</defName>
    <rulePack>
      <rulesStrings>
        <li>r_art_description->This [voidStrange] sculpture looks like it thinks about [voidDesc]</li>

        <li>voidStrange->strange</li>
        <li>voidStrange->disturbing</li>
        <li>voidStrange->unsettling</li>
        <li>voidStrange->frightening</li>
        <li>voidStrange->twisted</li>
        <li>voidStrange->misshapen</li>
        <li>voidStrange->inhuman</li>
        <li>voidStrange->weird</li>

        <li>voidDesc->war without a reason.</li>
        <li>voidDesc->countless battles under the mountains.</li>
        <li>voidDesc->rusted fortress covered in dark spikes of terror.</li>
        <li>voidDesc->fight between them and us.</li>
        <li>voidDesc->gorgeous rust on it.</li>
        <li>voidDesc->pain of billions.</li>
        <li>voidDesc->void ocean in the rusted jar.</li>
        <li>voidDesc->strange mass in a basement.</li>
        <li>voidDesc->two of them playing poker.</li>
        <li>voidDesc->cut off limbs.</li>
        <li>voidDesc->shape of G.O.D.</li>
        <li>voidDesc->dust in the sky.</li>
		<li>voidDesc->it.</li>
		<li>voidDesc->hundreds of tiny frogs nearby.</li>
		<li>voidDesc->freedom.</li>

      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>
