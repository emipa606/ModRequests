<?xml version="1.0" encoding="utf-8" ?>
<Defs>


      <ThingDef ParentName="ResourceBase">
    <defName>Parthive</defName>
    <label>Hive piece</label>
    <description>A part of an insectoid hive. Can no longer create bugs, but still partially functional and may be used to produce insect jelly.</description>
		<tickerType>Normal</tickerType>
    <graphicData>
      <texPath>JF/hivesost</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>25</stackLimit>
		<statBases>
      <MaxHitPoints>45</MaxHitPoints>
      <MarketValue>20</MarketValue>
      <Mass>2</Mass>
      <Flammability>1.0</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  
        <ThingDef ParentName="ResourceBase">
    <defName>Partpod</defName>
    <label>Glow pod part</label>
    <description>A part of a glow pod. Can be used to construct an insect jelly farm.</description>
		<tickerType>Normal</tickerType>
    <graphicData>
      <texPath>JF/glowsost</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>25</stackLimit>
		<statBases>
      <MaxHitPoints>30</MaxHitPoints>
      <MarketValue>75</MarketValue>
      <Mass>1</Mass>
      <Flammability>1.0</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
  
</Defs>
