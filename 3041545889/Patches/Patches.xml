<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- Heavy Mech Gestation Recipe Patches -->

<Operation Class="PatchOperationAdd">
    <xpath>/Defs</xpath>
  <value>
  <RecipeDef ParentName="HeavyMechanoidRecipe">
    <defName>SF_Dinergate</defName>
    <label>gestate Dinergate</label>
    <description>Gestate a Sangvis Ferri Dinergate.</description>
    <gestationCycles>1</gestationCycles>
    <researchPrerequisite>BasicMechtech</researchPrerequisite>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>40</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreBasic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <SF_Dinergate>1</SF_Dinergate>
    </products>
    <descriptionHyperlinks>
      <ThingDef>SF_Dinergate</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>
	  </value>
</Operation>

<Operation Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="MechGestator"]/recipes</xpath>
  <order>Prepend</order>
  <value>
      <li>SF_Dinergate</li>
  </value>
</Operation>


	<Operation Class="PatchOperationInsert">
		<xpath>Defs/RecipeDef[defName="ResurrectLightMech"]/fixedIngredientFilter/thingDefs/li</xpath>
		<value>
			<li>Corpse_SF_Dinergate</li>
		</value>
	</Operation>
	
	
	
	
	
	
	
	
	
	
