<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Militor - blaster-->
  <RecipeDef ParentName="LightMechanoidRecipe">
    <defName>Dinergate</defName>
    <label>gestate Sangvis Ferri Dinergate</label>
    <description>Gestate a Dinergate bot.</description>
    <researchPrerequisite>StandardMechtech</researchPrerequisite>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <products>
      <SF_Dinergate>1</SF_Dinergate>
    </products>
    <descriptionHyperlinks>
      <ThingDef>SF_Dinergate</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>
  
  
  <!-- SF tarantula-->
  <RecipeDef ParentName="LightMechanoidRecipe">
    <defName>SF_Tarantula</defName>
    <label> gestate Sangvis Ferri Tarantula</label>
    <description>Gestate a Sangivs Ferri Tarantula.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>40</count>
      </li>
    </ingredients>
    <products>
      <SF_Tarantula>1</SF_Tarantula>
    </products>
    <descriptionHyperlinks>
      <ThingDef>SF_Tarantula</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>
  
  
</Defs>