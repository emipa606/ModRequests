<?xml version="1.0" encoding="utf-8"?>

<Patch>

    <!-- Add robots to each trader carrying industrial components. -->

    <Operation Class="PatchOperationAdd">

        <xpath>Defs/TraderKindDef/stockGenerators[li/thingDef="ComponentIndustrial"]</xpath>

        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>MakeshiftBuilderStation</thingDef>
                <countRange>-4~3</countRange>
            </li>

            <li Class="StockGenerator_SingleDef">
                <thingDef>MakeshiftCleanerStation</thingDef>
                <countRange>-4~3</countRange>
            </li>

            <li Class="StockGenerator_SingleDef">
                <thingDef>MakeshiftHaulerStation</thingDef>
                <countRange>-4~3</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>CapMaidBotStation</thingDef>
                <countRange>-4~3</countRange>
            </li>
        </value>

    </Operation>




</Patch>