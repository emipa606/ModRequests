	<!-- Ambrosia -->


  <RecipeDef>
    <defName>MakeAmbrosiaExtract</defName>
    <label>make ambrosia extract</label>
    <description>Extract the active ingredients from ambrosia.</description>
    <jobString>Making ambrosia extract.</jobString>
    <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
    <workSkill>Intellectual</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>75</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Ambrosia</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Ambrosia</li>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <AmbrosiaExtract>1</AmbrosiaExtract>
    </products>
    <recipeUsers>
        <li>DrugLab</li>
    </recipeUsers>
    <researchPrerequisite>Social</researchPrerequisite>
  </RecipeDef>

