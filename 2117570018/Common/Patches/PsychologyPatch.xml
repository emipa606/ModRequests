<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[1.1] Psychology (unofficial)</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!--  Go Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Go"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>Sedentary</li></value>
				</li>
				
				<!--  Shoot Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Shoot"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>BleedingHeart</li></value>
				</li>
				
				<!--  Brawl Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Brawl"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>BleedingHeart</li></value>
				</li>
				
				<!--  Inspired Fighting  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Fighting"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>BleedingHeart</li></value>
				</li>
				
				<!--  Inspired Kindness  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Kindness"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>BleedingHeart</li><li>Socialite</li></value>
				</li>
				
				<!--  Inspired_Recruitment  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Recruitment"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>BleedingHeart</li></value>
				</li>
				
				<!--  Inspired_Mining  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Mining"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>Photosensitive</li></value>
				</li>
				
				<!--  Inspired_Cooking  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Cooking"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>Gourmet</li></value>
				</li>
				
				<!--  Inspired_Foraging  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Foraging"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>Outdoorsy</li></value>
				</li>
				
				<!--  Inspired_Travelling  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Travelling"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>Outdoorsy</li><li>Pluviophile</li></value>
				</li>
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Travelling"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>Photosensitive</li><li>Sedentary</li></value>
				</li>
				
				<!--  Focus_PsychicSoothe  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Focus_PsychicSoothe"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>BleedingHeart</li></value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>	