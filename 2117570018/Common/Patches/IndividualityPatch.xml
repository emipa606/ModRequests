<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[SYR] Individuality</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<!--  Go Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Go"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Agile</li></value>
				</li>
				
				<!--  Work Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Work"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Architect</li><li>SYR_HandEyeCoordination</li></value>
				</li>
				
				<!--  Shoot Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Shoot"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_GunNut</li><li>SYR_HandEyeCoordination</li></value>
				</li>
				
				<!--  Brawl Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Brawl"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Agile</li><li>SYR_HandEyeCoordination</li></value>
				</li>
				
				<!--  Inspired Fighting  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Fighting"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_HandEyeCoordination</li></value>
				</li>
				
				<!--  Inspired_Recruitment  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Recruitment"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Haggler</li></value>
				</li>

				<!--  Inspired_Trade  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Trade"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Haggler</li></value>
				</li>
				
				<!--  Inspired_Mining  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Mining"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_KeenEye</li></value>
				</li>
							
				<!--  Inspired_PlantWork  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_PlantWork"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_GreenThumb</li></value>
				</li>
				
				<!--  Inspired_Research  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Research"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_CreativeThinker</li></value>
				</li>
				
				<!--  Inspired_Learning  -->
				<li Class="PatchOperationRemove">
						<xpath>Defs/InspirationDef[defName="Inspired_Learning"]/statFactors/GlobalLearningFactor</xpath>
				</li>
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Learning"]/statOffsets</xpath>
						<value><GlobalLearningFactor>2</GlobalLearningFactor></value>
				</li>

				<!--  Inspired_Creativity  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Creativity"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Perfectionist</li><li>SYR_Architect</li><li>SYR_CreativeThinker</li></value>
				</li>
				
				<!--  Inspired_Hunting  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Hunting"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_Agile</li></value>
				</li>

				<!--  Inspired_Ranching  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Ranching"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_AnimalAffinity</li></value>
				</li>
				
				<!--  Inspired_Taming  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Taming"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_AnimalAffinity</li></value>
				</li>
				
				<!--  Inspired_Surgery  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Surgery"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_SteadyHands</li></value>
				</li>
				
				<!--  Inspired_Foraging  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Foraging"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_GreenThumb</li></value>
				</li>
				
				<!--  Inspired_Travelling  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Travelling"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>SYR_StrongBack</li></value>
				</li>
			
				
			</operations>
		</match>
	</Operation>
</Patch>	