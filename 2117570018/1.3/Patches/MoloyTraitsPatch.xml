<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Moloy Traits</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!--  Go Frenzy  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Frenzy_Go"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Peppy</li></value>
				</li>
				
				<!--  Inspired Kindness  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Kindness"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Charismatic</li><li>MT_Eccentric</li></value>
				</li>
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Kindness"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>MT_Petty</li><li>MT_Hermit</li></value>
				</li>
				
				<!--  Inspired_Recruitment  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Recruitment"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Charismatic</li></value>
				</li>
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Recruitment"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>MT_Hermit</li></value>
				</li>
				
				<!--  Inspired_Trade  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Trade"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Charismatic</li></value>
				</li>
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Trade"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/negativeTraitDefs</xpath>
						<value><li>MT_Hermit</li></value>
				</li>
				
				<!--  Inspired_PlantWork  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_PlantWork"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Hermit</li></value>
				</li>
				
				<!--  Inspired_Research  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Research"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Eccentric</li></value>
				</li>
				
				<!--  Inspired_Creativity  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Creativity"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Eccentric</li><li>MT_Volatile_Crafter</li></value>
				</li>
				
				<!--  Inspired_Foraging  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Foraging"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Hermit</li></value>
				</li>
				
				<!--  Inspired_Travelling  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationDef[defName="Inspired_Travelling"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Peppy</li><li>MT_Venturous</li></value>
				</li>
				
				<!--  Inspired_Leadership  -->
				<li Class="PatchOperationAdd">
						<xpath>Defs/InspirationTweaks.InspirationWithCompsDef[defName="Inspired_Leadership"]/modExtensions/li[@Class="InspirationTweaks.InspirationDefExtension"]/positiveTraitDefs</xpath>
						<value><li>MT_Charismatic</li><li>MT_Conductor</li><li>MT_Commander</li><li>MT_Tyrant</li></value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>	