<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <TerrainDef ParentName="FloorBase">
    <defName>HyperWeaveFloor</defName>
    <label>Hyper Weave Floor</label>
    <description>Have an excess of Hyperweave you can't sell? It looks great on the floor too. Less flamable and stays much cleaner than normal carpets.</description>
    <texturePath>Terrain/Surfaces/HyperFloor</texturePath>
	<researchPrerequisites><li>CarpetMaking</li></researchPrerequisites>
    <edgeType>Hard</edgeType>
    <renderPrecedence>240</renderPrecedence>
    <burnedDef>BurnedHyperFloor</burnedDef>
    <pathCost>1</pathCost>
    <statBases>
      <Beauty>3</Beauty>
      <Cleanliness>0.3</Cleanliness>
      <WorkToBuild>800</WorkToBuild>
      <Flammability>0.1</Flammability>
    </statBases>
    <costList>
      <Hyperweave>10</Hyperweave>
    </costList>
    <designationHotKey>Misc6</designationHotKey>
  </TerrainDef>
  
  
  <TerrainDef ParentName="BurnedFloorBase">
    <defName>BurnedHyperFloor</defName>
    <label>burned hyper floor</label>
    <renderPrecedence>201</renderPrecedence>
    <description>Burned-out hyperweave flooring.</description>
    <texturePath>Terrain/Surfaces/BurnedHyperFloor</texturePath>
    <edgeType>FadeRough</edgeType>
  </TerrainDef>
  

</Defs>