<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ======= Basic quality thoughts ========= -->

  <ThoughtDef>
    <defName>CQ_AteExcellentFood</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate excellent food</label>
        <description>I ate some excellent produce. Yum! </description>
        <baseMoodEffect>3</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

  <ThoughtDef>
    <defName>CQ_AteGoodFood</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate good food</label>
        <description>I ate some really tasty produce.</description>
        <baseMoodEffect>2</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

  <ThoughtDef>
    <defName>CQ_AtePoorFood</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate poor food</label>
        <description>I ate some overripe produce. Yuck!</description>
        <baseMoodEffect>-2</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
	
  <ThoughtDef>
    <defName>CQ_AteAwfulFood</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate awful meal</label>
        <description>I ate produce that was picked too soon and wasn't ready to be eaten. It tasted awful!</description>
        <baseMoodEffect>-4</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

</Defs>
