<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   <!-- ============================ Tribal Villager Base ========================== -->
   <!-- Poverty tier - penitent -->
   <PawnKindDef Name="WoWTribalPenitentBase" Abstract="True">
      <defName>WoWTribalPenitentBase</defName>
      <label>penitent</label>
      <combatPower>35</combatPower>
	  <defaultFactionType>TribeCivil</defaultFactionType>
      <chemicalAddictionChance>0.05</chemicalAddictionChance>
      <invNutrition>2.55</invNutrition>
      <invFoodDef>Pemmican</invFoodDef>
      <apparelTags>
         <li>Neolithic</li>
      </apparelTags>
      <itemQuality>Poor</itemQuality>
      <gearHealthRange>
         <min>0.2</min>
         <max>1.1</max>
      </gearHealthRange>
      <apparelMoney>
         <min>50</min>
         <max>100</max>
      </apparelMoney>
      <weaponMoney>
         <min>90</min>
         <max>150</max>
      </weaponMoney>
      <weaponTags>
         <li>NeolithicMeleeBasic</li>
      </weaponTags>
      <techHediffsMoney>
         <min>50</min>
         <max>50</max>
      </techHediffsMoney>
      <techHediffsTags>
         <li>Poor</li>
      </techHediffsTags>
      <techHediffsChance>0.15</techHediffsChance>
		<initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
   </PawnKindDef>
   <!-- Low tier - archer -->
   <PawnKindDef Name="WoWTribalArcherBase" Abstract="True">
      <defName>WoWTribalArcherBase</defName>
      <label>archer</label>
      <combatPower>48</combatPower>
	  <defaultFactionType>TribeCivil</defaultFactionType>
      <chemicalAddictionChance>0.05</chemicalAddictionChance>
      <invNutrition>2.55</invNutrition>
      <invFoodDef>Pemmican</invFoodDef>
      <apparelTags>
         <li>Neolithic</li>
      </apparelTags>
      <itemQuality>Poor</itemQuality>
      <gearHealthRange>
         <min>0.5</min>
         <max>1.8</max>
      </gearHealthRange>
      <apparelMoney>
         <min>180</min>
         <max>350</max>
      </apparelMoney>
      <weaponMoney>
         <min>80</min>
         <max>80</max>
      </weaponMoney>
      <weaponTags>
         <li>NeolithicRangedBasic</li>
      </weaponTags>
      <techHediffsMoney>
         <min>50</min>
         <max>50</max>
      </techHediffsMoney>
      <techHediffsTags>
         <li>Poor</li>
      </techHediffsTags>
      <techHediffsChance>0.03</techHediffsChance>
		<initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
   </PawnKindDef>
   <!-- Mid tier - warrior, hunter, trader -->
   <PawnKindDef Name="WoWTribalWarriorBase" Abstract="True">
      <defName>WoWTribalWarriorBase</defName>
      <label>warrior</label>
      <combatPower>60</combatPower>
	  <defaultFactionType>TribeCivil</defaultFactionType>
      <chemicalAddictionChance>0.05</chemicalAddictionChance>
      <invNutrition>2.55</invNutrition>
      <invFoodDef>Pemmican</invFoodDef>
      <apparelTags>
         <li>Neolithic</li>
      </apparelTags>
      <itemQuality>Poor</itemQuality>
      <canBeSapper>true</canBeSapper>
      <gearHealthRange>
         <min>0.5</min>
         <max>1.8</max>
      </gearHealthRange>
      <apparelMoney>
         <min>200</min>
         <max>300</max>
      </apparelMoney>
      <apparelRequired>
         <li>Apparel_WarVeil</li>
      </apparelRequired>
      <weaponMoney>
         <min>150</min>
         <max>150</max>
      </weaponMoney>
      <weaponTags>
         <li>NeolithicMeleeDecent</li>
      </weaponTags>
		<initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
   </PawnKindDef>
   
