<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerLapelli</defName>
    <rulePack>
      <rulesFiles>
        <li>malename->LapelliNames/malename</li>
        <li>femalename->LapelliNames/femalename</li>
        <li>lastname->LapelliNames/lastname</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NamerPersonLapelliMale</defName>
    <include>
      <li>NamerLapelli</li>
    </include>
    <rulePack>
		<rulesStrings>
			<li>r_name->[lastname] [malename]</li>
		</rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonLapelliFemale</defName>
    <include>
      <li>NamerLapelli</li>
    </include>
    <rulePack>
		<rulesStrings>
			<li>r_name->[lastname] [femalename]</li>
		</rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>