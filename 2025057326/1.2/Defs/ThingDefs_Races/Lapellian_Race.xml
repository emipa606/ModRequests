							</north>
							<east>
								<bodyTypes>
								<Thin>(-0.3,0.18)</Thin>
								<Male>(-0.3,0.18)</Male>
								<Female>(-0.3,0.18)</Female>
								</bodyTypes>
							</east>
							</offsets>
						</li>
						<li>
							<path>Things/Lapelli/wings/RWing</path>
							<bodyPart>Torso</bodyPart>
							<inFrontOfBody>false</inFrontOfBody>
							<drawnInBed>false</drawnInBed>
							<offsets>
							<south>
								<bodyTypes>
								<Thin>(-0.38,0.24)</Thin>
								<Male>(-0.38,0.24)</Male>
								<Female>(-0.38,0.24)</Female>
								</bodyTypes>
							</south>
							<north>
								<bodyTypes>
								<Thin>(-0.04,0.48)</Thin>
								<Male>(-0.04,0.48)</Male>
