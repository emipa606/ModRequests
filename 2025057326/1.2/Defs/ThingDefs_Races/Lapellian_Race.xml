							<path>Things/Lapelli/Tails/TailD</path>
							<bodyPart>Torso</bodyPart>
							<inFrontOfBody>false</inFrontOfBody>
							<drawnInBed>false</drawnInBed>
							<offsets>
							<south>
								<bodyTypes>
								<Thin>(-0.4,0.08)</Thin>
								<Male>(-0.4,0.08)</Male>
								<Female>(-0.4,0.08)</Female>
