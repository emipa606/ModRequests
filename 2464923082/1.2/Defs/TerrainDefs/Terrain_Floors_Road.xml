<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef ParentName="FloorBase">
    <defName>ATK_PackedDirt</defName>
    <label>packed dirt</label>
    <renderPrecedence>250</renderPrecedence>
    <description>Dirt packed together to create a path.</description>
    <texturePath>Terrain/Surfaces/PackedDirt</texturePath>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <statBases>
      <WorkToBuild>100</WorkToBuild>
      <Beauty>-1</Beauty>
      <Cleanliness>-1</Cleanliness>
    </statBases>
    <constructEffect>ConstructDirt</constructEffect>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <takeFootprints>true</takeFootprints>
  </TerrainDef>

