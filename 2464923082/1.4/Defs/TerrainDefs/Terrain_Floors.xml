	<TerrainDef ParentName="FloorBase">
		<defName>ATK_DuckboardFloor</defName>
		<label>duckboard floor</label>
		<renderPrecedence>249</renderPrecedence>
		<description>A surface consisting of wooden slats joined together, commonly used to form a walkable surface over mud and dirt. Quick to build, extremely flammable and somewhat ugly.</description>
		<texturePath>Terrain/Surfaces/DuckboardFloor</texturePath>
		<burnedDef>BurnedWoodPlankFloor</burnedDef>
		<!--<burnedDef>BurnedDuckboardFloor</burnedDef>-->
		<statBases>
			<WorkToBuild>85</WorkToBuild>
			<Flammability>0.22</Flammability>
			<Beauty>-1</Beauty>
			<Cleanliness>-1</Cleanliness>
		</statBases>
		<costList>
			<WoodLog>1</WoodLog>
		</costList>
		<constructEffect>ConstructWood</constructEffect>
		<terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
	</TerrainDef>

