<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<TerrainDef ParentName="FloorBase">
		<defName>ATK_Asphalt</defName>
		<label>asphalt</label>
		<description>A tarry substance produced from chemfuel that makes for a quick-setting surface for roads and outdoor walkways. Not very pretty. Deconstructing asphalt yields no resources.</description>
		<texturePath>Terrain/Surfaces/Asphalt</texturePath>
		<resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
		<renderPrecedence>160</renderPrecedence>
		<statBases>
			<Beauty>0</Beauty>
			<Cleanliness>-1</Cleanliness>
			<WorkToBuild>100</WorkToBuild>
		</statBases>
		<costList>
			<Chemfuel>1</Chemfuel>
		</costList>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
		<filthAcceptanceMask>
			<li>Unnatural</li>
		</filthAcceptanceMask>
	</TerrainDef>

	<TerrainDef ParentName="FloorBase">
		<defName>ATK_FineWoodPlankFloor</defName>
		<label>fine wood floor</label>
		<renderPrecedence>250</renderPrecedence>
		<description>Wooden flooring assembled from precisely cut wood for a more beautiful, patterned appearance. Takes much longer to build and requires more material than normal wood floor but is just as flammable.</description>
		<texturePath>Terrain/Surfaces/FineWoodFloorA</texturePath>
		<burnedDef>ATK_BurnedFineWoodPlankFloor</burnedDef>
		<color>(108,78,55)</color>
		<isPaintable>true</isPaintable>
		<statBases>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>0.22</Flammability>
			<Beauty>2</Beauty>
		</statBases>
		<costList>
			<WoodLog>10</WoodLog>
		</costList>
		<constructEffect>ConstructWood</constructEffect>
		<designatorDropdown>Floor_Wood_Fine</designatorDropdown>
		<tags>
			<li>FineFloor</li>
		</tags>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</TerrainDef>

