<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Author: TedDraws -->
	
	<!-- Removes WoodLog from torches refueling -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef/comps/li[@Class="CompProperties_Refuelable" and fuelFilter/thingDefs/li[.="WoodLog"]]/fuelFilter/thingDefs/li[.="WoodLog"]</xpath>
			<value>
				<li>WoodLog_Birch</li>
				<li>WoodLog_Cecropia</li>
				<li>WoodLog_Cocoa</li>
				<li>WoodLog_Cypress</li>
				<li>WoodLog_Drago</li>
				<li>WoodLog_Maple</li>
				<li>WoodLog_Oak</li>
				<li>WoodLog_Pine</li>
				<li>WoodLog_Palm</li>
				<li>WoodLog_Poplar</li>
				<li>WoodLog_Succulent</li>
				<li>WoodLog_Teak</li>
				<li>WoodLog_Willow</li>
				<li>Bamboo</li>
			</value>
	</Operation>
	
</Patch>
