<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
	    <mods>
            <li>[WD] Expanded Floors</li>
	    </mods>
		<match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/TerrainDef[defName = "WDCheckSterileTile"]/statBases/Beauty</xpath>
                    <value>
                        <Beauty>1</Beauty>
					</value>
                </li>
				<li Class="PatchOperationReplace">
                    <xpath>/Defs/TerrainDef[defName = "WDHexSterileTile"]/statBases/Beauty</xpath>
                    <value>
                        <Beauty>1</Beauty>
					</value>
                </li>
            </operations>
	    </match>
    </Operation>
</Patch>
