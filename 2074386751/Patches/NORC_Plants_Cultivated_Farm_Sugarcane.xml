<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  
  
  <Operation Class="PatchOperationFindMod">
	<mods>
		<li>RimCuisine 2 Core (Continued)</li>
	</mods>
	
	<!-- If you DO NOT have RC2 installed... -->
	
	<nomatch Class="PatchOperationSequence">
	<success>Always</success>
	
	<operations>
	

		<li Class="PatchOperationAdd">
		<xpath>Defs</xpath>
		<value>

	<ThingDef ParentName="PlantBase">
		<defName>PlantSugarcane</defName>
		<label>sugarcane plant</label>
		<description>A high yielding, slow growing plant used to make sugar. Stores extremely well, and animals love eating it.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
            <Nutrition>0.34</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Sugarcane</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<maxMeshCount>4</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>Canestalks</harvestedThingDef>
			<harvestYield>12</harvestYield>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>6</growDays>
			<visualSizeRange>0.3~1.1</visualSizeRange>
		</plant>
	</ThingDef>
		
	
		</value>
		</li>

	</operations>
	
	</nomatch>
  </Operation>
  
  
</Patch>