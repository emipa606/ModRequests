<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  
  
  <Operation Class="PatchOperationFindMod">
	<mods>
		<li>[1.0, 1.1] RimCuisine 2 Core</li>
	</mods>
	
	<!-- If you DO have RC2 installed... -->
	
	<nomatch Class="PatchOperationSequence">
	<success>Always</success>
	
	<operations>
	

		<li Class="PatchOperationAdd">
		<xpath>Defs</xpath>
		<value>

	<ThingDef ParentName="PlantFoodRawBase">
		<defName>Canestalks</defName>
		<label>canestalks</label>
		<description>Sugarcane stalks. The Filtered juice from this plant can be boiled down into sugar.</description>
		<graphicData>
			<texPath>Canestalks</texPath>
		</graphicData>
		<statBases>
			<MarketValue>2</MarketValue>
			<Mass>0.035</Mass>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<foodType>VegetableOrFruit</foodType>
			<optimalityOffsetFeedingAnimals>7</optimalityOffsetFeedingAnimals>
		</ingestible>
	</ThingDef>
		
	
		</value>
		</li>

	</operations>
	
	</nomatch>
  </Operation>
  
  
</Patch>