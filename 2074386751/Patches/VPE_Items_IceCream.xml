<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  
  
  <Operation Class="PatchOperationFindMod">
	<mods>
		<li>Vanilla Plants Expanded</li>
	</mods>
	
	<!-- If you DO have VGP installed... -->
	
	<match Class="PatchOperationSequence">
	<success>Always</success>
	
	<operations>
	

		<li Class="PatchOperationAdd">
		<xpath>Defs</xpath>
		<value>
		
	<!-- BearBerry  START  -->
	<ThingDef ParentName="ResourceBase">
    <defName>BearberryIceCream</defName>
    <label>bearberry ice cream</label>
    <description>The sweet, icy bovice secretion we all know and love.</description>
    <graphicData>
      <texPath>BearberryIceCream</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.88</drawSize>
    </graphicData>
	<equippedAngleOffset>-150</equippedAngleOffset>
    <rotatable>false</rotatable>
    <stackLimit>75</stackLimit>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>5</MarketValue>
      <Mass>0.075</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>0.5</DeteriorationRate>
      <Nutrition>0.7</Nutrition>
    </statBases>
    <tickerType>Rare</tickerType>
    <thingCategories>
      <li>IceCream</li>
    </thingCategories>
    <ingestible>
	  <tasteThought>AteFlavoredIceCream</tasteThought>
      <preferability>RawTasty</preferability>
      <foodType>AnimalProduct</foodType>
      <joy>0.20</joy>
      <joyKind>Gluttonous</joyKind>
      <drugCategory>Social</drugCategory>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <ingestSound>Meal_Eat</ingestSound>
      <chairSearchRadius>25</chairSearchRadius>
	  <ingestHoldOffsetStanding>
        <northDefault>
          <offset>(0.18,0,0)</offset>
        </northDefault>
      </ingestHoldOffsetStanding>
      <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>IceCreamHigh</hediffDef>
          <severity>1</severity>
        </li>
      </outcomeDoers>
    </ingestible>
	<comps>
		<li Class="CompProperties_Rottable">
			<daysToRotStart>1</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		</li>
		<li Class="CompProperties_Drug">
			<listOrder>1</listOrder>
		</li>
	</comps>
	</ThingDef>	
	<!-- END  -->
		
