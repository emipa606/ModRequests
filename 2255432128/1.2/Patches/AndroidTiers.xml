<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
	<mods>
        <li>Android tiers</li>
    </mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "AndroidDog" or defName = "RoboticCow" or defName = "RoboticSheep"]/race[not(packAnimal)]</xpath>
			<value>
				<packAnimal>true</packAnimal>
			</value>
		</match>
	</Operation>
</Patch>