<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ScenarioDef>
    <defName>Marro_Tribe</defName>
    <label>Marro Tribe</label>
    <description>You have split from your hive to form a new one.</description>
	<scenario>
		<name>Marro Tribe</name>
		<summary>Four Marro start a tribe.</summary>
		<playerFaction>
			<def>PlayerFaction</def>
			<factionDef>MarroPlayerTribe</factionDef>
		</playerFaction>
		<parts>
			<li Class="ScenPart_ConfigPage_ConfigureStartingPawns">
				<def>ConfigPage_ConfigureStartingPawns</def>
				<pawnCount>4</pawnCount>
				<pawnChoiceCount>8</pawnChoiceCount>
			</li>
			<li Class="ScenPart_PlayerPawnsArriveMethod">
				<def>PlayerPawnsArriveMethod</def>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>Pemmican</thingDef>
				<count>400</count>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>MedicineHerbal</thingDef>
				<count>32</count>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>WoodLog</thingDef>
				<count>500</count>
			</li>
			<li Class="ScenPart_StartingAnimal">
				<def>StartingAnimal</def>
				<animalKind>Naagrub</animalKind>
				<count>2</count>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>Gun_Biogun</thingDef>
				<count>2</count>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>MeleeWeapon_Impaler</thingDef>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>StartingThing_Defined</def>
				<thingDef>MeleeWeapon_Bone_Blade</thingDef>
			</li>
		</parts>
	</scenario>
  </ScenarioDef>

</Defs>