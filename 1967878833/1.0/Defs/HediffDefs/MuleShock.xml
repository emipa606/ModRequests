<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <HediffDef ParentName="DiseaseBase">
        <defName>MuleShocked</defName>
        <label>shocked</label>
        <labelNoun>being shocked</labelNoun>
        <lethalSeverity>1</lethalSeverity>
        <isBad>false</isBad>
        <comps>
            <li Class="HediffCompProperties_SeverityPerDay">
                <severityPerDay>-3.7</severityPerDay>
            </li>
        </comps>
        <injuryProps>
            <destroyedLabel>Euthanized</destroyedLabel>
            <destroyedOutLabel>Euthanized</destroyedOutLabel>
        </injuryProps>
        <stages>
            <li>
                <minSeverity>0.1</minSeverity>
                <label>minor</label>
                <painFactor>1.15</painFactor>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.85</setMax>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>0.35</minSeverity>
                <label>moderate</label>
                <painFactor>1.35</painFactor>
                <vomitMtbDays>0.01</vomitMtbDays>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.75</setMax>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>0.65</minSeverity>
                <label>severe</label>
                <painFactor>1.9</painFactor>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.65</setMax>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>0.9</minSeverity>
                <label>extreme</label>
                <painFactor>2.5</painFactor>
                <lifeThreatening>true</lifeThreatening>
                <deathMtbDays>5.2</deathMtbDays>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.5</setMax>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>1</minSeverity>
                <label>fatal</label>
                <painFactor>4</painFactor>
            </li>
        </stages>
    </HediffDef>
</Defs>