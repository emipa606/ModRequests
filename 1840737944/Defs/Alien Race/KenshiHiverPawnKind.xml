<!--Backstories.................................................................................-->

<AlienRace.BackstoryDef>
    <defName>HiveStandardBackground</defName>
    <title>Hiveborn</title>
    <titleShort>Hiveborn</titleShort>
    <baseDescription>This hiver was born from a Hive Queen's dispenser unit, and raised as part of its hive mind. However, the hivemind lost its grip, and this hiver found itself hiveless, driven out by its brethren.</baseDescription>
    <slot>Childhood</slot>
    <spawnCategories>
      <li>HiveBackstoryFree</li>
    </spawnCategories>
    <bodyTypeGlobal>Hulk</bodyTypeGlobal>
</AlienRace.BackstoryDef>

<AlienRace.BackstoryDef>
    <defName>HiveStandardBackgroundAdult</defName>
    <title>Aimless</title>
    <titleShort>Aimless</titleShort>
    <baseDescription>This hiver was chased out of its hive, and wandered aimlessly for some time.</baseDescription>
    <slot>Adulthood</slot>
    <spawnCategories>
      <li>HiveBackstoryFree</li>
    </spawnCategories>
    <bodyTypeGlobal>Hulk</bodyTypeGlobal>
</AlienRace.BackstoryDef>

<AlienRace.BackstoryDef>
    <defName>HiveStandardBackgroundAdult1</defName>
    <title>Escaped Slave</title>
    <titleShort>EscapedSlave</titleShort>
    <baseDescription>This hiver was captured and made a slave. However, it escaped and went into hiding.</baseDescription>
    <slot>Adulthood</slot>
    <spawnCategories>
      <li>HiveBackstoryFree</li>
    </spawnCategories>
    <bodyTypeGlobal>Hulk</bodyTypeGlobal>
        <skillGains>
      <li>
        <defName>Social</defName>
        <amount>-3</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>4</amount>
      </li>
    </skillGains>
</AlienRace.BackstoryDef>

