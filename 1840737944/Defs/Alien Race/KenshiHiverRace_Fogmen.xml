<Defs>
<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Kenshi_Fogman_Prince</defName>
    <label>fogman prince</label>
    
    <description>A stick-like humanoid driven mad by a parasite.</description>
        
    <statBases>
      <PsychicSensitivity>1.0</PsychicSensitivity>
      <ToxicSensitivity>1.0</ToxicSensitivity>
      <ComfyTemperatureMin>12</ComfyTemperatureMin>
      <ComfyTemperatureMax>52</ComfyTemperatureMax>
      <LeatherAmount>30</LeatherAmount>
      <MarketValue>1000</MarketValue>
      <MoveSpeed>4.61</MoveSpeed>
      <Flammability>1.0</Flammability>
      <CarryingCapacity>75</CarryingCapacity>
      <MentalBreakThreshold>0.01</MentalBreakThreshold>
      <ResearchSpeed>1.0</ResearchSpeed>
      <MiningSpeed>1.0</MiningSpeed>
      <SocialImpact>0.8</SocialImpact>
      <PlantWorkSpeed>1.0</PlantWorkSpeed>
      <PlantHarvestYield>1.0</PlantHarvestYield>
      <ConstructionSpeed>1.0</ConstructionSpeed>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.15</ArmorRating_Sharp>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>

<alienRace>

<generalSettings>
    <humanRecipeImport>true</humanRecipeImport>
    <alienPartGenerator>
    <alienbodytypes>
      <li>Male</li>
    </alienbodytypes>
    <useGenderedHeads>false</useGenderedHeads>
    <useGenderedBodies>false</useGenderedBodies>
    <alienskincolorgen Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>10</weight>
                    <only>RGBA(1.0,1.0,1.0,1)</only>
                </li>
            </options>
    </alienskincolorgen>
    </alienPartGenerator>
    
    <immuneToAge>true</immuneToAge>

    <factionRelations>
	<li>
                    <factions>
                       <li>WesternHive</li>
                    </factions>
                    <goodwill>
                       <min>100</min>
                       <max>100</max>
                    </goodwill>
        </li>              
    </factionRelations>

    <maleGenderProbability>1.0</maleGenderProbability>

    <chemicalSettings>
    </chemicalSettings>
    




</generalSettings>    


<graphicPaths>
    <li>
	<body>Races/Hiver_Fogmen/Body/</body>
        <head>Races/Hiver_Fogmen/Head_Prince/</head>
    </li>
</graphicPaths>


<hairSettings>
