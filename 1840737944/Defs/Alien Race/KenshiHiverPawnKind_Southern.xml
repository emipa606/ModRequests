  <PawnKindDef>
    <defName>HiverPlayerPrinceSouthern</defName>
    <race>Kenshi_Hiver_Prince_Southern</race>
    <combatPower>30</combatPower>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
    <label>hiver </label>
    <defaultFactionType>PlayerTribe</defaultFactionType>
    <!--backstoryCategories>
      <li>HiveBackstoryFree</li>
    </backstoryCategories-->
        <backstoryFiltersOverride>
        <li>
            <categories>
                <li>HiveBackstoryFree</li>
            </categories>
        </li>
    </backstoryFiltersOverride>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <apparelTags>
      <li>Neolithic</li>
    </apparelTags>
    <apparelMoney>
      <min>0</min>
      <max>200</max>
    </apparelMoney>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <techHediffsMoney>
      <min>50</min>
      <max>50</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <techHediffsChance>0.02</techHediffsChance>

    <initialWillRange>0~0</initialWillRange>
    <initialResistanceRange>9~15</initialResistanceRange>

  </PawnKindDef>

