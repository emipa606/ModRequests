<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="BuildingBase">
    <defName>SL_TungstenTableLamp</defName>
    <label>tungsten table lamp</label>
    <description>An incandescent table lamp that gives a warm glow to it's surrounding. Tungsten lamp requires more power and produces small amount of heat. (Power usage 25W)</description>
    <designatorDropdown>TableLamp</designatorDropdown>
    <category>Building</category>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Lights/TableLamp_Tungsten/SteamLight_TableLamp_Tungsten</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawRotated>false</drawRotated>
      <drawSize>(2,2)</drawSize>
      <shaderType>CutoutComplex</shaderType>
      <damageData>
        <rect>(0.35,0.35,0.3,0.3)</rect>
      </damageData>
    </graphicData>
    <uiIconPath>Lights/TableLamp_Tungsten/SteamLight_TableLamp_Tungsten</uiIconPath>
    <uiIconScale>1.2</uiIconScale>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>Standable</passability>
    <constructEffect>ConstructWood</constructEffect>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0</fillPercent>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Flammability>0</Flammability>
      <Mass>1</Mass>
      <Beauty>5</Beauty>
    </statBases>
    <selectable>true</selectable>
    <costList>
      <Steel>30</Steel>
    </costList>
    <rotatable>true</rotatable>
    <building>
      <isEdifice>false</isEdifice>
    </building>
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <comps>
      <li Class="CompProperties_Flickable">
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>5</glowRadius>
        <glowColor>(255,182,88,0)</glowColor>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>25</basePowerConsumption>
      </li>
      <li Class="CompProperties_HeatPusher">
        <compClass>CompHeatPusherPowered</compClass>
        <heatPerSecond>1</heatPerSecond>
      </li>
    </comps>
    <defaultPlacingRot>South</defaultPlacingRot>
    <designationCategory>Furniture</designationCategory>
    <destroyable>true</destroyable>
    <tradeability>None</tradeability>
    <clearBuildingArea>false</clearBuildingArea>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
  </ThingDef>


