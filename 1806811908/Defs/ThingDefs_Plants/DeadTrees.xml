<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="TreeBase">
    <defName>Plant_Ginkgo</defName>
    <label>ginkgo tree</label>
    <description>A prehistoric breed of tree which, even though rare nowadays, survived since the mesozoic. Their leaves make a healthy meal for herbivorous animals but are worthless for human use.</description>
    <graphicData>
      <texPath>Things/Plant/Ginkgo</texPath>
      <graphicClass>Graphic_Random</graphicClass>
        <shadowData>
          <volume>(0.15, 0.3, 0.15)</volume>
          <offset>(0,0,-0.38)</offset>
        </shadowData>
    </graphicData>
    <selectable>true</selectable>
    <statBases>
      <Beauty>2</Beauty>
      <Nutrition>1.25</Nutrition>
    </statBases>
    <ingestible />
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<growDays>16</growDays>
			<visualSizeRange>
        <min>0.8</min>
        <max>2.0</max>
      </visualSizeRange>
			<sowMinSkill>8</sowMinSkill>
      <sowTags>
				<li>Ginkgo</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>Cocoa</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.15</topWindExposure>
      <mustBeWildToSow>false</mustBeWildToSow>
      <immatureGraphicPath>Things/Plant/Ginkgo</immatureGraphicPath>
      <harvestWork>400</harvestWork>
			<harvestTag>Standard</harvestTag>
      <harvestYield>50</harvestYield>
      <harvestedThingDef>GinkgoLeaves</harvestedThingDef>
      <harvestFailable>true</harvestFailable>
    </plant>
  </ThingDef>
  
</Defs>