<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>PangaeaPachycephalosaurus</defName>
    <label>Pachycephalosaurus</label>
    <description>Pachycephalosaurus Wyomingnesis. A pachycephalosaurid from the USA. Despite being herbivores they are very aggressive and live up to their name. They have thick domes on their skulls. Male use those for dominance fights, however, it is more than enough to break some human bones.</description>
    <statBases>
      <MoveSpeed>5.1</MoveSpeed>
      <ComfyTemperatureMin>-5</ComfyTemperatureMin>
      <MarketValue>1000</MarketValue>
      <ComfyTemperatureMax>80</ComfyTemperatureMax>
    </statBases>
	<comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggPangaeaPachycephalosaurusFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>17</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1</eggCountRange>
      </li>
    </comps>
    <tools>
      <li>
        <label>Leg</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RearLeftLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>Leg</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RearRightLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>Tyrannosaurid</body>
      <herdAnimal>true</herdAnimal>
      <packAnimal>false</packAnimal>
      <baseBodySize>5</baseBodySize>
			<baseHealthScale>5</baseHealthScale>
			<baseHungerRate>5</baseHungerRate>
			<foodType>VegetarianRoughAnimal</foodType>
      <leatherDef>Leather_Dino</leatherDef>
      <wildness>0.65</wildness>
      <manhunterOnTameFailChance>0</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0</manhunterOnDamageChance>
      <gestationPeriodDays>25</gestationPeriodDays>
      <lifeExpectancy>75</lifeExpectancy>
	  <meatLabel>dinosaur flesh</meatLabel>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.2</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.45</minAge>
          <soundWounded>Pawn_Elk_Wounded</soundWounded>
          <soundDeath>Pawn_Elk_Death</soundDeath>
          <soundCall>Pawn_Elk_Call</soundCall>
          <soundAngry>Pawn_Elk_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
  </ThingDef>

