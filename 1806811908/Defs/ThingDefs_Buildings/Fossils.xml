<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=============== Cretaceous ====================-->

  <ThingDef Name="BuildingNaturalBase" Abstract="True">
    <category>Building</category>
    <selectable>true</selectable>
    <drawerType>MapMeshOnly</drawerType>
    <filthLeaving>Filth_RubbleRock</filthLeaving>
		<scatterableOnMapGen>false</scatterableOnMapGen>
	</ThingDef>
	
   <ThingDef ParentName="RockBase">
    <defName>CretaceousFossil</defName>
    <label>Cretaceous fossils</label>
    <description>The remains of prehistoric animals from the Cretaceous Period. Mine it to learn more.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(126,104,94)</color>
      <colorTwo>(70,70,70)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableScatterCommonality>0.3</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>1</min>
        <max>15</max>
      </mineableScatterLumpSizeRange>
    </building>
	<modExtensions>
			<li Class="RandomOreDrop.ModExt_Building_Mineable">
				<overallDropChance>0.875</overallDropChance>
				<mineables>
					<li>
						<thing>GallimimusFossil</thing>
						<commonality>0.5</commonality>
						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>TriceratopsFossil</thing>
						<commonality>0.5</commonality>
						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>ParasaurolophusFossil</thing>
						<commonality>0.5</commonality>
						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>TyrannosaurusFossil</thing>
						<commonality>0.5</commonality>
						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>VelociraptorFossil</thing>
						<commonality>0.5</commonality>
