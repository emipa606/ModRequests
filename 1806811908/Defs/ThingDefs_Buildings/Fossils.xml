						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>PachycephalosaurusFossil</thing>
						<commonality>0.5</commonality>
						<yield>
							<min>2</min>
							<max>14</max>
						</yield>
					</li>
					<li>
						<thing>SpinosaurusFossil</thing>
