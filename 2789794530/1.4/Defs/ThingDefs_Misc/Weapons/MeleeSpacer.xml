<Defs>
<!-- ===========================================================================Spacer Spear=========================================================================== -->
  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>ATM_MeleeWeapon_SpacerSpear</defName>
    <label>Spacer spear</label>
    <description>This spear uses space technologies to make it out of this world.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/WeaponMelee/Spacer_Spear</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <techLevel>Spacer</techLevel>
    <weaponTags>
		<li>SpacerGun</li>
		<li>Spear</li>
	</weaponTags>
    <costStuffCount>300</costStuffCount>
	<costList>
		<ComponentIndustrial>10</ComponentIndustrial>
		<ComponentSpacer>5</ComponentSpacer>
	</costList>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <statBases>
	<WorkToMake>40000</WorkToMake>
		<Mass>2.5</Mass>
		<MarketValue>1500</MarketValue>
	</statBases>
    <equippedAngleOffset>-25</equippedAngleOffset>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <tools>
      <li>
        <label>shaft</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>18</power>
        <cooldownTime>2.6</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>33.5</power>
        <armorPenetration>0.75</armorPenetration>
        <cooldownTime>2.6</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
      <researchPrerequisite>ATM_SpacerMelee</researchPrerequisite>
      <skillRequirements>
        <Crafting>10</Crafting>
      </skillRequirements>
	  <recipeUsers Inherit="False">
			<li>FabricationBench</li>
	  </recipeUsers>
    </recipeMaker>
  </ThingDef>
    
