<Defs>
<!-- ===========================================================================Industrial Spear=========================================================================== -->
  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>ATM_MeleeWeapon_IndustrialSpear</defName>
    <label>Industrial spear</label>
    <description>This spear uses industrial technologies to make the edge razor sharp. It's like holding a massive scalpel.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/WeaponMelee/Industrial_Spear</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <techLevel>Industrial</techLevel>
    <weaponTags>
		<li>IndustrialGunAdvanced</li>
		<li>Spear</li>
	</weaponTags>
    <costStuffCount>150</costStuffCount>
	<costList>
		<ComponentIndustrial>5</ComponentIndustrial>
	</costList>
    <thingSetMakerTags><li>RewardStandardQualitySuper</li></thingSetMakerTags>
    <statBases>
	<WorkToMake>20000</WorkToMake>
		<Mass>2.5</Mass>
		<MarketValue>750</MarketValue>
		</statBases>
    <equippedAngleOffset>-25</equippedAngleOffset>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <tools>
      <li>
        <label>shaft</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2.6</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>27.5</power>
        <armorPenetration>0.60</armorPenetration>
        <cooldownTime>2.6</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
      <researchPrerequisite>ATM_IndustrialMelee</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
	  <recipeUsers Inherit="False">
			<li>TableMachining</li>
	  </recipeUsers>
    </recipeMaker>
  </ThingDef>
    
