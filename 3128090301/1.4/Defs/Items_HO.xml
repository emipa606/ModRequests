  <!-- ================ ET_Liquid_Ether_DRY ================ -->
  <ThingDef ParentName="ETDrugBase">
    <defName>ET_Liquid_Ether_DRY</defName>
    <label>ET.Liquid Ether (DRY)</label>
    <description>Mysterious liquid secreted by Naiad (undiluted solution)\nIt has been confirmed that when taken orally into the body, it activates the secretion of oxytocin, but the principle behind this has not been elucidated.</description>
    <descriptionHyperlinks>
      <ThingDef>Naiad</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Liquid_Ether_DRY</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <rotatable>false</rotatable>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <MarketValue>0</MarketValue>
      <Mass>0</Mass>
      <Beauty>10</Beauty>
      <Flammability>0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
    </statBases>
    <techLevel>Ultra</techLevel>
    <burnableByRecipe>false</burnableByRecipe>
    <thingCategories>
      <li>ET</li>
    </thingCategories>
    <ingestible>
      <joyKind>Meditative</joyKind>
      <joy>1.0</joy>
      <foodType>Fluid</foodType>
      <baseIngestTicks>80</baseIngestTicks>
      <ingestSound>Ingest_Inject</ingestSound>
      <ingestEffect>Smoke_Flake</ingestEffect>
      <ingestHoldOffsetStanding>
        <northDefault>
          <offset>(0.18,0,0)</offset>
        </northDefault>
      </ingestHoldOffsetStanding>
      <ingestCommandString>Inject {0}</ingestCommandString>
      <ingestReportString>Injecting {0}.</ingestReportString>
      <useEatingSpeedStat>false</useEatingSpeedStat>
      <chairSearchRadius>0</chairSearchRadius>
      <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>Oxytocin_rush</hediffDef>
          <severity>1.00</severity>
          <doToGeneratedPawnIfAddicted>false</doToGeneratedPawnIfAddicted>
        </li>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>E_Climax</hediffDef>
          <severity>1.00</severity>
          <doToGeneratedPawnIfAddicted>false</doToGeneratedPawnIfAddicted>
        </li>
      </outcomeDoers>
    </ingestible>
    <tradeability>None</tradeability>
    <stackLimit>200</stackLimit>
    <allowedArchonexusCount>0</allowedArchonexusCount>
  </ThingDef>

  <HediffDef>
    <defName>Oxytocin_rush</defName>
    <hediffClass>Hediff_High</hediffClass>
    <label>oxytocin rush</label>
    <description>A condition in which oxytocin is secreted in excess.</description>
    <defaultLabelColor>(1,1,0.5)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <isBad>false</isBad>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-1</severityPerDay>
        <showHoursToRecover>true</showHoursToRecover>
      </li>
    </comps>
    <stages>
      <li><capMods><li><capacity>Talking</capacity><offset>0.4</offset></li></capMods></li>
    </stages>
  </HediffDef>
  
  <HediffDef ParentName="DiseaseBase">
    <defName>E_Climax</defName>
    <label>hallucinatory sleep</label>
    <labelNoun>a hallucinatory sleep</labelNoun>
    <description>Reaching climax in a pleasant hallucinatory sleep.</description>
    <defaultLabelColor>(1,0,0.5)</defaultLabelColor>
    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>5000~7500</disappearsAfterTicks>
        <showRemainingTime>true</showRemainingTime>
      </li>
    </comps>
    <stages>
      <li><capMods><li><capacity>Consciousness</capacity><setMax>0.1</setMax></li></capMods></li>
    </stages>
  </HediffDef>

  <ThoughtDef>
    <defName>I_orgasm</defName>
    <workerClass>ThoughtWorker_Hediff</workerClass>
    <hediff>Oxytocin_rush</hediff>
    <validWhileDespawned>true</validWhileDespawned>
    <stages>
      <li>
        <label>orgasm</label>
        <description>feels good...</description>
        <baseMoodEffect>100</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

