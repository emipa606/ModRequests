<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!--==============　PsychoCaramel　==============-->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName = "PsychoCaramel"]/costList</xpath>
    <value>
      <costList>
        <DryadPetal>4</DryadPetal>
        <PsychoidLeaves>1</PsychoidLeaves>
        <NymphMilk>2</NymphMilk>      
    </costList>
    </value>
  </Operation>

  <!--==============　ApparelDryadBase add recipeUsers　==============-->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[@Name = "ApparelDryadBase"]/recipeMaker/recipeUsers</xpath>
    <value>
      <recipeUsers>
        <li>FabricationBench</li>
        <li>ET_Manipulator</li>
      </recipeUsers>
    </value>
  </Operation>
  
</Patch>