<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <FactionDef ParentName="PlayerFactionBase">
    <defName>VFEA_NewVault</defName>
    <label>New Vault</label>
    <description>A vault of ancient soldiers.</description>
    <isPlayer>true</isPlayer>
    <basicMemberKind>VFEA_PlayerAncientSoldierOneAbility</basicMemberKind>
    <pawnSingular>ancient soldier</pawnSingular>
    <pawnsPlural>ancient soldiers</pawnsPlural>
    <techLevel>Spacer</techLevel>
    <factionNameMaker>NamerFactionOutlander</factionNameMaker>
