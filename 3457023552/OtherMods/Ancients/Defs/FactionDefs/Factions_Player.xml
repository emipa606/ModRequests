      <li>Astropolitan</li>
    </allowedCultures>
    <requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">VFEA_Isolationist</li>
    </requiredMemes>
    <backstoryFilters>
      <li>
        <categories>
          <li>Ancient_Pre</li>
        </categories>
        <commonality>0.6</commonality>
      </li>
	  <li>
        <categories>
          <li>Ancient_Elite</li>
        </categories>
        <commonality>0.2</commonality>
      </li>
    </backstoryFilters>
    <factionIconPath>World/WorldObjects/Expanding/PlayerVault</factionIconPath>
    <settlementTexturePath>World/WorldObjects/PlayerVault</settlementTexturePath>
    <startingResearchTags>
      <li>ClassicStart</li>
    </startingResearchTags>
    <startingTechprintsResearchTags>
      <li>ClassicStart</li>
      <li>ClassicStartTechprints</li>
    </startingTechprintsResearchTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
        <li>Hyperweave</li>
      </thingDefs>
    </apparelStuffFilter>
